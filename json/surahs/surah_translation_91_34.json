{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":91,"VerseNumber":1,"Text":"والشمس وضحىها"},{"SurahNumber":91,"VerseNumber":2,"Text":"والقمر اذا تلىها"},{"SurahNumber":91,"VerseNumber":3,"Text":"والنهار اذا جلىها"},{"SurahNumber":91,"VerseNumber":4,"Text":"واليل اذا يغشىها"},{"SurahNumber":91,"VerseNumber":5,"Text":"والسماء وما بنىها"},{"SurahNumber":91,"VerseNumber":6,"Text":"والارض وما طحىها"},{"SurahNumber":91,"VerseNumber":7,"Text":"ونفس وما سوىها"},{"SurahNumber":91,"VerseNumber":8,"Text":"فالهمها فجورها وتقوىها"},{"SurahNumber":91,"VerseNumber":9,"Text":"قد افلح من زكىها"},{"SurahNumber":91,"VerseNumber":10,"Text":"وقد خاب من دسىها"},{"SurahNumber":91,"VerseNumber":11,"Text":"كذبت ثمود بطغوىها"},{"SurahNumber":91,"VerseNumber":12,"Text":"اذ انبعث اشقىها"},{"SurahNumber":91,"VerseNumber":13,"Text":"فقال لهم رسول الله ناقة الله وسقيها"},{"SurahNumber":91,"VerseNumber":14,"Text":"فكذبوه فعقروها فدمدم عليهم ربهم بذنبهم فسوىها"},{"SurahNumber":91,"VerseNumber":15,"Text":"ولا يخاف عقبها"}]}