{"Id":419,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"veẕẕâriyâti ẕervâ"},{"SurahNumber":51,"VerseNumber":2,"Text":"felḥâmilâti viḳrâ"},{"SurahNumber":51,"VerseNumber":3,"Text":"felcâriyâti yüsrâ"},{"SurahNumber":51,"VerseNumber":4,"Text":"felmüḳassimâti emrâ"},{"SurahNumber":51,"VerseNumber":5,"Text":"innemâ tû`adûne leṣâdiḳ"},{"SurahNumber":51,"VerseNumber":6,"Text":"veinne-ddîne levâḳi"},{"SurahNumber":51,"VerseNumber":7,"Text":"vessemâi ẕâti-lḥubük"},{"SurahNumber":51,"VerseNumber":8,"Text":"inneküm lefî ḳavlim muḫtelif"},{"SurahNumber":51,"VerseNumber":9,"Text":"yü'fekü `anhü men üfik"},{"SurahNumber":51,"VerseNumber":10,"Text":"ḳutile-lḫarrâṣûn"},{"SurahNumber":51,"VerseNumber":11,"Text":"elleẕîne hüm fî gamratin sâhûn"},{"SurahNumber":51,"VerseNumber":12,"Text":"yes'elûne eyyâne yevmü-ddîn"},{"SurahNumber":51,"VerseNumber":13,"Text":"yevme hüm `ale-nnâri yüftenûn"},{"SurahNumber":51,"VerseNumber":14,"Text":"ẕûḳû fitneteküm. hâẕe-lleẕî küntüm bihî testa`cilûn"},{"SurahNumber":51,"VerseNumber":15,"Text":"inne-lmütteḳîne fî cennâtiv ve`uyûn"},{"SurahNumber":51,"VerseNumber":16,"Text":"âḫiẕîne mâ âtâhüm rabbühüm. innehüm kânû ḳable ẕâlike muḥsinîn"},{"SurahNumber":51,"VerseNumber":17,"Text":"kânû ḳalîlem mine-lleyli mâ yehce`ûn"},{"SurahNumber":51,"VerseNumber":18,"Text":"vebil'esḥâri hüm yestagfirûn"},{"SurahNumber":51,"VerseNumber":19,"Text":"vefî emvâlihim ḥaḳḳul lissâili velmaḥrûm"},{"SurahNumber":51,"VerseNumber":20,"Text":"vefi-l'arḍi âyâtül lilmûḳinîn"},{"SurahNumber":51,"VerseNumber":21,"Text":"vefî enfüsiküm. efelâ tübṣirûn"},{"SurahNumber":51,"VerseNumber":22,"Text":"vefi-ssemâi rizḳuküm vemâ tû`adûn"},{"SurahNumber":51,"VerseNumber":23,"Text":"feverabbi-ssemâi vel'arḍi innehû leḥaḳḳum miŝle mâ enneküm tenṭiḳûn"},{"SurahNumber":51,"VerseNumber":24,"Text":"hel etâke ḥadîŝü ḍayfi ibrâhîme-lmükramîn"},{"SurahNumber":51,"VerseNumber":25,"Text":"iẕ deḫalû `aleyhi feḳâlû selâmâ. ḳâle selâm. ḳavmüm münkerûn"},{"SurahNumber":51,"VerseNumber":26,"Text":"ferâga ilâ ehlihî fecâe bi`iclin semîn"},{"SurahNumber":51,"VerseNumber":27,"Text":"feḳarrabehû ileyhim ḳâle elâ te'külûn"},{"SurahNumber":51,"VerseNumber":28,"Text":"feevcese minhüm ḫîfeh. ḳâlû lâ teḫaf. vebeşşerûhü bigulâmin `alîm"},{"SurahNumber":51,"VerseNumber":29,"Text":"feaḳbeleti-mraetühû fî ṣarratin feṣakket vechehâ veḳâlet `acûzün `aḳîm"},{"SurahNumber":51,"VerseNumber":30,"Text":"ḳâlû keẕâliki ḳâle rabbük. innehû hüve-lḥakîmü-l`alîm"},{"SurahNumber":51,"VerseNumber":31,"Text":"ḳâle femâ ḫaṭbüküm eyyühe-lmürselûn"},{"SurahNumber":51,"VerseNumber":32,"Text":"ḳâlû innâ ürsilnâ ilâ ḳavmim mücrimîn"},{"SurahNumber":51,"VerseNumber":33,"Text":"linürsile `aleyhim ḥicâratem min ṭîn"},{"SurahNumber":51,"VerseNumber":34,"Text":"müsevvemeten `inde rabbike lilmüsrifîn"},{"SurahNumber":51,"VerseNumber":35,"Text":"feaḫracnâ men kâne fîhâ mine-lmü'minîn"},{"SurahNumber":51,"VerseNumber":36,"Text":"femâ vecednâ fîhâ gayra beytim mine-lmüslimîn"},{"SurahNumber":51,"VerseNumber":37,"Text":"veteraknâ fîhâ âyetel lilleẕîne yeḫâfûne-l`aẕâbe-l'elîm"},{"SurahNumber":51,"VerseNumber":38,"Text":"vefî mûsâ iẕ erselnâhü ilâ fir`avne bisülṭânim mübîn"},{"SurahNumber":51,"VerseNumber":39,"Text":"fetevellâ biruknihî veḳâle sâḥirun ev mecnûn"},{"SurahNumber":51,"VerseNumber":40,"Text":"feeḫaẕnâhü vecünûdehû fenebeẕnâhüm fi-lyemmi vehüve mülîm"},{"SurahNumber":51,"VerseNumber":41,"Text":"vefî `âdin iẕ erselnâ `aleyhimü-rrîḥa-l`aḳîm"},{"SurahNumber":51,"VerseNumber":42,"Text":"mâ teẕeru min şey'in etet `aleyhi illâ ce`alethü kelramîm"},{"SurahNumber":51,"VerseNumber":43,"Text":"vefî ŝemûde iẕ ḳîle lehüm temette`û ḥattâ ḥîn"},{"SurahNumber":51,"VerseNumber":44,"Text":"fe`atev `an emri rabbihim feeḫaẕethümu-ṣṣâ`iḳatü vehüm yenżurûn"},{"SurahNumber":51,"VerseNumber":45,"Text":"feme-steṭâ`û min ḳiyâmiv vemâ kânû münteṣirîn"},{"SurahNumber":51,"VerseNumber":46,"Text":"veḳavme nûḥim min ḳabl. innehüm kânû ḳavmen fâsiḳîn"},{"SurahNumber":51,"VerseNumber":47,"Text":"vessemâe beneynâhâ bieydiv veinnâ lemûsi`ûn"},{"SurahNumber":51,"VerseNumber":48,"Text":"vel'arḍa feraşnâhâ feni`me-lmâhidûn"},{"SurahNumber":51,"VerseNumber":49,"Text":"vemin külli şey'in ḫalaḳnâ zevceyni le`alleküm teẕekkerûn"},{"SurahNumber":51,"VerseNumber":50,"Text":"fefirrû ile-llâh. innî leküm minhü neẕîrum mübîn"},{"SurahNumber":51,"VerseNumber":51,"Text":"velâ tec`alû me`a-llâhi ilâhen âḫar. innî leküm minhü neẕîrum mübîn"},{"SurahNumber":51,"VerseNumber":52,"Text":"keẕâlike mâ ete-lleẕîne min ḳablihim mir rasûlin illâ ḳâlû sâḥirun ev mecnûn"},{"SurahNumber":51,"VerseNumber":53,"Text":"etevâṣav bih. bel hüm ḳavmün ṭâgûn"},{"SurahNumber":51,"VerseNumber":54,"Text":"fetevelle `anhüm femâ ente bimelûm"},{"SurahNumber":51,"VerseNumber":55,"Text":"veẕekkir feinne-ẕẕikrâ tenfe`u-lmü'minîn"},{"SurahNumber":51,"VerseNumber":56,"Text":"vemâ ḫalaḳtü-lcinne vel'inse illâ liya`büdûn"},{"SurahNumber":51,"VerseNumber":57,"Text":"mâ ürîdü minhüm mir rizḳiv vemâ ürîdü ey yuṭ`imûn"},{"SurahNumber":51,"VerseNumber":58,"Text":"inne-llâhe hüve-rrazzâḳu ẕü-lḳuvveti-lmetîn"},{"SurahNumber":51,"VerseNumber":59,"Text":"feinne lilleẕîne żalemû ẕenûbem miŝle ẕenûbi aṣḥâbihim felâ yesta`cilûn"},{"SurahNumber":51,"VerseNumber":60,"Text":"feveylül lilleẕîne keferû miy yevmihimü-lleẕî yû`adûn"}]}