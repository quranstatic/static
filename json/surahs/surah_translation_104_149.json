{"Id":149,"LanguageCodes":["eo","epo"],"Name":"epo-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Esperanto","Description":"Esperanto","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":104,"VerseNumber":1,"Text":"Ve al cxiu backbiter slanderer"},{"SurahNumber":104,"VerseNumber":2,"Text":"Li hoards mon counts gxi"},{"SurahNumber":104,"VerseNumber":3,"Text":"As se his mon far him immortal"},{"SurahNumber":104,"VerseNumber":4,"Text":"Neniam; li jxet Devastator"},{"SurahNumber":104,"VerseNumber":5,"Text":"Vi kon ki Devastator Do est"},{"SurahNumber":104,"VerseNumber":6,"Text":"Di blazing Hellfire"},{"SurahNumber":104,"VerseNumber":7,"Text":"Gxi brul them inside out"},{"SurahNumber":104,"VerseNumber":8,"Text":"Ili confined therein"},{"SurahNumber":104,"VerseNumber":9,"Text":"En etend kolon"}]}