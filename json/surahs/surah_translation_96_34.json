{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":96,"VerseNumber":1,"Text":"اقرا باسم ربك الذي خلق"},{"SurahNumber":96,"VerseNumber":2,"Text":"خلق الانسن من علق"},{"SurahNumber":96,"VerseNumber":3,"Text":"اقرا وربك الاكرم"},{"SurahNumber":96,"VerseNumber":4,"Text":"الذي علم بالقلم"},{"SurahNumber":96,"VerseNumber":5,"Text":"علم الانسن ما لم يعلم"},{"SurahNumber":96,"VerseNumber":6,"Text":"كلا ان الانسن ليطغى"},{"SurahNumber":96,"VerseNumber":7,"Text":"ان رءاه استغنى"},{"SurahNumber":96,"VerseNumber":8,"Text":"ان الى ربك الرجعى"},{"SurahNumber":96,"VerseNumber":9,"Text":"ارءيت الذي ينهى"},{"SurahNumber":96,"VerseNumber":10,"Text":"عبدا اذا صلى"},{"SurahNumber":96,"VerseNumber":11,"Text":"ارءيت ان كان على الهدى"},{"SurahNumber":96,"VerseNumber":12,"Text":"او امر بالتقوى"},{"SurahNumber":96,"VerseNumber":13,"Text":"ارءيت ان كذب وتولى"},{"SurahNumber":96,"VerseNumber":14,"Text":"الم يعلم بان الله يرى"},{"SurahNumber":96,"VerseNumber":15,"Text":"كلا لين لم ينته لنسفعا بالناصية"},{"SurahNumber":96,"VerseNumber":16,"Text":"ناصية كذبة خاطية"},{"SurahNumber":96,"VerseNumber":17,"Text":"فليدع ناديه"},{"SurahNumber":96,"VerseNumber":18,"Text":"سندع الزبانية"},{"SurahNumber":96,"VerseNumber":19,"Text":"كلا لا تطعه واسجد واقترب۩"}]}