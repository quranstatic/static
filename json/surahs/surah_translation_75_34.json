{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":75,"VerseNumber":1,"Text":"لا اقسم بيوم القيمة"},{"SurahNumber":75,"VerseNumber":2,"Text":"ولا اقسم بالنفس اللوامة"},{"SurahNumber":75,"VerseNumber":3,"Text":"ايحسب الانسن الن نجمع عظامه"},{"SurahNumber":75,"VerseNumber":4,"Text":"بلى قدرين على ان نسوي بنانه"},{"SurahNumber":75,"VerseNumber":5,"Text":"بل يريد الانسن ليفجر امامه"},{"SurahNumber":75,"VerseNumber":6,"Text":"يسل ايان يوم القيمة"},{"SurahNumber":75,"VerseNumber":7,"Text":"فاذا برق البصر"},{"SurahNumber":75,"VerseNumber":8,"Text":"وخسف القمر"},{"SurahNumber":75,"VerseNumber":9,"Text":"وجمع الشمس والقمر"},{"SurahNumber":75,"VerseNumber":10,"Text":"يقول الانسن يوميذ اين المفر"},{"SurahNumber":75,"VerseNumber":11,"Text":"كلا لا وزر"},{"SurahNumber":75,"VerseNumber":12,"Text":"الى ربك يوميذ المستقر"},{"SurahNumber":75,"VerseNumber":13,"Text":"ينبوا الانسن يوميذ بما قدم واخر"},{"SurahNumber":75,"VerseNumber":14,"Text":"بل الانسن على نفسه بصيرة"},{"SurahNumber":75,"VerseNumber":15,"Text":"ولو القى معاذيره"},{"SurahNumber":75,"VerseNumber":16,"Text":"لا تحرك به لسانك لتعجل به"},{"SurahNumber":75,"VerseNumber":17,"Text":"ان علينا جمعه وقرءانه"},{"SurahNumber":75,"VerseNumber":18,"Text":"فاذا قرانه فاتبع قرءانه"},{"SurahNumber":75,"VerseNumber":19,"Text":"ثم ان علينا بيانه"},{"SurahNumber":75,"VerseNumber":20,"Text":"كلا بل تحبون العاجلة"},{"SurahNumber":75,"VerseNumber":21,"Text":"وتذرون الاخرة"},{"SurahNumber":75,"VerseNumber":22,"Text":"وجوه يوميذ ناضرة"},{"SurahNumber":75,"VerseNumber":23,"Text":"الى ربها ناظرة"},{"SurahNumber":75,"VerseNumber":24,"Text":"ووجوه يوميذ باسرة"},{"SurahNumber":75,"VerseNumber":25,"Text":"تظن ان يفعل بها فاقرة"},{"SurahNumber":75,"VerseNumber":26,"Text":"كلا اذا بلغت التراقي"},{"SurahNumber":75,"VerseNumber":27,"Text":"وقيل من راق"},{"SurahNumber":75,"VerseNumber":28,"Text":"وظن انه الفراق"},{"SurahNumber":75,"VerseNumber":29,"Text":"والتفت الساق بالساق"},{"SurahNumber":75,"VerseNumber":30,"Text":"الى ربك يوميذ المساق"},{"SurahNumber":75,"VerseNumber":31,"Text":"فلا صدق ولا صلى"},{"SurahNumber":75,"VerseNumber":32,"Text":"ولكن كذب وتولى"},{"SurahNumber":75,"VerseNumber":33,"Text":"ثم ذهب الى اهله يتمطى"},{"SurahNumber":75,"VerseNumber":34,"Text":"اولى لك فاولى"},{"SurahNumber":75,"VerseNumber":35,"Text":"ثم اولى لك فاولى"},{"SurahNumber":75,"VerseNumber":36,"Text":"ايحسب الانسن ان يترك سدى"},{"SurahNumber":75,"VerseNumber":37,"Text":"الم يك نطفة من مني يمنى"},{"SurahNumber":75,"VerseNumber":38,"Text":"ثم كان علقة فخلق فسوى"},{"SurahNumber":75,"VerseNumber":39,"Text":"فجعل منه الزوجين الذكر والانثى"},{"SurahNumber":75,"VerseNumber":40,"Text":"اليس ذلك بقدر على ان يحي الموتى"}]}