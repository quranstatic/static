{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":83,"VerseNumber":1,"Text":"Veylun lilmutaffifiyne"},{"SurahNumber":83,"VerseNumber":2,"Text":"Elleziyne izektalu ´alennasi yestevfune"},{"SurahNumber":83,"VerseNumber":3,"Text":"Ve iza kaluhum ev vezenuhum yuhsirune"},{"SurahNumber":83,"VerseNumber":4,"Text":"Ela yezunnu ulaike ennehum meb´usune"},{"SurahNumber":83,"VerseNumber":5,"Text":"Liyevmin ´azıymin"},{"SurahNumber":83,"VerseNumber":6,"Text":"Yevme yekumunnasu lirabbil´alemiyne"},{"SurahNumber":83,"VerseNumber":7,"Text":"Kella inne kitabelfuccari lefiy sicciynin"},{"SurahNumber":83,"VerseNumber":8,"Text":"Ve ma edrake ma sicciynun"},{"SurahNumber":83,"VerseNumber":9,"Text":"Kitabun merkumun"},{"SurahNumber":83,"VerseNumber":10,"Text":"Veylun yevmeizin lilmukezzibiyne"},{"SurahNumber":83,"VerseNumber":11,"Text":"Elleziyne yukezzibune biyevmiddiyni"},{"SurahNumber":83,"VerseNumber":12,"Text":"Ve ma yukezzibu bihi illa kullu mu´tedin esiymin"},{"SurahNumber":83,"VerseNumber":13,"Text":"İza tutla aleyhi ayatuna kale esatıyrul´evveliyne"},{"SurahNumber":83,"VerseNumber":14,"Text":"Kella bel rane ´ala kulubihim ma kanu yeksibune"},{"SurahNumber":83,"VerseNumber":15,"Text":"Kella innehum ´an rabbihim yevmeizin lemahcubune"},{"SurahNumber":83,"VerseNumber":16,"Text":"Summe innehum lesalulcahıymi"},{"SurahNumber":83,"VerseNumber":17,"Text":"Summe yukalu hazelleziy kuntum bihi tukezzibune"},{"SurahNumber":83,"VerseNumber":18,"Text":"Kella inne kitabel´ebrari lefiy ´ılliyyiyne"},{"SurahNumber":83,"VerseNumber":19,"Text":"Ve ma edrake ma ´ılliyyune"},{"SurahNumber":83,"VerseNumber":20,"Text":"Kitabun merkumun"},{"SurahNumber":83,"VerseNumber":21,"Text":"Yeşheduhulmukarrebune"},{"SurahNumber":83,"VerseNumber":22,"Text":"İnnelebrare. Lefiy na´ıymin"},{"SurahNumber":83,"VerseNumber":23,"Text":"´Alel´eraiki yenzurune"},{"SurahNumber":83,"VerseNumber":24,"Text":"Ta´rifu fiy vucuhihim nadretenna´ıymi"},{"SurahNumber":83,"VerseNumber":25,"Text":"Yuskavne min rahıykın mahtumin"},{"SurahNumber":83,"VerseNumber":26,"Text":"Hıtamuhu miskun ve fiy zalike felyetenafesilmutenasifune"},{"SurahNumber":83,"VerseNumber":27,"Text":"Ve mizacuhu min tesniymin"},{"SurahNumber":83,"VerseNumber":28,"Text":"Aynen yeşrebu bihelmukarrebune"},{"SurahNumber":83,"VerseNumber":29,"Text":"İnnelleziyne ecremu kanu minelleziyne amenu yadhakune"},{"SurahNumber":83,"VerseNumber":30,"Text":"Ve iza merru bihim yeteğamezune"},{"SurahNumber":83,"VerseNumber":31,"Text":"Ve izenkalebu ila ehlihimunkalebu fekihiyne"},{"SurahNumber":83,"VerseNumber":32,"Text":"Ve iza reevhum kalu inne haulai ledallune"},{"SurahNumber":83,"VerseNumber":33,"Text":"Ve ma ursilu ´aleyhim hafizıyne"},{"SurahNumber":83,"VerseNumber":34,"Text":"Felyevmelleziyne amenu minelkuffari yadhakune"},{"SurahNumber":83,"VerseNumber":35,"Text":"´Alel´eraiki yenzurune"},{"SurahNumber":83,"VerseNumber":36,"Text":"Hel suvvibelkuffaru ma kanu yef´alune"}]}