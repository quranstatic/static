{"Id":64,"LanguageCodes":["bs","bos"],"Name":"bos-korkut","Author":"Korkut","Language":"Bosnian","Description":"Bosnian","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"Smak svijeta"},{"SurahNumber":101,"VerseNumber":2,"Text":"Šta je smak svijeta"},{"SurahNumber":101,"VerseNumber":3,"Text":"i šta ti znaš o Smaku svijeta"},{"SurahNumber":101,"VerseNumber":4,"Text":"Toga dana će ljudi biti kao leptiri raštrkani"},{"SurahNumber":101,"VerseNumber":5,"Text":"a planine kao šarena vuna iščupana"},{"SurahNumber":101,"VerseNumber":6,"Text":"Onaj u koga njegova djela budu teška"},{"SurahNumber":101,"VerseNumber":7,"Text":"u ugodnu životu će živjeti"},{"SurahNumber":101,"VerseNumber":8,"Text":"a onaj u koga njegova djela budu lahka"},{"SurahNumber":101,"VerseNumber":9,"Text":"boravište će mu bezdan biti"},{"SurahNumber":101,"VerseNumber":10,"Text":"A znaš li ti šta će to biti"},{"SurahNumber":101,"VerseNumber":11,"Text":"vatra užarena"}]}