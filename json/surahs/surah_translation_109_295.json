{"Id":295,"LanguageCodes":["rn","run"],"Name":"run-sheikhamissirad","Author":"Sheikh Amissi Radjabu Ntamba","Language":"Rundi","Description":"Rundi","Direction":"ltr","Source":"aradjantas@gmail.com , www.islamic-invitation.com","Translations":[{"SurahNumber":109,"VerseNumber":1,"Text":"Vuga uti:- mwebwe bahakanyi b’Imana y’ukuri"},{"SurahNumber":109,"VerseNumber":2,"Text":"Jewe si nsenga ivyo musenga"},{"SurahNumber":109,"VerseNumber":3,"Text":"Na mwebwe ntimusenga uwo nsenga"},{"SurahNumber":109,"VerseNumber":4,"Text":"Kandi sinzo senga ico musenga"},{"SurahNumber":109,"VerseNumber":5,"Text":"Namwe ntimuzo senga uwo nsenga"},{"SurahNumber":109,"VerseNumber":6,"Text":"Mwebwe mufise idini ryanyu, nanje mfise idini ryanje"}]}