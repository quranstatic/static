{"Id":64,"LanguageCodes":["bs","bos"],"Name":"bos-korkut","Author":"Korkut","Language":"Bosnian","Description":"Bosnian","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":95,"VerseNumber":1,"Text":"Tako mi smokve i masline"},{"SurahNumber":95,"VerseNumber":2,"Text":"i Sinajske gore"},{"SurahNumber":95,"VerseNumber":3,"Text":"i grada ovog, bezbjednog"},{"SurahNumber":95,"VerseNumber":4,"Text":"Mi čovjeka stvaramo u skladu najljepšem"},{"SurahNumber":95,"VerseNumber":5,"Text":"zatim ćemo ga u najnakazniji lik vratiti"},{"SurahNumber":95,"VerseNumber":6,"Text":"samo ne one koji budu vjerovali i dobra djela činili, njih čeka nagrada neprekidna"},{"SurahNumber":95,"VerseNumber":7,"Text":"Pa šta te onda navodi da poričeš onaj svijet"},{"SurahNumber":95,"VerseNumber":8,"Text":"zar Allah nije sudija najpravedniji"}]}