{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":78,"VerseNumber":1,"Text":"عم يتساءلون"},{"SurahNumber":78,"VerseNumber":2,"Text":"عن النبا العظيم"},{"SurahNumber":78,"VerseNumber":3,"Text":"الذي هم فيه مختلفون"},{"SurahNumber":78,"VerseNumber":4,"Text":"كلا سيعلمون"},{"SurahNumber":78,"VerseNumber":5,"Text":"ثم كلا سيعلمون"},{"SurahNumber":78,"VerseNumber":6,"Text":"الم نجعل الارض مهد ا"},{"SurahNumber":78,"VerseNumber":7,"Text":"والجبال اوتاد ا"},{"SurahNumber":78,"VerseNumber":8,"Text":"وخلقنكم ازوج ا"},{"SurahNumber":78,"VerseNumber":9,"Text":"وجعلنا نومكم سبات ا"},{"SurahNumber":78,"VerseNumber":10,"Text":"وجعلنا اليل لباس ا"},{"SurahNumber":78,"VerseNumber":11,"Text":"وجعلنا النهار معاش ا"},{"SurahNumber":78,"VerseNumber":12,"Text":"وبنينا فوقكم سبع ا شداد ا"},{"SurahNumber":78,"VerseNumber":13,"Text":"وجعلنا سراج ا وهاج ا"},{"SurahNumber":78,"VerseNumber":14,"Text":"وانزلنا من المعصرت ماء ثجاج ا"},{"SurahNumber":78,"VerseNumber":15,"Text":"لنخرج به حب ا ونبات ا"},{"SurahNumber":78,"VerseNumber":16,"Text":"وجنت الفافا"},{"SurahNumber":78,"VerseNumber":17,"Text":"ان يوم الفصل كان ميقت ا"},{"SurahNumber":78,"VerseNumber":18,"Text":"يوم ينفخ في الصور فتاتون افواج ا"},{"SurahNumber":78,"VerseNumber":19,"Text":"وفتحت السماء فكانت ابوب ا"},{"SurahNumber":78,"VerseNumber":20,"Text":"وسيرت الجبال فكانت سرابا"},{"SurahNumber":78,"VerseNumber":21,"Text":"ان جهنم كانت مرصاد ا"},{"SurahNumber":78,"VerseNumber":22,"Text":"للطغين ماب ا"},{"SurahNumber":78,"VerseNumber":23,"Text":"لبثين فيها احقاب ا"},{"SurahNumber":78,"VerseNumber":24,"Text":"لا يذوقون فيها برد ا ولا شرابا"},{"SurahNumber":78,"VerseNumber":25,"Text":"الا حميم ا وغساق ا"},{"SurahNumber":78,"VerseNumber":26,"Text":"جزاء وفاقا"},{"SurahNumber":78,"VerseNumber":27,"Text":"انهم كانوا لا يرجون حساب ا"},{"SurahNumber":78,"VerseNumber":28,"Text":"وكذبوا بايتنا كذاب ا"},{"SurahNumber":78,"VerseNumber":29,"Text":"وكل شيء احصينه كتب ا"},{"SurahNumber":78,"VerseNumber":30,"Text":"فذوقوا فلن نزيدكم الا عذابا"},{"SurahNumber":78,"VerseNumber":31,"Text":"ان للمتقين مفازا"},{"SurahNumber":78,"VerseNumber":32,"Text":"حدايق واعنب ا"},{"SurahNumber":78,"VerseNumber":33,"Text":"وكواعب اتراب ا"},{"SurahNumber":78,"VerseNumber":34,"Text":"وكاس ا دهاق ا"},{"SurahNumber":78,"VerseNumber":35,"Text":"لا يسمعون فيها لغو ا ولا كذب ا"},{"SurahNumber":78,"VerseNumber":36,"Text":"جزاء من ربك عطاء حساب ا"},{"SurahNumber":78,"VerseNumber":37,"Text":"رب السموت والارض وما بينهما الرحمن لا يملكون منه خطاب ا"},{"SurahNumber":78,"VerseNumber":38,"Text":"يوم يقوم الروح والمليكة صف ا لا يتكلمون الا من اذن له الرحمن وقال صواب ا"},{"SurahNumber":78,"VerseNumber":39,"Text":"ذلك اليوم الحق فمن شاء اتخذ الى ربه مابا"},{"SurahNumber":78,"VerseNumber":40,"Text":"انا انذرنكم عذاب ا قريب ا يوم ينظر المرء ما قدمت يداه ويقول الكافر يليتني كنت تربا"}]}