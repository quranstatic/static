{"Id":347,"LanguageCodes":["sr","srp"],"Name":"srp-rowwadtranslati","Author":"Rowwad Translation Center Islamhouse","Language":"Serbian","Description":"Serbian","Direction":"ltr","Source":"https://quranenc.com/en/browse/serbian_rwwad","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"Застрашујући Удар (Смак света)"},{"SurahNumber":101,"VerseNumber":2,"Text":"Шта је застрашујући Удар"},{"SurahNumber":101,"VerseNumber":3,"Text":"И шта ти знаш шта је застрашујући Удар"},{"SurahNumber":101,"VerseNumber":4,"Text":"Тог Дана људи ће да буду као раштркани лептири"},{"SurahNumber":101,"VerseNumber":5,"Text":"а планине као шарена вуна ишчупана"},{"SurahNumber":101,"VerseNumber":6,"Text":"Онај чија добра дела буду тешка"},{"SurahNumber":101,"VerseNumber":7,"Text":"у угодном животу ће да живи"},{"SurahNumber":101,"VerseNumber":8,"Text":"а онај чија добра дела буду лака"},{"SurahNumber":101,"VerseNumber":9,"Text":"боравиште ће му бездан бити"},{"SurahNumber":101,"VerseNumber":10,"Text":"А знаш ли ти шта ће то да буде"},{"SurahNumber":101,"VerseNumber":11,"Text":"Ужарена Ватра"}]}