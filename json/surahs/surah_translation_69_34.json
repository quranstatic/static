{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":69,"VerseNumber":1,"Text":"الحاقة"},{"SurahNumber":69,"VerseNumber":2,"Text":"ما الحاقة"},{"SurahNumber":69,"VerseNumber":3,"Text":"وما ادرىك ما الحاقة"},{"SurahNumber":69,"VerseNumber":4,"Text":"كذبت ثمود وعاد بالقارعة"},{"SurahNumber":69,"VerseNumber":5,"Text":"فاما ثمود فاهلكوا بالطاغية"},{"SurahNumber":69,"VerseNumber":6,"Text":"واما عاد فاهلكوا بريح صرصر عاتية"},{"SurahNumber":69,"VerseNumber":7,"Text":"سخرها عليهم سبع ليال وثمنية ايام حسوم ا فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"},{"SurahNumber":69,"VerseNumber":8,"Text":"فهل ترى لهم من باقية"},{"SurahNumber":69,"VerseNumber":9,"Text":"وجاء فرعون ومن قبله والموتفكت بالخاطية"},{"SurahNumber":69,"VerseNumber":10,"Text":"فعصوا رسول ربهم فاخذهم اخذة رابية"},{"SurahNumber":69,"VerseNumber":11,"Text":"انا لما طغا الماء حملنكم في الجارية"},{"SurahNumber":69,"VerseNumber":12,"Text":"لنجعلها لكم تذكرة وتعيها اذن وعية"},{"SurahNumber":69,"VerseNumber":13,"Text":"فاذا نفخ في الصور نفخة وحدة"},{"SurahNumber":69,"VerseNumber":14,"Text":"وحملت الارض والجبال فدكتا دكة وحدة"},{"SurahNumber":69,"VerseNumber":15,"Text":"فيوميذ وقعت الواقعة"},{"SurahNumber":69,"VerseNumber":16,"Text":"وانشقت السماء فهي يوميذ واهية"},{"SurahNumber":69,"VerseNumber":17,"Text":"والملك على ارجايها ويحمل عرش ربك فوقهم يوميذ ثمنية"},{"SurahNumber":69,"VerseNumber":18,"Text":"يوميذ تعرضون لا تخفى منكم خافية"},{"SurahNumber":69,"VerseNumber":19,"Text":"فاما من اوتي كتبه بيمينه فيقول هاوم اقرءوا كتبيه"},{"SurahNumber":69,"VerseNumber":20,"Text":"اني ظننت اني ملق حسابيه"},{"SurahNumber":69,"VerseNumber":21,"Text":"فهو في عيشة راضية"},{"SurahNumber":69,"VerseNumber":22,"Text":"في جنة عالية"},{"SurahNumber":69,"VerseNumber":23,"Text":"قطوفها دانية"},{"SurahNumber":69,"VerseNumber":24,"Text":"كلوا واشربوا هنيا بما اسلفتم في الايام الخالية"},{"SurahNumber":69,"VerseNumber":25,"Text":"واما من اوتي كتبه بشماله فيقول يليتني لم اوت كتبيه"},{"SurahNumber":69,"VerseNumber":26,"Text":"ولم ادر ما حسابيه"},{"SurahNumber":69,"VerseNumber":27,"Text":"يليتها كانت القاضية"},{"SurahNumber":69,"VerseNumber":28,"Text":"ما اغنى عني ماليه"},{"SurahNumber":69,"VerseNumber":29,"Text":"هلك عني سلطنيه"},{"SurahNumber":69,"VerseNumber":30,"Text":"خذوه فغلوه"},{"SurahNumber":69,"VerseNumber":31,"Text":"ثم الجحيم صلوه"},{"SurahNumber":69,"VerseNumber":32,"Text":"ثم في سلسلة ذرعها سبعون ذراع ا فاسلكوه"},{"SurahNumber":69,"VerseNumber":33,"Text":"انه كان لا يومن بالله العظيم"},{"SurahNumber":69,"VerseNumber":34,"Text":"ولا يحض على طعام المسكين"},{"SurahNumber":69,"VerseNumber":35,"Text":"فليس له اليوم ههنا حميم"},{"SurahNumber":69,"VerseNumber":36,"Text":"ولا طعام الا من غسلين"},{"SurahNumber":69,"VerseNumber":37,"Text":"لا ياكله الا الخطون"},{"SurahNumber":69,"VerseNumber":38,"Text":"فلا اقسم بما تبصرون"},{"SurahNumber":69,"VerseNumber":39,"Text":"وما لا تبصرون"},{"SurahNumber":69,"VerseNumber":40,"Text":"انه لقول رسول كريم"},{"SurahNumber":69,"VerseNumber":41,"Text":"وما هو بقول شاعر قليل ا ما تومنون"},{"SurahNumber":69,"VerseNumber":42,"Text":"ولا بقول كاهن قليل ا ما تذكرون"},{"SurahNumber":69,"VerseNumber":43,"Text":"تنزيل من رب العلمين"},{"SurahNumber":69,"VerseNumber":44,"Text":"ولو تقول علينا بعض الاقاويل"},{"SurahNumber":69,"VerseNumber":45,"Text":"لاخذنا منه باليمين"},{"SurahNumber":69,"VerseNumber":46,"Text":"ثم لقطعنا منه الوتين"},{"SurahNumber":69,"VerseNumber":47,"Text":"فما منكم من احد عنه حجزين"},{"SurahNumber":69,"VerseNumber":48,"Text":"وانه لتذكرة للمتقين"},{"SurahNumber":69,"VerseNumber":49,"Text":"وانا لنعلم ان منكم مكذبين"},{"SurahNumber":69,"VerseNumber":50,"Text":"وانه لحسرة على الكفرين"},{"SurahNumber":69,"VerseNumber":51,"Text":"وانه لحق اليقين"},{"SurahNumber":69,"VerseNumber":52,"Text":"فسبح باسم ربك العظيم"}]}