{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"اذا السماء انفطرت"},{"SurahNumber":82,"VerseNumber":2,"Text":"واذا الكواكب انتثرت"},{"SurahNumber":82,"VerseNumber":3,"Text":"واذا البحار فجرت"},{"SurahNumber":82,"VerseNumber":4,"Text":"واذا القبور بعثرت"},{"SurahNumber":82,"VerseNumber":5,"Text":"علمت نفس ما قدمت واخرت"},{"SurahNumber":82,"VerseNumber":6,"Text":"يايها الانسن ما غرك بربك الكريم"},{"SurahNumber":82,"VerseNumber":7,"Text":"الذي خلقك فسوىك فعدلك"},{"SurahNumber":82,"VerseNumber":8,"Text":"في اي صورة ما شاء ركبك"},{"SurahNumber":82,"VerseNumber":9,"Text":"كلا بل تكذبون بالدين"},{"SurahNumber":82,"VerseNumber":10,"Text":"وان عليكم لحفظين"},{"SurahNumber":82,"VerseNumber":11,"Text":"كرام ا كتبين"},{"SurahNumber":82,"VerseNumber":12,"Text":"يعلمون ما تفعلون"},{"SurahNumber":82,"VerseNumber":13,"Text":"ان الابرار لفي نعيم"},{"SurahNumber":82,"VerseNumber":14,"Text":"وان الفجار لفي جحيم"},{"SurahNumber":82,"VerseNumber":15,"Text":"يصلونها يوم الدين"},{"SurahNumber":82,"VerseNumber":16,"Text":"وما هم عنها بغايبين"},{"SurahNumber":82,"VerseNumber":17,"Text":"وما ادرىك ما يوم الدين"},{"SurahNumber":82,"VerseNumber":18,"Text":"ثم ما ادرىك ما يوم الدين"},{"SurahNumber":82,"VerseNumber":19,"Text":"يوم لا تملك نفس لنفس شي ا والامر يوميذ لله"}]}