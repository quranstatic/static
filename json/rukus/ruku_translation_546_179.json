{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":104,"VerseNumber":1,"Text":"Woyli woodanii ño*oowo sanne e juwoowo sanne"},{"SurahNumber":104,"VerseNumber":2,"Text":"Oon denndinɗo jawdi o limi ndi"},{"SurahNumber":104,"VerseNumber":3,"Text":"Hombo sikka jawdi makko ndi saasnat mbo"},{"SurahNumber":104,"VerseNumber":4,"Text":"Alah maɓe mberle e yiite morñatnge"},{"SurahNumber":104,"VerseNumber":5,"Text":"Ko honɗum anndin maa ko woni yiite heltoowe"},{"SurahNumber":104,"VerseNumber":6,"Text":"Ko yiite Alla huɓɓatnge"},{"SurahNumber":104,"VerseNumber":7,"Text":"Ngeen woni yulatnge keeñe"},{"SurahNumber":104,"VerseNumber":8,"Text":"Ngeen de ko nge sokaange e dow maɓɓe"},{"SurahNumber":104,"VerseNumber":9,"Text":"E nge doosɗe pooɗtiiɗe"}]}