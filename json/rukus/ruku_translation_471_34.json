{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":56,"VerseNumber":75,"Text":"۞فلا اقسم بموقع النجوم"},{"SurahNumber":56,"VerseNumber":76,"Text":"وانه لقسم لو تعلمون عظيم"},{"SurahNumber":56,"VerseNumber":77,"Text":"انه لقرءان كريم"},{"SurahNumber":56,"VerseNumber":78,"Text":"في كتب مكنون"},{"SurahNumber":56,"VerseNumber":79,"Text":"لا يمسه الا المطهرون"},{"SurahNumber":56,"VerseNumber":80,"Text":"تنزيل من رب العلمين"},{"SurahNumber":56,"VerseNumber":81,"Text":"افبهذا الحديث انتم مدهنون"},{"SurahNumber":56,"VerseNumber":82,"Text":"وتجعلون رزقكم انكم تكذبون"},{"SurahNumber":56,"VerseNumber":83,"Text":"فلولا اذا بلغت الحلقوم"},{"SurahNumber":56,"VerseNumber":84,"Text":"وانتم حينيذ تنظرون"},{"SurahNumber":56,"VerseNumber":85,"Text":"ونحن اقرب اليه منكم ولكن لا تبصرون"},{"SurahNumber":56,"VerseNumber":86,"Text":"فلولا ان كنتم غير مدينين"},{"SurahNumber":56,"VerseNumber":87,"Text":"ترجعونها ان كنتم صدقين"},{"SurahNumber":56,"VerseNumber":88,"Text":"فاما ان كان من المقربين"},{"SurahNumber":56,"VerseNumber":89,"Text":"فروح وريحان وجنت نعيم"},{"SurahNumber":56,"VerseNumber":90,"Text":"واما ان كان من اصحب اليمين"},{"SurahNumber":56,"VerseNumber":91,"Text":"فسلم لك من اصحب اليمين"},{"SurahNumber":56,"VerseNumber":92,"Text":"واما ان كان من المكذبين الضالين"},{"SurahNumber":56,"VerseNumber":93,"Text":"فنزل من حميم"},{"SurahNumber":56,"VerseNumber":94,"Text":"وتصلية جحيم"},{"SurahNumber":56,"VerseNumber":95,"Text":"ان هذا لهو حق اليقين"},{"SurahNumber":56,"VerseNumber":96,"Text":"فسبح باسم ربك العظيم"}]}