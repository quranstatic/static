{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":105,"Text":"كذبت قوم نوح المرسلين"},{"SurahNumber":26,"VerseNumber":106,"Text":"اذ قال لهم اخوهم نوح الا تتقون"},{"SurahNumber":26,"VerseNumber":107,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":108,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":109,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":110,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":111,"Text":"۞قالوا انومن لك واتبعك الارذلون"},{"SurahNumber":26,"VerseNumber":112,"Text":"قال وما علمي بما كانوا يعملون"},{"SurahNumber":26,"VerseNumber":113,"Text":"ان حسابهم الا على ربي لو تشعرون"},{"SurahNumber":26,"VerseNumber":114,"Text":"وما انا بطارد المومنين"},{"SurahNumber":26,"VerseNumber":115,"Text":"ان انا الا نذير مبين"},{"SurahNumber":26,"VerseNumber":116,"Text":"قالوا لين لم تنته ينوح لتكونن من المرجومين"},{"SurahNumber":26,"VerseNumber":117,"Text":"قال رب ان قومي كذبون"},{"SurahNumber":26,"VerseNumber":118,"Text":"فافتح بيني وبينهم فتح ا ونجني ومن معي من المومنين"},{"SurahNumber":26,"VerseNumber":119,"Text":"فانجينه ومن معه في الفلك المشحون"},{"SurahNumber":26,"VerseNumber":120,"Text":"ثم اغرقنا بعد الباقين"},{"SurahNumber":26,"VerseNumber":121,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":122,"Text":"وان ربك لهو العزيز الرحيم"}]}