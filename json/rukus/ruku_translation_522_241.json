{"Id":241,"LanguageCodes":["luy"],"Name":"luy-mohammadramadha","Author":"Mohammad Ramadhan Sebwa, Ramadhan Hajj Ndeka, Ismail Were, Musa Murasi Salim Kukuya, Yusuf Shiundu Malala, Yahya Wafula Ojwang, Haroon Albert Msah","Language":"Luyia","Description":"Luyia","Direction":"ltr","Source":"https://www.islamic-invitation.com/","Translations":[{"SurahNumber":80,"VerseNumber":1,"Text":"Yakhulusia obweni namukhupila likosi"},{"SurahNumber":80,"VerseNumber":2,"Text":"Khulwokhwitsilwa nende Omuwofu"},{"SurahNumber":80,"VerseNumber":3,"Text":"Ne nishina shikhumanyia ye awundi anyala ukhwitwenula"},{"SurahNumber":80,"VerseNumber":4,"Text":"Nohomba aletsulisibwa mana ametsulisio kamukhoonye"},{"SurahNumber":80,"VerseNumber":5,"Text":"Ne\tulia wilola mbu kamubulilamwo oluchendo"},{"SurahNumber":80,"VerseNumber":6,"Text":"Nawe niye oworeerekhwo eshinani"},{"SurahNumber":80,"VerseNumber":7,"Text":"Shikali\tkhwiwe\ttawe\tnakhaya okhutswenukha"},{"SurahNumber":80,"VerseNumber":8,"Text":"Ne ulia ukhwitsila neshinani"},{"SurahNumber":80,"VerseNumber":9,"Text":"Naye naririre"},{"SurahNumber":80,"VerseNumber":10,"Text":"Niye niwokhupila likosi"},{"SurahNumber":80,"VerseNumber":11,"Text":"shikali\tkario\ttawe!\tkhuno nokhwitsulisania"},{"SurahNumber":80,"VerseNumber":12,"Text":"Ulia uchama yetsulile"},{"SurahNumber":80,"VerseNumber":13,"Text":"Kali mutsimbapulo tsiahebwa oluyali"},{"SurahNumber":80,"VerseNumber":14,"Text":"Tsiarebwa hekulu tsinzwenufu"},{"SurahNumber":80,"VerseNumber":15,"Text":"Tsili mumikhono chia Abamalaika Abahandichi"},{"SurahNumber":80,"VerseNumber":16,"Text":"Boluyali abalayi"},{"SurahNumber":80,"VerseNumber":17,"Text":"Omundu\tyasikha!\tNishina shimukholanga aba omukhayi"},{"SurahNumber":80,"VerseNumber":18,"Text":"Yalongwa okhurula khushina"},{"SurahNumber":80,"VerseNumber":19,"Text":"okhurulana nende indonyande ya amaatsi kolwibulo, yamuloonga mana namubalilila"},{"SurahNumber":80,"VerseNumber":20,"Text":"Mana namwanguyilisilia injila"},{"SurahNumber":80,"VerseNumber":21,"Text":"Mana namufwisia namura mushilindwa"},{"SurahNumber":80,"VerseNumber":22,"Text":"Mana nalichama alimulamusia"},{"SurahNumber":80,"VerseNumber":23,"Text":"Tawe! Ashili akhumala kayamulomesia"},{"SurahNumber":80,"VerseNumber":24,"Text":"Kho Omundu ahenjekhwo eshilibwa shishie"},{"SurahNumber":80,"VerseNumber":25,"Text":"Toto efwe khwayomboola amatsi buyomboola"},{"SurahNumber":80,"VerseNumber":26,"Text":"Ne nikhukalukhasia liloba bukalukhasia"},{"SurahNumber":80,"VerseNumber":27,"Text":"Ne nikhumesiamwo imele"},{"SurahNumber":80,"VerseNumber":28,"Text":"Nende tsizabibu nende eliani liamasafu"},{"SurahNumber":80,"VerseNumber":29,"Text":"Nende emizaituna nende mitende"},{"SurahNumber":80,"VerseNumber":30,"Text":"Nende\temikunda\tchiemisala chiasitanakhwo obulayi"},{"SurahNumber":80,"VerseNumber":31,"Text":"Nende\tebiamo,\tnende\tolwayilo lwemiruko"},{"SurahNumber":80,"VerseNumber":32,"Text":"Khulwo obukhala bwenyu nende ebiruche bienyu"},{"SurahNumber":80,"VerseNumber":33,"Text":"Mana shingala omwayukho kuliitsa"},{"SurahNumber":80,"VerseNumber":34,"Text":"Inyanga yo Omundu alimwilukha Omusiani wabu"},{"SurahNumber":80,"VerseNumber":35,"Text":"Nende Nyinamwana nende Samwana"},{"SurahNumber":80,"VerseNumber":36,"Text":"Nende\tOmukhaye\twuwe\tnende Abanabe"},{"SurahNumber":80,"VerseNumber":37,"Text":"Buli shiamulala aliba nende akeke kamuyela"},{"SurahNumber":80,"VerseNumber":38,"Text":"Inyanga\teyo\tobweni\tbundi buling’enang’ena"},{"SurahNumber":80,"VerseNumber":39,"Text":"Bulitsekha nibusangalile"},{"SurahNumber":80,"VerseNumber":40,"Text":"Ne obweni bundi inyanga eyo buliba nende olufu"},{"SurahNumber":80,"VerseNumber":41,"Text":"Ishilima tititi shilibufunikha"},{"SurahNumber":80,"VerseNumber":42,"Text":"Abo nibo Abakhayi abakholi betsimbi"}]}