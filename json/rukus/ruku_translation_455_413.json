{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"Vezzariyati zerva"},{"SurahNumber":51,"VerseNumber":2,"Text":"Fel hamilati vıkra"},{"SurahNumber":51,"VerseNumber":3,"Text":"Fel cariyati yusra"},{"SurahNumber":51,"VerseNumber":4,"Text":"Fel mukassimati emra"},{"SurahNumber":51,"VerseNumber":5,"Text":"İnnema tuadune le sadık"},{"SurahNumber":51,"VerseNumber":6,"Text":"Ve inned dine le vakı´"},{"SurahNumber":51,"VerseNumber":7,"Text":"Ves semai zatil hubuk"},{"SurahNumber":51,"VerseNumber":8,"Text":"İnnekum le fi kavlim muhtelif"},{"SurahNumber":51,"VerseNumber":9,"Text":"Yu´feku anhu men ufik"},{"SurahNumber":51,"VerseNumber":10,"Text":"Kutilel harrasun"},{"SurahNumber":51,"VerseNumber":11,"Text":"Ellezine hum fi ğamratin sahun"},{"SurahNumber":51,"VerseNumber":12,"Text":"Yes´elune eyyane yevmud din"},{"SurahNumber":51,"VerseNumber":13,"Text":"Yevme hum alen nari yuftenun"},{"SurahNumber":51,"VerseNumber":14,"Text":"Zuku fitnetekum hazellezi kuntum bihi testa´cilun"},{"SurahNumber":51,"VerseNumber":15,"Text":"İnnel muttekıyne fi cennativ ve uyun"},{"SurahNumber":51,"VerseNumber":16,"Text":"Ahızıne ma atahum rabbuhum innehum kanu kable zalike muhsinin"},{"SurahNumber":51,"VerseNumber":17,"Text":"Kanu kalilem minel leyli ma yehceun"},{"SurahNumber":51,"VerseNumber":18,"Text":"Ve bil eshari hum yestağfirun"},{"SurahNumber":51,"VerseNumber":19,"Text":"Ve fi emvalihim hakkul lis saili vel mahrum"},{"SurahNumber":51,"VerseNumber":20,"Text":"Ve fil erdı ayatul lil mukınin"},{"SurahNumber":51,"VerseNumber":21,"Text":"Ve fi enfusikum e fe la tubrırun"},{"SurahNumber":51,"VerseNumber":22,"Text":"Ve fis semai rizkukum ve ma tuadun"},{"SurahNumber":51,"VerseNumber":23,"Text":"Fe ve rabbis semai vel erdı innehu lehakkum misle ma ennekum tentıkun"}]}