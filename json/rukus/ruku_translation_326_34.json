{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":192,"Text":"وانه لتنزيل رب العلمين"},{"SurahNumber":26,"VerseNumber":193,"Text":"نزل به الروح الامين"},{"SurahNumber":26,"VerseNumber":194,"Text":"على قلبك لتكون من المنذرين"},{"SurahNumber":26,"VerseNumber":195,"Text":"بلسان عربي مبين"},{"SurahNumber":26,"VerseNumber":196,"Text":"وانه لفي زبر الاولين"},{"SurahNumber":26,"VerseNumber":197,"Text":"اولم يكن لهم ءاية ان يعلمه علموا بني اسرءيل"},{"SurahNumber":26,"VerseNumber":198,"Text":"ولو نزلنه على بعض الاعجمين"},{"SurahNumber":26,"VerseNumber":199,"Text":"فقراه عليهم ما كانوا به مومنين"},{"SurahNumber":26,"VerseNumber":200,"Text":"كذلك سلكنه في قلوب المجرمين"},{"SurahNumber":26,"VerseNumber":201,"Text":"لا يومنون به حتى يروا العذاب الاليم"},{"SurahNumber":26,"VerseNumber":202,"Text":"فياتيهم بغتة وهم لا يشعرون"},{"SurahNumber":26,"VerseNumber":203,"Text":"فيقولوا هل نحن منظرون"},{"SurahNumber":26,"VerseNumber":204,"Text":"افبعذابنا يستعجلون"},{"SurahNumber":26,"VerseNumber":205,"Text":"افرءيت ان متعنهم سنين"},{"SurahNumber":26,"VerseNumber":206,"Text":"ثم جاءهم ما كانوا يوعدون"},{"SurahNumber":26,"VerseNumber":207,"Text":"ما اغنى عنهم ما كانوا يمتعون"},{"SurahNumber":26,"VerseNumber":208,"Text":"وما اهلكنا من قرية الا لها منذرون"},{"SurahNumber":26,"VerseNumber":209,"Text":"ذكرى وما كنا ظلمين"},{"SurahNumber":26,"VerseNumber":210,"Text":"وما تنزلت به الشيطين"},{"SurahNumber":26,"VerseNumber":211,"Text":"وما ينبغي لهم وما يستطيعون"},{"SurahNumber":26,"VerseNumber":212,"Text":"انهم عن السمع لمعزولون"},{"SurahNumber":26,"VerseNumber":213,"Text":"فلا تدع مع الله الها ءاخر فتكون من المعذبين"},{"SurahNumber":26,"VerseNumber":214,"Text":"وانذر عشيرتك الاقربين"},{"SurahNumber":26,"VerseNumber":215,"Text":"واخفض جناحك لمن اتبعك من المومنين"},{"SurahNumber":26,"VerseNumber":216,"Text":"فان عصوك فقل اني بريء مما تعملون"},{"SurahNumber":26,"VerseNumber":217,"Text":"وتوكل على العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":218,"Text":"الذي يرىك حين تقوم"},{"SurahNumber":26,"VerseNumber":219,"Text":"وتقلبك في السجدين"},{"SurahNumber":26,"VerseNumber":220,"Text":"انه هو السميع العليم"},{"SurahNumber":26,"VerseNumber":221,"Text":"هل انبيكم على من تنزل الشيطين"},{"SurahNumber":26,"VerseNumber":222,"Text":"تنزل على كل افاك اثيم"},{"SurahNumber":26,"VerseNumber":223,"Text":"يلقون السمع واكثرهم كذبون"},{"SurahNumber":26,"VerseNumber":224,"Text":"والشعراء يتبعهم الغاون"},{"SurahNumber":26,"VerseNumber":225,"Text":"الم تر انهم في كل واد يهيمون"},{"SurahNumber":26,"VerseNumber":226,"Text":"وانهم يقولون ما لا يفعلون"},{"SurahNumber":26,"VerseNumber":227,"Text":"الا الذين ءامنوا وعملوا الصلحت وذكروا الله كثير ا وانتصروا من بعد ما ظلموا وسيعلم الذين ظلموا اي منقلب ينقلبون"}]}