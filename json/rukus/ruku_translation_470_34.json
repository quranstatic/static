{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":56,"VerseNumber":39,"Text":"ثلة من الاولين"},{"SurahNumber":56,"VerseNumber":40,"Text":"وثلة من الاخرين"},{"SurahNumber":56,"VerseNumber":41,"Text":"واصحب الشمال ما اصحب الشمال"},{"SurahNumber":56,"VerseNumber":42,"Text":"في سموم وحميم"},{"SurahNumber":56,"VerseNumber":43,"Text":"وظل من يحموم"},{"SurahNumber":56,"VerseNumber":44,"Text":"لا بارد ولا كريم"},{"SurahNumber":56,"VerseNumber":45,"Text":"انهم كانوا قبل ذلك مترفين"},{"SurahNumber":56,"VerseNumber":46,"Text":"وكانوا يصرون على الحنث العظيم"},{"SurahNumber":56,"VerseNumber":47,"Text":"وكانوا يقولون ايذا متنا وكنا تراب ا وعظما اءنا لمبعوثون"},{"SurahNumber":56,"VerseNumber":48,"Text":"اوءاباونا الاولون"},{"SurahNumber":56,"VerseNumber":49,"Text":"قل ان الاولين والاخرين"},{"SurahNumber":56,"VerseNumber":50,"Text":"لمجموعون الى ميقت يوم معلوم"},{"SurahNumber":56,"VerseNumber":51,"Text":"ثم انكم ايها الضالون المكذبون"},{"SurahNumber":56,"VerseNumber":52,"Text":"لاكلون من شجر من زقوم"},{"SurahNumber":56,"VerseNumber":53,"Text":"فمالون منها البطون"},{"SurahNumber":56,"VerseNumber":54,"Text":"فشربون عليه من الحميم"},{"SurahNumber":56,"VerseNumber":55,"Text":"فشربون شرب الهيم"},{"SurahNumber":56,"VerseNumber":56,"Text":"هذا نزلهم يوم الدين"},{"SurahNumber":56,"VerseNumber":57,"Text":"نحن خلقنكم فلولا تصدقون"},{"SurahNumber":56,"VerseNumber":58,"Text":"افرءيتم ما تمنون"},{"SurahNumber":56,"VerseNumber":59,"Text":"ءانتم تخلقونه ام نحن الخلقون"},{"SurahNumber":56,"VerseNumber":60,"Text":"نحن قدرنا بينكم الموت وما نحن بمسبوقين"},{"SurahNumber":56,"VerseNumber":61,"Text":"على ان نبدل امثلكم وننشيكم في ما لا تعلمون"},{"SurahNumber":56,"VerseNumber":62,"Text":"ولقد علمتم النشاة الاولى فلولا تذكرون"},{"SurahNumber":56,"VerseNumber":63,"Text":"افرءيتم ما تحرثون"},{"SurahNumber":56,"VerseNumber":64,"Text":"ءانتم تزرعونه ام نحن الزرعون"},{"SurahNumber":56,"VerseNumber":65,"Text":"لو نشاء لجعلنه حطم ا فظلتم تفكهون"},{"SurahNumber":56,"VerseNumber":66,"Text":"انا لمغرمون"},{"SurahNumber":56,"VerseNumber":67,"Text":"بل نحن محرومون"},{"SurahNumber":56,"VerseNumber":68,"Text":"افرءيتم الماء الذي تشربون"},{"SurahNumber":56,"VerseNumber":69,"Text":"ءانتم انزلتموه من المزن ام نحن المنزلون"},{"SurahNumber":56,"VerseNumber":70,"Text":"لو نشاء جعلنه اجاج ا فلولا تشكرون"},{"SurahNumber":56,"VerseNumber":71,"Text":"افرءيتم النار التي تورون"},{"SurahNumber":56,"VerseNumber":72,"Text":"ءانتم انشاتم شجرتها ام نحن المنشون"},{"SurahNumber":56,"VerseNumber":73,"Text":"نحن جعلنها تذكرة ومتع ا للمقوين"},{"SurahNumber":56,"VerseNumber":74,"Text":"فسبح باسم ربك العظيم"}]}