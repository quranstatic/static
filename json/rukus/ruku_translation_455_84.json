{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"puste vindene"},{"SurahNumber":51,"VerseNumber":2,"Text":"Fødende regn"},{"SurahNumber":51,"VerseNumber":3,"Text":"bringe forråd"},{"SurahNumber":51,"VerseNumber":4,"Text":"Distribuere dem ligesom befalede"},{"SurahNumber":51,"VerseNumber":5,"Text":"Hvad loves jer sikkert komme vedtage"},{"SurahNumber":51,"VerseNumber":6,"Text":"Dagen Bedømmelse er uundgåelige"},{"SurahNumber":51,"VerseNumber":7,"Text":"Uagtet perfectly oprettede himlen"},{"SurahNumber":51,"VerseNumber":8,"Text":"Du fortsætter dispute sandheden"},{"SurahNumber":51,"VerseNumber":9,"Text":"Afvige therefrom er deviators"},{"SurahNumber":51,"VerseNumber":10,"Text":"Ve til falsifiers"},{"SurahNumber":51,"VerseNumber":11,"Text":"Ind deres kludrende de er totalt heedless"},{"SurahNumber":51,"VerseNumber":12,"Text":"De spørgsmål Dagen Bedømmelse"},{"SurahNumber":51,"VerseNumber":13,"Text":"Dagen de præsenteres ilden"},{"SurahNumber":51,"VerseNumber":14,"Text":"Smager gengældelsen; den er hvad jer brugte udfordre"},{"SurahNumber":51,"VerseNumber":15,"Text":"righteous trænge haver springe"},{"SurahNumber":51,"VerseNumber":16,"Text":"De modtar deres Lord's belønninger de brugte være pious"},{"SurahNumber":51,"VerseNumber":17,"Text":"Sjældent gjorde de sover hele natten"},{"SurahNumber":51,"VerseNumber":18,"Text":"Hos daggry, de bede tilgivelse"},{"SurahNumber":51,"VerseNumber":19,"Text":"Portion deres penge var sætte aside i tiggeren behøvende"},{"SurahNumber":51,"VerseNumber":20,"Text":"Jorden være fuld af tegn de er vise"},{"SurahNumber":51,"VerseNumber":21,"Text":"indenfor I se"},{"SurahNumber":51,"VerseNumber":22,"Text":"Ind himlen er Deres forråd alt loves jer"},{"SurahNumber":51,"VerseNumber":23,"Text":"Ved Lord af himlen jorden den er idet sande idet at du taler"}]}