{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":83,"VerseNumber":1,"Text":"ويل للمطففين"},{"SurahNumber":83,"VerseNumber":2,"Text":"الذين اذا اكتالوا على الناس يستوفون"},{"SurahNumber":83,"VerseNumber":3,"Text":"واذا كالوهم او وزنوهم يخسرون"},{"SurahNumber":83,"VerseNumber":4,"Text":"الا يظن اوليك انهم مبعوثون"},{"SurahNumber":83,"VerseNumber":5,"Text":"ليوم عظيم"},{"SurahNumber":83,"VerseNumber":6,"Text":"يوم يقوم الناس لرب العلمين"},{"SurahNumber":83,"VerseNumber":7,"Text":"كلا ان كتب الفجار لفي سجين"},{"SurahNumber":83,"VerseNumber":8,"Text":"وما ادرىك ما سجين"},{"SurahNumber":83,"VerseNumber":9,"Text":"كتب مرقوم"},{"SurahNumber":83,"VerseNumber":10,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":83,"VerseNumber":11,"Text":"الذين يكذبون بيوم الدين"},{"SurahNumber":83,"VerseNumber":12,"Text":"وما يكذب به الا كل معتد اثيم"},{"SurahNumber":83,"VerseNumber":13,"Text":"اذا تتلى عليه ءايتنا قال اسطير الاولين"},{"SurahNumber":83,"VerseNumber":14,"Text":"كلا بل ران على قلوبهم ما كانوا يكسبون"},{"SurahNumber":83,"VerseNumber":15,"Text":"كلا انهم عن ربهم يوميذ لمحجوبون"},{"SurahNumber":83,"VerseNumber":16,"Text":"ثم انهم لصالوا الجحيم"},{"SurahNumber":83,"VerseNumber":17,"Text":"ثم يقال هذا الذي كنتم به تكذبون"},{"SurahNumber":83,"VerseNumber":18,"Text":"كلا ان كتب الابرار لفي عليين"},{"SurahNumber":83,"VerseNumber":19,"Text":"وما ادرىك ما عليون"},{"SurahNumber":83,"VerseNumber":20,"Text":"كتب مرقوم"},{"SurahNumber":83,"VerseNumber":21,"Text":"يشهده المقربون"},{"SurahNumber":83,"VerseNumber":22,"Text":"ان الابرار لفي نعيم"},{"SurahNumber":83,"VerseNumber":23,"Text":"على الارايك ينظرون"},{"SurahNumber":83,"VerseNumber":24,"Text":"تعرف في وجوههم نضرة النعيم"},{"SurahNumber":83,"VerseNumber":25,"Text":"يسقون من رحيق مختوم"},{"SurahNumber":83,"VerseNumber":26,"Text":"ختمه مسك وفي ذلك فليتنافس المتنفسون"},{"SurahNumber":83,"VerseNumber":27,"Text":"ومزاجه من تسنيم"},{"SurahNumber":83,"VerseNumber":28,"Text":"عين ا يشرب بها المقربون"},{"SurahNumber":83,"VerseNumber":29,"Text":"ان الذين اجرموا كانوا من الذين ءامنوا يضحكون"},{"SurahNumber":83,"VerseNumber":30,"Text":"واذا مروا بهم يتغامزون"},{"SurahNumber":83,"VerseNumber":31,"Text":"واذا انقلبوا الى اهلهم انقلبوا فكهين"},{"SurahNumber":83,"VerseNumber":32,"Text":"واذا راوهم قالوا ان هولاء لضالون"},{"SurahNumber":83,"VerseNumber":33,"Text":"وما ارسلوا عليهم حفظين"},{"SurahNumber":83,"VerseNumber":34,"Text":"فاليوم الذين ءامنوا من الكفار يضحكون"},{"SurahNumber":83,"VerseNumber":35,"Text":"على الارايك ينظرون"},{"SurahNumber":83,"VerseNumber":36,"Text":"هل ثوب الكفار ما كانوا يفعلون"}]}