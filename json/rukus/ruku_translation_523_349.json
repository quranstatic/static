{"Id":349,"LanguageCodes":["sw","swa"],"Name":"swa-alimuhsinalbarw","Author":"Ali Muhsin Al Barwani","Language":"Swahili","Description":"Swahili","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":81,"VerseNumber":1,"Text":"Jua litakapo kunjwa"},{"SurahNumber":81,"VerseNumber":2,"Text":"Na nyota zikazimwa"},{"SurahNumber":81,"VerseNumber":3,"Text":"Na milima ikaondolewa"},{"SurahNumber":81,"VerseNumber":4,"Text":"Na ngamia wenye mimba pevu watakapo achwa wasishughulikiwe"},{"SurahNumber":81,"VerseNumber":5,"Text":"Na wanyama wa mwituni wakakusanywa"},{"SurahNumber":81,"VerseNumber":6,"Text":"Na bahari zikawaka moto"},{"SurahNumber":81,"VerseNumber":7,"Text":"Na nafsi zikaunganishwa"},{"SurahNumber":81,"VerseNumber":8,"Text":"Na msichana aliye zikwa hai atapo ulizwa"},{"SurahNumber":81,"VerseNumber":9,"Text":"Kwa kosa gani aliuliwa"},{"SurahNumber":81,"VerseNumber":10,"Text":"Na madaftari yatakapo enezwa"},{"SurahNumber":81,"VerseNumber":11,"Text":"Na mbingu itapo tanduliwa"},{"SurahNumber":81,"VerseNumber":12,"Text":"Na Jahannamu itapo chochewa"},{"SurahNumber":81,"VerseNumber":13,"Text":"Na Pepo ikasogezwa"},{"SurahNumber":81,"VerseNumber":14,"Text":"Kila nafsi itajua ilicho kihudhurisha"},{"SurahNumber":81,"VerseNumber":15,"Text":"Naapa kwa nyota zinapo rejea nyuma"},{"SurahNumber":81,"VerseNumber":16,"Text":"Zinazo kwenda, kisha zikajificha"},{"SurahNumber":81,"VerseNumber":17,"Text":"Na kwa usiku unapo pungua"},{"SurahNumber":81,"VerseNumber":18,"Text":"Na kwa asubuhi inapo pambazuka"},{"SurahNumber":81,"VerseNumber":19,"Text":"Kwamba hakika bila ya shaka hii ni kauli ya Mjumbe mtukufu"},{"SurahNumber":81,"VerseNumber":20,"Text":"Mwenye nguvu na mwenye cheo kwa huyo Mwenye Kiti cha Enzi"},{"SurahNumber":81,"VerseNumber":21,"Text":"Anaye t'iiwa, tena muaminifu"},{"SurahNumber":81,"VerseNumber":22,"Text":"Na wala huyu mwenzenu hana wazimu"},{"SurahNumber":81,"VerseNumber":23,"Text":"Na hakika yeye alimwona kwenye upeo wa macho ulio safi"},{"SurahNumber":81,"VerseNumber":24,"Text":"Wala yeye si bakhili kwa mambo ya ghaibu"},{"SurahNumber":81,"VerseNumber":25,"Text":"Wala hii si kauli ya Shetani maluuni"},{"SurahNumber":81,"VerseNumber":26,"Text":"Basi mnakwenda wapi"},{"SurahNumber":81,"VerseNumber":27,"Text":"Haikuwa hii ila ni ukumbusho kwa walimwengu wote"},{"SurahNumber":81,"VerseNumber":28,"Text":"Kwa yule miongoni mwenu anaye taka kwenda sawa"},{"SurahNumber":81,"VerseNumber":29,"Text":"Wala nyinyi hamtataka isipo kuwa atake Mwenyezi Mungu Mola Mlezi wa walimwengu wote"}]}