{"Id":349,"LanguageCodes":["sw","swa"],"Name":"swa-alimuhsinalbarw","Author":"Ali Muhsin Al Barwani","Language":"Swahili","Description":"Swahili","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":56,"VerseNumber":1,"Text":"Litakapo tukia hilo Tukio"},{"SurahNumber":56,"VerseNumber":2,"Text":"Hapana cha kukanusha kutukia kwake"},{"SurahNumber":56,"VerseNumber":3,"Text":"Literemshalo linyanyualo"},{"SurahNumber":56,"VerseNumber":4,"Text":"Itakapo tikiswa ardhi kwa mtikiso"},{"SurahNumber":56,"VerseNumber":5,"Text":"Na milima itapo sagwasagwa"},{"SurahNumber":56,"VerseNumber":6,"Text":"Iwe mavumbi yanayo peperushwa"},{"SurahNumber":56,"VerseNumber":7,"Text":"Na nyinyi mtakuwa namna tatu"},{"SurahNumber":56,"VerseNumber":8,"Text":"Basi watakuwepo wa kuliani; je, ni wepi wa kuliani"},{"SurahNumber":56,"VerseNumber":9,"Text":"Na wa kushotoni; je, ni wepi wa kushotoni"},{"SurahNumber":56,"VerseNumber":10,"Text":"Na wa mbele watakuwa mbele"},{"SurahNumber":56,"VerseNumber":11,"Text":"Hao ndio watakao karibishwa"},{"SurahNumber":56,"VerseNumber":12,"Text":"Katika Bustani zenye neema"},{"SurahNumber":56,"VerseNumber":13,"Text":"Fungu kubwa katika wa mwanzo"},{"SurahNumber":56,"VerseNumber":14,"Text":"Na wachache katika wa mwisho"},{"SurahNumber":56,"VerseNumber":15,"Text":"Watakuwa juu ya viti vya fakhari vilivyo tonewa"},{"SurahNumber":56,"VerseNumber":16,"Text":"Wakiviegemea wakielekeana"},{"SurahNumber":56,"VerseNumber":17,"Text":"Wakitumikiwa na wavulana wa ujana wa milele"},{"SurahNumber":56,"VerseNumber":18,"Text":"Kwa vikombe na mabirika na bilauri za vinywaji kutoka chemchem safi"},{"SurahNumber":56,"VerseNumber":19,"Text":"Hawataumwa kichwa kwa vinywaji hivyo wala hawatoleweshwa"},{"SurahNumber":56,"VerseNumber":20,"Text":"Na matunda wayapendayo"},{"SurahNumber":56,"VerseNumber":21,"Text":"Na nyama za ndege kama wanavyo tamani"},{"SurahNumber":56,"VerseNumber":22,"Text":"Na Mahurulaini"},{"SurahNumber":56,"VerseNumber":23,"Text":"Walio kama mfano wa lulu zilio hifadhiwa"},{"SurahNumber":56,"VerseNumber":24,"Text":"Ni malipo kwa waliyo kuwa wakiyatenda"},{"SurahNumber":56,"VerseNumber":25,"Text":"Humo hawatasikia porojo wala maneno ya dhambi"},{"SurahNumber":56,"VerseNumber":26,"Text":"Isipo kuwa maneno ya Salama, Salama"},{"SurahNumber":56,"VerseNumber":27,"Text":"Na wale wa mkono wa kulia, ni yepi yatakuwa ya wa kuliani"},{"SurahNumber":56,"VerseNumber":28,"Text":"Katika mikunazi isiyo na miba"},{"SurahNumber":56,"VerseNumber":29,"Text":"Na migomba iliyo pangiliwa"},{"SurahNumber":56,"VerseNumber":30,"Text":"Na kivuli kilicho tanda"},{"SurahNumber":56,"VerseNumber":31,"Text":"Na maji yanayo miminika"},{"SurahNumber":56,"VerseNumber":32,"Text":"Na matunda mengi"},{"SurahNumber":56,"VerseNumber":33,"Text":"Hayatindikii wala hayakatazwi"},{"SurahNumber":56,"VerseNumber":34,"Text":"Na matandiko yaliyo nyanyuliwa"},{"SurahNumber":56,"VerseNumber":35,"Text":"Hakika Sisi tutawaumba (Mahurulaini) upya"},{"SurahNumber":56,"VerseNumber":36,"Text":"Na tutawafanya vijana"},{"SurahNumber":56,"VerseNumber":37,"Text":"Wanapendana na waume zao, hirimu moja"},{"SurahNumber":56,"VerseNumber":38,"Text":"Kwa ajili ya watu wa kuliani"}]}