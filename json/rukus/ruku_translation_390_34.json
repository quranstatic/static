{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":37,"VerseNumber":139,"Text":"وان يونس لمن المرسلين"},{"SurahNumber":37,"VerseNumber":140,"Text":"اذ ابق الى الفلك المشحون"},{"SurahNumber":37,"VerseNumber":141,"Text":"فساهم فكان من المدحضين"},{"SurahNumber":37,"VerseNumber":142,"Text":"فالتقمه الحوت وهو مليم"},{"SurahNumber":37,"VerseNumber":143,"Text":"فلولا انه كان من المسبحين"},{"SurahNumber":37,"VerseNumber":144,"Text":"للبث في بطنه الى يوم يبعثون"},{"SurahNumber":37,"VerseNumber":145,"Text":"۞فنبذنه بالعراء وهو سقيم"},{"SurahNumber":37,"VerseNumber":146,"Text":"وانبتنا عليه شجرة من يقطين"},{"SurahNumber":37,"VerseNumber":147,"Text":"وارسلنه الى ماية الف او يزيدون"},{"SurahNumber":37,"VerseNumber":148,"Text":"فامنوا فمتعنهم الى حين"},{"SurahNumber":37,"VerseNumber":149,"Text":"فاستفتهم الربك البنات ولهم البنون"},{"SurahNumber":37,"VerseNumber":150,"Text":"ام خلقنا المليكة انث ا وهم شهدون"},{"SurahNumber":37,"VerseNumber":151,"Text":"الا انهم من افكهم ليقولون"},{"SurahNumber":37,"VerseNumber":152,"Text":"ولد الله وانهم لكذبون"},{"SurahNumber":37,"VerseNumber":153,"Text":"اصطفى البنات على البنين"},{"SurahNumber":37,"VerseNumber":154,"Text":"ما لكم كيف تحكمون"},{"SurahNumber":37,"VerseNumber":155,"Text":"افلا تذكرون"},{"SurahNumber":37,"VerseNumber":156,"Text":"ام لكم سلطن مبين"},{"SurahNumber":37,"VerseNumber":157,"Text":"فاتوا بكتبكم ان كنتم صدقين"},{"SurahNumber":37,"VerseNumber":158,"Text":"وجعلوا بينه وبين الجنة نسب ا ولقد علمت الجنة انهم لمحضرون"},{"SurahNumber":37,"VerseNumber":159,"Text":"سبحن الله عما يصفون"},{"SurahNumber":37,"VerseNumber":160,"Text":"الا عباد الله المخلصين"},{"SurahNumber":37,"VerseNumber":161,"Text":"فانكم وما تعبدون"},{"SurahNumber":37,"VerseNumber":162,"Text":"ما انتم عليه بفتنين"},{"SurahNumber":37,"VerseNumber":163,"Text":"الا من هو صال الجحيم"},{"SurahNumber":37,"VerseNumber":164,"Text":"وما منا الا له مقام معلوم"},{"SurahNumber":37,"VerseNumber":165,"Text":"وانا لنحن الصافون"},{"SurahNumber":37,"VerseNumber":166,"Text":"وانا لنحن المسبحون"},{"SurahNumber":37,"VerseNumber":167,"Text":"وان كانوا ليقولون"},{"SurahNumber":37,"VerseNumber":168,"Text":"لو ان عندنا ذكر ا من الاولين"},{"SurahNumber":37,"VerseNumber":169,"Text":"لكنا عباد الله المخلصين"},{"SurahNumber":37,"VerseNumber":170,"Text":"فكفروا به فسوف يعلمون"},{"SurahNumber":37,"VerseNumber":171,"Text":"ولقد سبقت كلمتنا لعبادنا المرسلين"},{"SurahNumber":37,"VerseNumber":172,"Text":"انهم لهم المنصورون"},{"SurahNumber":37,"VerseNumber":173,"Text":"وان جندنا لهم الغلبون"},{"SurahNumber":37,"VerseNumber":174,"Text":"فتول عنهم حتى حين"},{"SurahNumber":37,"VerseNumber":175,"Text":"وابصرهم فسوف يبصرون"},{"SurahNumber":37,"VerseNumber":176,"Text":"افبعذابنا يستعجلون"},{"SurahNumber":37,"VerseNumber":177,"Text":"فاذا نزل بساحتهم فساء صباح المنذرين"},{"SurahNumber":37,"VerseNumber":178,"Text":"وتول عنهم حتى حين"},{"SurahNumber":37,"VerseNumber":179,"Text":"وابصر فسوف يبصرون"},{"SurahNumber":37,"VerseNumber":180,"Text":"سبحن ربك رب العزة عما يصفون"},{"SurahNumber":37,"VerseNumber":181,"Text":"وسلم على المرسلين"},{"SurahNumber":37,"VerseNumber":182,"Text":"والحمد لله رب العلمين"}]}