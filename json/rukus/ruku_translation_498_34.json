{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":68,"VerseNumber":1,"Text":"ن والقلم وما يسطرون"},{"SurahNumber":68,"VerseNumber":2,"Text":"ما انت بنعمة ربك بمجنون"},{"SurahNumber":68,"VerseNumber":3,"Text":"وان لك لاجرا غير ممنون"},{"SurahNumber":68,"VerseNumber":4,"Text":"وانك لعلى خلق عظيم"},{"SurahNumber":68,"VerseNumber":5,"Text":"فستبصر ويبصرون"},{"SurahNumber":68,"VerseNumber":6,"Text":"باييكم المفتون"},{"SurahNumber":68,"VerseNumber":7,"Text":"ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بالمهتدين"},{"SurahNumber":68,"VerseNumber":8,"Text":"فلا تطع المكذبين"},{"SurahNumber":68,"VerseNumber":9,"Text":"ودوا لو تدهن فيدهنون"},{"SurahNumber":68,"VerseNumber":10,"Text":"ولا تطع كل حلاف مهين"},{"SurahNumber":68,"VerseNumber":11,"Text":"هماز مشاء بنميم"},{"SurahNumber":68,"VerseNumber":12,"Text":"مناع للخير معتد اثيم"},{"SurahNumber":68,"VerseNumber":13,"Text":"عتل بعد ذلك زنيم"},{"SurahNumber":68,"VerseNumber":14,"Text":"ان كان ذا مال وبنين"},{"SurahNumber":68,"VerseNumber":15,"Text":"اذا تتلى عليه ءايتنا قال اسطير الاولين"},{"SurahNumber":68,"VerseNumber":16,"Text":"سنسمه على الخرطوم"},{"SurahNumber":68,"VerseNumber":17,"Text":"انا بلونهم كما بلونا اصحب الجنة اذ اقسموا ليصرمنها مصبحين"},{"SurahNumber":68,"VerseNumber":18,"Text":"ولا يستثنون"},{"SurahNumber":68,"VerseNumber":19,"Text":"فطاف عليها طايف من ربك وهم نايمون"},{"SurahNumber":68,"VerseNumber":20,"Text":"فاصبحت كالصريم"},{"SurahNumber":68,"VerseNumber":21,"Text":"فتنادوا مصبحين"},{"SurahNumber":68,"VerseNumber":22,"Text":"ان اغدوا على حرثكم ان كنتم صرمين"},{"SurahNumber":68,"VerseNumber":23,"Text":"فانطلقوا وهم يتخفتون"},{"SurahNumber":68,"VerseNumber":24,"Text":"ان لا يدخلنها اليوم عليكم مسكين"},{"SurahNumber":68,"VerseNumber":25,"Text":"وغدوا على حرد قدرين"},{"SurahNumber":68,"VerseNumber":26,"Text":"فلما راوها قالوا انا لضالون"},{"SurahNumber":68,"VerseNumber":27,"Text":"بل نحن محرومون"},{"SurahNumber":68,"VerseNumber":28,"Text":"قال اوسطهم الم اقل لكم لولا تسبحون"},{"SurahNumber":68,"VerseNumber":29,"Text":"قالوا سبحن ربنا انا كنا ظلمين"},{"SurahNumber":68,"VerseNumber":30,"Text":"فاقبل بعضهم على بعض يتلومون"},{"SurahNumber":68,"VerseNumber":31,"Text":"قالوا يويلنا انا كنا طغين"},{"SurahNumber":68,"VerseNumber":32,"Text":"عسى ربنا ان يبدلنا خير ا منها انا الى ربنا رغبون"},{"SurahNumber":68,"VerseNumber":33,"Text":"كذلك العذاب ولعذاب الاخرة اكبر لو كانوا يعلمون"}]}