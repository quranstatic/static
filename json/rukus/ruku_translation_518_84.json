{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":78,"VerseNumber":1,"Text":"Hvad være de questioning"},{"SurahNumber":78,"VerseNumber":2,"Text":"great begivenheden"},{"SurahNumber":78,"VerseNumber":3,"Text":"Den disputed dem"},{"SurahNumber":78,"VerseNumber":4,"Text":"Indeed de gøre det grundlægge ydre"},{"SurahNumber":78,"VerseNumber":5,"Text":"Fleste assuredly de gøre det grundlægge ydre"},{"SurahNumber":78,"VerseNumber":6,"Text":"Vi ikke stillede jorden beboelige"},{"SurahNumber":78,"VerseNumber":7,"Text":"Bjergene stabilisatorer"},{"SurahNumber":78,"VerseNumber":8,"Text":"Vi oprettede jer mager ene another"},{"SurahNumber":78,"VerseNumber":9,"Text":"Vi oprettede sovende så du hvile"},{"SurahNumber":78,"VerseNumber":10,"Text":"Vi stillede natten bedække"},{"SurahNumber":78,"VerseNumber":11,"Text":"Dagen søge forråd"},{"SurahNumber":78,"VerseNumber":12,"Text":"Vi byggede I syv universer"},{"SurahNumber":78,"VerseNumber":13,"Text":"Vi oprettede lyse lampe"},{"SurahNumber":78,"VerseNumber":14,"Text":"Vi sender skyerne hælde vand"},{"SurahNumber":78,"VerseNumber":15,"Text":"Til producere det korn planter"},{"SurahNumber":78,"VerseNumber":16,"Text":"Og adskillige frugthaver"},{"SurahNumber":78,"VerseNumber":17,"Text":"Dagen Bestemmelse udnævnes"},{"SurahNumber":78,"VerseNumber":18,"Text":"Dagen hornet pustes du kommer ind trængsel"},{"SurahNumber":78,"VerseNumber":19,"Text":"Himlen være åbnede låger"},{"SurahNumber":78,"VerseNumber":20,"Text":"Bjergene flyttes idet de var luftspejling"},{"SurahNumber":78,"VerseNumber":21,"Text":"Gehenna er uundgåelige"},{"SurahNumber":78,"VerseNumber":22,"Text":"I overtræderne; det være deres bolig"},{"SurahNumber":78,"VerseNumber":23,"Text":"De bliver det for ælde"},{"SurahNumber":78,"VerseNumber":24,"Text":"De aldrig smager det kølighed nor drink"},{"SurahNumber":78,"VerseNumber":25,"Text":"Kun inferno bitre mad"},{"SurahNumber":78,"VerseNumber":26,"Text":"Retfærdige requital"},{"SurahNumber":78,"VerseNumber":27,"Text":"De aldrig forventede holdes accountable"},{"SurahNumber":78,"VerseNumber":28,"Text":"utterly forkaste vore tegn"},{"SurahNumber":78,"VerseNumber":29,"Text":"Vi talte alt rekord"},{"SurahNumber":78,"VerseNumber":30,"Text":"Lide konsekvenserne; vi kun forøge Deres gengældelse"}]}