{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":37,"VerseNumber":114,"Text":"ولقد مننا على موسى وهرون"},{"SurahNumber":37,"VerseNumber":115,"Text":"ونجينهما وقومهما من الكرب العظيم"},{"SurahNumber":37,"VerseNumber":116,"Text":"ونصرنهم فكانوا هم الغلبين"},{"SurahNumber":37,"VerseNumber":117,"Text":"وءاتينهما الكتب المستبين"},{"SurahNumber":37,"VerseNumber":118,"Text":"وهدينهما الصرط المستقيم"},{"SurahNumber":37,"VerseNumber":119,"Text":"وتركنا عليهما في الاخرين"},{"SurahNumber":37,"VerseNumber":120,"Text":"سلم على موسى وهرون"},{"SurahNumber":37,"VerseNumber":121,"Text":"انا كذلك نجزي المحسنين"},{"SurahNumber":37,"VerseNumber":122,"Text":"انهما من عبادنا المومنين"},{"SurahNumber":37,"VerseNumber":123,"Text":"وان الياس لمن المرسلين"},{"SurahNumber":37,"VerseNumber":124,"Text":"اذ قال لقومه الا تتقون"},{"SurahNumber":37,"VerseNumber":125,"Text":"اتدعون بعل ا وتذرون احسن الخلقين"},{"SurahNumber":37,"VerseNumber":126,"Text":"الله ربكم ورب ءابايكم الاولين"},{"SurahNumber":37,"VerseNumber":127,"Text":"فكذبوه فانهم لمحضرون"},{"SurahNumber":37,"VerseNumber":128,"Text":"الا عباد الله المخلصين"},{"SurahNumber":37,"VerseNumber":129,"Text":"وتركنا عليه في الاخرين"},{"SurahNumber":37,"VerseNumber":130,"Text":"سلم على ال ياسين"},{"SurahNumber":37,"VerseNumber":131,"Text":"انا كذلك نجزي المحسنين"},{"SurahNumber":37,"VerseNumber":132,"Text":"انه من عبادنا المومنين"},{"SurahNumber":37,"VerseNumber":133,"Text":"وان لوط ا لمن المرسلين"},{"SurahNumber":37,"VerseNumber":134,"Text":"اذ نجينه واهله اجمعين"},{"SurahNumber":37,"VerseNumber":135,"Text":"الا عجوز ا في الغبرين"},{"SurahNumber":37,"VerseNumber":136,"Text":"ثم دمرنا الاخرين"},{"SurahNumber":37,"VerseNumber":137,"Text":"وانكم لتمرون عليهم مصبحين"},{"SurahNumber":37,"VerseNumber":138,"Text":"وباليل افلا تعقلون"}]}