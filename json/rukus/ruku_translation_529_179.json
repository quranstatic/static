{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":87,"VerseNumber":1,"Text":"Senin innde joom Ma ɓurnde toowde"},{"SurahNumber":87,"VerseNumber":2,"Text":"Oon takɗo O fonndi"},{"SurahNumber":87,"VerseNumber":3,"Text":"Oon koddirɗo refti O feewni"},{"SurahNumber":87,"VerseNumber":4,"Text":"Oon yaltinɗo durngol"},{"SurahNumber":87,"VerseNumber":5,"Text":"O watti ngol yoorko wayliiko"},{"SurahNumber":87,"VerseNumber":6,"Text":"Ma Min njanngin Ma A yeɓjjittah"},{"SurahNumber":87,"VerseNumber":7,"Text":"Si wonah ko Alla haaji pellet hombo anndi ko feeñi e ko wirnii"},{"SurahNumber":87,"VerseNumber":8,"Text":"Min newnan Ma na newori"},{"SurahNumber":87,"VerseNumber":9,"Text":"Siftinir ɗoon ɗo ciftanirgol na nafata"},{"SurahNumber":87,"VerseNumber":10,"Text":"Ma waacta oon kulɗo Alla"},{"SurahNumber":87,"VerseNumber":11,"Text":"Woɗɗitoo mbo ɓurɗo hiiteede"},{"SurahNumber":87,"VerseNumber":12,"Text":"Oon naatoowo yiite mannge nge"},{"SurahNumber":87,"VerseNumber":13,"Text":"Refti o maayatah toon o wuuratah"},{"SurahNumber":87,"VerseNumber":14,"Text":"Malaama oon laaɓɗo e sirku"},{"SurahNumber":87,"VerseNumber":15,"Text":"O jantii Innde joom makko o juuli"},{"SurahNumber":87,"VerseNumber":16,"Text":"Ko woni, ko hoɗon ɓurna nguurndam aduna"},{"SurahNumber":87,"VerseNumber":17,"Text":"Ko laakira ɓuri moƴƴude e heddaade"},{"SurahNumber":87,"VerseNumber":18,"Text":"ɗum ina e ɗereeji gadiiɗi"},{"SurahNumber":87,"VerseNumber":19,"Text":"ɗereeji Ibraahiima e Muusa"}]}