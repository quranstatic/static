{"Id":149,"LanguageCodes":["eo","epo"],"Name":"epo-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Esperanto","Description":"Esperanto","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"blov vent"},{"SurahNumber":51,"VerseNumber":2,"Text":"Bor pluv"},{"SurahNumber":51,"VerseNumber":3,"Text":"Bringing provisions"},{"SurahNumber":51,"VerseNumber":4,"Text":"Distribu them as komand"},{"SurahNumber":51,"VerseNumber":5,"Text":"Ki promes vi surely ven pas"},{"SurahNumber":51,"VerseNumber":6,"Text":"Tag Judgment est inevitable"},{"SurahNumber":51,"VerseNumber":7,"Text":"Despite perfectly kre cxiel"},{"SurahNumber":51,"VerseNumber":8,"Text":"Vi ad disput truth"},{"SurahNumber":51,"VerseNumber":9,"Text":"Deviating therefrom est deviators"},{"SurahNumber":51,"VerseNumber":10,"Text":"Ve al falsifiers"},{"SurahNumber":51,"VerseNumber":11,"Text":"En their blundering ili est totally heedless"},{"SurahNumber":51,"VerseNumber":12,"Text":"Ili question Tag Judgment"},{"SurahNumber":51,"VerseNumber":13,"Text":"Tag ili prezent fajr"},{"SurahNumber":51,"VerseNumber":14,"Text":"Gust retribution; this est ki vi uz challenge"},{"SurahNumber":51,"VerseNumber":15,"Text":"righteous ind gxarden printemp"},{"SurahNumber":51,"VerseNumber":16,"Text":"Ili ricev their Lord's rekompenc ili uz est pi"},{"SurahNumber":51,"VerseNumber":17,"Text":"Rarely did ili dorm whole nokt"},{"SurahNumber":51,"VerseNumber":18,"Text":"Je dawn ili pregx forgiveness"},{"SurahNumber":51,"VerseNumber":19,"Text":"portion their mon est set aside por beggar needy"},{"SurahNumber":51,"VerseNumber":20,"Text":"Ter est plen da sign those est nepr"},{"SurahNumber":51,"VerseNumber":21,"Text":"within vi seg"},{"SurahNumber":51,"VerseNumber":22,"Text":"En heaven est your provision cxio promes vi"},{"SurahNumber":51,"VerseNumber":23,"Text":"Per Lord de heaven ter this est as prav as ti vi parol"}]}