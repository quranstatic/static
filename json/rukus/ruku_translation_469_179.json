{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":56,"VerseNumber":1,"Text":"Si darotoongal ngal darike"},{"SurahNumber":56,"VerseNumber":2,"Text":"alanaa daragol maggal ngol fennoowo"},{"SurahNumber":56,"VerseNumber":3,"Text":"Hoyfinɗinayngal (yedduɓe), ɓamtayngal (gomɗimɓe)"},{"SurahNumber":56,"VerseNumber":4,"Text":"Si leydi ndin dilliniraama dillannde"},{"SurahNumber":56,"VerseNumber":5,"Text":"pelle ɗen munciroyee muncannde"},{"SurahNumber":56,"VerseNumber":6,"Text":"ɗe wonta collaari ruuynayndi"},{"SurahNumber":56,"VerseNumber":7,"Text":"wattinon nooneeji tati"},{"SurahNumber":56,"VerseNumber":8,"Text":"Ko wontirooɓe ñaamo ɓen - iskin ɓen yimɓe ñaamo"},{"SurahNumber":56,"VerseNumber":9,"Text":"e wontirooɓe nano ɓen, bis e ɓen yimɓe nano"},{"SurahNumber":56,"VerseNumber":10,"Text":"Ko adondiraynooɓe ɓen (e moƴƴere ka aduna), adoytee (naadeede Aljanna)"},{"SurahNumber":56,"VerseNumber":11,"Text":"Ko ɓen woni badnitaaɓe ɓen"},{"SurahNumber":56,"VerseNumber":12,"Text":"ka Aljannaaji neeminiiɗi"},{"SurahNumber":56,"VerseNumber":13,"Text":"dental jeyaangal e [mofte] adinooɓe ɓen"},{"SurahNumber":56,"VerseNumber":14,"Text":"e seeɗaaɓe jeyaaɓe e [mofte] sakkitiiɓe ɓen"},{"SurahNumber":56,"VerseNumber":15,"Text":"hiɓe e hoore danɗe yuɓɓiraaɗe [kaŋŋe]"},{"SurahNumber":56,"VerseNumber":16,"Text":"hara hiɓe soɓɓindii e majje, fewtondirɓe"},{"SurahNumber":56,"VerseNumber":17,"Text":"Sukaaɓe duuminaaɓe hino wanngoo hakkunde maɓɓe"},{"SurahNumber":56,"VerseNumber":18,"Text":"wonndude e ñedduɗe, e cataloy, e koroy e [hebbiniraakoy] beere ilaynde"},{"SurahNumber":56,"VerseNumber":19,"Text":"ɓe muusetaake ko*e nder ton, ɓe siwritaa"},{"SurahNumber":56,"VerseNumber":20,"Text":"e muuyanteeɗi ɗi ɓe suɓotoo"},{"SurahNumber":56,"VerseNumber":21,"Text":"e teewu colli ngu ɓe tuuyatee"},{"SurahNumber":56,"VerseNumber":22,"Text":"e huurul'ayniiɓe -yaaja gite en"},{"SurahNumber":56,"VerseNumber":23,"Text":"nannduɓe e kaŋŋe lu'lu'u maraaɗe"},{"SurahNumber":56,"VerseNumber":24,"Text":"njoɓdi ko ɓe gollaynoo kon"},{"SurahNumber":56,"VerseNumber":25,"Text":"Ɓe nanataa nder ton haala meere, wanaa ko bakkondiŋta"},{"SurahNumber":56,"VerseNumber":26,"Text":"si wanaa wi'aneede : \"Salaaman! Salaaman!\" [Saɗi mon! saɗi mon]"},{"SurahNumber":56,"VerseNumber":27,"Text":"E wontirooɓe ñaamo ɓen, ko woni ɓen yimɓe ñaamo"},{"SurahNumber":56,"VerseNumber":28,"Text":"[Ɓe wonay] e leɗɗe jaaɓe newɗuɗe"},{"SurahNumber":56,"VerseNumber":29,"Text":"e banaanaje fawandirɗe"},{"SurahNumber":56,"VerseNumber":30,"Text":"e ɗowdi duumiindi"},{"SurahNumber":56,"VerseNumber":31,"Text":"e ndiyam juurayɗam"},{"SurahNumber":56,"VerseNumber":32,"Text":"e ɓiɗɓe leɗɗe ɗuuɗuɗe"},{"SurahNumber":56,"VerseNumber":33,"Text":"ɗe taƴondirtaa ɗe haɗetaake"},{"SurahNumber":56,"VerseNumber":34,"Text":"e danɗe ɓamtaaɗe"},{"SurahNumber":56,"VerseNumber":35,"Text":"Ko Menen tagi fuɗɗi ɓe"},{"SurahNumber":56,"VerseNumber":36,"Text":"Men waɗi ɓe curbaaɓe-boomi"},{"SurahNumber":56,"VerseNumber":37,"Text":"yiɗuɓe worɓe mun, goreeɓe"},{"SurahNumber":56,"VerseNumber":38,"Text":"[Men marani ɓe] yimɓe ñaamo ɓen"}]}