{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":86,"VerseNumber":1,"Text":"O woondirii kammu e hoodere fuɗoore jemma"},{"SurahNumber":86,"VerseNumber":2,"Text":"Hol ko anndin ma ko woni hoodere feeñoore jemma"},{"SurahNumber":86,"VerseNumber":3,"Text":"Hoodere fooynunde"},{"SurahNumber":86,"VerseNumber":4,"Text":"Alah fittaandu si wonah ina e dow mayru deenoowo"},{"SurahNumber":86,"VerseNumber":5,"Text":"Yo aade ndaaru ko o tagraa ko"},{"SurahNumber":86,"VerseNumber":6,"Text":"O tagiraama ko e ndiyam nduñotoɗam"},{"SurahNumber":86,"VerseNumber":7,"Text":"Hombo e hakkunde keeci gorko e ƴi*e ɓernde debba"},{"SurahNumber":86,"VerseNumber":8,"Text":"Hombo hattani ruttude mbo"},{"SurahNumber":86,"VerseNumber":9,"Text":"ñande cuuɗiiɗi kuncaa"},{"SurahNumber":86,"VerseNumber":10,"Text":"Alanah mbo semmbe wonah balloowo"},{"SurahNumber":86,"VerseNumber":11,"Text":"Mi woondirii kammu jom ɗate ngun"},{"SurahNumber":86,"VerseNumber":12,"Text":"O woondirii leydi mbaɗndi petti"},{"SurahNumber":86,"VerseNumber":13,"Text":"Pellet o Quraana kombo ceerndoowol"},{"SurahNumber":86,"VerseNumber":14,"Text":"Mbo wonah gaajaate"},{"SurahNumber":86,"VerseNumber":15,"Text":"Pellet kamɓe hoɓe pewja peeje"},{"SurahNumber":86,"VerseNumber":16,"Text":"Mi fewja feere"},{"SurahNumber":86,"VerseNumber":17,"Text":"Muñanɓe muñangol setta"}]}