{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":74,"VerseNumber":32,"Text":"Absolut (jeg sværger) månen"},{"SurahNumber":74,"VerseNumber":33,"Text":"Og natten det vedta'r"},{"SurahNumber":74,"VerseNumber":34,"Text":"Og formiddagen det skinner"},{"SurahNumber":74,"VerseNumber":35,"Text":"Den er ene af great miraklerne"},{"SurahNumber":74,"VerseNumber":36,"Text":"Varsel til menneskelige væddeløbet"},{"SurahNumber":74,"VerseNumber":37,"Text":"De hos jer ønsker avancere regress"},{"SurahNumber":74,"VerseNumber":38,"Text":"Enhvere sjæl trapped dens synder"},{"SurahNumber":74,"VerseNumber":39,"Text":"Frelser de på retten"},{"SurahNumber":74,"VerseNumber":40,"Text":"Mens Paradis de spørge"},{"SurahNumber":74,"VerseNumber":41,"Text":"Omkring skyldig"},{"SurahNumber":74,"VerseNumber":42,"Text":"Hvad bragte dig den gengældelse"},{"SurahNumber":74,"VerseNumber":43,"Text":"De sie Vi ikke observe kontakten bønner (Salat)"},{"SurahNumber":74,"VerseNumber":44,"Text":"Vi ikke fodrede fattige"},{"SurahNumber":74,"VerseNumber":45,"Text":"Vi blundered blunderers"},{"SurahNumber":74,"VerseNumber":46,"Text":"Vi disbelieved Dagen Bedømmelse"},{"SurahNumber":74,"VerseNumber":47,"Text":"Indtil certainty kom os omgående"},{"SurahNumber":74,"VerseNumber":48,"Text":"intercession intercessors aldrig hjælpes dem"},{"SurahNumber":74,"VerseNumber":49,"Text":"Hvorfor være de så averse til den påmindelse"},{"SurahNumber":74,"VerseNumber":50,"Text":"Løbende zebraer"},{"SurahNumber":74,"VerseNumber":51,"Text":"Som fleeing løven"},{"SurahNumber":74,"VerseNumber":52,"Text":"Hvert en af dem vil gøre modta scripture personally"},{"SurahNumber":74,"VerseNumber":53,"Text":"Indeed de ikke frygter Herefter"},{"SurahNumber":74,"VerseNumber":54,"Text":"Indeed den er påmindelse"},{"SurahNumber":74,"VerseNumber":55,"Text":"De ønsker tage heed"},{"SurahNumber":74,"VerseNumber":56,"Text":"De ikke tage heed imod gud ville Han er kilden retmæssighed; Han er kilden tilgivelse"}]}