{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":90,"VerseNumber":1,"Text":"La uksimü bilhazelbeledi"},{"SurahNumber":90,"VerseNumber":2,"Text":"Ve ente hıllün bihazelbeledi"},{"SurahNumber":90,"VerseNumber":3,"Text":"Ve validin ve ma velede"},{"SurahNumber":90,"VerseNumber":4,"Text":"Lekad halaknel´insane fiy kebedin"},{"SurahNumber":90,"VerseNumber":5,"Text":"Eyahsebü en len yakdire ´aleyhi ehadün"},{"SurahNumber":90,"VerseNumber":6,"Text":"Yekulü ehlektü malen lübeden"},{"SurahNumber":90,"VerseNumber":7,"Text":"Eyahsebü en lem yerehu ehadün"},{"SurahNumber":90,"VerseNumber":8,"Text":"Elem nec´al lehu ´ayneyni"},{"SurahNumber":90,"VerseNumber":9,"Text":"Ve lisanen ve şefeteyni"},{"SurahNumber":90,"VerseNumber":10,"Text":"Ve hedeynahünnecdeyni"},{"SurahNumber":90,"VerseNumber":11,"Text":"Felaktehamel´akabete"},{"SurahNumber":90,"VerseNumber":12,"Text":"Ve ma edrake mel´akabetü"},{"SurahNumber":90,"VerseNumber":13,"Text":"Fekkü rekabetin"},{"SurahNumber":90,"VerseNumber":14,"Text":"Ev ıt´amün fiy yevmin ziy mesğabetin"},{"SurahNumber":90,"VerseNumber":15,"Text":"Yetiymen za makrebetin"},{"SurahNumber":90,"VerseNumber":16,"Text":"Ev miskiynen za metrebetin"},{"SurahNumber":90,"VerseNumber":17,"Text":"Sümme kane minelleziyne amenu ve tevasav bissabri ve tevasav bilmerhameti"},{"SurahNumber":90,"VerseNumber":18,"Text":"Ol,eke ashabülmeymene"},{"SurahNumber":90,"VerseNumber":19,"Text":"Velleziyne keferu biayatina hüm ashabülmeş´emeti"},{"SurahNumber":90,"VerseNumber":20,"Text":"´Aleyhim narün mü´sadetün"}]}