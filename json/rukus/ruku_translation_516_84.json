{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":77,"VerseNumber":1,"Text":"(Engle) dispatched ind succession"},{"SurahNumber":77,"VerseNumber":2,"Text":"Til drive vinden"},{"SurahNumber":77,"VerseNumber":3,"Text":"Stir skyer"},{"SurahNumber":77,"VerseNumber":4,"Text":"Distribuere forrådene"},{"SurahNumber":77,"VerseNumber":5,"Text":"afsie meddelelse"},{"SurahNumber":77,"VerseNumber":6,"Text":"Gode nyhed også idet varsler"},{"SurahNumber":77,"VerseNumber":7,"Text":"Hvad loves komme vedtage"},{"SurahNumber":77,"VerseNumber":8,"Text":"Dermed hvornår stjernerne lægge"},{"SurahNumber":77,"VerseNumber":9,"Text":"Himlen er åbne oppe"},{"SurahNumber":77,"VerseNumber":10,"Text":"Bjergene puste"},{"SurahNumber":77,"VerseNumber":11,"Text":"Budene summoned"},{"SurahNumber":77,"VerseNumber":12,"Text":"Den er udnævne dagen"},{"SurahNumber":77,"VerseNumber":13,"Text":"Dagen Bestemmelse"},{"SurahNumber":77,"VerseNumber":14,"Text":"Hvad Dag Bestemmelse"},{"SurahNumber":77,"VerseNumber":15,"Text":"Ve den dag til rejectors"},{"SurahNumber":77,"VerseNumber":16,"Text":"Vi ikke annihilate tidligere generationerne"},{"SurahNumber":77,"VerseNumber":17,"Text":"Derefter vi stillede others følger dem"},{"SurahNumber":77,"VerseNumber":18,"Text":"Den er hvad vi gøre forbryderne"},{"SurahNumber":77,"VerseNumber":19,"Text":"Ve den dag til rejectors"},{"SurahNumber":77,"VerseNumber":20,"Text":"Vi ikke oprettede jer lowly væske"},{"SurahNumber":77,"VerseNumber":21,"Text":"Derefter vi placerede det well-protected opbevaringsted"},{"SurahNumber":77,"VerseNumber":22,"Text":"I specifikke periode"},{"SurahNumber":77,"VerseNumber":23,"Text":"Vi målte det netop. Vi er god konstruktørerne"},{"SurahNumber":77,"VerseNumber":24,"Text":"Ve den dag til rejectors"},{"SurahNumber":77,"VerseNumber":25,"Text":"Vi ikke stillede jorden bolig"},{"SurahNumber":77,"VerseNumber":26,"Text":"I leve afdødt"},{"SurahNumber":77,"VerseNumber":27,"Text":"Vi placerede det højeste bjerge forsynede jer med friske vande drikke"},{"SurahNumber":77,"VerseNumber":28,"Text":"Ve den dag til rejectors"},{"SurahNumber":77,"VerseNumber":29,"Text":"Gå til hvad du brugte disbelieve ind"},{"SurahNumber":77,"VerseNumber":30,"Text":"Gå shade af tre forskellige tætheder"},{"SurahNumber":77,"VerseNumber":31,"Text":"Endnu det forsyner heller kølighed nor beskyttelse fra varmen"},{"SurahNumber":77,"VerseNumber":32,"Text":"Det kaster gnister idet stor idet palæer"},{"SurahNumber":77,"VerseNumber":33,"Text":"Idet gule idet farven kameler"},{"SurahNumber":77,"VerseNumber":34,"Text":"Ve den dag til rejectors"},{"SurahNumber":77,"VerseNumber":35,"Text":"Den er dagen de ikke taler"},{"SurahNumber":77,"VerseNumber":36,"Text":"Nor være de give tilladelse undskylde"},{"SurahNumber":77,"VerseNumber":37,"Text":"Ve den dag til rejectors"},{"SurahNumber":77,"VerseNumber":38,"Text":"Den er Dagen Bestemmelse. Vi summoned jer hidtige generationerne"},{"SurahNumber":77,"VerseNumber":39,"Text":"Du haver nogen schemes går fremad scheme"},{"SurahNumber":77,"VerseNumber":40,"Text":"Ve den dag til rejectors"}]}