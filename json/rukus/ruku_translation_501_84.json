{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":69,"VerseNumber":38,"Text":"Jeg sværger hvad du ser"},{"SurahNumber":69,"VerseNumber":39,"Text":"Og hvad du ikke ser"},{"SurahNumber":69,"VerseNumber":40,"Text":"Den er utterance ærværdige bud"},{"SurahNumber":69,"VerseNumber":41,"Text":"Ikke utterance digter; sjældent gør du tror"},{"SurahNumber":69,"VerseNumber":42,"Text":"Nor utterance spåmand; sjældent gør du tager heed"},{"SurahNumber":69,"VerseNumber":43,"Text":"Åbenbaring fra Lord universet"},{"SurahNumber":69,"VerseNumber":44,"Text":"Havde han uttered nogen øvrige undervisninger"},{"SurahNumber":69,"VerseNumber":45,"Text":"Vi straffe ham"},{"SurahNumber":69,"VerseNumber":46,"Text":"Vi stoppe åbenbaringerne ham"},{"SurahNumber":69,"VerseNumber":47,"Text":"Ingen af du hjælpes ham"},{"SurahNumber":69,"VerseNumber":48,"Text":"Den er påmindelse i de righteous"},{"SurahNumber":69,"VerseNumber":49,"Text":"Vi kender; visse af du er rejectors"},{"SurahNumber":69,"VerseNumber":50,"Text":"det er sorrow tvivlerne"},{"SurahNumber":69,"VerseNumber":51,"Text":"Det er absolutte sandheden"},{"SurahNumber":69,"VerseNumber":52,"Text":"Derfor du forherlige navnet Deres Lord Great"}]}