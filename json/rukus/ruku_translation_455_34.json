{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"والذريت ذرو ا"},{"SurahNumber":51,"VerseNumber":2,"Text":"فالحملت وقر ا"},{"SurahNumber":51,"VerseNumber":3,"Text":"فالجريت يسر ا"},{"SurahNumber":51,"VerseNumber":4,"Text":"فالمقسمت امرا"},{"SurahNumber":51,"VerseNumber":5,"Text":"انما توعدون لصادق"},{"SurahNumber":51,"VerseNumber":6,"Text":"وان الدين لوقع"},{"SurahNumber":51,"VerseNumber":7,"Text":"والسماء ذات الحبك"},{"SurahNumber":51,"VerseNumber":8,"Text":"انكم لفي قول مختلف"},{"SurahNumber":51,"VerseNumber":9,"Text":"يوفك عنه من افك"},{"SurahNumber":51,"VerseNumber":10,"Text":"قتل الخرصون"},{"SurahNumber":51,"VerseNumber":11,"Text":"الذين هم في غمرة ساهون"},{"SurahNumber":51,"VerseNumber":12,"Text":"يسلون ايان يوم الدين"},{"SurahNumber":51,"VerseNumber":13,"Text":"يوم هم على النار يفتنون"},{"SurahNumber":51,"VerseNumber":14,"Text":"ذوقوا فتنتكم هذا الذي كنتم به تستعجلون"},{"SurahNumber":51,"VerseNumber":15,"Text":"ان المتقين في جنت وعيون"},{"SurahNumber":51,"VerseNumber":16,"Text":"ءاخذين ما ءاتىهم ربهم انهم كانوا قبل ذلك محسنين"},{"SurahNumber":51,"VerseNumber":17,"Text":"كانوا قليل ا من اليل ما يهجعون"},{"SurahNumber":51,"VerseNumber":18,"Text":"وبالاسحار هم يستغفرون"},{"SurahNumber":51,"VerseNumber":19,"Text":"وفي امولهم حق للسايل والمحروم"},{"SurahNumber":51,"VerseNumber":20,"Text":"وفي الارض ءايت للموقنين"},{"SurahNumber":51,"VerseNumber":21,"Text":"وفي انفسكم افلا تبصرون"},{"SurahNumber":51,"VerseNumber":22,"Text":"وفي السماء رزقكم وما توعدون"},{"SurahNumber":51,"VerseNumber":23,"Text":"فورب السماء والارض انه لحق مثل ما انكم تنطقون"}]}