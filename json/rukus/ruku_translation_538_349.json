{"Id":349,"LanguageCodes":["sw","swa"],"Name":"swa-alimuhsinalbarw","Author":"Ali Muhsin Al Barwani","Language":"Swahili","Description":"Swahili","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":96,"VerseNumber":1,"Text":"Soma kwa jina la Mola wako Mlezi aliye umba"},{"SurahNumber":96,"VerseNumber":2,"Text":"Amemuumba binaadamu kwa tone la damu"},{"SurahNumber":96,"VerseNumber":3,"Text":"Soma! Na Mola wako Mlezi ni Karimu kushinda wote"},{"SurahNumber":96,"VerseNumber":4,"Text":"Ambaye amefundisha kwa kalamu"},{"SurahNumber":96,"VerseNumber":5,"Text":"Kamfundisha mtu aliyo kuwa hayajui"},{"SurahNumber":96,"VerseNumber":6,"Text":"Kwani! Hakika mtu bila ya shaka huwa jeuri"},{"SurahNumber":96,"VerseNumber":7,"Text":"Akijiona katajirika"},{"SurahNumber":96,"VerseNumber":8,"Text":"Hakika kwa Mola wako Mlezi ndio marejeo"},{"SurahNumber":96,"VerseNumber":9,"Text":"Umemwona yule anaye mkataza"},{"SurahNumber":96,"VerseNumber":10,"Text":"Mja anapo sali"},{"SurahNumber":96,"VerseNumber":11,"Text":"Umeona kama yeye yuko juu ya uwongofu"},{"SurahNumber":96,"VerseNumber":12,"Text":"Au anaamrisha uchamngu"},{"SurahNumber":96,"VerseNumber":13,"Text":"Umeona kama yeye akikanusha na anarudi nyuma"},{"SurahNumber":96,"VerseNumber":14,"Text":"Hajui ya kwamba Mwenyezi Mungu anaona"},{"SurahNumber":96,"VerseNumber":15,"Text":"Kwani! Kama haachi, tutamkokota kwa shungi la nywele"},{"SurahNumber":96,"VerseNumber":16,"Text":"Shungi la uwongo, lenye makosa"},{"SurahNumber":96,"VerseNumber":17,"Text":"Basi na awaite wenzake"},{"SurahNumber":96,"VerseNumber":18,"Text":"Nasi tutawaita Mazabania"},{"SurahNumber":96,"VerseNumber":19,"Text":"Hasha! Usimt'ii! Nawe sujudu na ujongee"}]}