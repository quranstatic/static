{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":15,"VerseNumber":80,"Text":"ولقد كذب اصحب الحجر المرسلين"},{"SurahNumber":15,"VerseNumber":81,"Text":"وءاتينهم ءايتنا فكانوا عنها معرضين"},{"SurahNumber":15,"VerseNumber":82,"Text":"وكانوا ينحتون من الجبال بيوتا ءامنين"},{"SurahNumber":15,"VerseNumber":83,"Text":"فاخذتهم الصيحة مصبحين"},{"SurahNumber":15,"VerseNumber":84,"Text":"فما اغنى عنهم ما كانوا يكسبون"},{"SurahNumber":15,"VerseNumber":85,"Text":"وما خلقنا السموت والارض وما بينهما الا بالحق وان الساعة لاتية فاصفح الصفح الجميل"},{"SurahNumber":15,"VerseNumber":86,"Text":"ان ربك هو الخلق العليم"},{"SurahNumber":15,"VerseNumber":87,"Text":"ولقد ءاتينك سبع ا من المثاني والقرءان العظيم"},{"SurahNumber":15,"VerseNumber":88,"Text":"لا تمدن عينيك الى ما متعنا به ازوج ا منهم ولا تحزن عليهم واخفض جناحك للمومنين"},{"SurahNumber":15,"VerseNumber":89,"Text":"وقل اني انا النذير المبين"},{"SurahNumber":15,"VerseNumber":90,"Text":"كما انزلنا على المقتسمين"},{"SurahNumber":15,"VerseNumber":91,"Text":"الذين جعلوا القرءان عضين"},{"SurahNumber":15,"VerseNumber":92,"Text":"فوربك لنسلنهم اجمعين"},{"SurahNumber":15,"VerseNumber":93,"Text":"عما كانوا يعملون"},{"SurahNumber":15,"VerseNumber":94,"Text":"فاصدع بما تومر واعرض عن المشركين"},{"SurahNumber":15,"VerseNumber":95,"Text":"انا كفينك المستهزءين"},{"SurahNumber":15,"VerseNumber":96,"Text":"الذين يجعلون مع الله الها ءاخر فسوف يعلمون"},{"SurahNumber":15,"VerseNumber":97,"Text":"ولقد نعلم انك يضيق صدرك بما يقولون"},{"SurahNumber":15,"VerseNumber":98,"Text":"فسبح بحمد ربك وكن من السجدين"},{"SurahNumber":15,"VerseNumber":99,"Text":"واعبد ربك حتى ياتيك اليقين"}]}