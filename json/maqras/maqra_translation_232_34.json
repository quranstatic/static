{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":76,"VerseNumber":19,"Text":"۞ويطوف عليهم ولدن مخلدون اذا رايتهم حسبتهم لولو ا منثور ا"},{"SurahNumber":76,"VerseNumber":20,"Text":"واذا رايت ثم رايت نعيم ا وملك ا كبيرا"},{"SurahNumber":76,"VerseNumber":21,"Text":"عليهم ثياب سندس خضر واستبرق وحلوا اساور من فضة وسقىهم ربهم شراب ا طهورا"},{"SurahNumber":76,"VerseNumber":22,"Text":"ان هذا كان لكم جزاء وكان سعيكم مشكورا"},{"SurahNumber":76,"VerseNumber":23,"Text":"انا نحن نزلنا عليك القرءان تنزيل ا"},{"SurahNumber":76,"VerseNumber":24,"Text":"فاصبر لحكم ربك ولا تطع منهم ءاثما او كفور ا"},{"SurahNumber":76,"VerseNumber":25,"Text":"واذكر اسم ربك بكرة واصيل ا"},{"SurahNumber":76,"VerseNumber":26,"Text":"ومن اليل فاسجد له وسبحه ليل ا طويلا"},{"SurahNumber":76,"VerseNumber":27,"Text":"ان هولاء يحبون العاجلة ويذرون وراءهم يوم ا ثقيل ا"},{"SurahNumber":76,"VerseNumber":28,"Text":"نحن خلقنهم وشددنا اسرهم واذا شينا بدلنا امثلهم تبديلا"},{"SurahNumber":76,"VerseNumber":29,"Text":"ان هذه تذكرة فمن شاء اتخذ الى ربه سبيل ا"},{"SurahNumber":76,"VerseNumber":30,"Text":"وما تشاءون الا ان يشاء الله ان الله كان عليما حكيم ا"},{"SurahNumber":76,"VerseNumber":31,"Text":"يدخل من يشاء في رحمته والظلمين اعد لهم عذابا اليما"},{"SurahNumber":77,"VerseNumber":1,"Text":"والمرسلت عرف ا"},{"SurahNumber":77,"VerseNumber":2,"Text":"فالعصفت عصف ا"},{"SurahNumber":77,"VerseNumber":3,"Text":"والنشرت نشر ا"},{"SurahNumber":77,"VerseNumber":4,"Text":"فالفرقت فرق ا"},{"SurahNumber":77,"VerseNumber":5,"Text":"فالملقيت ذكرا"},{"SurahNumber":77,"VerseNumber":6,"Text":"عذرا او نذرا"},{"SurahNumber":77,"VerseNumber":7,"Text":"انما توعدون لوقع"},{"SurahNumber":77,"VerseNumber":8,"Text":"فاذا النجوم طمست"},{"SurahNumber":77,"VerseNumber":9,"Text":"واذا السماء فرجت"},{"SurahNumber":77,"VerseNumber":10,"Text":"واذا الجبال نسفت"},{"SurahNumber":77,"VerseNumber":11,"Text":"واذا الرسل اقتت"},{"SurahNumber":77,"VerseNumber":12,"Text":"لاي يوم اجلت"},{"SurahNumber":77,"VerseNumber":13,"Text":"ليوم الفصل"},{"SurahNumber":77,"VerseNumber":14,"Text":"وما ادرىك ما يوم الفصل"},{"SurahNumber":77,"VerseNumber":15,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":16,"Text":"الم نهلك الاولين"},{"SurahNumber":77,"VerseNumber":17,"Text":"ثم نتبعهم الاخرين"},{"SurahNumber":77,"VerseNumber":18,"Text":"كذلك نفعل بالمجرمين"},{"SurahNumber":77,"VerseNumber":19,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":20,"Text":"الم نخلقكم من ماء مهين"},{"SurahNumber":77,"VerseNumber":21,"Text":"فجعلنه في قرار مكين"},{"SurahNumber":77,"VerseNumber":22,"Text":"الى قدر معلوم"},{"SurahNumber":77,"VerseNumber":23,"Text":"فقدرنا فنعم القدرون"},{"SurahNumber":77,"VerseNumber":24,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":25,"Text":"الم نجعل الارض كفاتا"},{"SurahNumber":77,"VerseNumber":26,"Text":"احياء واموت ا"},{"SurahNumber":77,"VerseNumber":27,"Text":"وجعلنا فيها روسي شمخت واسقينكم ماء فرات ا"},{"SurahNumber":77,"VerseNumber":28,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":29,"Text":"انطلقوا الى ما كنتم به تكذبون"},{"SurahNumber":77,"VerseNumber":30,"Text":"انطلقوا الى ظل ذي ثلث شعب"},{"SurahNumber":77,"VerseNumber":31,"Text":"لا ظليل ولا يغني من اللهب"},{"SurahNumber":77,"VerseNumber":32,"Text":"انها ترمي بشرر كالقصر"},{"SurahNumber":77,"VerseNumber":33,"Text":"كانه جملت صفر"},{"SurahNumber":77,"VerseNumber":34,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":35,"Text":"هذا يوم لا ينطقون"},{"SurahNumber":77,"VerseNumber":36,"Text":"ولا يوذن لهم فيعتذرون"},{"SurahNumber":77,"VerseNumber":37,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":38,"Text":"هذا يوم الفصل جمعنكم والاولين"},{"SurahNumber":77,"VerseNumber":39,"Text":"فان كان لكم كيد فكيدون"},{"SurahNumber":77,"VerseNumber":40,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":41,"Text":"ان المتقين في ظلل وعيون"},{"SurahNumber":77,"VerseNumber":42,"Text":"وفوكه مما يشتهون"},{"SurahNumber":77,"VerseNumber":43,"Text":"كلوا واشربوا هنيا بما كنتم تعملون"},{"SurahNumber":77,"VerseNumber":44,"Text":"انا كذلك نجزي المحسنين"},{"SurahNumber":77,"VerseNumber":45,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":46,"Text":"كلوا وتمتعوا قليلا انكم مجرمون"},{"SurahNumber":77,"VerseNumber":47,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":48,"Text":"واذا قيل لهم اركعوا لا يركعون"},{"SurahNumber":77,"VerseNumber":49,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":50,"Text":"فباي حديث بعده يومنون"}]}