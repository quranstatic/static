{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":80,"VerseNumber":1,"Text":"Han (Muhammad) frowned og dreje"},{"SurahNumber":80,"VerseNumber":2,"Text":"Hvornår blænde mand kom ham"},{"SurahNumber":80,"VerseNumber":3,"Text":"Hvordan du kender? Han purify"},{"SurahNumber":80,"VerseNumber":4,"Text":"Han skiftes heed gavn fra meddelelsen"},{"SurahNumber":80,"VerseNumber":5,"Text":"Angående rige manden"},{"SurahNumber":80,"VerseNumber":6,"Text":"Du gav ham Deres opmærksomhed"},{"SurahNumber":80,"VerseNumber":7,"Text":"Jævne skønt du ikke garantere hans frelsning"},{"SurahNumber":80,"VerseNumber":8,"Text":"En kom jer eagerly"},{"SurahNumber":80,"VerseNumber":9,"Text":"Og er virkelig ærværdige"},{"SurahNumber":80,"VerseNumber":10,"Text":"Du tilsidesatte ham"},{"SurahNumber":80,"VerseNumber":11,"Text":"Indeed den er påmindelse"},{"SurahNumber":80,"VerseNumber":12,"Text":"Whoever viljer tage heed"},{"SurahNumber":80,"VerseNumber":13,"Text":"Ind ærværdige scriptures"},{"SurahNumber":80,"VerseNumber":14,"Text":"Exalted pure"},{"SurahNumber":80,"VerseNumber":15,"Text":"(Skrevet) hænderne bude"},{"SurahNumber":80,"VerseNumber":16,"Text":"Som er ærværdige righteous"},{"SurahNumber":80,"VerseNumber":17,"Text":"Ve til menneskelig være han er så unappreciative"},{"SurahNumber":80,"VerseNumber":18,"Text":"hvad Han oprettede ham fra"},{"SurahNumber":80,"VerseNumber":19,"Text":"Fra tiny nedgang Han opretter ham formgive ham"},{"SurahNumber":80,"VerseNumber":20,"Text":"Derefter Han punkt stien ham"},{"SurahNumber":80,"VerseNumber":21,"Text":"Derefter Han lægger ham død into graven"},{"SurahNumber":80,"VerseNumber":22,"Text":"Hvornår Han viljer Han resurrects ham"},{"SurahNumber":80,"VerseNumber":23,"Text":"Han uphold Hans commandments"},{"SurahNumber":80,"VerseNumber":24,"Text":"Lade mennesket overvejer hans mad"},{"SurahNumber":80,"VerseNumber":25,"Text":"Vi hælder vandet storsindet"},{"SurahNumber":80,"VerseNumber":26,"Text":"Derefter vi splittede jorden åbne"},{"SurahNumber":80,"VerseNumber":27,"Text":"Vi vokser det korn"},{"SurahNumber":80,"VerseNumber":28,"Text":"drue græsgang"},{"SurahNumber":80,"VerseNumber":29,"Text":"Olivener håndflader"},{"SurahNumber":80,"VerseNumber":30,"Text":"Mangfoldighed frugthaver"},{"SurahNumber":80,"VerseNumber":31,"Text":"Frugter grønsager"},{"SurahNumber":80,"VerseNumber":32,"Text":"Til forsyne liv understøttelse I Deres dyr"},{"SurahNumber":80,"VerseNumber":33,"Text":"Derefter hvornår slaget kommer vedtage"},{"SurahNumber":80,"VerseNumber":34,"Text":"Den er dagen hvornår en flees hans boder"},{"SurahNumber":80,"VerseNumber":35,"Text":"Fra hans mor og far"},{"SurahNumber":80,"VerseNumber":36,"Text":"Fra hans ægtefælle børn"},{"SurahNumber":80,"VerseNumber":37,"Text":"Hvert en af dem på den dag bekymre omkring hans egen destiny"},{"SurahNumber":80,"VerseNumber":38,"Text":"visse ansigter den dag være glade"},{"SurahNumber":80,"VerseNumber":39,"Text":"le lykkelige"},{"SurahNumber":80,"VerseNumber":40,"Text":"Øvrige ansigt at dag bedækkes elendighed"},{"SurahNumber":80,"VerseNumber":41,"Text":"Overvældede samvittighedsnag"},{"SurahNumber":80,"VerseNumber":42,"Text":"De er wicked tvivlerne"},{"SurahNumber":81,"VerseNumber":1,"Text":"Hvornår solen rulles"},{"SurahNumber":81,"VerseNumber":2,"Text":"Stjernerne styrtes into hvert øvrig"},{"SurahNumber":81,"VerseNumber":3,"Text":"Bjergene wiped ydre"},{"SurahNumber":81,"VerseNumber":4,"Text":"Reproduktionen halted"},{"SurahNumber":81,"VerseNumber":5,"Text":"beasts summoned"},{"SurahNumber":81,"VerseNumber":6,"Text":"Oceanerne sættes aflame"},{"SurahNumber":81,"VerseNumber":7,"Text":"Sjælene restitueres deres kroppe"},{"SurahNumber":81,"VerseNumber":8,"Text":"Pigen begravedes alive spørges"},{"SurahNumber":81,"VerseNumber":9,"Text":"I hvad forbrydelse hun dræbtes"},{"SurahNumber":81,"VerseNumber":10,"Text":"Rekorderne stilles kendt"},{"SurahNumber":81,"VerseNumber":11,"Text":"Himlen flyttes"},{"SurahNumber":81,"VerseNumber":12,"Text":"Helvede ignited"},{"SurahNumber":81,"VerseNumber":13,"Text":"Paradis præsenteres"},{"SurahNumber":81,"VerseNumber":14,"Text":"Enhvere sjæl kende alt det bragte"},{"SurahNumber":81,"VerseNumber":15,"Text":"Jeg solemnly sværger mælkevejene"},{"SurahNumber":81,"VerseNumber":16,"Text":"Netop løbende deres omløbsbaner"},{"SurahNumber":81,"VerseNumber":17,"Text":"Ved natten det falde"},{"SurahNumber":81,"VerseNumber":18,"Text":"Og morn det ånder"},{"SurahNumber":81,"VerseNumber":19,"Text":"Den er utterance ærværdige bud"},{"SurahNumber":81,"VerseNumber":20,"Text":"Autoriserede Possessor Tronen fully understøttede"},{"SurahNumber":81,"VerseNumber":21,"Text":"Han adlydes stolede"},{"SurahNumber":81,"VerseNumber":22,"Text":"Deres kammerat (Rashad) ikke er crazy"},{"SurahNumber":81,"VerseNumber":23,"Text":"Han så ham høje horisonten"},{"SurahNumber":81,"VerseNumber":24,"Text":"Han ikke er holde tilbage nogen nyhed"},{"SurahNumber":81,"VerseNumber":25,"Text":"Det ikke er snakken forkaste djævel"},{"SurahNumber":81,"VerseNumber":26,"Text":"omgående derefter hvor du gå"},{"SurahNumber":81,"VerseNumber":27,"Text":"Den er meddelelse al folkene"},{"SurahNumber":81,"VerseNumber":28,"Text":"De ønsker gå lige"},{"SurahNumber":81,"VerseNumber":29,"Text":"Whatever du gøre det er ind overensstemmelse med viljen GUD Lord universet"}]}