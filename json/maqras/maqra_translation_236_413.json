{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":84,"VerseNumber":1,"Text":"İzessemaunşakkat"},{"SurahNumber":84,"VerseNumber":2,"Text":"Ve ezinet lirabbiha ve hukkat"},{"SurahNumber":84,"VerseNumber":3,"Text":"Ve izel´ardu muddet"},{"SurahNumber":84,"VerseNumber":4,"Text":"Ve elkat ma fiyha ve tehallet"},{"SurahNumber":84,"VerseNumber":5,"Text":"Ve ezinet lirabbiha ve hukkat"},{"SurahNumber":84,"VerseNumber":6,"Text":"Ya eyyuhel´insanu inneke kadihun ila rabbike kedhan femulakıyhi"},{"SurahNumber":84,"VerseNumber":7,"Text":"Femma men utiye kitabehu biyemiynihi"},{"SurahNumber":84,"VerseNumber":8,"Text":"Fesevfe yuhasebu hısaben yesiyren"},{"SurahNumber":84,"VerseNumber":9,"Text":"Ve yenkalibu ila ehlihi mesruren"},{"SurahNumber":84,"VerseNumber":10,"Text":"Ve emma men utiye kitabehu verae zahrihi"},{"SurahNumber":84,"VerseNumber":11,"Text":"Fesevfe yed´u suburen"},{"SurahNumber":84,"VerseNumber":12,"Text":"Ve yasla se´ıyren"},{"SurahNumber":84,"VerseNumber":13,"Text":"İnnehu kane fiy ehlihi mesruren"},{"SurahNumber":84,"VerseNumber":14,"Text":"İnnehu zanne en len yehure"},{"SurahNumber":84,"VerseNumber":15,"Text":"Bela inne rabbehu kane bihi basıyren"},{"SurahNumber":84,"VerseNumber":16,"Text":"Fela uksimü bişşefekı"},{"SurahNumber":84,"VerseNumber":17,"Text":"Velleyli ve ma veseka"},{"SurahNumber":84,"VerseNumber":18,"Text":"Velkameri izetteseka"},{"SurahNumber":84,"VerseNumber":19,"Text":"Leterkebunne tabekan ´an tabekın"},{"SurahNumber":84,"VerseNumber":20,"Text":"Femalehum la yu´minune"},{"SurahNumber":84,"VerseNumber":21,"Text":"Ve iza kurie ´aleyhimülkur´anu la yescudune"},{"SurahNumber":84,"VerseNumber":22,"Text":"Belilleziyne keferu yukezzibune"},{"SurahNumber":84,"VerseNumber":23,"Text":"Vallahu a´lemu bima yu´une"},{"SurahNumber":84,"VerseNumber":24,"Text":"Febeşşirhüm bi´azabin eliymin"},{"SurahNumber":84,"VerseNumber":25,"Text":"İllelleziyne amenu ve ´amilussalihati lehum erun gayru memnunin"},{"SurahNumber":85,"VerseNumber":1,"Text":"Vessemai zatilbüruci"},{"SurahNumber":85,"VerseNumber":2,"Text":"Velyevmilmev´udi"},{"SurahNumber":85,"VerseNumber":3,"Text":"Ve şahidin ve meşhudin"},{"SurahNumber":85,"VerseNumber":4,"Text":"Kutile ashabül´uhdudi"},{"SurahNumber":85,"VerseNumber":5,"Text":"En nari zatelvekudi"},{"SurahNumber":85,"VerseNumber":6,"Text":"İz hüm ´aleyha ku´udün"},{"SurahNumber":85,"VerseNumber":7,"Text":"Ve hüm ´ala ma yef´alune bilmü´miniyne şühudün"},{"SurahNumber":85,"VerseNumber":8,"Text":"Ve ma nekamu minhüm illa en yü´minu billahil´aziyzilhamiydi"},{"SurahNumber":85,"VerseNumber":9,"Text":"Elleziy lehu mülküssemavati vel´ardı vallahü ´ala külli şey´in şehiydün"},{"SurahNumber":85,"VerseNumber":10,"Text":"İnnelleziyne fetenülmü´miniyne velmü´minati sümme lem yetubu felehüm ´azabü cehenneme ve lehüm ´azabülhariykı"},{"SurahNumber":85,"VerseNumber":11,"Text":"İnnelleziyne amenu ve ´amilussalihati lehüm cennatün tecriy min tahtihel´enharü zalikelfevzülkebiyrü"},{"SurahNumber":85,"VerseNumber":12,"Text":"İnne batşe rabbike leşediydün"},{"SurahNumber":85,"VerseNumber":13,"Text":"İnnehu hüve yübdiü ve yü´ıydü"},{"SurahNumber":85,"VerseNumber":14,"Text":"Ve hüvelğafurülvedudü"},{"SurahNumber":85,"VerseNumber":15,"Text":"Zül´arşilmeciydü"},{"SurahNumber":85,"VerseNumber":16,"Text":"Fa´´alün lima yüriydü"},{"SurahNumber":85,"VerseNumber":17,"Text":"Hel etake hadiysülcünudi"},{"SurahNumber":85,"VerseNumber":18,"Text":"Fir´avne ve semude"},{"SurahNumber":85,"VerseNumber":19,"Text":"Belilleziyne keferu fiy tekziybin"},{"SurahNumber":85,"VerseNumber":20,"Text":"Vallahü min veraihim muhıytun"},{"SurahNumber":85,"VerseNumber":21,"Text":"Bel hüve kur´anün meciydün"},{"SurahNumber":85,"VerseNumber":22,"Text":"Fiy levhın mahfuzın"},{"SurahNumber":86,"VerseNumber":1,"Text":"Vessemai vettarikı"},{"SurahNumber":86,"VerseNumber":2,"Text":"Ve ma edrake mettariku"},{"SurahNumber":86,"VerseNumber":3,"Text":"Ennecmüssakıbü"},{"SurahNumber":86,"VerseNumber":4,"Text":"İn küllü nefsin lemma ´aleyha hafizun"},{"SurahNumber":86,"VerseNumber":5,"Text":"Felyenzuril´insanü mimme hulika"},{"SurahNumber":86,"VerseNumber":6,"Text":"Hulika min main dafikın"},{"SurahNumber":86,"VerseNumber":7,"Text":"Yahrücü min beynissulbi vetteraibi"},{"SurahNumber":86,"VerseNumber":8,"Text":"İnnehu ´ala rec´ıhı lekadirün"},{"SurahNumber":86,"VerseNumber":9,"Text":"Yevme tüblesserairü"},{"SurahNumber":86,"VerseNumber":10,"Text":"Fema lehu min kuvvetin ve la nasırin"},{"SurahNumber":86,"VerseNumber":11,"Text":"Vessemai zatirrec´ı"},{"SurahNumber":86,"VerseNumber":12,"Text":"Vel´ardı zatissad´ı"},{"SurahNumber":86,"VerseNumber":13,"Text":"İnnehu likavlün faslün"},{"SurahNumber":86,"VerseNumber":14,"Text":"Ve ma hüve bilhezli"},{"SurahNumber":86,"VerseNumber":15,"Text":"İnnehüm yekiydune keyden"},{"SurahNumber":86,"VerseNumber":16,"Text":"Ve ekiydü keyden"},{"SurahNumber":86,"VerseNumber":17,"Text":"Femehhililkafiriyne emhilhüm rüveyden"}]}