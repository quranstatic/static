{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":78,"VerseNumber":1,"Text":"Hvad være de questioning"},{"SurahNumber":78,"VerseNumber":2,"Text":"great begivenheden"},{"SurahNumber":78,"VerseNumber":3,"Text":"Den disputed dem"},{"SurahNumber":78,"VerseNumber":4,"Text":"Indeed de gøre det grundlægge ydre"},{"SurahNumber":78,"VerseNumber":5,"Text":"Fleste assuredly de gøre det grundlægge ydre"},{"SurahNumber":78,"VerseNumber":6,"Text":"Vi ikke stillede jorden beboelige"},{"SurahNumber":78,"VerseNumber":7,"Text":"Bjergene stabilisatorer"},{"SurahNumber":78,"VerseNumber":8,"Text":"Vi oprettede jer mager ene another"},{"SurahNumber":78,"VerseNumber":9,"Text":"Vi oprettede sovende så du hvile"},{"SurahNumber":78,"VerseNumber":10,"Text":"Vi stillede natten bedække"},{"SurahNumber":78,"VerseNumber":11,"Text":"Dagen søge forråd"},{"SurahNumber":78,"VerseNumber":12,"Text":"Vi byggede I syv universer"},{"SurahNumber":78,"VerseNumber":13,"Text":"Vi oprettede lyse lampe"},{"SurahNumber":78,"VerseNumber":14,"Text":"Vi sender skyerne hælde vand"},{"SurahNumber":78,"VerseNumber":15,"Text":"Til producere det korn planter"},{"SurahNumber":78,"VerseNumber":16,"Text":"Og adskillige frugthaver"},{"SurahNumber":78,"VerseNumber":17,"Text":"Dagen Bestemmelse udnævnes"},{"SurahNumber":78,"VerseNumber":18,"Text":"Dagen hornet pustes du kommer ind trængsel"},{"SurahNumber":78,"VerseNumber":19,"Text":"Himlen være åbnede låger"},{"SurahNumber":78,"VerseNumber":20,"Text":"Bjergene flyttes idet de var luftspejling"},{"SurahNumber":78,"VerseNumber":21,"Text":"Gehenna er uundgåelige"},{"SurahNumber":78,"VerseNumber":22,"Text":"I overtræderne; det være deres bolig"},{"SurahNumber":78,"VerseNumber":23,"Text":"De bliver det for ælde"},{"SurahNumber":78,"VerseNumber":24,"Text":"De aldrig smager det kølighed nor drink"},{"SurahNumber":78,"VerseNumber":25,"Text":"Kun inferno bitre mad"},{"SurahNumber":78,"VerseNumber":26,"Text":"Retfærdige requital"},{"SurahNumber":78,"VerseNumber":27,"Text":"De aldrig forventede holdes accountable"},{"SurahNumber":78,"VerseNumber":28,"Text":"utterly forkaste vore tegn"},{"SurahNumber":78,"VerseNumber":29,"Text":"Vi talte alt rekord"},{"SurahNumber":78,"VerseNumber":30,"Text":"Lide konsekvenserne; vi kun forøge Deres gengældelse"},{"SurahNumber":78,"VerseNumber":31,"Text":"righteous trænge belønning"},{"SurahNumber":78,"VerseNumber":32,"Text":"Frugthaver drue"},{"SurahNumber":78,"VerseNumber":33,"Text":"Storslåede ægtefæller"},{"SurahNumber":78,"VerseNumber":34,"Text":"Dejlige drinks"},{"SurahNumber":78,"VerseNumber":35,"Text":"De aldrig høre det nogen nonsens løgne"},{"SurahNumber":78,"VerseNumber":36,"Text":"Belønning fra Deres Lord gavmilde recompense"},{"SurahNumber":78,"VerseNumber":37,"Text":"Lord af himlene jorden alt dem. Mest Gracious. Ingen abrogate Hans bestemmelser"},{"SurahNumber":78,"VerseNumber":38,"Text":"Dagen komme hvornår Ånden englene stå række! Ingen tale de permitted ved mest Gracious de utter kun hvad er omgående"},{"SurahNumber":78,"VerseNumber":39,"Text":"Such er uundgåelige dagen. Whoever viljer lade ham tage ly hans Lord"},{"SurahNumber":78,"VerseNumber":40,"Text":"Vi tilstrækkeligt advarer jer forestående gengældelse. Den er dagen hvornår enhver ransage hvad hans hænder sender væk tvivleren sie Ohio Jeg ønske jeg var støv"},{"SurahNumber":79,"VerseNumber":1,"Text":"Englene snatch (sjælene tvivlerne) forcibly"},{"SurahNumber":79,"VerseNumber":2,"Text":"Og de gently tager (sjælene troendene) joyfully"},{"SurahNumber":79,"VerseNumber":3,"Text":"Og de flyde overalt"},{"SurahNumber":79,"VerseNumber":4,"Text":"Eagerly racing ene another"},{"SurahNumber":79,"VerseNumber":5,"Text":"Til bære adskillige befalinger"},{"SurahNumber":79,"VerseNumber":6,"Text":"Dagen quake quakes"},{"SurahNumber":79,"VerseNumber":7,"Text":"Fulgte sekundet puste"},{"SurahNumber":79,"VerseNumber":8,"Text":"Vise forstand terrified"},{"SurahNumber":79,"VerseNumber":9,"Text":"Deres øjne subdued"},{"SurahNumber":79,"VerseNumber":10,"Text":"De sie Vi genskabes graven"},{"SurahNumber":79,"VerseNumber":11,"Text":"Hvordan den hænder efter vi gøre havde dreje into rådne bones"},{"SurahNumber":79,"VerseNumber":12,"Text":"De sagde Den er umulige recurrence"},{"SurahNumber":79,"VerseNumber":13,"Text":"al det tager er ene nudge"},{"SurahNumber":79,"VerseNumber":14,"Text":"Hvorpå de enes"},{"SurahNumber":79,"VerseNumber":15,"Text":"Du kender historien Moses"},{"SurahNumber":79,"VerseNumber":16,"Text":"Hans Lord benævnede ham hellige dalen Tuwaa"},{"SurahNumber":79,"VerseNumber":17,"Text":"Går Pharaoh; han transgressed"},{"SurahNumber":79,"VerseNumber":18,"Text":"Fortælle ham jer ikke reformere"},{"SurahNumber":79,"VerseNumber":19,"Text":"Lade mig guide dig til Deres Lord at du dreje ærværdige"},{"SurahNumber":79,"VerseNumber":20,"Text":"Han derefter udfoldede ham great miraklet"},{"SurahNumber":79,"VerseNumber":21,"Text":"Han disbelieved rebelled"},{"SurahNumber":79,"VerseNumber":22,"Text":"Derefter han dreje ind et fare"},{"SurahNumber":79,"VerseNumber":23,"Text":"Han summoned proklamerede"},{"SurahNumber":79,"VerseNumber":24,"Text":"Han sagde jeg er Deres Lord højest"},{"SurahNumber":79,"VerseNumber":25,"Text":"Følgelig GUD forpligte ham til gengældelsen Herefter også idet ind først liv"},{"SurahNumber":79,"VerseNumber":26,"Text":"Den er lektion i de ærværdige"},{"SurahNumber":79,"VerseNumber":27,"Text":"I er vanskeligere oprette end himlen? Han konstruerede det"},{"SurahNumber":79,"VerseNumber":28,"Text":"Han rejst dens hobe perfektionere det"},{"SurahNumber":79,"VerseNumber":29,"Text":"Han stillede dens nat mørke lysnede dens morn"},{"SurahNumber":79,"VerseNumber":30,"Text":"Han stillede jorden egg-shaped"},{"SurahNumber":79,"VerseNumber":31,"Text":"Fra det, Han producerede dens egne vand græsgang"},{"SurahNumber":79,"VerseNumber":32,"Text":"Han etablerede bjergene"},{"SurahNumber":79,"VerseNumber":33,"Text":"Al den til forsyne liv understøttelse I Deres dyr"},{"SurahNumber":79,"VerseNumber":34,"Text":"Derefter hvornår great slaget kommer"},{"SurahNumber":79,"VerseNumber":35,"Text":"Den er dagen hvornår menneskelige viljen mindes alt han gjorde"},{"SurahNumber":79,"VerseNumber":36,"Text":"Helvede bringes eksistens"},{"SurahNumber":79,"VerseNumber":37,"Text":"Angående en transgressed"},{"SurahNumber":79,"VerseNumber":38,"Text":"Som preoccupied den liv"},{"SurahNumber":79,"VerseNumber":39,"Text":"Helvede være boligen"},{"SurahNumber":79,"VerseNumber":40,"Text":"Angående ene som reverenced majestæten hans Lord enjoined selvet fra syndige lusts"},{"SurahNumber":79,"VerseNumber":41,"Text":"Paradis være boligen"},{"SurahNumber":79,"VerseNumber":42,"Text":"De spørger jer omkring Timen hvornår det skiftes sted"},{"SurahNumber":79,"VerseNumber":43,"Text":"Ikke er jer (Muhammad) destineres kungøre dens tid"},{"SurahNumber":79,"VerseNumber":44,"Text":"Deres Lord beslutter dens skæbne"},{"SurahNumber":79,"VerseNumber":45,"Text":"Deres mission er advare de forventer det"},{"SurahNumber":79,"VerseNumber":46,"Text":"Dagen de ser de syne idet de varede ene jævnende halve dag"}]}