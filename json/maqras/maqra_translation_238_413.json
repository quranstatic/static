{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":90,"VerseNumber":1,"Text":"La uksimü bilhazelbeledi"},{"SurahNumber":90,"VerseNumber":2,"Text":"Ve ente hıllün bihazelbeledi"},{"SurahNumber":90,"VerseNumber":3,"Text":"Ve validin ve ma velede"},{"SurahNumber":90,"VerseNumber":4,"Text":"Lekad halaknel´insane fiy kebedin"},{"SurahNumber":90,"VerseNumber":5,"Text":"Eyahsebü en len yakdire ´aleyhi ehadün"},{"SurahNumber":90,"VerseNumber":6,"Text":"Yekulü ehlektü malen lübeden"},{"SurahNumber":90,"VerseNumber":7,"Text":"Eyahsebü en lem yerehu ehadün"},{"SurahNumber":90,"VerseNumber":8,"Text":"Elem nec´al lehu ´ayneyni"},{"SurahNumber":90,"VerseNumber":9,"Text":"Ve lisanen ve şefeteyni"},{"SurahNumber":90,"VerseNumber":10,"Text":"Ve hedeynahünnecdeyni"},{"SurahNumber":90,"VerseNumber":11,"Text":"Felaktehamel´akabete"},{"SurahNumber":90,"VerseNumber":12,"Text":"Ve ma edrake mel´akabetü"},{"SurahNumber":90,"VerseNumber":13,"Text":"Fekkü rekabetin"},{"SurahNumber":90,"VerseNumber":14,"Text":"Ev ıt´amün fiy yevmin ziy mesğabetin"},{"SurahNumber":90,"VerseNumber":15,"Text":"Yetiymen za makrebetin"},{"SurahNumber":90,"VerseNumber":16,"Text":"Ev miskiynen za metrebetin"},{"SurahNumber":90,"VerseNumber":17,"Text":"Sümme kane minelleziyne amenu ve tevasav bissabri ve tevasav bilmerhameti"},{"SurahNumber":90,"VerseNumber":18,"Text":"Ol,eke ashabülmeymene"},{"SurahNumber":90,"VerseNumber":19,"Text":"Velleziyne keferu biayatina hüm ashabülmeş´emeti"},{"SurahNumber":90,"VerseNumber":20,"Text":"´Aleyhim narün mü´sadetün"},{"SurahNumber":91,"VerseNumber":1,"Text":"Veşşemsi ve duhaha"},{"SurahNumber":91,"VerseNumber":2,"Text":"Velkameri iza telaha"},{"SurahNumber":91,"VerseNumber":3,"Text":"Vennehari iza cellaha"},{"SurahNumber":91,"VerseNumber":4,"Text":"Velleyli iza yağşaha"},{"SurahNumber":91,"VerseNumber":5,"Text":"Vessmai ve ma benaha"},{"SurahNumber":91,"VerseNumber":6,"Text":"Vel´ardı ve ma tahaha"},{"SurahNumber":91,"VerseNumber":7,"Text":"Ve nefsin ve ma sevvaha"},{"SurahNumber":91,"VerseNumber":8,"Text":"Feelhemeha fücureha ve takvaha"},{"SurahNumber":91,"VerseNumber":9,"Text":"Kad efleha men zekkaha"},{"SurahNumber":91,"VerseNumber":10,"Text":"Ve kad habe men dessaha"},{"SurahNumber":91,"VerseNumber":11,"Text":"Kezzebet semudü bitağvaha"},{"SurahNumber":91,"VerseNumber":12,"Text":"İzinbe´ase eşkaha"},{"SurahNumber":91,"VerseNumber":13,"Text":"Fekale lehüm resulullahi nakatallahi ve sukyaha"},{"SurahNumber":91,"VerseNumber":14,"Text":"Fekezzebuhü fe´akaruha fedemdeme ´aleyhim rabbühüm bizenbihim fesevvaha"},{"SurahNumber":91,"VerseNumber":15,"Text":"Ve la yehafi ´ukbaha"},{"SurahNumber":92,"VerseNumber":1,"Text":"Velleyli iza yağşa"},{"SurahNumber":92,"VerseNumber":2,"Text":"Vennehari iza tecella"},{"SurahNumber":92,"VerseNumber":3,"Text":"Ve ma halekazzekere vel´ünsa"},{"SurahNumber":92,"VerseNumber":4,"Text":"İnne sa´yeküm leşetta"},{"SurahNumber":92,"VerseNumber":5,"Text":"Feemma men a´ta vetteka"},{"SurahNumber":92,"VerseNumber":6,"Text":"Ve saddeka bilhusna"},{"SurahNumber":92,"VerseNumber":7,"Text":"Fesenüyessirühu lilyüsra"},{"SurahNumber":92,"VerseNumber":8,"Text":"Ve emma men bahıle vestağna"},{"SurahNumber":92,"VerseNumber":9,"Text":"Ve kezzebe bilhusna"},{"SurahNumber":92,"VerseNumber":10,"Text":"Fesenüyessirühu lil´usra"},{"SurahNumber":92,"VerseNumber":11,"Text":"Ve ma yuğniy ´anhü malühu iza teredda"},{"SurahNumber":92,"VerseNumber":12,"Text":"İnne ´aleyna lelhüda"},{"SurahNumber":92,"VerseNumber":13,"Text":"Ve inne lena lel´ahırete vel´ula"},{"SurahNumber":92,"VerseNumber":14,"Text":"Feenzertüküm naren telezza"},{"SurahNumber":92,"VerseNumber":15,"Text":"La yaslaha illel´eşka"},{"SurahNumber":92,"VerseNumber":16,"Text":"Elleziy kezzebe ve tevella"},{"SurahNumber":92,"VerseNumber":17,"Text":"Ve seyücennebühel´etka"},{"SurahNumber":92,"VerseNumber":18,"Text":"Elleziy yü´tiy malehu yetezekka"},{"SurahNumber":92,"VerseNumber":19,"Text":"Ve ma liehadin ´ındehu min nı´metin tücza"},{"SurahNumber":92,"VerseNumber":20,"Text":"İllebtiğae vechi rabbihil´a´la"},{"SurahNumber":92,"VerseNumber":21,"Text":"Ve lesevfe yerda"},{"SurahNumber":93,"VerseNumber":1,"Text":"Vedduha"},{"SurahNumber":93,"VerseNumber":2,"Text":"Velleyli iza seca"},{"SurahNumber":93,"VerseNumber":3,"Text":"Ma vedde´ake rabbüke ve ma kala"},{"SurahNumber":93,"VerseNumber":4,"Text":"Ve lel´ahıretü hayrün leke minel´ula"},{"SurahNumber":93,"VerseNumber":5,"Text":"Ve lesevfe yu´tıyke rabbüke feterda"},{"SurahNumber":93,"VerseNumber":6,"Text":"Elem yecidke yetiymen feava"},{"SurahNumber":93,"VerseNumber":7,"Text":"Ve vecedeke dallen feheda"},{"SurahNumber":93,"VerseNumber":8,"Text":"Ve vecedeke ´ailen feağna"},{"SurahNumber":93,"VerseNumber":9,"Text":"Femmel yetiyme fela takher"},{"SurahNumber":93,"VerseNumber":10,"Text":"Ve emmessaile fela tenher"},{"SurahNumber":93,"VerseNumber":11,"Text":"Ve emma binı´meti rabbike fehaddis"}]}