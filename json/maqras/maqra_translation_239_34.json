{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":94,"VerseNumber":1,"Text":"الم نشرح لك صدرك"},{"SurahNumber":94,"VerseNumber":2,"Text":"ووضعنا عنك وزرك"},{"SurahNumber":94,"VerseNumber":3,"Text":"الذي انقض ظهرك"},{"SurahNumber":94,"VerseNumber":4,"Text":"ورفعنا لك ذكرك"},{"SurahNumber":94,"VerseNumber":5,"Text":"فان مع العسر يسرا"},{"SurahNumber":94,"VerseNumber":6,"Text":"ان مع العسر يسر ا"},{"SurahNumber":94,"VerseNumber":7,"Text":"فاذا فرغت فانصب"},{"SurahNumber":94,"VerseNumber":8,"Text":"والى ربك فارغب"},{"SurahNumber":95,"VerseNumber":1,"Text":"والتين والزيتون"},{"SurahNumber":95,"VerseNumber":2,"Text":"وطور سينين"},{"SurahNumber":95,"VerseNumber":3,"Text":"وهذا البلد الامين"},{"SurahNumber":95,"VerseNumber":4,"Text":"لقد خلقنا الانسن في احسن تقويم"},{"SurahNumber":95,"VerseNumber":5,"Text":"ثم رددنه اسفل سفلين"},{"SurahNumber":95,"VerseNumber":6,"Text":"الا الذين ءامنوا وعملوا الصلحت فلهم اجر غير ممنون"},{"SurahNumber":95,"VerseNumber":7,"Text":"فما يكذبك بعد بالدين"},{"SurahNumber":95,"VerseNumber":8,"Text":"اليس الله باحكم الحكمين"},{"SurahNumber":96,"VerseNumber":1,"Text":"اقرا باسم ربك الذي خلق"},{"SurahNumber":96,"VerseNumber":2,"Text":"خلق الانسن من علق"},{"SurahNumber":96,"VerseNumber":3,"Text":"اقرا وربك الاكرم"},{"SurahNumber":96,"VerseNumber":4,"Text":"الذي علم بالقلم"},{"SurahNumber":96,"VerseNumber":5,"Text":"علم الانسن ما لم يعلم"},{"SurahNumber":96,"VerseNumber":6,"Text":"كلا ان الانسن ليطغى"},{"SurahNumber":96,"VerseNumber":7,"Text":"ان رءاه استغنى"},{"SurahNumber":96,"VerseNumber":8,"Text":"ان الى ربك الرجعى"},{"SurahNumber":96,"VerseNumber":9,"Text":"ارءيت الذي ينهى"},{"SurahNumber":96,"VerseNumber":10,"Text":"عبدا اذا صلى"},{"SurahNumber":96,"VerseNumber":11,"Text":"ارءيت ان كان على الهدى"},{"SurahNumber":96,"VerseNumber":12,"Text":"او امر بالتقوى"},{"SurahNumber":96,"VerseNumber":13,"Text":"ارءيت ان كذب وتولى"},{"SurahNumber":96,"VerseNumber":14,"Text":"الم يعلم بان الله يرى"},{"SurahNumber":96,"VerseNumber":15,"Text":"كلا لين لم ينته لنسفعا بالناصية"},{"SurahNumber":96,"VerseNumber":16,"Text":"ناصية كذبة خاطية"},{"SurahNumber":96,"VerseNumber":17,"Text":"فليدع ناديه"},{"SurahNumber":96,"VerseNumber":18,"Text":"سندع الزبانية"},{"SurahNumber":96,"VerseNumber":19,"Text":"كلا لا تطعه واسجد واقترب۩"},{"SurahNumber":97,"VerseNumber":1,"Text":"الرحيمانا انزلنه في ليلة القدر"},{"SurahNumber":97,"VerseNumber":2,"Text":"وما ادرىك ما ليلة القدر"},{"SurahNumber":97,"VerseNumber":3,"Text":"ليلة القدر خير من الف شهر"},{"SurahNumber":97,"VerseNumber":4,"Text":"تنزل المليكة والروح فيها باذن ربهم من كل امر"},{"SurahNumber":97,"VerseNumber":5,"Text":"سلم هي حتى مطلع الفجر"},{"SurahNumber":98,"VerseNumber":1,"Text":"لم يكن الذين كفروا من اهل الكتب والمشركين منفكين حتى تاتيهم البينة"},{"SurahNumber":98,"VerseNumber":2,"Text":"رسول من الله يتلوا صحف ا مطهرة"},{"SurahNumber":98,"VerseNumber":3,"Text":"فيها كتب قيمة"},{"SurahNumber":98,"VerseNumber":4,"Text":"وما تفرق الذين اوتوا الكتب الا من بعد ما جاءتهم البينة"},{"SurahNumber":98,"VerseNumber":5,"Text":"وما امروا الا ليعبدوا الله مخلصين له الدين حنفاء ويقيموا الصلوة ويوتوا الزكوة وذلك دين القيمة"},{"SurahNumber":98,"VerseNumber":6,"Text":"ان الذين كفروا من اهل الكتب والمشركين في نار جهنم خلدين فيها اوليك هم شر البرية"},{"SurahNumber":98,"VerseNumber":7,"Text":"ان الذين ءامنوا وعملوا الصلحت اوليك هم خير البرية"},{"SurahNumber":98,"VerseNumber":8,"Text":"جزاوهم عند ربهم جنت عدن تجري من تحتها الانهر خلدين فيها ابد ا رضي الله عنهم ورضوا عنه ذلك لمن خشي ربه"},{"SurahNumber":99,"VerseNumber":1,"Text":"اذا زلزلت الارض زلزالها"},{"SurahNumber":99,"VerseNumber":2,"Text":"واخرجت الارض اثقالها"},{"SurahNumber":99,"VerseNumber":3,"Text":"وقال الانسن ما لها"},{"SurahNumber":99,"VerseNumber":4,"Text":"يوميذ تحدث اخبارها"},{"SurahNumber":99,"VerseNumber":5,"Text":"بان ربك اوحى لها"},{"SurahNumber":99,"VerseNumber":6,"Text":"يوميذ يصدر الناس اشتات ا ليروا اعملهم"},{"SurahNumber":99,"VerseNumber":7,"Text":"فمن يعمل مثقال ذرة خير ا يره"},{"SurahNumber":99,"VerseNumber":8,"Text":"ومن يعمل مثقال ذرة شر ا يره"},{"SurahNumber":100,"VerseNumber":1,"Text":"والعديت ضبح ا"},{"SurahNumber":100,"VerseNumber":2,"Text":"فالموريت قدح ا"},{"SurahNumber":100,"VerseNumber":3,"Text":"فالمغيرت صبح ا"},{"SurahNumber":100,"VerseNumber":4,"Text":"فاثرن به نقع ا"},{"SurahNumber":100,"VerseNumber":5,"Text":"فوسطن به جمعا"},{"SurahNumber":100,"VerseNumber":6,"Text":"ان الانسن لربه لكنود"},{"SurahNumber":100,"VerseNumber":7,"Text":"وانه على ذلك لشهيد"},{"SurahNumber":100,"VerseNumber":8,"Text":"وانه لحب الخير لشديد"}]}