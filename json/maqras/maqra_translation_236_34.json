{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":84,"VerseNumber":1,"Text":"اذا السماء انشقت"},{"SurahNumber":84,"VerseNumber":2,"Text":"واذنت لربها وحقت"},{"SurahNumber":84,"VerseNumber":3,"Text":"واذا الارض مدت"},{"SurahNumber":84,"VerseNumber":4,"Text":"والقت ما فيها وتخلت"},{"SurahNumber":84,"VerseNumber":5,"Text":"واذنت لربها وحقت"},{"SurahNumber":84,"VerseNumber":6,"Text":"يايها الانسن انك كادح الى ربك كدح ا فملقيه"},{"SurahNumber":84,"VerseNumber":7,"Text":"فاما من اوتي كتبه بيمينه"},{"SurahNumber":84,"VerseNumber":8,"Text":"فسوف يحاسب حساب ا يسير ا"},{"SurahNumber":84,"VerseNumber":9,"Text":"وينقلب الى اهله مسرور ا"},{"SurahNumber":84,"VerseNumber":10,"Text":"واما من اوتي كتبه وراء ظهره"},{"SurahNumber":84,"VerseNumber":11,"Text":"فسوف يدعوا ثبور ا"},{"SurahNumber":84,"VerseNumber":12,"Text":"ويصلى سعيرا"},{"SurahNumber":84,"VerseNumber":13,"Text":"انه كان في اهله مسرورا"},{"SurahNumber":84,"VerseNumber":14,"Text":"انه ظن ان لن يحور"},{"SurahNumber":84,"VerseNumber":15,"Text":"بلى ان ربه كان به بصير ا"},{"SurahNumber":84,"VerseNumber":16,"Text":"فلا اقسم بالشفق"},{"SurahNumber":84,"VerseNumber":17,"Text":"واليل وما وسق"},{"SurahNumber":84,"VerseNumber":18,"Text":"والقمر اذا اتسق"},{"SurahNumber":84,"VerseNumber":19,"Text":"لتركبن طبقا عن طبق"},{"SurahNumber":84,"VerseNumber":20,"Text":"فما لهم لا يومنون"},{"SurahNumber":84,"VerseNumber":21,"Text":"واذا قري عليهم القرءان لا يسجدون۩"},{"SurahNumber":84,"VerseNumber":22,"Text":"بل الذين كفروا يكذبون"},{"SurahNumber":84,"VerseNumber":23,"Text":"والله اعلم بما يوعون"},{"SurahNumber":84,"VerseNumber":24,"Text":"فبشرهم بعذاب اليم"},{"SurahNumber":84,"VerseNumber":25,"Text":"الا الذين ءامنوا وعملوا الصلحت لهم اجر غير ممنون"},{"SurahNumber":85,"VerseNumber":1,"Text":"والسماء ذات البروج"},{"SurahNumber":85,"VerseNumber":2,"Text":"واليوم الموعود"},{"SurahNumber":85,"VerseNumber":3,"Text":"وشاهد ومشهود"},{"SurahNumber":85,"VerseNumber":4,"Text":"قتل اصحب الاخدود"},{"SurahNumber":85,"VerseNumber":5,"Text":"النار ذات الوقود"},{"SurahNumber":85,"VerseNumber":6,"Text":"اذ هم عليها قعود"},{"SurahNumber":85,"VerseNumber":7,"Text":"وهم على ما يفعلون بالمومنين شهود"},{"SurahNumber":85,"VerseNumber":8,"Text":"وما نقموا منهم الا ان يومنوا بالله العزيز الحميد"},{"SurahNumber":85,"VerseNumber":9,"Text":"الذي له ملك السموت والارض والله على كل شيء شهيد"},{"SurahNumber":85,"VerseNumber":10,"Text":"ان الذين فتنوا المومنين والمومنت ثم لم يتوبوا فلهم عذاب جهنم ولهم عذاب الحريق"},{"SurahNumber":85,"VerseNumber":11,"Text":"ان الذين ءامنوا وعملوا الصلحت لهم جنت تجري من تحتها الانهر ذلك الفوز الكبير"},{"SurahNumber":85,"VerseNumber":12,"Text":"ان بطش ربك لشديد"},{"SurahNumber":85,"VerseNumber":13,"Text":"انه هو يبدي ويعيد"},{"SurahNumber":85,"VerseNumber":14,"Text":"وهو الغفور الودود"},{"SurahNumber":85,"VerseNumber":15,"Text":"ذو العرش المجيد"},{"SurahNumber":85,"VerseNumber":16,"Text":"فعال لما يريد"},{"SurahNumber":85,"VerseNumber":17,"Text":"هل اتىك حديث الجنود"},{"SurahNumber":85,"VerseNumber":18,"Text":"فرعون وثمود"},{"SurahNumber":85,"VerseNumber":19,"Text":"بل الذين كفروا في تكذيب"},{"SurahNumber":85,"VerseNumber":20,"Text":"والله من ورايهم محيط"},{"SurahNumber":85,"VerseNumber":21,"Text":"بل هو قرءان مجيد"},{"SurahNumber":85,"VerseNumber":22,"Text":"في لوح محفوظ"},{"SurahNumber":86,"VerseNumber":1,"Text":"والسماء والطارق"},{"SurahNumber":86,"VerseNumber":2,"Text":"وما ادرىك ما الطارق"},{"SurahNumber":86,"VerseNumber":3,"Text":"النجم الثاقب"},{"SurahNumber":86,"VerseNumber":4,"Text":"ان كل نفس لما عليها حافظ"},{"SurahNumber":86,"VerseNumber":5,"Text":"فلينظر الانسن مم خلق"},{"SurahNumber":86,"VerseNumber":6,"Text":"خلق من ماء دافق"},{"SurahNumber":86,"VerseNumber":7,"Text":"يخرج من بين الصلب والترايب"},{"SurahNumber":86,"VerseNumber":8,"Text":"انه على رجعه لقادر"},{"SurahNumber":86,"VerseNumber":9,"Text":"يوم تبلى السراير"},{"SurahNumber":86,"VerseNumber":10,"Text":"فما له من قوة ولا ناصر"},{"SurahNumber":86,"VerseNumber":11,"Text":"والسماء ذات الرجع"},{"SurahNumber":86,"VerseNumber":12,"Text":"والارض ذات الصدع"},{"SurahNumber":86,"VerseNumber":13,"Text":"انه لقول فصل"},{"SurahNumber":86,"VerseNumber":14,"Text":"وما هو بالهزل"},{"SurahNumber":86,"VerseNumber":15,"Text":"انهم يكيدون كيد ا"},{"SurahNumber":86,"VerseNumber":16,"Text":"واكيد كيد ا"},{"SurahNumber":86,"VerseNumber":17,"Text":"فمهل الكفرين امهلهم رويدا"}]}