{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":90,"VerseNumber":1,"Text":"Jeg solemnly sværger den by"},{"SurahNumber":90,"VerseNumber":2,"Text":"Byen du lever"},{"SurahNumber":90,"VerseNumber":3,"Text":"begetting og begotten"},{"SurahNumber":90,"VerseNumber":4,"Text":"Vi oprettede menneskelig være arbejde hård indfri"},{"SurahNumber":90,"VerseNumber":5,"Text":"Han tro den ingen gøre nogensinde benævne ham konto"},{"SurahNumber":90,"VerseNumber":6,"Text":"Han skryder jeg spent så mere penge"},{"SurahNumber":90,"VerseNumber":7,"Text":"Han tro ingen gøre ser ham"},{"SurahNumber":90,"VerseNumber":8,"Text":"Vi ikke gav ham to øjne"},{"SurahNumber":90,"VerseNumber":9,"Text":"Tunge to læber"},{"SurahNumber":90,"VerseNumber":10,"Text":"Vi ikke udfoldede ham to stierne"},{"SurahNumber":90,"VerseNumber":11,"Text":"Han udvælge vanskelige stien"},{"SurahNumber":90,"VerseNumber":12,"Text":"Hvilke en er vanskelige stien"},{"SurahNumber":90,"VerseNumber":13,"Text":"freeing slaver"},{"SurahNumber":90,"VerseNumber":14,"Text":"fodre tid hardship"},{"SurahNumber":90,"VerseNumber":15,"Text":"Forældreløse berettes"},{"SurahNumber":90,"VerseNumber":16,"Text":"fattig er ind behøver"},{"SurahNumber":90,"VerseNumber":17,"Text":"Værende ene af de tro, exhorting ene another være steadfast exhorting ene another være venlige"},{"SurahNumber":90,"VerseNumber":18,"Text":"De trænger lykke"},{"SurahNumber":90,"VerseNumber":19,"Text":"Angående de disbelieved vore åbenbaringer de incurred elendighed"},{"SurahNumber":90,"VerseNumber":20,"Text":"De confined Hellfire"},{"SurahNumber":91,"VerseNumber":1,"Text":"Ved solen dens brightness"},{"SurahNumber":91,"VerseNumber":2,"Text":"Månen følger det"},{"SurahNumber":91,"VerseNumber":3,"Text":"Dagen afslører"},{"SurahNumber":91,"VerseNumber":4,"Text":"Natten bedækker"},{"SurahNumber":91,"VerseNumber":5,"Text":"Himlen Ham byggede det"},{"SurahNumber":91,"VerseNumber":6,"Text":"Jorden Ham sustains det"},{"SurahNumber":91,"VerseNumber":7,"Text":"Sjælen Ham oprettede det"},{"SurahNumber":91,"VerseNumber":8,"Text":"Derefter udfoldede det hvad er onde hvad er gode"},{"SurahNumber":91,"VerseNumber":9,"Text":"Succesrige er en indfrir det"},{"SurahNumber":91,"VerseNumber":10,"Text":"mislykkes er en forsømmer det"},{"SurahNumber":91,"VerseNumber":11,"Text":"Thamoud's vantro voldte dem transgress"},{"SurahNumber":91,"VerseNumber":12,"Text":"De fulgte værst hos dem"},{"SurahNumber":91,"VerseNumber":13,"Text":"Gud bud sie til dem Den er gud kamel; lade hendes drink"},{"SurahNumber":91,"VerseNumber":14,"Text":"De disbelieved ham slaughtered hende! Deres Lord derefter requited dem i deres synd annihilated dem"},{"SurahNumber":91,"VerseNumber":15,"Text":"men de som kom efter dem vedvare heedless"},{"SurahNumber":92,"VerseNumber":1,"Text":"Ved natten det bedække"},{"SurahNumber":92,"VerseNumber":2,"Text":"Dagen det afslører"},{"SurahNumber":92,"VerseNumber":3,"Text":"Ham oprettede mandlig kvindelig"},{"SurahNumber":92,"VerseNumber":4,"Text":"Deres arbejder er af adskillige arter"},{"SurahNumber":92,"VerseNumber":5,"Text":"Angående ham giver velgørenhed vedligeholde retmæssighed"},{"SurahNumber":92,"VerseNumber":6,"Text":"Og upholds scripture"},{"SurahNumber":92,"VerseNumber":7,"Text":"Vi forestå ham lykke"},{"SurahNumber":92,"VerseNumber":8,"Text":"Han som er nærige han er rige"},{"SurahNumber":92,"VerseNumber":9,"Text":"Og disbelieves scripture"},{"SurahNumber":92,"VerseNumber":10,"Text":"Vi forestå ham elendighed"},{"SurahNumber":92,"VerseNumber":11,"Text":"Hans penge ikke hjælpes ham hvornår han falder"},{"SurahNumber":92,"VerseNumber":12,"Text":"Vi forsyner vejledningen"},{"SurahNumber":92,"VerseNumber":13,"Text":"Vi kontrollerer Herefter også ligesom den liv"},{"SurahNumber":92,"VerseNumber":14,"Text":"Jeg advarer jer blussende Hellfire"},{"SurahNumber":92,"VerseNumber":15,"Text":"Ingen forbrænder therein frelser de wicked"},{"SurahNumber":92,"VerseNumber":16,"Text":"Som disbelieves sving"},{"SurahNumber":92,"VerseNumber":17,"Text":"Undladende det være de righteous"},{"SurahNumber":92,"VerseNumber":18,"Text":"Som giver hans penge velgørenhed"},{"SurahNumber":92,"VerseNumber":19,"Text":"Søgende intet tilbage"},{"SurahNumber":92,"VerseNumber":20,"Text":"Søgende kun hans Lord mest Højeste"},{"SurahNumber":92,"VerseNumber":21,"Text":"Han certainly attain frelsning"},{"SurahNumber":93,"VerseNumber":1,"Text":"Ved formiddagen"},{"SurahNumber":93,"VerseNumber":2,"Text":"Ved natten det falde"},{"SurahNumber":93,"VerseNumber":3,"Text":"Deres Lord aldrig forladte I nor gjorde Han glemmer"},{"SurahNumber":93,"VerseNumber":4,"Text":"Herefter er fjerneste godt i jer end den først (liv)"},{"SurahNumber":93,"VerseNumber":5,"Text":"Deres Lord give jer nok; du behages"},{"SurahNumber":93,"VerseNumber":6,"Text":"Han ikke grundlægger du orphaned Han gøre gav jer hjem"},{"SurahNumber":93,"VerseNumber":7,"Text":"Han grundlægge jer astray guided I"},{"SurahNumber":93,"VerseNumber":8,"Text":"Han grundlægge jer fattige stillede I rige"},{"SurahNumber":93,"VerseNumber":9,"Text":"Derfor du ikke forsake forældreløse"},{"SurahNumber":93,"VerseNumber":10,"Text":"Nor skulle du tilretteviser tiggeren"},{"SurahNumber":93,"VerseNumber":11,"Text":"Du proklamere velsignende Deres Lord bestowed jer"}]}