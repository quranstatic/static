{"Id":419,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":37,"VerseNumber":22,"Text":"uḥşürü-lleẕîne żalemû veezvâcehüm vemâ kânû ya`büdûn"},{"SurahNumber":37,"VerseNumber":23,"Text":"min dûni-llâhi fehdûhüm ilâ ṣirâṭi-lceḥîm"},{"SurahNumber":37,"VerseNumber":24,"Text":"veḳifûhüm innehüm mes'ûlûn"},{"SurahNumber":37,"VerseNumber":25,"Text":"mâ leküm lâ tenâṣarûn"},{"SurahNumber":37,"VerseNumber":26,"Text":"bel hümü-lyevme müsteslimûn"},{"SurahNumber":37,"VerseNumber":27,"Text":"veaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn"},{"SurahNumber":37,"VerseNumber":28,"Text":"ḳâlû inneküm küntüm te'tûnenâ `ani-lyemîn"},{"SurahNumber":37,"VerseNumber":29,"Text":"ḳâlû bel lem tekûnû mü'minîn"},{"SurahNumber":37,"VerseNumber":30,"Text":"vemâ kâne lenâ `aleyküm min sülṭân. bel küntüm ḳavmen ṭâgîn"},{"SurahNumber":37,"VerseNumber":31,"Text":"feḥaḳḳa `aleynâ ḳavlü rabbinâ. innâ leẕâiḳûn"},{"SurahNumber":37,"VerseNumber":32,"Text":"feagveynâküm innâ künnâ gâvîn"},{"SurahNumber":37,"VerseNumber":33,"Text":"feinnehüm yevmeiẕin fi-l`aẕâbi müşterikûn"},{"SurahNumber":37,"VerseNumber":34,"Text":"innâ keẕâlike nef`alü bilmücrimîn"},{"SurahNumber":37,"VerseNumber":35,"Text":"innehüm kânû iẕâ ḳîle lehüm lâ ilâhe ille-llâhü yestekbirûn"},{"SurahNumber":37,"VerseNumber":36,"Text":"veyeḳûlûne einnâ letârikû âlihetinâ lişâ`irim mecnûn"},{"SurahNumber":37,"VerseNumber":37,"Text":"bel câe bilḥaḳḳi veṣaddeḳa-lmürselîn"},{"SurahNumber":37,"VerseNumber":38,"Text":"inneküm leẕâiḳu-l`aẕâbi-l'elîm"},{"SurahNumber":37,"VerseNumber":39,"Text":"vemâ tüczevne illâ mâ küntüm ta`melûn"},{"SurahNumber":37,"VerseNumber":40,"Text":"illâ `ibâde-llâhi-lmuḫleṣîn"},{"SurahNumber":37,"VerseNumber":41,"Text":"ülâike lehüm rizḳum ma`lûm"},{"SurahNumber":37,"VerseNumber":42,"Text":"fevâkih. vehüm mükramûn"},{"SurahNumber":37,"VerseNumber":43,"Text":"fî cennâti-nne`îm"},{"SurahNumber":37,"VerseNumber":44,"Text":"alâ sürurim müteḳâbilîn"},{"SurahNumber":37,"VerseNumber":45,"Text":"yüṭâfü `aleyhim bike'sim mim me`în"},{"SurahNumber":37,"VerseNumber":46,"Text":"beyḍâe leẕẕetil lişşâribîn"},{"SurahNumber":37,"VerseNumber":47,"Text":"lâ fîhâ gavlüv velâ hüm `anhâ yünzefûn"},{"SurahNumber":37,"VerseNumber":48,"Text":"ve`indehüm ḳâṣirâtu-ṭṭarfi `în"},{"SurahNumber":37,"VerseNumber":49,"Text":"keennehünne beyḍum meknûn"},{"SurahNumber":37,"VerseNumber":50,"Text":"feaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn"},{"SurahNumber":37,"VerseNumber":51,"Text":"ḳâle ḳâilüm minhüm innî kâne lî ḳarîn"},{"SurahNumber":37,"VerseNumber":52,"Text":"yeḳûlü einneke lemine-lmüṣaddiḳîn"},{"SurahNumber":37,"VerseNumber":53,"Text":"eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemedînûn"},{"SurahNumber":37,"VerseNumber":54,"Text":"ḳâle hel entüm müṭṭali`ûn"},{"SurahNumber":37,"VerseNumber":55,"Text":"feṭṭale`a feraâhü fî sevâi-lceḥîm"},{"SurahNumber":37,"VerseNumber":56,"Text":"ḳâle tellâhi in kitte letürdîn"},{"SurahNumber":37,"VerseNumber":57,"Text":"velevlâ ni`metü rabbî leküntü mine-lmuḥḍarîn"},{"SurahNumber":37,"VerseNumber":58,"Text":"efemâ naḥnü bimeyyitîn"},{"SurahNumber":37,"VerseNumber":59,"Text":"illâ mevtetene-l'ûlâ vemâ naḥnü bimü`aẕẕebîn"},{"SurahNumber":37,"VerseNumber":60,"Text":"inne hâẕâ lehüve-lfevzü-l`ażîm"},{"SurahNumber":37,"VerseNumber":61,"Text":"limiŝli hâẕâ felya`meli-l`âmilûn"},{"SurahNumber":37,"VerseNumber":62,"Text":"eẕâlike ḫayrun nüzülen em şeceratü-zzeḳḳûm"},{"SurahNumber":37,"VerseNumber":63,"Text":"innâ ce`alnâhâ fitnetel liżżâlimîn"},{"SurahNumber":37,"VerseNumber":64,"Text":"innehâ şeceratün taḫrucü fî aṣli-lceḥîm"},{"SurahNumber":37,"VerseNumber":65,"Text":"ṭal`uhâ keennehû ruûsü-şşeyâṭîn"},{"SurahNumber":37,"VerseNumber":66,"Text":"feinnehüm leâkilûne minhâ femâliûne minhe-lbüṭûn"},{"SurahNumber":37,"VerseNumber":67,"Text":"ŝümme inne lehüm `aleyhâ leşevbem min ḥamîm"},{"SurahNumber":37,"VerseNumber":68,"Text":"ŝümme inne merci`ahüm leile-lceḥîm"},{"SurahNumber":37,"VerseNumber":69,"Text":"innehüm elfev âbâehüm ḍâllîn"},{"SurahNumber":37,"VerseNumber":70,"Text":"fehüm `alâ âŝârihim yühra`ûn"},{"SurahNumber":37,"VerseNumber":71,"Text":"veleḳad ḍalle ḳablehüm ekŝeru-l'evvelîn"},{"SurahNumber":37,"VerseNumber":72,"Text":"veleḳad erselnâ fîhim münẕirîn"},{"SurahNumber":37,"VerseNumber":73,"Text":"fenżur keyfe kâne `âḳibetü-lmünẕerîn"},{"SurahNumber":37,"VerseNumber":74,"Text":"illâ `ibâde-llâhi-lmuḫleṣîn"},{"SurahNumber":37,"VerseNumber":75,"Text":"veleḳad nâdânâ nûḥun feleni`me-lmücîbûn"},{"SurahNumber":37,"VerseNumber":76,"Text":"venecceynâhü veehlehû mine-lkerbi-l`ażîm"},{"SurahNumber":37,"VerseNumber":77,"Text":"vece`alnâ ẕürriyyetehû hümü-lbâḳîn"},{"SurahNumber":37,"VerseNumber":78,"Text":"veteraknâ `aleyhi fi-l'âḫirîn"},{"SurahNumber":37,"VerseNumber":79,"Text":"selâmün `alâ nûḥin fi-l`âlemîn"},{"SurahNumber":37,"VerseNumber":80,"Text":"innâ keẕâlike neczi-lmuḥsinîn"},{"SurahNumber":37,"VerseNumber":81,"Text":"innehû min `ibâdine-lmü'minîn"},{"SurahNumber":37,"VerseNumber":82,"Text":"ŝümme agraḳne-l'âḫarîn"}]}