{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":94,"VerseNumber":1,"Text":"Alam nashrah laka sadrak"},{"SurahNumber":94,"VerseNumber":2,"Text":"Wa wa d'ana 'anka wizrak"},{"SurahNumber":94,"VerseNumber":3,"Text":"Allazee anqada zahrak"},{"SurahNumber":94,"VerseNumber":4,"Text":"Wa raf 'ana laka zikrak"},{"SurahNumber":94,"VerseNumber":5,"Text":"Fa inna ma'al usri yusra"},{"SurahNumber":94,"VerseNumber":6,"Text":"Inna ma'al 'usri yusra"},{"SurahNumber":94,"VerseNumber":7,"Text":"Fa iza faragh ta fansab"},{"SurahNumber":94,"VerseNumber":8,"Text":"Wa ilaa rabbika far ghab"},{"SurahNumber":95,"VerseNumber":1,"Text":"Wat teeni waz zaitoon"},{"SurahNumber":95,"VerseNumber":2,"Text":"Wa toori sineen"},{"SurahNumber":95,"VerseNumber":3,"Text":"Wa haazal balad-il ameen"},{"SurahNumber":95,"VerseNumber":4,"Text":"Laqad khalaqnal insaana fee ahsani taqweem"},{"SurahNumber":95,"VerseNumber":5,"Text":"Thumma ra dad naahu asfala saafileen"},{"SurahNumber":95,"VerseNumber":6,"Text":"Ill-lal lazeena aamanoo wa 'amilus saalihaati; falahum ajrun ghairu mamnoon"},{"SurahNumber":95,"VerseNumber":7,"Text":"Fama yu kaz zibuka b'adu bid deen"},{"SurahNumber":95,"VerseNumber":8,"Text":"Alai sal laahu bi-ahkamil haakimeen"},{"SurahNumber":96,"VerseNumber":1,"Text":"Iqra bismi rab bikal lazee khalaq"},{"SurahNumber":96,"VerseNumber":2,"Text":"Khalaqal insaana min 'alaq"},{"SurahNumber":96,"VerseNumber":3,"Text":"Iqra wa rab bukal akram"},{"SurahNumber":96,"VerseNumber":4,"Text":"Al lazee 'allama bil qalam"},{"SurahNumber":96,"VerseNumber":5,"Text":"Al lamal insaana ma lam y'alam"},{"SurahNumber":96,"VerseNumber":6,"Text":"Kallaa innal insaana layatghaa"},{"SurahNumber":96,"VerseNumber":7,"Text":"Ar-ra aahus taghnaa"},{"SurahNumber":96,"VerseNumber":8,"Text":"Innna ilaa rabbikar ruj'aa"},{"SurahNumber":96,"VerseNumber":9,"Text":"Ara-aital lazee yanhaa"},{"SurahNumber":96,"VerseNumber":10,"Text":"Abdan iza sallaa"},{"SurahNumber":96,"VerseNumber":11,"Text":"Ara-aita in kana 'alal hudaa"},{"SurahNumber":96,"VerseNumber":12,"Text":"Au amara bit taqwaa"},{"SurahNumber":96,"VerseNumber":13,"Text":"Ara-aita in kaz zaba wa ta walla"},{"SurahNumber":96,"VerseNumber":14,"Text":"Alam y'alam bi-an nal lahaa yaraa"},{"SurahNumber":96,"VerseNumber":15,"Text":"Kalla la illam yantahi la nasfa'am bin nasiyah"},{"SurahNumber":96,"VerseNumber":16,"Text":"Nasiyatin kazi batin khaatiyah"},{"SurahNumber":96,"VerseNumber":17,"Text":"Fal yad'u naadiyah"},{"SurahNumber":96,"VerseNumber":18,"Text":"Sanad 'uz zabaaniyah"},{"SurahNumber":96,"VerseNumber":19,"Text":"Kalla; la tuti'hu wasjud waqtarib"},{"SurahNumber":97,"VerseNumber":1,"Text":"Innaa anzalnaahu fee lailatil qadr"},{"SurahNumber":97,"VerseNumber":2,"Text":"Wa maa adraaka ma lailatul qadr"},{"SurahNumber":97,"VerseNumber":3,"Text":"Lailatul qadri khairum min alfee shahr"},{"SurahNumber":97,"VerseNumber":4,"Text":"Tanaz zalul malaa-ikatu war roohu feeha bi izni-rab bihim min kulli amr"},{"SurahNumber":97,"VerseNumber":5,"Text":"Salaamun hiya hattaa mat la'il fajr"},{"SurahNumber":98,"VerseNumber":1,"Text":"Lam ya kunil lazeena kafaru min ahlil kitaabi wal mushri keena mun fak keena hattaa ta-tiya humul bayyinah"},{"SurahNumber":98,"VerseNumber":2,"Text":"Rasoolum minal laahi yatlu suhufam mutahharah"},{"SurahNumber":98,"VerseNumber":3,"Text":"Feeha kutubun qaiyimah"},{"SurahNumber":98,"VerseNumber":4,"Text":"Wa maa tafarraqal lazeena ootul kitaaba il-la mim b'adi ma jaa-at humul baiyyinah"},{"SurahNumber":98,"VerseNumber":5,"Text":"Wa maa umiroo il-la liy'abu dul laaha mukhliseena lahud-deena huna faa-a wa yuqeemus salaahta wa yu-tuz zakaata; wa zaalika deenul qaiyimah"},{"SurahNumber":98,"VerseNumber":6,"Text":"Innal lazeena kafaru min ahlil kitaabi wal mushri keena fee nari jahan nama khaali deena feeha; ulaa-ika hum shar rul ba reeyah"},{"SurahNumber":98,"VerseNumber":7,"Text":"Innal lazeena aamanu wa 'amilus saalihaati ula-ika hum khairul bareey yah"},{"SurahNumber":98,"VerseNumber":8,"Text":"Jazaa-uhum inda rabbihim jan naatu 'adnin tajree min tahtihal an haaru khalideena feeha abada; radiy-yallaahu 'anhum wa ra du 'an zaalika liman khashiya rabbah"},{"SurahNumber":99,"VerseNumber":1,"Text":"Izaa zul zilatil ardu zil zaalaha"},{"SurahNumber":99,"VerseNumber":2,"Text":"Wa akh rajatil ardu athqaalaha"},{"SurahNumber":99,"VerseNumber":3,"Text":"Wa qaalal insaanu ma laha"},{"SurahNumber":99,"VerseNumber":4,"Text":"Yawmaa izin tuhad dithu akhbaaraha"},{"SurahNumber":99,"VerseNumber":5,"Text":"Bi-anna rabbaka awhaa laha"},{"SurahNumber":99,"VerseNumber":6,"Text":"Yawma iziy yas durun naasu ash tatal liyuraw a'maalahum"},{"SurahNumber":99,"VerseNumber":7,"Text":"Famaiy ya'mal mithqala zarratin khai raiy-yarah"},{"SurahNumber":99,"VerseNumber":8,"Text":"Wa maiy-y'amal mithqala zarratin sharraiy-yarah"},{"SurahNumber":100,"VerseNumber":1,"Text":"Wal'aadi yaati dabha"},{"SurahNumber":100,"VerseNumber":2,"Text":"Fal moori yaati qadha"},{"SurahNumber":100,"VerseNumber":3,"Text":"Fal mugheeraati subha"},{"SurahNumber":100,"VerseNumber":4,"Text":"Fa atharna bihee naq'a"},{"SurahNumber":100,"VerseNumber":5,"Text":"Fawa satna bihee jam'a"},{"SurahNumber":100,"VerseNumber":6,"Text":"Innal-insana lirabbihee lakanood"},{"SurahNumber":100,"VerseNumber":7,"Text":"Wa innahu 'alaa zaalika la shaheed"},{"SurahNumber":100,"VerseNumber":8,"Text":"Wa innahu lihubbil khairi la shadeed"}]}