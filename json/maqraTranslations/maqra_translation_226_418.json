{"Id":418,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay-la","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":68,"VerseNumber":1,"Text":"nun. velkalemi vema yesturun"},{"SurahNumber":68,"VerseNumber":2,"Text":"ma ente bini`meti rabbike bimecnun"},{"SurahNumber":68,"VerseNumber":3,"Text":"veinne leke leecran gayra memnun"},{"SurahNumber":68,"VerseNumber":4,"Text":"veinneke le`ala hulukin `azim"},{"SurahNumber":68,"VerseNumber":5,"Text":"fesetubsiru veyubsirun"},{"SurahNumber":68,"VerseNumber":6,"Text":"bieyyikumu-lmeftun"},{"SurahNumber":68,"VerseNumber":7,"Text":"inne rabbeke huve a`lemu bimen dalle `an sebilih. vehuve a`lemu bilmuhtedin"},{"SurahNumber":68,"VerseNumber":8,"Text":"fela tuti`i-lmukezzibin"},{"SurahNumber":68,"VerseNumber":9,"Text":"veddu lev tudhinu feyudhinun"},{"SurahNumber":68,"VerseNumber":10,"Text":"vela tuti` kulle hallafim mehin"},{"SurahNumber":68,"VerseNumber":11,"Text":"hemmazim messaim binemim"},{"SurahNumber":68,"VerseNumber":12,"Text":"menna`il lilhayri mu`tedin esim"},{"SurahNumber":68,"VerseNumber":13,"Text":"utullim ba`de zalike zenim"},{"SurahNumber":68,"VerseNumber":14,"Text":"en kane za maliv vebenin"},{"SurahNumber":68,"VerseNumber":15,"Text":"iza tutla `aleyhi ayatuna kale esatiru-l'evvelin"},{"SurahNumber":68,"VerseNumber":16,"Text":"senesimuhu `ale-lhurtum"},{"SurahNumber":68,"VerseNumber":17,"Text":"inna belevnahum kema belevna ashabe-lcenneh. iz aksemu leyasrimunneha musbihin"},{"SurahNumber":68,"VerseNumber":18,"Text":"vela yestesnun"},{"SurahNumber":68,"VerseNumber":19,"Text":"fetafe `aleyha taifum mir rabbike vehum naimun"},{"SurahNumber":68,"VerseNumber":20,"Text":"feasbehat kessarim"},{"SurahNumber":68,"VerseNumber":21,"Text":"fetenadev musbihin"},{"SurahNumber":68,"VerseNumber":22,"Text":"eni-gdu `ala harsikum in kuntum sarimin"},{"SurahNumber":68,"VerseNumber":23,"Text":"fentaleku vehum yetehafetun"},{"SurahNumber":68,"VerseNumber":24,"Text":"el la yedhulennehe-lyevme `aleykum miskin"},{"SurahNumber":68,"VerseNumber":25,"Text":"vegadev `ala hardin kadirin"},{"SurahNumber":68,"VerseNumber":26,"Text":"felemma raevha kalu inna ledallun"},{"SurahNumber":68,"VerseNumber":27,"Text":"bel nahnu mahrumun"},{"SurahNumber":68,"VerseNumber":28,"Text":"kale evsetuhum elem ekul lekum levla tusebbihun"},{"SurahNumber":68,"VerseNumber":29,"Text":"kalu subhane rabbina inna kunna zalimin"},{"SurahNumber":68,"VerseNumber":30,"Text":"feakbele ba`duhum `ala ba`diy yetelavemun"},{"SurahNumber":68,"VerseNumber":31,"Text":"kalu ya veylena inna kunna tagin"},{"SurahNumber":68,"VerseNumber":32,"Text":"asa rabbuna ey yubdilena hayram minha inna ila rabbina ragibun"},{"SurahNumber":68,"VerseNumber":33,"Text":"kezalike-l`azab. vele`azabu-l'ahirati ekber. lev kanu ya`lemun"},{"SurahNumber":68,"VerseNumber":34,"Text":"inne lilmuttekine `inde rabbihim cennati-nne`im"},{"SurahNumber":68,"VerseNumber":35,"Text":"efenec`alu-lmuslimine kelmucrimin"},{"SurahNumber":68,"VerseNumber":36,"Text":"ma lekum. keyfe tahkumun"},{"SurahNumber":68,"VerseNumber":37,"Text":"em lekum kitabun fihi tedrusun"},{"SurahNumber":68,"VerseNumber":38,"Text":"inne lekum fihi lema tehayyerun"},{"SurahNumber":68,"VerseNumber":39,"Text":"em lekum eymanun `aleyna baligatun ila yevmi-lkiyameti inne lekum lema tahkumun"},{"SurahNumber":68,"VerseNumber":40,"Text":"selhum eyyuhum bizalike za`im"},{"SurahNumber":68,"VerseNumber":41,"Text":"em lehum suraka'. felye'tu bisurakaihim in kanu sadikin"},{"SurahNumber":68,"VerseNumber":42,"Text":"yevme yuksefu `an sakiv veyud`avne ile-ssucudi fela yesteti`un"},{"SurahNumber":68,"VerseNumber":43,"Text":"hasi`aten ebsaruhum terhekuhum zilleh. vekad kanu yud`avne ile-ssucudi vehum salimun"},{"SurahNumber":68,"VerseNumber":44,"Text":"fezerni vemey yukezzibu bihaze-lhadis. senestedricuhum min haysu la ya`lemun"},{"SurahNumber":68,"VerseNumber":45,"Text":"veumli lehum. inne keydi metin"},{"SurahNumber":68,"VerseNumber":46,"Text":"em tes'eluhum ecran fehum mim magramim muskalun"},{"SurahNumber":68,"VerseNumber":47,"Text":"em `indehumu-lgaybu fehum yektubun"},{"SurahNumber":68,"VerseNumber":48,"Text":"fasbir lihukmi rabbike vela tekun kesahibi-lhut. iz nada vehuve mekzum"},{"SurahNumber":68,"VerseNumber":49,"Text":"levla en tedarakehu ni`metum mir rabbihi lenubize bil`arai vehuve mezmum"},{"SurahNumber":68,"VerseNumber":50,"Text":"fectebahu rabbuhu fece`alehu mine-ssalihin"},{"SurahNumber":68,"VerseNumber":51,"Text":"veiy yekadu-llezine keferu leyuzlikuneke biebsarihim lemma semi`u-zzikra veyekulune innehu lemecnun"},{"SurahNumber":68,"VerseNumber":52,"Text":"vema huve illa zikrul lil`alemin"}]}