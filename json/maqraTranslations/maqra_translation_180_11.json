{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":37,"VerseNumber":83,"Text":"Wa ina min shee'atihee la Ibraaheem"},{"SurahNumber":37,"VerseNumber":84,"Text":"Iz jaaa'a Rabbahoo bi qalbin saleem"},{"SurahNumber":37,"VerseNumber":85,"Text":"Iz qaala li abeehi wa qawmihee maazaa ta'budoon"},{"SurahNumber":37,"VerseNumber":86,"Text":"A'ifkan aalihatan doonal laahi tureedoon"},{"SurahNumber":37,"VerseNumber":87,"Text":"Famaa zannukum bi Rabbil'aalameen"},{"SurahNumber":37,"VerseNumber":88,"Text":"Fanazara nazratan finnujoom"},{"SurahNumber":37,"VerseNumber":89,"Text":"Faqaala inee saqeem"},{"SurahNumber":37,"VerseNumber":90,"Text":"Fatawallaw 'anhu mudbireen"},{"SurahNumber":37,"VerseNumber":91,"Text":"Faraagha ilaaa aalihatihim faqaala alaa taakuloon"},{"SurahNumber":37,"VerseNumber":92,"Text":"Maa lakum laa tantiqoon"},{"SurahNumber":37,"VerseNumber":93,"Text":"Faraagha 'alaihim darbam bilyameen"},{"SurahNumber":37,"VerseNumber":94,"Text":"Fa aqbalooo ilaihi yaziffoon"},{"SurahNumber":37,"VerseNumber":95,"Text":"Qaala ata'budoona maa tanhitoon"},{"SurahNumber":37,"VerseNumber":96,"Text":"Wallaahu khalaqakum wa maa ta'maloon"},{"SurahNumber":37,"VerseNumber":97,"Text":"Qaalub noo lahoo bun yaanan fa alqoohu fil jaheem"},{"SurahNumber":37,"VerseNumber":98,"Text":"Fa araadoo bihee kaidan faja 'alnaahumul asfaleen"},{"SurahNumber":37,"VerseNumber":99,"Text":"Wa qaala innee zaahibun ilaa Rabbee sa yahdeen"},{"SurahNumber":37,"VerseNumber":100,"Text":"Rabbi hab lee minas saaliheen"},{"SurahNumber":37,"VerseNumber":101,"Text":"Fabashsharnaahu bighulaamin haleem"},{"SurahNumber":37,"VerseNumber":102,"Text":"Falamma balagha ma'a hus sa'ya qaala yaa buniya inneee araa fil manaami anneee azbahuka fanzur maazaa taraa; qaala yaaa abatif 'al maa tu'maru satajidunee in shaaa'allaahu minas saabireen"},{"SurahNumber":37,"VerseNumber":103,"Text":"Falammaaa aslamaa wa tallahoo liljabeen"},{"SurahNumber":37,"VerseNumber":104,"Text":"Wa naadainaahu ai yaaaa Ibraheem"},{"SurahNumber":37,"VerseNumber":105,"Text":"Qad saddaqtar ru'yaa; innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":106,"Text":"Inna haazaa lahuwal balaaa'ul mubeen"},{"SurahNumber":37,"VerseNumber":107,"Text":"Wa fadainaahu bizibhin 'azeem"},{"SurahNumber":37,"VerseNumber":108,"Text":"Wa taraknaa 'alaihi fil aakhireen"},{"SurahNumber":37,"VerseNumber":109,"Text":"Salaamun 'alaaa Ibraaheem"},{"SurahNumber":37,"VerseNumber":110,"Text":"Kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":111,"Text":"Innahoo min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":112,"Text":"Wa bashsharnaahu bi Ishaaqa Nabiyayam minas saaliheen"},{"SurahNumber":37,"VerseNumber":113,"Text":"Wa baaraknaa 'alaihi wa 'alaaa Ishaaq; wa min zurriyya tihimaa muhsinunw wa zaalimul linafshihee mubeen"},{"SurahNumber":37,"VerseNumber":114,"Text":"Wa laqad mananna alaa Moosaa wa Haaroon"},{"SurahNumber":37,"VerseNumber":115,"Text":"Wa najjainaahumaa wa qawmahumaa minal karbil 'azeem"},{"SurahNumber":37,"VerseNumber":116,"Text":"Wa nasarnaahum fakaanoo humul ghaalibeen"},{"SurahNumber":37,"VerseNumber":117,"Text":"Wa aatainaahumal Kitaabal mustabeen"},{"SurahNumber":37,"VerseNumber":118,"Text":"Wa hadainaahumus Siraatal Mustaqeem"},{"SurahNumber":37,"VerseNumber":119,"Text":"Wa taraknaa 'alaihimaa fil aakhireen"},{"SurahNumber":37,"VerseNumber":120,"Text":"Salaamun 'alaa Moosaa wa Haaroon"},{"SurahNumber":37,"VerseNumber":121,"Text":"Innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":122,"Text":"Innahumaa min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":123,"Text":"Wa inna Ilyaasa laminal mursaleen"},{"SurahNumber":37,"VerseNumber":124,"Text":"Iz qaala liqawmiheee alaa tattaqoon"},{"SurahNumber":37,"VerseNumber":125,"Text":"Atad'oona Ba'lanw wa tazaroona ahsanal khaaliqeen"},{"SurahNumber":37,"VerseNumber":126,"Text":"Allaaha Rabbakum wa Rabba aabaaa'ikumul awwaleen"},{"SurahNumber":37,"VerseNumber":127,"Text":"Fakazzaboohu fa inna hum lamuhdaroon"},{"SurahNumber":37,"VerseNumber":128,"Text":"Illaa 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":129,"Text":"Wa taraknaa 'alaihi fil aakhireen"},{"SurahNumber":37,"VerseNumber":130,"Text":"Salaamun 'alaaa Ilyaaseen"},{"SurahNumber":37,"VerseNumber":131,"Text":"Innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":132,"Text":"Innahoo min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":133,"Text":"Wa inna Lootal laminal mursaleen"},{"SurahNumber":37,"VerseNumber":134,"Text":"Iz najjainaahu wa ahlahooo ajma'een"},{"SurahNumber":37,"VerseNumber":135,"Text":"Illaa 'ajoozan fil ghaabireen"},{"SurahNumber":37,"VerseNumber":136,"Text":"Summa dammarnal aakhareen"},{"SurahNumber":37,"VerseNumber":137,"Text":"Wa innakum latamurroona 'alaihim musbiheen"},{"SurahNumber":37,"VerseNumber":138,"Text":"Wa billail; afalaa ta'qiloon"},{"SurahNumber":37,"VerseNumber":139,"Text":"Wa inna Yoonusa laminal mursaleen"},{"SurahNumber":37,"VerseNumber":140,"Text":"Iz abaqa ilal fulkil mash hoon"},{"SurahNumber":37,"VerseNumber":141,"Text":"Fasaahama fakaana minal mudhadeen"},{"SurahNumber":37,"VerseNumber":142,"Text":"Faltaqamahul hootu wa huwa muleem"},{"SurahNumber":37,"VerseNumber":143,"Text":"Falaw laaa annahoo kaana minal musabbiheen"},{"SurahNumber":37,"VerseNumber":144,"Text":"Lalabisa fee batniheee ilaa Yawmi yub'asoon"}]}