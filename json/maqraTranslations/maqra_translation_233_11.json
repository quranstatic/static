{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":78,"VerseNumber":1,"Text":"Amma Yatasaa-aloon"},{"SurahNumber":78,"VerseNumber":2,"Text":"Anin-nabaa-il 'azeem"},{"SurahNumber":78,"VerseNumber":3,"Text":"Allazi hum feehi mukh talifoon"},{"SurahNumber":78,"VerseNumber":4,"Text":"Kallaa sa y'alamoon"},{"SurahNumber":78,"VerseNumber":5,"Text":"Thumma kallaa sa y'alamoon"},{"SurahNumber":78,"VerseNumber":6,"Text":"Alam naj'alil arda mihaa da"},{"SurahNumber":78,"VerseNumber":7,"Text":"Wal jibaala au taada"},{"SurahNumber":78,"VerseNumber":8,"Text":"Wa khalaq naakum azwaaja"},{"SurahNumber":78,"VerseNumber":9,"Text":"Waja'alnan naumakum subata"},{"SurahNumber":78,"VerseNumber":10,"Text":"Waja'alnal laila libasa"},{"SurahNumber":78,"VerseNumber":11,"Text":"Waja'alnan nahara ma 'aasha"},{"SurahNumber":78,"VerseNumber":12,"Text":"Wa banaina fauqakum sab 'an shi daada"},{"SurahNumber":78,"VerseNumber":13,"Text":"Waja'alna siraajaw wah haaja"},{"SurahNumber":78,"VerseNumber":14,"Text":"Wa anzalna minal m'usiraati maa-an saj-jaaja"},{"SurahNumber":78,"VerseNumber":15,"Text":"Linukh rija bihee habbaw wana baata"},{"SurahNumber":78,"VerseNumber":16,"Text":"Wa jan naatin alfafa"},{"SurahNumber":78,"VerseNumber":17,"Text":"Inna yaumal-fasli kana miqaata"},{"SurahNumber":78,"VerseNumber":18,"Text":"Yauma yun fakhu fis-soori fataa toona afwaaja"},{"SurahNumber":78,"VerseNumber":19,"Text":"Wa futiha tis samaa-u fakaanat abwaaba"},{"SurahNumber":78,"VerseNumber":20,"Text":"Wa suyyi raatil jibaalu fa kaanat saraaba"},{"SurahNumber":78,"VerseNumber":21,"Text":"Inna jahan nama kaanat mirsaada"},{"SurahNumber":78,"VerseNumber":22,"Text":"Lit taa gheena ma aaba"},{"SurahNumber":78,"VerseNumber":23,"Text":"Laa bitheena feehaa ahqaaba"},{"SurahNumber":78,"VerseNumber":24,"Text":"Laa ya zooqoona feeha bar daw walaa sharaaba"},{"SurahNumber":78,"VerseNumber":25,"Text":"Illa hamee maw-wa ghas saaqa"},{"SurahNumber":78,"VerseNumber":26,"Text":"Jazaa-aw wi faaqa"},{"SurahNumber":78,"VerseNumber":27,"Text":"Innahum kaanu laa yarjoona hisaaba"},{"SurahNumber":78,"VerseNumber":28,"Text":"Wa kazzabu bi aayaa tina kizzaba"},{"SurahNumber":78,"VerseNumber":29,"Text":"Wa kulla shai-in ahsai naahu kitaa ba"},{"SurahNumber":78,"VerseNumber":30,"Text":"Fa zooqoo falan-nazee dakum ill-laa azaaba"},{"SurahNumber":78,"VerseNumber":31,"Text":"Inna lil mutta qeena mafaaza"},{"SurahNumber":78,"VerseNumber":32,"Text":"Hadaa-iqa wa a'anaa ba"},{"SurahNumber":78,"VerseNumber":33,"Text":"Wa kaawa 'iba at raaba"},{"SurahNumber":78,"VerseNumber":34,"Text":"Wa ka'san di haaqa"},{"SurahNumber":78,"VerseNumber":35,"Text":"Laa yasma'oona fiha lagh waw walaa kizzaba"},{"SurahNumber":78,"VerseNumber":36,"Text":"Jazaa-am mir-rabbika ataa-an hisaaba"},{"SurahNumber":78,"VerseNumber":37,"Text":"Rabbis samaa waati wal ardi wa maa baina humar rahmaani laa yam likoona minhu khi taaba"},{"SurahNumber":78,"VerseNumber":38,"Text":"Yauma yaqoo mur roohu wal malaa-ikatu saf-fal laa yata kalla moona illa man azina lahur rahmaanu wa qaala sawaaba"},{"SurahNumber":78,"VerseNumber":39,"Text":"Zaalikal yaumul haqqu faman shaa-at ta khaaza ill-laa rabbihi ma-aaba"},{"SurahNumber":78,"VerseNumber":40,"Text":"In naa anzar naakum azaaban qareebaiy-yauma yan zurul marr-u maa qaddamat yadaahu wa ya qoolul-kaafiru yaa lai tanee kuntu turaaba"},{"SurahNumber":79,"VerseNumber":1,"Text":"Wan naazi 'aati gharqa"},{"SurahNumber":79,"VerseNumber":2,"Text":"Wan naa shi taati nashta"},{"SurahNumber":79,"VerseNumber":3,"Text":"Wass saabi-haati sabha"},{"SurahNumber":79,"VerseNumber":4,"Text":"Fass saabi qaati sabqa"},{"SurahNumber":79,"VerseNumber":5,"Text":"Fal mu dab-bi raati amra"},{"SurahNumber":79,"VerseNumber":6,"Text":"Yawma tarjufur raajifa"},{"SurahNumber":79,"VerseNumber":7,"Text":"Tatba'u har raadifa"},{"SurahNumber":79,"VerseNumber":8,"Text":"Quloobuny-yau maaiziw-waaji-fa"},{"SurahNumber":79,"VerseNumber":9,"Text":"Absaa ruhaa khashi'ah"},{"SurahNumber":79,"VerseNumber":10,"Text":"Ya qoo loona a-inna lamar doo doona fil haafirah"},{"SurahNumber":79,"VerseNumber":11,"Text":"Aizaa kunna 'izaa man-nakhirah"},{"SurahNumber":79,"VerseNumber":12,"Text":"Qaalu tilka izan karratun khaasirah"},{"SurahNumber":79,"VerseNumber":13,"Text":"Fa inna ma hiya zajratuw-waahida"},{"SurahNumber":79,"VerseNumber":14,"Text":"Faizaa hum biss saahirah"},{"SurahNumber":79,"VerseNumber":15,"Text":"Hal ataaka hadeethu Musaa"},{"SurahNumber":79,"VerseNumber":16,"Text":"Iz nadaahu rabbuhu bil waadil-muqad dasi tuwa"},{"SurahNumber":79,"VerseNumber":17,"Text":"Izhab ilaa fir'auna innahu taghaa"},{"SurahNumber":79,"VerseNumber":18,"Text":"Faqul hal laka ilaa-an tazakka"},{"SurahNumber":79,"VerseNumber":19,"Text":"Wa ahdi yaka ila rabbika fatakh sha"},{"SurahNumber":79,"VerseNumber":20,"Text":"Fa araahul-aayatal kubra"},{"SurahNumber":79,"VerseNumber":21,"Text":"Fa kazzaba wa asaa"},{"SurahNumber":79,"VerseNumber":22,"Text":"Thumma adbara yas'aa"},{"SurahNumber":79,"VerseNumber":23,"Text":"Fa hashara fanada"},{"SurahNumber":79,"VerseNumber":24,"Text":"Faqala ana rabbu kumul-a'laa"},{"SurahNumber":79,"VerseNumber":25,"Text":"Fa-akha zahul laahu nakalal aakhirati wal-oola"},{"SurahNumber":79,"VerseNumber":26,"Text":"Inna fee zaalika la'ibratal limaiy-yaksha"},{"SurahNumber":79,"VerseNumber":27,"Text":"A-antum a shaddu khalqan amis samaa-u banaaha"},{"SurahNumber":79,"VerseNumber":28,"Text":"Raf'a sam kaha fasaw waaha"},{"SurahNumber":79,"VerseNumber":29,"Text":"Wa aghtasha lailaha wa akhraja duhaaha"},{"SurahNumber":79,"VerseNumber":30,"Text":"Wal arda b'ada zaalika dahaaha"},{"SurahNumber":79,"VerseNumber":31,"Text":"Akhraja minha maa-aha wa mar 'aaha"},{"SurahNumber":79,"VerseNumber":32,"Text":"Wal jibala arsaaha"},{"SurahNumber":79,"VerseNumber":33,"Text":"Mataa'al lakum wali an 'aamikum"},{"SurahNumber":79,"VerseNumber":34,"Text":"Fa-izaa jaaa'atit taaam matul kubraa"},{"SurahNumber":79,"VerseNumber":35,"Text":"Yauma Yata zakkarul insaanu ma sa'aa"},{"SurahNumber":79,"VerseNumber":36,"Text":"Wa burrizatil-jaheemu limany-yaraa"},{"SurahNumber":79,"VerseNumber":37,"Text":"Fa ammaa man taghaa"},{"SurahNumber":79,"VerseNumber":38,"Text":"Wa aasaral hayaatad dunyaa"},{"SurahNumber":79,"VerseNumber":39,"Text":"Fa innal jaheema hiyal maawaa"},{"SurahNumber":79,"VerseNumber":40,"Text":"Wa ammaa man khaafa maqaama Rabbihee wa nahan nafsa 'anil hawaa"},{"SurahNumber":79,"VerseNumber":41,"Text":"Fa innal jannata hiyal maawaa"},{"SurahNumber":79,"VerseNumber":42,"Text":"Yas'aloonaka 'anis saa'ati ayyaana mursaahaa"},{"SurahNumber":79,"VerseNumber":43,"Text":"Feema anta min zikraahaa"},{"SurahNumber":79,"VerseNumber":44,"Text":"Ilaa Rabbika muntahaa haa"},{"SurahNumber":79,"VerseNumber":45,"Text":"Innamaaa anta munziru maiy yakshaahaa"},{"SurahNumber":79,"VerseNumber":46,"Text":"Ka annahum Yawma yarawnahaa lam yalbasooo illaa 'ashiyyatan aw duhaahaa"}]}