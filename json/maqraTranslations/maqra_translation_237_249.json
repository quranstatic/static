{"Id":249,"LanguageCodes":["ml","mal"],"Name":"mal-muhammadkarakun-lad","Author":"Muhammad Karakunnu And Vanidas Elayavoor","Language":"Malayalam","Description":"Malayalam - Transliteration with diacritics","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":87,"VerseNumber":1,"Text":"atyunnatanāya ninṟe nāthanṟe nāmaṁ kīrttikkuka"},{"SurahNumber":87,"VerseNumber":2,"Text":"avanēā sr̥ṣṭicc santulitamākkiyavan"},{"SurahNumber":87,"VerseNumber":3,"Text":"kramīkaricc nērvaḻi kāṇiccavan"},{"SurahNumber":87,"VerseNumber":4,"Text":"mēccilppuṟaṅṅaḷ orukkiyavan"},{"SurahNumber":87,"VerseNumber":5,"Text":"enniṭṭavanatine uṇaṅṅikkariñña cavaṟākki"},{"SurahNumber":87,"VerseNumber":6,"Text":"ninakku nāṁ ōtittaruṁ. nīyat maṟakkukayilla"},{"SurahNumber":87,"VerseNumber":7,"Text":"allāhu ichiccateāḻike. parasyavuṁ rahasyavuṁ avanaṟiyunnu"},{"SurahNumber":87,"VerseNumber":8,"Text":"eḷuppamāya vaḻi ninakku nāṁ orukkittarāṁ"},{"SurahNumber":87,"VerseNumber":9,"Text":"atināl nī udbēādhippikkuka- udbēādhanaṁ upakarikkumeṅkil"},{"SurahNumber":87,"VerseNumber":10,"Text":"daivabhayamuḷḷavan udbēādhanaṁ uḷkkeāḷḷuṁ"},{"SurahNumber":87,"VerseNumber":11,"Text":"keāṭiya nirbhāgyavān atil ninn akalukayuṁ ceyyuṁ"},{"SurahNumber":87,"VerseNumber":12,"Text":"avanēā, kaṭhinamāya narakattīyil kiṭanneriyunnavan"},{"SurahNumber":87,"VerseNumber":13,"Text":"pinnīṭ avanatil marikkukayilla; jīvikkukayumilla"},{"SurahNumber":87,"VerseNumber":14,"Text":"tīrccayāyuṁ viśud'dhi variccavan vijayiccu"},{"SurahNumber":87,"VerseNumber":15,"Text":"avan tanṟe nāthanṟe nāmamēārttu. aṅṅane avan namaskariccu"},{"SurahNumber":87,"VerseNumber":16,"Text":"ennāl niṅṅaḷ ī lēāka jīvitattināṇ prāmukhyaṁ nalkunnat"},{"SurahNumber":87,"VerseNumber":17,"Text":"paralēākamāṇ ēṟṟaṁ uttamavuṁ ēṟe śāśvatavuṁ"},{"SurahNumber":87,"VerseNumber":18,"Text":"sanśayaṁ vēṇṭā, it pūrva vēdaṅṅaḷilumuṇṭ"},{"SurahNumber":87,"VerseNumber":19,"Text":"athavā, ibṟāhīminṟeyuṁ mūsāyuṭeyuṁ granthattāḷukaḷil"},{"SurahNumber":88,"VerseNumber":1,"Text":"āvaraṇaṁ ceyyunna mahāvipattinṟe vārtta ninakku vannettiyēā"},{"SurahNumber":88,"VerseNumber":2,"Text":"ann cila mukhaṅṅaḷ pēṭiccaraṇṭavayāyirikkuṁ"},{"SurahNumber":88,"VerseNumber":3,"Text":"adhvānicc taḷarnnavayuṁ"},{"SurahNumber":88,"VerseNumber":4,"Text":"cuṭṭeriyuṁ narakattilavar cennettuṁ"},{"SurahNumber":88,"VerseNumber":5,"Text":"tiḷaccu maṟiyunna uṟavayilninnāṇavarkk kuṭikkān kiṭṭuka"},{"SurahNumber":88,"VerseNumber":6,"Text":"kaypuḷḷa muḷceṭiyil ninnallāte avarkkeārāhāravumilla"},{"SurahNumber":88,"VerseNumber":7,"Text":"at śarīratte pēāṣippikkilla. viśappinu śamanamēkukayumilla"},{"SurahNumber":88,"VerseNumber":8,"Text":"ennāl maṟṟu cila mukhaṅṅaḷ ann prasannaṅṅaḷāyirikkuṁ"},{"SurahNumber":88,"VerseNumber":9,"Text":"taṅṅaḷuṭe karmaṅṅaḷekkuṟicc santr̥ptaruṁ"},{"SurahNumber":88,"VerseNumber":10,"Text":"avar atyunnatamāya svargīyārāmattilāyirikkuṁ"},{"SurahNumber":88,"VerseNumber":11,"Text":"viṭuvākkukaḷ aviṭe kēḷkkukayilla"},{"SurahNumber":88,"VerseNumber":12,"Text":"aviṭe oḻukunna aruviyuṇṭ"},{"SurahNumber":88,"VerseNumber":13,"Text":"uyarttiyeārukkiya mañcaṅṅaḷuṁ"},{"SurahNumber":88,"VerseNumber":14,"Text":"tayyāṟākkivecca pānapātraṅṅaḷuṁ"},{"SurahNumber":88,"VerseNumber":15,"Text":"nirattivecca talayaṇakaḷuṁ"},{"SurahNumber":88,"VerseNumber":16,"Text":"parattivecca paravatānikaḷuṁ"},{"SurahNumber":88,"VerseNumber":17,"Text":"avar nēākkunnillē? oṭṭakatte; atine eṅṅane sr̥ṣṭiccuvenn"},{"SurahNumber":88,"VerseNumber":18,"Text":"ākāśatte; atine evvidhaṁ uyarttiyenn"},{"SurahNumber":88,"VerseNumber":19,"Text":"parvataṅṅaḷe, avaye eṅṅane sthāpiccuvenn"},{"SurahNumber":88,"VerseNumber":20,"Text":"bhūmiye, atine eṅṅane viśālamākkiyenn"},{"SurahNumber":88,"VerseNumber":21,"Text":"atināl nī udbēādhippikkuka. nī orudbēādhakan mātramāṇ"},{"SurahNumber":88,"VerseNumber":22,"Text":"nī avaruṭe mēl nirbandhaṁ celuttunnavanalla"},{"SurahNumber":88,"VerseNumber":23,"Text":"ār pintiriyukayuṁ satyatte taḷḷippaṟayukayuṁ ceyyunnuvēā"},{"SurahNumber":88,"VerseNumber":24,"Text":"avane allāhu kaṭhinamāyi śikṣikkuṁ"},{"SurahNumber":88,"VerseNumber":25,"Text":"niścayamāyuṁ nam'muṭe aṭuttēkkāṇ avaruṭe maṭakkaṁ"},{"SurahNumber":88,"VerseNumber":26,"Text":"pinne avaruṭe vicāraṇayuṁ nam'muṭe cumatalayilāṇ"},{"SurahNumber":89,"VerseNumber":1,"Text":"prabhātaṁ sākṣi"},{"SurahNumber":89,"VerseNumber":2,"Text":"pattu rāvukaḷ sākṣi"},{"SurahNumber":89,"VerseNumber":3,"Text":"iraṭṭayuṁ oṟṟayuṁ sākṣi"},{"SurahNumber":89,"VerseNumber":4,"Text":"rāvu sākṣi- atu kaṭannupēāyikkeāṇṭirikke"},{"SurahNumber":89,"VerseNumber":5,"Text":"kāryamaṟiyunnavan avayil śapathamuṇṭēā"},{"SurahNumber":89,"VerseNumber":6,"Text":"ād janataye ninṟe nāthan entu ceytuvenn nī kaṇṭillē"},{"SurahNumber":89,"VerseNumber":7,"Text":"unnatastūpaṅṅaḷuṭe uṭamakaḷāya iṟaṁ gēātratte"},{"SurahNumber":89,"VerseNumber":8,"Text":"avareppēāle śaktarāyeāru janata maṟṟeāru nāṭṭiluṁ sr̥ṣṭikkappeṭṭiṭṭilla"},{"SurahNumber":89,"VerseNumber":9,"Text":"tāḻvarakaḷil pāṟaveṭṭippeāḷicc pārppiṭaṅṅaḷuṇṭākkiya thamūd gēātratteyuṁ"},{"SurahNumber":89,"VerseNumber":10,"Text":"āṇikaḷuṭe āḷāya phaṟavēāneyuṁ"},{"SurahNumber":89,"VerseNumber":11,"Text":"avarēā, ā nāṭukaḷil atikramaṁ pravartticcavarāyirunnu"},{"SurahNumber":89,"VerseNumber":12,"Text":"avaraviṭe kuḻappaṁ peruppiccu"},{"SurahNumber":89,"VerseNumber":13,"Text":"appēāḷ ninṟe nāthan avarkkumēl śikṣayuṭe cāṭṭavār varṣiccu"},{"SurahNumber":89,"VerseNumber":14,"Text":"ninṟe nāthan patisthalattu tanneyuṇṭ; tīrcca"},{"SurahNumber":89,"VerseNumber":15,"Text":"ennāl manuṣyane avanṟe nāthan parīkṣikkukayuṁ, aṅṅane avane ādarikkukayuṁ anugrahikkukayuṁ ceytāl avan paṟayuṁ: “enṟe nāthan enne ādariccirikkunnu.”"},{"SurahNumber":89,"VerseNumber":16,"Text":"ennāl allāhu avane parīkṣikkukayuṁ, aṅṅane avanṟe jīvitavibhavaṁ parimitappeṭuttukayuṁ ceytālēā, avan paṟayuṁ: “enṟe nāthan enne nindiccirikkunnu.”"},{"SurahNumber":89,"VerseNumber":17,"Text":"kāryaṁ atalla; niṅṅaḷ anāthaye parigaṇikkunnilla"},{"SurahNumber":89,"VerseNumber":18,"Text":"agatikk annaṁ nalkān prērippikkunnumilla"},{"SurahNumber":89,"VerseNumber":19,"Text":"pāramparyamāyikkiṭṭiya svatt vārikkūṭṭi veṭṭiviḻuṅṅukayuṁ ceyyunnu"},{"SurahNumber":89,"VerseNumber":20,"Text":"dhanatte niṅṅaḷ atiraṟṟ snēhikkunnu"},{"SurahNumber":89,"VerseNumber":21,"Text":"atalla; bhūmiyāke iṭiccu nirappākkukayuṁ"},{"SurahNumber":89,"VerseNumber":22,"Text":"ninṟe nāthanuṁ aṇiyaṇiyāyi malakkukaḷuṁ varikayuṁ"},{"SurahNumber":89,"VerseNumber":23,"Text":"ann narakatte niṅṅaḷuṭe aṭuttēkk keāṇṭuvarikayuṁ ceyyumpēāḷ; ann manuṣyan ellāṁ ōrmavaruṁ. ā samayatt ōrma vanniṭṭentu kāryaṁ"},{"SurahNumber":89,"VerseNumber":24,"Text":"avan paṟayuṁ: ayyēā, enṟe ī jīvitattināyi ñān nēratte ceytuveccirunneṅkil"},{"SurahNumber":89,"VerseNumber":25,"Text":"annāḷil allāhu śikṣikkuṁ vidhaṁ maṟṟāruṁ śikṣikkukayilla"},{"SurahNumber":89,"VerseNumber":26,"Text":"avan piṭiccukeṭṭumpēāle maṟṟāruṁ piṭiccukeṭṭukayumilla"},{"SurahNumber":89,"VerseNumber":27,"Text":"allayēā śānti nēṭiya ātmāvē"},{"SurahNumber":89,"VerseNumber":28,"Text":"nī ninṟe nāthaṅkalēkk tr̥ptippeṭṭavanāyuṁ tr̥pti nēṭiyavanāyuṁ tiriccu celluka"},{"SurahNumber":89,"VerseNumber":29,"Text":"aṅṅane enṟe uttama dāsanmāruṭe kūṭṭattil pravēśiccu keāḷḷuka"},{"SurahNumber":89,"VerseNumber":30,"Text":"enṟe svargattil pravēśiccukeāḷḷuka"}]}