{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":52,"VerseNumber":24,"Text":"۞ويطوف عليهم غلمان لهم كانهم لولو مكنون"},{"SurahNumber":52,"VerseNumber":25,"Text":"واقبل بعضهم على بعض يتساءلون"},{"SurahNumber":52,"VerseNumber":26,"Text":"قالوا انا كنا قبل في اهلنا مشفقين"},{"SurahNumber":52,"VerseNumber":27,"Text":"فمن الله علينا ووقىنا عذاب السموم"},{"SurahNumber":52,"VerseNumber":28,"Text":"انا كنا من قبل ندعوه انه هو البر الرحيم"},{"SurahNumber":52,"VerseNumber":29,"Text":"فذكر فما انت بنعمت ربك بكاهن ولا مجنون"},{"SurahNumber":52,"VerseNumber":30,"Text":"ام يقولون شاعر نتربص به ريب المنون"},{"SurahNumber":52,"VerseNumber":31,"Text":"قل تربصوا فاني معكم من المتربصين"},{"SurahNumber":52,"VerseNumber":32,"Text":"ام تامرهم احلمهم بهذا ام هم قوم طاغون"},{"SurahNumber":52,"VerseNumber":33,"Text":"ام يقولون تقوله بل لا يومنون"},{"SurahNumber":52,"VerseNumber":34,"Text":"فلياتوا بحديث مثله ان كانوا صدقين"},{"SurahNumber":52,"VerseNumber":35,"Text":"ام خلقوا من غير شيء ام هم الخلقون"},{"SurahNumber":52,"VerseNumber":36,"Text":"ام خلقوا السموت والارض بل لا يوقنون"},{"SurahNumber":52,"VerseNumber":37,"Text":"ام عندهم خزاين ربك ام هم المصيطرون"},{"SurahNumber":52,"VerseNumber":38,"Text":"ام لهم سلم يستمعون فيه فليات مستمعهم بسلطن مبين"},{"SurahNumber":52,"VerseNumber":39,"Text":"ام له البنت ولكم البنون"},{"SurahNumber":52,"VerseNumber":40,"Text":"ام تسلهم اجر ا فهم من مغرم مثقلون"},{"SurahNumber":52,"VerseNumber":41,"Text":"ام عندهم الغيب فهم يكتبون"},{"SurahNumber":52,"VerseNumber":42,"Text":"ام يريدون كيد ا فالذين كفروا هم المكيدون"},{"SurahNumber":52,"VerseNumber":43,"Text":"ام لهم اله غير الله سبحن الله عما يشركون"},{"SurahNumber":52,"VerseNumber":44,"Text":"وان يروا كسف ا من السماء ساقط ا يقولوا سحاب مركوم"},{"SurahNumber":52,"VerseNumber":45,"Text":"فذرهم حتى يلقوا يومهم الذي فيه يصعقون"},{"SurahNumber":52,"VerseNumber":46,"Text":"يوم لا يغني عنهم كيدهم شي ا ولا هم ينصرون"},{"SurahNumber":52,"VerseNumber":47,"Text":"وان للذين ظلموا عذاب ا دون ذلك ولكن اكثرهم لا يعلمون"},{"SurahNumber":52,"VerseNumber":48,"Text":"واصبر لحكم ربك فانك باعيننا وسبح بحمد ربك حين تقوم"},{"SurahNumber":52,"VerseNumber":49,"Text":"ومن اليل فسبحه وادبر النجوم"},{"SurahNumber":53,"VerseNumber":1,"Text":"والنجم اذا هوى"},{"SurahNumber":53,"VerseNumber":2,"Text":"ما ضل صاحبكم وما غوى"},{"SurahNumber":53,"VerseNumber":3,"Text":"وما ينطق عن الهوى"},{"SurahNumber":53,"VerseNumber":4,"Text":"ان هو الا وحي يوحى"},{"SurahNumber":53,"VerseNumber":5,"Text":"علمه شديد القوى"},{"SurahNumber":53,"VerseNumber":6,"Text":"ذو مرة فاستوى"},{"SurahNumber":53,"VerseNumber":7,"Text":"وهو بالافق الاعلى"},{"SurahNumber":53,"VerseNumber":8,"Text":"ثم دنا فتدلى"},{"SurahNumber":53,"VerseNumber":9,"Text":"فكان قاب قوسين او ادنى"},{"SurahNumber":53,"VerseNumber":10,"Text":"فاوحى الى عبده ما اوحى"},{"SurahNumber":53,"VerseNumber":11,"Text":"ما كذب الفواد ما راى"},{"SurahNumber":53,"VerseNumber":12,"Text":"افتمرونه على ما يرى"},{"SurahNumber":53,"VerseNumber":13,"Text":"ولقد رءاه نزلة اخرى"},{"SurahNumber":53,"VerseNumber":14,"Text":"عند سدرة المنتهى"},{"SurahNumber":53,"VerseNumber":15,"Text":"عندها جنة الماوى"},{"SurahNumber":53,"VerseNumber":16,"Text":"اذ يغشى السدرة ما يغشى"},{"SurahNumber":53,"VerseNumber":17,"Text":"ما زاغ البصر وما طغى"},{"SurahNumber":53,"VerseNumber":18,"Text":"لقد راى من ءايت ربه الكبرى"},{"SurahNumber":53,"VerseNumber":19,"Text":"افرءيتم اللت والعزى"},{"SurahNumber":53,"VerseNumber":20,"Text":"ومنوة الثالثة الاخرى"},{"SurahNumber":53,"VerseNumber":21,"Text":"الكم الذكر وله الانثى"},{"SurahNumber":53,"VerseNumber":22,"Text":"تلك اذ ا قسمة ضيزى"},{"SurahNumber":53,"VerseNumber":23,"Text":"ان هي الا اسماء سميتموها انتم وءاباوكم ما انزل الله بها من سلطن ان يتبعون الا الظن وما تهوى الانفس ولقد جاءهم من ربهم الهدى"},{"SurahNumber":53,"VerseNumber":24,"Text":"ام للانسن ما تمنى"},{"SurahNumber":53,"VerseNumber":25,"Text":"فلله الاخرة والاولى"}]}