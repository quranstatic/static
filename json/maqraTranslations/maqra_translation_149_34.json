{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":111,"Text":"۞قالوا انومن لك واتبعك الارذلون"},{"SurahNumber":26,"VerseNumber":112,"Text":"قال وما علمي بما كانوا يعملون"},{"SurahNumber":26,"VerseNumber":113,"Text":"ان حسابهم الا على ربي لو تشعرون"},{"SurahNumber":26,"VerseNumber":114,"Text":"وما انا بطارد المومنين"},{"SurahNumber":26,"VerseNumber":115,"Text":"ان انا الا نذير مبين"},{"SurahNumber":26,"VerseNumber":116,"Text":"قالوا لين لم تنته ينوح لتكونن من المرجومين"},{"SurahNumber":26,"VerseNumber":117,"Text":"قال رب ان قومي كذبون"},{"SurahNumber":26,"VerseNumber":118,"Text":"فافتح بيني وبينهم فتح ا ونجني ومن معي من المومنين"},{"SurahNumber":26,"VerseNumber":119,"Text":"فانجينه ومن معه في الفلك المشحون"},{"SurahNumber":26,"VerseNumber":120,"Text":"ثم اغرقنا بعد الباقين"},{"SurahNumber":26,"VerseNumber":121,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":122,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":123,"Text":"كذبت عاد المرسلين"},{"SurahNumber":26,"VerseNumber":124,"Text":"اذ قال لهم اخوهم هود الا تتقون"},{"SurahNumber":26,"VerseNumber":125,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":126,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":127,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":128,"Text":"اتبنون بكل ريع ءاية تعبثون"},{"SurahNumber":26,"VerseNumber":129,"Text":"وتتخذون مصانع لعلكم تخلدون"},{"SurahNumber":26,"VerseNumber":130,"Text":"واذا بطشتم بطشتم جبارين"},{"SurahNumber":26,"VerseNumber":131,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":132,"Text":"واتقوا الذي امدكم بما تعلمون"},{"SurahNumber":26,"VerseNumber":133,"Text":"امدكم بانعم وبنين"},{"SurahNumber":26,"VerseNumber":134,"Text":"وجنت وعيون"},{"SurahNumber":26,"VerseNumber":135,"Text":"اني اخاف عليكم عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":136,"Text":"قالوا سواء علينا اوعظت ام لم تكن من الوعظين"},{"SurahNumber":26,"VerseNumber":137,"Text":"ان هذا الا خلق الاولين"},{"SurahNumber":26,"VerseNumber":138,"Text":"وما نحن بمعذبين"},{"SurahNumber":26,"VerseNumber":139,"Text":"فكذبوه فاهلكنهم ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":140,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":141,"Text":"كذبت ثمود المرسلين"},{"SurahNumber":26,"VerseNumber":142,"Text":"اذ قال لهم اخوهم صلح الا تتقون"},{"SurahNumber":26,"VerseNumber":143,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":144,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":145,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":146,"Text":"اتتركون في ما ههنا ءامنين"},{"SurahNumber":26,"VerseNumber":147,"Text":"في جنت وعيون"},{"SurahNumber":26,"VerseNumber":148,"Text":"وزروع ونخل طلعها هضيم"},{"SurahNumber":26,"VerseNumber":149,"Text":"وتنحتون من الجبال بيوت ا فرهين"},{"SurahNumber":26,"VerseNumber":150,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":151,"Text":"ولا تطيعوا امر المسرفين"},{"SurahNumber":26,"VerseNumber":152,"Text":"الذين يفسدون في الارض ولا يصلحون"},{"SurahNumber":26,"VerseNumber":153,"Text":"قالوا انما انت من المسحرين"},{"SurahNumber":26,"VerseNumber":154,"Text":"ما انت الا بشر مثلنا فات باية ان كنت من الصدقين"},{"SurahNumber":26,"VerseNumber":155,"Text":"قال هذه ناقة لها شرب ولكم شرب يوم معلوم"},{"SurahNumber":26,"VerseNumber":156,"Text":"ولا تمسوها بسوء فياخذكم عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":157,"Text":"فعقروها فاصبحوا ندمين"},{"SurahNumber":26,"VerseNumber":158,"Text":"فاخذهم العذاب ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":159,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":160,"Text":"كذبت قوم لوط المرسلين"},{"SurahNumber":26,"VerseNumber":161,"Text":"اذ قال لهم اخوهم لوط الا تتقون"},{"SurahNumber":26,"VerseNumber":162,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":163,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":164,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":165,"Text":"اتاتون الذكران من العلمين"},{"SurahNumber":26,"VerseNumber":166,"Text":"وتذرون ما خلق لكم ربكم من ازوجكم بل انتم قوم عادون"},{"SurahNumber":26,"VerseNumber":167,"Text":"قالوا لين لم تنته يلوط لتكونن من المخرجين"},{"SurahNumber":26,"VerseNumber":168,"Text":"قال اني لعملكم من القالين"},{"SurahNumber":26,"VerseNumber":169,"Text":"رب نجني واهلي مما يعملون"},{"SurahNumber":26,"VerseNumber":170,"Text":"فنجينه واهله اجمعين"},{"SurahNumber":26,"VerseNumber":171,"Text":"الا عجوز ا في الغبرين"},{"SurahNumber":26,"VerseNumber":172,"Text":"ثم دمرنا الاخرين"},{"SurahNumber":26,"VerseNumber":173,"Text":"وامطرنا عليهم مطر ا فساء مطر المنذرين"},{"SurahNumber":26,"VerseNumber":174,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":175,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":176,"Text":"كذب اصحب ليكة المرسلين"},{"SurahNumber":26,"VerseNumber":177,"Text":"اذ قال لهم شعيب الا تتقون"},{"SurahNumber":26,"VerseNumber":178,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":179,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":180,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"}]}