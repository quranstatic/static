{"Id":418,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay-la","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":37,"VerseNumber":145,"Text":"fenebeznahu bil`arai vehuve sekim"},{"SurahNumber":37,"VerseNumber":146,"Text":"veembetna `aleyhi seceratem miy yaktin"},{"SurahNumber":37,"VerseNumber":147,"Text":"veerselnahu ila mieti elfin ev yezidun"},{"SurahNumber":37,"VerseNumber":148,"Text":"feamenu femetta`nahum ila hin"},{"SurahNumber":37,"VerseNumber":149,"Text":"festeftihim elirabbike-lbenatu velehumu-lbenun"},{"SurahNumber":37,"VerseNumber":150,"Text":"em halakne-lmelaikete inasev vehum sahidun"},{"SurahNumber":37,"VerseNumber":151,"Text":"ela innehum min ifkihim leyekulun"},{"SurahNumber":37,"VerseNumber":152,"Text":"velede-llahu veinnehum lekazibun"},{"SurahNumber":37,"VerseNumber":153,"Text":"astafe-lbenati `ale-lbenin"},{"SurahNumber":37,"VerseNumber":154,"Text":"ma lekum. keyfe tahkumun"},{"SurahNumber":37,"VerseNumber":155,"Text":"efela tezekkerun"},{"SurahNumber":37,"VerseNumber":156,"Text":"em lekum sultanum mubin"},{"SurahNumber":37,"VerseNumber":157,"Text":"fe'tu bikitabikum in kuntum sadikin"},{"SurahNumber":37,"VerseNumber":158,"Text":"vece`alu beynehu vebeyne-lcinneti neseba. velekad `alimeti-lcinnetu innehum lemuhdarun"},{"SurahNumber":37,"VerseNumber":159,"Text":"subhane-llahi `amma yesifun"},{"SurahNumber":37,"VerseNumber":160,"Text":"illa `ibade-llahi-lmuhlesin"},{"SurahNumber":37,"VerseNumber":161,"Text":"feinnekum vema ta`budun"},{"SurahNumber":37,"VerseNumber":162,"Text":"ma entum `aleyhi bifatinin"},{"SurahNumber":37,"VerseNumber":163,"Text":"illa men huve sali-lcehim"},{"SurahNumber":37,"VerseNumber":164,"Text":"vema minna illa lehu mekamum ma`lum"},{"SurahNumber":37,"VerseNumber":165,"Text":"veinna lenahnu-ssaffun"},{"SurahNumber":37,"VerseNumber":166,"Text":"veinna lenahnu-lmusebbihun"},{"SurahNumber":37,"VerseNumber":167,"Text":"vein kanu leyekulun"},{"SurahNumber":37,"VerseNumber":168,"Text":"lev enne `indena zikram mine-l'evvelin"},{"SurahNumber":37,"VerseNumber":169,"Text":"lekunna `ibade-llahi-lmuhlesin"},{"SurahNumber":37,"VerseNumber":170,"Text":"fekeferu bih. fesevfe ya`lemun"},{"SurahNumber":37,"VerseNumber":171,"Text":"velekad sebekat kelimetuna li`ibadine-lmurselin"},{"SurahNumber":37,"VerseNumber":172,"Text":"innehum lehumu-lmensurun"},{"SurahNumber":37,"VerseNumber":173,"Text":"veinne cundena lehumu-lgalibun"},{"SurahNumber":37,"VerseNumber":174,"Text":"fetevelle `anhum hatta hin"},{"SurahNumber":37,"VerseNumber":175,"Text":"veebsirhum fesevfe yubsirun"},{"SurahNumber":37,"VerseNumber":176,"Text":"efebi`azabina yesta`cilun"},{"SurahNumber":37,"VerseNumber":177,"Text":"feiza nezele bisahatihim fesae sabahu-lmunzerin"},{"SurahNumber":37,"VerseNumber":178,"Text":"vetevelle `anhum hatta hin"},{"SurahNumber":37,"VerseNumber":179,"Text":"veebsir fesevfe yubsirun"},{"SurahNumber":37,"VerseNumber":180,"Text":"subhane rabbike rabbi-l`izzeti `amma yesifun"},{"SurahNumber":37,"VerseNumber":181,"Text":"veselamun `ale-lmurselin"},{"SurahNumber":37,"VerseNumber":182,"Text":"velhamdu lillahi rabbi-l`alemin"},{"SurahNumber":38,"VerseNumber":1,"Text":"sad. velkur'ani zi-zzikr"},{"SurahNumber":38,"VerseNumber":2,"Text":"beli-llezine keferu fi `izzetiv vesikak"},{"SurahNumber":38,"VerseNumber":3,"Text":"kem ehlekna min kablihim min karnin fenadev velate hine menas"},{"SurahNumber":38,"VerseNumber":4,"Text":"ve`acibu en caehum munzirum minhum. vekale-lkafirune haza sahirun kezzab"},{"SurahNumber":38,"VerseNumber":5,"Text":"ece`ale-l'alihete ilahev vahida. inne haza lesey'un `ucab"},{"SurahNumber":38,"VerseNumber":6,"Text":"ventaleka-lmeleu minhum eni-msu vasbiru `ala alihetikum. inne haza lesey'uy yurad"},{"SurahNumber":38,"VerseNumber":7,"Text":"ma semi`na bihaza fi-lmilleti-l'ahirah. in haza ille-htilak"},{"SurahNumber":38,"VerseNumber":8,"Text":"eunzile `aleyhi-zzikru mim beynina. bel hum fi sekkim min zikri. bel lemma yezuku `azab"},{"SurahNumber":38,"VerseNumber":9,"Text":"em `indehum hazainu rahmeti rabbike-l`azizi-lvehhab"},{"SurahNumber":38,"VerseNumber":10,"Text":"em lehum mulku-ssemavati vel'ardi vema beynehuma. felyerteku fi-l'esbab"},{"SurahNumber":38,"VerseNumber":11,"Text":"cundum ma hunalike mehzumum mine-l'ahzab"},{"SurahNumber":38,"VerseNumber":12,"Text":"kezzebet kablehum kavmu nuhiv ve`aduv vefir`avnu zu-l'evtad"},{"SurahNumber":38,"VerseNumber":13,"Text":"vesemudu vekavmu lutiv veashabu-l'eykeh. ulaike-l'ahzab"},{"SurahNumber":38,"VerseNumber":14,"Text":"in kullun illa kezzebe-rrusule fehakka `ikab"},{"SurahNumber":38,"VerseNumber":15,"Text":"vema yenzuru haulai illa sayhatev vahidetem ma leha min fevak"},{"SurahNumber":38,"VerseNumber":16,"Text":"vekalu rabbena `accil lena kittana kable yevmi-lhisab"},{"SurahNumber":38,"VerseNumber":17,"Text":"isbir `ala ma yekulune vezkur `abdena davude ze-l'eyd. innehu evvab"},{"SurahNumber":38,"VerseNumber":18,"Text":"inna sehharne-lcibale me`ahu yusebbihne bil`asiyyi vel'israk"},{"SurahNumber":38,"VerseNumber":19,"Text":"vettayra mahsurah. kullul lehu evvab"},{"SurahNumber":38,"VerseNumber":20,"Text":"vesededna mulkehu veateynahu-lhikmete vefasle-lhitab"}]}