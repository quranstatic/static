{"Id":412,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet-la","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":90,"VerseNumber":1,"Text":"La uksimu bilhazelbeledi"},{"SurahNumber":90,"VerseNumber":2,"Text":"Ve ente hıllun bihazelbeledi"},{"SurahNumber":90,"VerseNumber":3,"Text":"Ve validin ve ma velede"},{"SurahNumber":90,"VerseNumber":4,"Text":"Lekad halaknel´insane fiy kebedin"},{"SurahNumber":90,"VerseNumber":5,"Text":"Eyahsebu en len yakdire ´aleyhi ehadun"},{"SurahNumber":90,"VerseNumber":6,"Text":"Yekulu ehlektu malen lubeden"},{"SurahNumber":90,"VerseNumber":7,"Text":"Eyahsebu en lem yerehu ehadun"},{"SurahNumber":90,"VerseNumber":8,"Text":"Elem nec´al lehu ´ayneyni"},{"SurahNumber":90,"VerseNumber":9,"Text":"Ve lisanen ve sefeteyni"},{"SurahNumber":90,"VerseNumber":10,"Text":"Ve hedeynahunnecdeyni"},{"SurahNumber":90,"VerseNumber":11,"Text":"Felaktehamel´akabete"},{"SurahNumber":90,"VerseNumber":12,"Text":"Ve ma edrake mel´akabetu"},{"SurahNumber":90,"VerseNumber":13,"Text":"Fekku rekabetin"},{"SurahNumber":90,"VerseNumber":14,"Text":"Ev ıt´amun fiy yevmin ziy mesgabetin"},{"SurahNumber":90,"VerseNumber":15,"Text":"Yetiymen za makrebetin"},{"SurahNumber":90,"VerseNumber":16,"Text":"Ev miskiynen za metrebetin"},{"SurahNumber":90,"VerseNumber":17,"Text":"Summe kane minelleziyne amenu ve tevasav bissabri ve tevasav bilmerhameti"},{"SurahNumber":90,"VerseNumber":18,"Text":"Ol,eke ashabulmeymene"},{"SurahNumber":90,"VerseNumber":19,"Text":"Velleziyne keferu biayatina hum ashabulmes´emeti"},{"SurahNumber":90,"VerseNumber":20,"Text":"´Aleyhim narun mu´sadetun"},{"SurahNumber":91,"VerseNumber":1,"Text":"Vessemsi ve duhaha"},{"SurahNumber":91,"VerseNumber":2,"Text":"Velkameri iza telaha"},{"SurahNumber":91,"VerseNumber":3,"Text":"Vennehari iza cellaha"},{"SurahNumber":91,"VerseNumber":4,"Text":"Velleyli iza yagsaha"},{"SurahNumber":91,"VerseNumber":5,"Text":"Vessmai ve ma benaha"},{"SurahNumber":91,"VerseNumber":6,"Text":"Vel´ardı ve ma tahaha"},{"SurahNumber":91,"VerseNumber":7,"Text":"Ve nefsin ve ma sevvaha"},{"SurahNumber":91,"VerseNumber":8,"Text":"Feelhemeha fucureha ve takvaha"},{"SurahNumber":91,"VerseNumber":9,"Text":"Kad efleha men zekkaha"},{"SurahNumber":91,"VerseNumber":10,"Text":"Ve kad habe men dessaha"},{"SurahNumber":91,"VerseNumber":11,"Text":"Kezzebet semudu bitagvaha"},{"SurahNumber":91,"VerseNumber":12,"Text":"Izinbe´ase eskaha"},{"SurahNumber":91,"VerseNumber":13,"Text":"Fekale lehum resulullahi nakatallahi ve sukyaha"},{"SurahNumber":91,"VerseNumber":14,"Text":"Fekezzebuhu fe´akaruha fedemdeme ´aleyhim rabbuhum bizenbihim fesevvaha"},{"SurahNumber":91,"VerseNumber":15,"Text":"Ve la yehafi ´ukbaha"},{"SurahNumber":92,"VerseNumber":1,"Text":"Velleyli iza yagsa"},{"SurahNumber":92,"VerseNumber":2,"Text":"Vennehari iza tecella"},{"SurahNumber":92,"VerseNumber":3,"Text":"Ve ma halekazzekere vel´unsa"},{"SurahNumber":92,"VerseNumber":4,"Text":"Inne sa´yekum lesetta"},{"SurahNumber":92,"VerseNumber":5,"Text":"Feemma men a´ta vetteka"},{"SurahNumber":92,"VerseNumber":6,"Text":"Ve saddeka bilhusna"},{"SurahNumber":92,"VerseNumber":7,"Text":"Fesenuyessiruhu lilyusra"},{"SurahNumber":92,"VerseNumber":8,"Text":"Ve emma men bahıle vestagna"},{"SurahNumber":92,"VerseNumber":9,"Text":"Ve kezzebe bilhusna"},{"SurahNumber":92,"VerseNumber":10,"Text":"Fesenuyessiruhu lil´usra"},{"SurahNumber":92,"VerseNumber":11,"Text":"Ve ma yugniy ´anhu maluhu iza teredda"},{"SurahNumber":92,"VerseNumber":12,"Text":"Inne ´aleyna lelhuda"},{"SurahNumber":92,"VerseNumber":13,"Text":"Ve inne lena lel´ahırete vel´ula"},{"SurahNumber":92,"VerseNumber":14,"Text":"Feenzertukum naren telezza"},{"SurahNumber":92,"VerseNumber":15,"Text":"La yaslaha illel´eska"},{"SurahNumber":92,"VerseNumber":16,"Text":"Elleziy kezzebe ve tevella"},{"SurahNumber":92,"VerseNumber":17,"Text":"Ve seyucennebuhel´etka"},{"SurahNumber":92,"VerseNumber":18,"Text":"Elleziy yu´tiy malehu yetezekka"},{"SurahNumber":92,"VerseNumber":19,"Text":"Ve ma liehadin ´ındehu min nı´metin tucza"},{"SurahNumber":92,"VerseNumber":20,"Text":"Illebtigae vechi rabbihil´a´la"},{"SurahNumber":92,"VerseNumber":21,"Text":"Ve lesevfe yerda"},{"SurahNumber":93,"VerseNumber":1,"Text":"Vedduha"},{"SurahNumber":93,"VerseNumber":2,"Text":"Velleyli iza seca"},{"SurahNumber":93,"VerseNumber":3,"Text":"Ma vedde´ake rabbuke ve ma kala"},{"SurahNumber":93,"VerseNumber":4,"Text":"Ve lel´ahıretu hayrun leke minel´ula"},{"SurahNumber":93,"VerseNumber":5,"Text":"Ve lesevfe yu´tıyke rabbuke feterda"},{"SurahNumber":93,"VerseNumber":6,"Text":"Elem yecidke yetiymen feava"},{"SurahNumber":93,"VerseNumber":7,"Text":"Ve vecedeke dallen feheda"},{"SurahNumber":93,"VerseNumber":8,"Text":"Ve vecedeke ´ailen feagna"},{"SurahNumber":93,"VerseNumber":9,"Text":"Femmel yetiyme fela takher"},{"SurahNumber":93,"VerseNumber":10,"Text":"Ve emmessaile fela tenher"},{"SurahNumber":93,"VerseNumber":11,"Text":"Ve emma binı´meti rabbike fehaddis"}]}