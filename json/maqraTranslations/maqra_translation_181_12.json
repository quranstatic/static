{"Id":12,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la2","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"","Translations":[{"SurahNumber":37,"VerseNumber":145,"Text":"Fanabathnahu bilAAara-iwahuwa saqeem"},{"SurahNumber":37,"VerseNumber":146,"Text":"Waanbatna AAalayhi shajaratan minyaqteen"},{"SurahNumber":37,"VerseNumber":147,"Text":"Waarsalnahu ila mi-ati alfinaw yazeedoon"},{"SurahNumber":37,"VerseNumber":148,"Text":"Faamanoo famattaAAnahum ilaheen"},{"SurahNumber":37,"VerseNumber":149,"Text":"Fastaftihim alirabbika albanatuwalahumu albanoon"},{"SurahNumber":37,"VerseNumber":150,"Text":"Am khalaqna almala-ikata inathanwahum shahidoon"},{"SurahNumber":37,"VerseNumber":151,"Text":"Ala innahum min ifkihim layaqooloon"},{"SurahNumber":37,"VerseNumber":152,"Text":"Walada Allahu wa-innahum lakathiboon"},{"SurahNumber":37,"VerseNumber":153,"Text":"Astafa albanati AAalaalbaneen"},{"SurahNumber":37,"VerseNumber":154,"Text":"Ma lakum kayfa tahkumoon"},{"SurahNumber":37,"VerseNumber":155,"Text":"Afala tathakkaroon"},{"SurahNumber":37,"VerseNumber":156,"Text":"Am lakum sultanun mubeen"},{"SurahNumber":37,"VerseNumber":157,"Text":"Fa/too bikitabikum in kuntum sadiqeen"},{"SurahNumber":37,"VerseNumber":158,"Text":"WajaAAaloo baynahu wabayna aljinnatinasaban walaqad AAalimati aljinnatu innahum lamuhdaroon"},{"SurahNumber":37,"VerseNumber":159,"Text":"Subhana Allahi AAammayasifoon"},{"SurahNumber":37,"VerseNumber":160,"Text":"Illa AAibada Allahialmukhlaseen"},{"SurahNumber":37,"VerseNumber":161,"Text":"Fa-innakum wama taAAbudoon"},{"SurahNumber":37,"VerseNumber":162,"Text":"Ma antum AAalayhi bifatineen"},{"SurahNumber":37,"VerseNumber":163,"Text":"Illa man huwa sali aljaheem"},{"SurahNumber":37,"VerseNumber":164,"Text":"Wama minna illa lahumaqamun maAAloom"},{"SurahNumber":37,"VerseNumber":165,"Text":"Wa-inna lanahnu assaffoon"},{"SurahNumber":37,"VerseNumber":166,"Text":"Wa-inna lanahnu almusabbihoon"},{"SurahNumber":37,"VerseNumber":167,"Text":"Wa-in kanoo layaqooloon"},{"SurahNumber":37,"VerseNumber":168,"Text":"Law anna AAindana thikranmina al-awwaleen"},{"SurahNumber":37,"VerseNumber":169,"Text":"Lakunna AAibada Allahialmukhlaseen"},{"SurahNumber":37,"VerseNumber":170,"Text":"Fakafaroo bihi fasawfa yaAAlamoon"},{"SurahNumber":37,"VerseNumber":171,"Text":"Walaqad sabaqat kalimatuna liAAibadinaalmursaleen"},{"SurahNumber":37,"VerseNumber":172,"Text":"Innahum lahumu almansooroon"},{"SurahNumber":37,"VerseNumber":173,"Text":"Wa-inna jundana lahumu alghaliboon"},{"SurahNumber":37,"VerseNumber":174,"Text":"Fatawalla AAanhum hatta heen"},{"SurahNumber":37,"VerseNumber":175,"Text":"Waabsirhum fasawfa yubsiroon"},{"SurahNumber":37,"VerseNumber":176,"Text":"AfabiAAathabina yastaAAjiloon"},{"SurahNumber":37,"VerseNumber":177,"Text":"Fa-itha nazala bisahatihimfasaa sabahu almunthareen"},{"SurahNumber":37,"VerseNumber":178,"Text":"Watawalla AAanhum hatta heen"},{"SurahNumber":37,"VerseNumber":179,"Text":"Waabsir fasawfa yubsiroon"},{"SurahNumber":37,"VerseNumber":180,"Text":"Subhana rabbika rabbi alAAizzatiAAamma yasifoon"},{"SurahNumber":37,"VerseNumber":181,"Text":"Wasalamun AAala almursaleen"},{"SurahNumber":37,"VerseNumber":182,"Text":"Walhamdu lillahirabbi alAAalameen"},{"SurahNumber":38,"VerseNumber":1,"Text":"Sad walqur-ani theeaththikr"},{"SurahNumber":38,"VerseNumber":2,"Text":"Bali allatheena kafaroo fee AAizzatinwashiqaq"},{"SurahNumber":38,"VerseNumber":3,"Text":"Kam ahlakna min qablihim min qarninfanadaw walata heena manas"},{"SurahNumber":38,"VerseNumber":4,"Text":"WaAAajiboo an jaahum munthirunminhum waqala alkafiroona hatha sahirunkaththab"},{"SurahNumber":38,"VerseNumber":5,"Text":"AjaAAala al-alihata ilahan wahidaninna hatha lashay-on AAujab"},{"SurahNumber":38,"VerseNumber":6,"Text":"Wantalaqa almalao minhum aniimshoo wasbiroo AAala alihatikuminna hatha lashay-on yurad"},{"SurahNumber":38,"VerseNumber":7,"Text":"Ma samiAAna bihatha feealmillati al-akhirati in hatha illa ikhtilaq"},{"SurahNumber":38,"VerseNumber":8,"Text":"Aonzila AAalayhi aththikru minbaynina bal hum fee shakkin min thikree bal lammayathooqoo AAathab"},{"SurahNumber":38,"VerseNumber":9,"Text":"Am AAindahum khaza-inu rahmatirabbika alAAazeezi alwahhab"},{"SurahNumber":38,"VerseNumber":10,"Text":"Am lahum mulku assamawatiwal-ardi wama baynahuma falyartaqoofee al-asbab"},{"SurahNumber":38,"VerseNumber":11,"Text":"Jundun ma hunalika mahzoomunmina al-ahzab"},{"SurahNumber":38,"VerseNumber":12,"Text":"Kaththabat qablahum qawmu noohinwaAAadun wafirAAawnu thoo al-awtad"},{"SurahNumber":38,"VerseNumber":13,"Text":"Wathamoodu waqawmu lootin waas-habual-aykati ola-ika al-ahzab"},{"SurahNumber":38,"VerseNumber":14,"Text":"In kullun illa kaththaba arrusulafahaqqa AAiqab"},{"SurahNumber":38,"VerseNumber":15,"Text":"Wama yanthuru haola-iilla sayhatan wahidatan ma lahamin fawaq"},{"SurahNumber":38,"VerseNumber":16,"Text":"Waqaloo rabbana AAajjil lanaqittana qabla yawmi alhisab"},{"SurahNumber":38,"VerseNumber":17,"Text":"Isbir AAala mayaqooloona wathkur AAabdana dawooda thaal-aydi innahu awwab"},{"SurahNumber":38,"VerseNumber":18,"Text":"Inna sakhkharna aljibalamaAAahu yusabbihna bilAAashiyyi wal-ishraq"},{"SurahNumber":38,"VerseNumber":19,"Text":"Wattayra mahshooratankullun lahu awwab"},{"SurahNumber":38,"VerseNumber":20,"Text":"Washadadna mulkahu waataynahualhikmata wafasla alkhitab"}]}