{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":37,"VerseNumber":145,"Text":"۞فنبذنه بالعراء وهو سقيم"},{"SurahNumber":37,"VerseNumber":146,"Text":"وانبتنا عليه شجرة من يقطين"},{"SurahNumber":37,"VerseNumber":147,"Text":"وارسلنه الى ماية الف او يزيدون"},{"SurahNumber":37,"VerseNumber":148,"Text":"فامنوا فمتعنهم الى حين"},{"SurahNumber":37,"VerseNumber":149,"Text":"فاستفتهم الربك البنات ولهم البنون"},{"SurahNumber":37,"VerseNumber":150,"Text":"ام خلقنا المليكة انث ا وهم شهدون"},{"SurahNumber":37,"VerseNumber":151,"Text":"الا انهم من افكهم ليقولون"},{"SurahNumber":37,"VerseNumber":152,"Text":"ولد الله وانهم لكذبون"},{"SurahNumber":37,"VerseNumber":153,"Text":"اصطفى البنات على البنين"},{"SurahNumber":37,"VerseNumber":154,"Text":"ما لكم كيف تحكمون"},{"SurahNumber":37,"VerseNumber":155,"Text":"افلا تذكرون"},{"SurahNumber":37,"VerseNumber":156,"Text":"ام لكم سلطن مبين"},{"SurahNumber":37,"VerseNumber":157,"Text":"فاتوا بكتبكم ان كنتم صدقين"},{"SurahNumber":37,"VerseNumber":158,"Text":"وجعلوا بينه وبين الجنة نسب ا ولقد علمت الجنة انهم لمحضرون"},{"SurahNumber":37,"VerseNumber":159,"Text":"سبحن الله عما يصفون"},{"SurahNumber":37,"VerseNumber":160,"Text":"الا عباد الله المخلصين"},{"SurahNumber":37,"VerseNumber":161,"Text":"فانكم وما تعبدون"},{"SurahNumber":37,"VerseNumber":162,"Text":"ما انتم عليه بفتنين"},{"SurahNumber":37,"VerseNumber":163,"Text":"الا من هو صال الجحيم"},{"SurahNumber":37,"VerseNumber":164,"Text":"وما منا الا له مقام معلوم"},{"SurahNumber":37,"VerseNumber":165,"Text":"وانا لنحن الصافون"},{"SurahNumber":37,"VerseNumber":166,"Text":"وانا لنحن المسبحون"},{"SurahNumber":37,"VerseNumber":167,"Text":"وان كانوا ليقولون"},{"SurahNumber":37,"VerseNumber":168,"Text":"لو ان عندنا ذكر ا من الاولين"},{"SurahNumber":37,"VerseNumber":169,"Text":"لكنا عباد الله المخلصين"},{"SurahNumber":37,"VerseNumber":170,"Text":"فكفروا به فسوف يعلمون"},{"SurahNumber":37,"VerseNumber":171,"Text":"ولقد سبقت كلمتنا لعبادنا المرسلين"},{"SurahNumber":37,"VerseNumber":172,"Text":"انهم لهم المنصورون"},{"SurahNumber":37,"VerseNumber":173,"Text":"وان جندنا لهم الغلبون"},{"SurahNumber":37,"VerseNumber":174,"Text":"فتول عنهم حتى حين"},{"SurahNumber":37,"VerseNumber":175,"Text":"وابصرهم فسوف يبصرون"},{"SurahNumber":37,"VerseNumber":176,"Text":"افبعذابنا يستعجلون"},{"SurahNumber":37,"VerseNumber":177,"Text":"فاذا نزل بساحتهم فساء صباح المنذرين"},{"SurahNumber":37,"VerseNumber":178,"Text":"وتول عنهم حتى حين"},{"SurahNumber":37,"VerseNumber":179,"Text":"وابصر فسوف يبصرون"},{"SurahNumber":37,"VerseNumber":180,"Text":"سبحن ربك رب العزة عما يصفون"},{"SurahNumber":37,"VerseNumber":181,"Text":"وسلم على المرسلين"},{"SurahNumber":37,"VerseNumber":182,"Text":"والحمد لله رب العلمين"},{"SurahNumber":38,"VerseNumber":1,"Text":"ص والقرءان ذي الذكر"},{"SurahNumber":38,"VerseNumber":2,"Text":"بل الذين كفروا في عزة وشقاق"},{"SurahNumber":38,"VerseNumber":3,"Text":"كم اهلكنا من قبلهم من قرن فنادوا ولات حين مناص"},{"SurahNumber":38,"VerseNumber":4,"Text":"وعجبوا ان جاءهم منذر منهم وقال الكفرون هذا سحر كذاب"},{"SurahNumber":38,"VerseNumber":5,"Text":"اجعل الالهة اله ا وحدا ان هذا لشيء عجاب"},{"SurahNumber":38,"VerseNumber":6,"Text":"وانطلق الملا منهم ان امشوا واصبروا على ءالهتكم ان هذا لشيء يراد"},{"SurahNumber":38,"VerseNumber":7,"Text":"ما سمعنا بهذا في الملة الاخرة ان هذا الا اختلق"},{"SurahNumber":38,"VerseNumber":8,"Text":"اءنزل عليه الذكر من بيننا بل هم في شك من ذكري بل لما يذوقوا عذاب"},{"SurahNumber":38,"VerseNumber":9,"Text":"ام عندهم خزاين رحمة ربك العزيز الوهاب"},{"SurahNumber":38,"VerseNumber":10,"Text":"ام لهم ملك السموت والارض وما بينهما فليرتقوا في الاسبب"},{"SurahNumber":38,"VerseNumber":11,"Text":"جند ما هنالك مهزوم من الاحزاب"},{"SurahNumber":38,"VerseNumber":12,"Text":"كذبت قبلهم قوم نوح وعاد وفرعون ذو الاوتاد"},{"SurahNumber":38,"VerseNumber":13,"Text":"وثمود وقوم لوط واصحب ليكة اوليك الاحزاب"},{"SurahNumber":38,"VerseNumber":14,"Text":"ان كل الا كذب الرسل فحق عقاب"},{"SurahNumber":38,"VerseNumber":15,"Text":"وما ينظر هولاء الا صيحة وحدة ما لها من فواق"},{"SurahNumber":38,"VerseNumber":16,"Text":"وقالوا ربنا عجل لنا قطنا قبل يوم الحساب"},{"SurahNumber":38,"VerseNumber":17,"Text":"اصبر على ما يقولون واذكر عبدنا داود ذا الايد انه اواب"},{"SurahNumber":38,"VerseNumber":18,"Text":"انا سخرنا الجبال معه يسبحن بالعشي والاشراق"},{"SurahNumber":38,"VerseNumber":19,"Text":"والطير محشورة كل له اواب"},{"SurahNumber":38,"VerseNumber":20,"Text":"وشددنا ملكه وءاتينه الحكمة وفصل الخطاب"}]}