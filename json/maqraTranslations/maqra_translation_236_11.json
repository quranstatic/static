{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":84,"VerseNumber":1,"Text":"Izas samaaa'un shaqqat"},{"SurahNumber":84,"VerseNumber":2,"Text":"Wa azinat li Rabbihaa wa huqqat"},{"SurahNumber":84,"VerseNumber":3,"Text":"Wa izal ardu muddat"},{"SurahNumber":84,"VerseNumber":4,"Text":"Wa alqat maa feehaa wa takhallat"},{"SurahNumber":84,"VerseNumber":5,"Text":"Wa azinat li Rabbihaa wa huqqat"},{"SurahNumber":84,"VerseNumber":6,"Text":"Yaaa ayyuhal insaanu innaka kaadihun ilaa Rabbika kad han famulaaqeeh"},{"SurahNumber":84,"VerseNumber":7,"Text":"Fa ammaa man ootiya kitaabahoo biyameenih"},{"SurahNumber":84,"VerseNumber":8,"Text":"Fasawfa yuhaasabu hi saabai yaseeraa"},{"SurahNumber":84,"VerseNumber":9,"Text":"Wa yanqalibu ilaaa ahlihee masrooraa"},{"SurahNumber":84,"VerseNumber":10,"Text":"Wa ammaa man ootiya kitaabahoo waraaa'a zahrih"},{"SurahNumber":84,"VerseNumber":11,"Text":"Fasawfa yad'oo subooraa"},{"SurahNumber":84,"VerseNumber":12,"Text":"Wa yaslaa sa'eeraa"},{"SurahNumber":84,"VerseNumber":13,"Text":"Innahoo kaana feee ahlihee masrooraa"},{"SurahNumber":84,"VerseNumber":14,"Text":"Innahoo zanna al lai yahoor"},{"SurahNumber":84,"VerseNumber":15,"Text":"Balaaa inna Rabbahoo kaana bihee baseeraa"},{"SurahNumber":84,"VerseNumber":16,"Text":"Falaaa uqsimu bishshafaq"},{"SurahNumber":84,"VerseNumber":17,"Text":"Wallaili wa maa wasaq"},{"SurahNumber":84,"VerseNumber":18,"Text":"Walqamari izat tasaq"},{"SurahNumber":84,"VerseNumber":19,"Text":"Latarkabunna tabaqan 'an tabaq"},{"SurahNumber":84,"VerseNumber":20,"Text":"Famaa lahum laa yu'minoon"},{"SurahNumber":84,"VerseNumber":21,"Text":"Wa izaa quri'a 'alaihimul Quraanu laa yasjudoon"},{"SurahNumber":84,"VerseNumber":22,"Text":"Balil lazeena kafaroo yukazziboon"},{"SurahNumber":84,"VerseNumber":23,"Text":"Wallaahu a'lamu bimaa yoo'oon"},{"SurahNumber":84,"VerseNumber":24,"Text":"Fabashshirhum bi'azaabin aleem"},{"SurahNumber":84,"VerseNumber":25,"Text":"Illal lazeena aamanoo wa 'amilus saalihaati lahum ajrun ghairu mamnoon"},{"SurahNumber":85,"VerseNumber":1,"Text":"Wassamaaa'i zaatil burooj"},{"SurahNumber":85,"VerseNumber":2,"Text":"Wal yawmil maw'ood"},{"SurahNumber":85,"VerseNumber":3,"Text":"Wa shaahidinw wa mashhood"},{"SurahNumber":85,"VerseNumber":4,"Text":"Qutila as haabul ukhdood"},{"SurahNumber":85,"VerseNumber":5,"Text":"Annaari zaatil waqood"},{"SurahNumber":85,"VerseNumber":6,"Text":"Iz hum 'alaihaa qu'ood"},{"SurahNumber":85,"VerseNumber":7,"Text":"Wa hum 'alaa maa yaf'aloona bilmu 'mineena shuhood"},{"SurahNumber":85,"VerseNumber":8,"Text":"Wa maa naqamoo minhum illaaa aiyu'minoo billaahil 'azeezil Hameed"},{"SurahNumber":85,"VerseNumber":9,"Text":"Allazee lahoo mulkus samaawaati wal ard; wallaahu 'alaa kulli shai 'in Shaheed"},{"SurahNumber":85,"VerseNumber":10,"Text":"Innal lazeena fatanul mu'mineena wal mu'minaati summa lam yatooboo falahum 'azaabu Jahannama wa lahum 'azaabul hareeq"},{"SurahNumber":85,"VerseNumber":11,"Text":"Innal lazeena aamanoo wa 'amilus saalihaati lahum Jannaatun tajree min tahtihal anhaar; zaalikal fawzul kabeer"},{"SurahNumber":85,"VerseNumber":12,"Text":"Inna batsha Rabbika lashadeed"},{"SurahNumber":85,"VerseNumber":13,"Text":"Innahoo Huwa yubdi'u wa yu'eed"},{"SurahNumber":85,"VerseNumber":14,"Text":"Wa Huwal Ghafoorul Wadood"},{"SurahNumber":85,"VerseNumber":15,"Text":"Zul 'Arshil Majeed"},{"SurahNumber":85,"VerseNumber":16,"Text":"Fa' 'aalul limaa yureed"},{"SurahNumber":85,"VerseNumber":17,"Text":"Hal ataaka hadeesul junood"},{"SurahNumber":85,"VerseNumber":18,"Text":"Fir'awna wa Samood"},{"SurahNumber":85,"VerseNumber":19,"Text":"Balil lazeena kafaroo fee takzeeb"},{"SurahNumber":85,"VerseNumber":20,"Text":"Wallaahu minw waraaa'ihim muheet"},{"SurahNumber":85,"VerseNumber":21,"Text":"Bal huwa Quraanum Majeed"},{"SurahNumber":85,"VerseNumber":22,"Text":"Fee Lawhim Mahfooz"},{"SurahNumber":86,"VerseNumber":1,"Text":"Wassamaaa'i wattaariq"},{"SurahNumber":86,"VerseNumber":2,"Text":"Wa maaa adraaka mattaariq"},{"SurahNumber":86,"VerseNumber":3,"Text":"Annajmus saaqib"},{"SurahNumber":86,"VerseNumber":4,"Text":"In kullu nafsil lammaa 'alaihaa haafiz"},{"SurahNumber":86,"VerseNumber":5,"Text":"Fal yanzuril insaanu mimma khuliq"},{"SurahNumber":86,"VerseNumber":6,"Text":"Khuliqa mim maaa'in daafiq"},{"SurahNumber":86,"VerseNumber":7,"Text":"Yakhruju mim bainissulbi wat taraaa'ib"},{"SurahNumber":86,"VerseNumber":8,"Text":"Innahoo 'alaa raj'ihee laqaadir"},{"SurahNumber":86,"VerseNumber":9,"Text":"Yawma tublas saraaa'ir"},{"SurahNumber":86,"VerseNumber":10,"Text":"Famaa lahoo min quwwatinw wa laa naasir"},{"SurahNumber":86,"VerseNumber":11,"Text":"Wassamaaa'i zaatir raj"},{"SurahNumber":86,"VerseNumber":12,"Text":"Wal ardi zaatis sad"},{"SurahNumber":86,"VerseNumber":13,"Text":"Innahoo laqawlun fasl"},{"SurahNumber":86,"VerseNumber":14,"Text":"Wa maa huwa bil hazl"},{"SurahNumber":86,"VerseNumber":15,"Text":"Innahum yakeedoona kaidaa"},{"SurahNumber":86,"VerseNumber":16,"Text":"Wa akeedu kaidaa"},{"SurahNumber":86,"VerseNumber":17,"Text":"Famahhilil kaafireena amhilhum ruwaidaa"}]}