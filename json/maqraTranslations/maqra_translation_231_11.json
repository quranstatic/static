{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":75,"VerseNumber":1,"Text":"Laaa uqsimu bi yawmil qiyaamah"},{"SurahNumber":75,"VerseNumber":2,"Text":"Wa laaa uqsimu bin nafsil lawwaamah"},{"SurahNumber":75,"VerseNumber":3,"Text":"Ayahsabul insaanu al lan najm'a 'izaamah"},{"SurahNumber":75,"VerseNumber":4,"Text":"Balaa qaadireena 'alaaa an nusawwiya banaanah"},{"SurahNumber":75,"VerseNumber":5,"Text":"Bal yureedul insaanu liyafjura amaamah"},{"SurahNumber":75,"VerseNumber":6,"Text":"Yas'alu ayyyaana yawmul qiyaamah"},{"SurahNumber":75,"VerseNumber":7,"Text":"Fa izaa bariqal basar"},{"SurahNumber":75,"VerseNumber":8,"Text":"We khasafal qamar"},{"SurahNumber":75,"VerseNumber":9,"Text":"Wa jumi'ash shamusu wal qamar"},{"SurahNumber":75,"VerseNumber":10,"Text":"Yaqoolul insaanu yaw ma 'izin aynal mafarr"},{"SurahNumber":75,"VerseNumber":11,"Text":"Kallaa laa wazar"},{"SurahNumber":75,"VerseNumber":12,"Text":"Ilaa rabbika yawma 'izinil mustaqarr"},{"SurahNumber":75,"VerseNumber":13,"Text":"Yunabba 'ul insaanu yawma 'izim bimaa qaddama wa akhkhar"},{"SurahNumber":75,"VerseNumber":14,"Text":"Balil insaanu 'alaa nafsihee baseerah"},{"SurahNumber":75,"VerseNumber":15,"Text":"Wa law alqaa ma'aazeerah"},{"SurahNumber":75,"VerseNumber":16,"Text":"Laa tuharrik bihee lisaa naka lita'jala bih"},{"SurahNumber":75,"VerseNumber":17,"Text":"Inna 'alainaa jam'ahoo wa qur aanah"},{"SurahNumber":75,"VerseNumber":18,"Text":"Fa izaa qaraanaahu fattabi' qur aanah"},{"SurahNumber":75,"VerseNumber":19,"Text":"Summa inna 'alainaa bayaanah"},{"SurahNumber":75,"VerseNumber":20,"Text":"Kallaa bal tuhibboonal 'aajilah"},{"SurahNumber":75,"VerseNumber":21,"Text":"Wa tazaroonal Aakhirah"},{"SurahNumber":75,"VerseNumber":22,"Text":"Wujoohuny yawma 'izin naadirah"},{"SurahNumber":75,"VerseNumber":23,"Text":"Ilaa rabbihaa naazirah"},{"SurahNumber":75,"VerseNumber":24,"Text":"Wa wujoohuny yawma 'izim baasirah"},{"SurahNumber":75,"VerseNumber":25,"Text":"Tazunnu any yuf'ala bihaa faaqirah"},{"SurahNumber":75,"VerseNumber":26,"Text":"Kallaaa izaa balaghatit taraaqee"},{"SurahNumber":75,"VerseNumber":27,"Text":"Wa qeela man raaq"},{"SurahNumber":75,"VerseNumber":28,"Text":"Wa zanna annahul firaaq"},{"SurahNumber":75,"VerseNumber":29,"Text":"Waltaffatis saaqu bissaaq"},{"SurahNumber":75,"VerseNumber":30,"Text":"Ilaa rabbika yawma'izinil masaaq"},{"SurahNumber":75,"VerseNumber":31,"Text":"Falaa saddaqa wa laa sallaa"},{"SurahNumber":75,"VerseNumber":32,"Text":"Wa laakin kazzaba wa tawalla"},{"SurahNumber":75,"VerseNumber":33,"Text":"Summa zahaba ilaaa ahlihee yatamatta"},{"SurahNumber":75,"VerseNumber":34,"Text":"Awlaa laka fa awlaa"},{"SurahNumber":75,"VerseNumber":35,"Text":"Summa awlaa laka fa awla"},{"SurahNumber":75,"VerseNumber":36,"Text":"Ayahsabul insaanu anyytraka sudaa"},{"SurahNumber":75,"VerseNumber":37,"Text":"Alam yaku nutfatam mim maniyyiny yumnaa"},{"SurahNumber":75,"VerseNumber":38,"Text":"Summa kaana 'alaqata fakhalaq fasawwaa"},{"SurahNumber":75,"VerseNumber":39,"Text":"Faja'ala minhuz zawjayniz zakara wal unsaa"},{"SurahNumber":75,"VerseNumber":40,"Text":"Alaisa zaalika biqaadirin 'alaaa any yuhyiyal mawtaa"},{"SurahNumber":76,"VerseNumber":1,"Text":"Hal ataa 'alal insaani heenum minad dahri lam yakun shai'am mazkooraa"},{"SurahNumber":76,"VerseNumber":2,"Text":"Innaa khalaqnal insaana min nutfatin amshaajin nabta leehi faja'alnaahu samee'am baseeraa"},{"SurahNumber":76,"VerseNumber":3,"Text":"Innaa hadainaahus sabeela immaa shaakiranw wa immaa kafoora"},{"SurahNumber":76,"VerseNumber":4,"Text":"Innaaa a'tadnaa lilkaa fireena salaasila wa aghlaalanw wa sa'eeraa"},{"SurahNumber":76,"VerseNumber":5,"Text":"innal abraara yashra boona min kaasin kaana mizaa juhaa kaafooraa"},{"SurahNumber":76,"VerseNumber":6,"Text":"Aynany yashrabu bihaa 'ibaadul laahi yafajjiroonahaa tafjeeraa"},{"SurahNumber":76,"VerseNumber":7,"Text":"Yoofoona binnazri wa yakhaafoona yawman kaana sharruhoo mustateeraa"},{"SurahNumber":76,"VerseNumber":8,"Text":"Wa yut''imoonat ta'aama 'alaa hubbihee miskeenanw wa yatemanw wa aseeraa"},{"SurahNumber":76,"VerseNumber":9,"Text":"Innaamaa nut'imukum li wajhil laahi laa nureedu minkum jazaaa'anw wa laa shukooraa"},{"SurahNumber":76,"VerseNumber":10,"Text":"Innaa nakhaafu mir Rabbinna Yawman 'aboosan qamtareeraa"},{"SurahNumber":76,"VerseNumber":11,"Text":"Fa waqaahumul laahu sharra zaalikal yawmi wa laqqaahum nadratanw wa surooraa"},{"SurahNumber":76,"VerseNumber":12,"Text":"Wa jazaahum bimaa sabaroo janatanw wa hareeraa"},{"SurahNumber":76,"VerseNumber":13,"Text":"muttaki'eena feeha 'alal araaa 'iki laa yarawna feehaa shamsanw wa laa zamhareeraa"},{"SurahNumber":76,"VerseNumber":14,"Text":"Wa daaniyatan 'alaihim zilaaluhaa wa zullilat qutoofu haa tazleela"},{"SurahNumber":76,"VerseNumber":15,"Text":"Wa yutaafu 'alaihim bi aaniyatim min fiddatinw wa akwaabin kaanat qawaareeraa"},{"SurahNumber":76,"VerseNumber":16,"Text":"Qawaareera min fiddatin qaddaroohaa taqdeeraa"},{"SurahNumber":76,"VerseNumber":17,"Text":"Wa yusawna feehaa kaasan kaana mizaajuhaa zanjabeelaa"},{"SurahNumber":76,"VerseNumber":18,"Text":"Aynan feeha tusammaa salsabeelaa"}]}