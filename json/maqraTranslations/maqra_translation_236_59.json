{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":84,"VerseNumber":1,"Text":"Ma icerreg igenni"},{"SurahNumber":84,"VerseNumber":2,"Text":"issemeess i Mass is, ippueeppem"},{"SurahNumber":84,"VerseNumber":3,"Text":"Ma temsawi tmurt"},{"SurahNumber":84,"VerseNumber":4,"Text":"tveggeo i illan degs, texla"},{"SurahNumber":84,"VerseNumber":5,"Text":"teess i Mass is, teppueeppem"},{"SurahNumber":84,"VerseNumber":6,"Text":"a yamdan ioe$ben aias Mass ik, yideS a ppemlilev"},{"SurahNumber":84,"VerseNumber":7,"Text":"Wanag, win ibbwev leesab is, yeffus is"},{"SurahNumber":84,"VerseNumber":8,"Text":"ad ishel leisab is"},{"SurahNumber":84,"VerseNumber":9,"Text":"s lfere ara iqqwel ar twacult is"},{"SurahNumber":84,"VerseNumber":10,"Text":"Ma d win ibbwev leesab is, deffir waarur is"},{"SurahNumber":84,"VerseNumber":11,"Text":"ad issawal i ssxev"},{"SurahNumber":84,"VerseNumber":12,"Text":"ad ir$ di tmessi"},{"SurahNumber":84,"VerseNumber":13,"Text":"Ni$ illa ifoee ar twacult is"},{"SurahNumber":84,"VerseNumber":14,"Text":"Neppa, ih, inwa ur d ippu$al"},{"SurahNumber":84,"VerseNumber":15,"Text":"Xaii! Zi$ la t Issikid Mass is"},{"SurahNumber":84,"VerseNumber":16,"Text":"Xaii! Gulle$ s lme$oeb"},{"SurahNumber":84,"VerseNumber":17,"Text":"S yiv akked wayen i$umm"},{"SurahNumber":84,"VerseNumber":18,"Text":"S waggur n tziri ma iccuo"},{"SurahNumber":84,"VerseNumber":19,"Text":"War ccekk, a ppeddum si leala ar tayev"},{"SurahNumber":84,"VerseNumber":20,"Text":"Acu iten iu$en, ur uminen"},{"SurahNumber":84,"VerseNumber":21,"Text":"Mara sen $oen Leqwoan, ur kennun"},{"SurahNumber":84,"VerseNumber":22,"Text":"Xaii! Wid ijehlen skiddiben"},{"SurahNumber":84,"VerseNumber":23,"Text":"Oebbi Issen, sennig akw, ayen teffren"},{"SurahNumber":84,"VerseNumber":24,"Text":"Becceo iten s uaaqeb aqeoean"},{"SurahNumber":84,"VerseNumber":25,"Text":"Kkes ed wid iumnen, ixeddmen ayen ilhan; d imezgi warraz nnsen"},{"SurahNumber":85,"VerseNumber":1,"Text":"S igenni bu itran"},{"SurahNumber":85,"VerseNumber":2,"Text":"S wass ippuwaaaden"},{"SurahNumber":85,"VerseNumber":3,"Text":"S unagi akked ccada s"},{"SurahNumber":85,"VerseNumber":4,"Text":"At uxenduq pwan$an"},{"SurahNumber":85,"VerseNumber":5,"Text":"Times m iseo$an"},{"SurahNumber":85,"VerseNumber":6,"Text":"mi nitni qqimen $uoes"},{"SurahNumber":85,"VerseNumber":7,"Text":"D nitni ay d inagan n lmumnin f wayen i sen xedmen"},{"SurahNumber":85,"VerseNumber":8,"Text":"Ur illi ay sen stebaan, siwa imi umnen s Oebbi, Uzmir, Amaazuz"},{"SurahNumber":85,"VerseNumber":9,"Text":"IneS tagelda n tmurt akked igenwan. Oebbi d Anagi n yal cci"},{"SurahNumber":85,"VerseNumber":10,"Text":"Ih, wid ihewwlen lmumnin, argaz tameiiut, ur steooaen, ad ppuaaqben s Loahennama, ad ppuaaqben s tmeo$iwt"},{"SurahNumber":85,"VerseNumber":11,"Text":"Wanag, i wid iumnen, ixeddmen ayen ilhan, leonanat, ddawat sen isaffen ppazzalen. D wagi ay d rrbee ameqwoan"},{"SurahNumber":85,"VerseNumber":12,"Text":"Tiyta n Mass ik, s tidep, tewaao"},{"SurahNumber":85,"VerseNumber":13,"Text":"Neppa, s tidep, Ibeddu, Ippales"},{"SurahNumber":85,"VerseNumber":14,"Text":"Neppa, Ipsemmie, Ieemmel"},{"SurahNumber":85,"VerseNumber":15,"Text":"Bab n usgeld, Amaazuz"},{"SurahNumber":85,"VerseNumber":16,"Text":"Ixeddem ayen i S ihwan"},{"SurahNumber":85,"VerseNumber":17,"Text":"Ma tebbwev ik id teqsii iasekoiwen"},{"SurahNumber":85,"VerseNumber":18,"Text":"n Feoaun akked Tamud"},{"SurahNumber":85,"VerseNumber":19,"Text":"Maca, wid ijehlen, deg uskiddeb"},{"SurahNumber":85,"VerseNumber":20,"Text":"Oebbi, deffir, Izzi yasen"},{"SurahNumber":85,"VerseNumber":21,"Text":"Maca, wagi d Leqwoan muccaa"},{"SurahNumber":85,"VerseNumber":22,"Text":"di telwiet ipwaeerzen"},{"SurahNumber":86,"VerseNumber":1,"Text":"S igenni, akked yitri n yiv"},{"SurahNumber":86,"VerseNumber":2,"Text":"W ara k imlen d acu t itri n yiv"},{"SurahNumber":86,"VerseNumber":3,"Text":"Ipoeoiq itri n yiv"},{"SurahNumber":86,"VerseNumber":4,"Text":"Yal iman fellas ameareb"},{"SurahNumber":86,"VerseNumber":5,"Text":"Ad iwali umdan seg wacu ipwaxleq"},{"SurahNumber":86,"VerseNumber":6,"Text":"Ipwaxleq s waman id ifin"},{"SurahNumber":86,"VerseNumber":7,"Text":"Ff$en ed ger tgeeeal akked tgwejda"},{"SurahNumber":86,"VerseNumber":8,"Text":"Ih, Izmer a t id Issker, Neppa"},{"SurahNumber":86,"VerseNumber":9,"Text":"Ass ideg lesrar ur drigen ara"},{"SurahNumber":86,"VerseNumber":10,"Text":"Ur isai kra n tezmert ne$ n tallalt"},{"SurahNumber":86,"VerseNumber":11,"Text":"S igenni bab n tririt"},{"SurahNumber":86,"VerseNumber":12,"Text":"S tmurt ipceqqiqen"},{"SurahNumber":86,"VerseNumber":13,"Text":"Ar neppa d awal ifeuulen"},{"SurahNumber":86,"VerseNumber":14,"Text":"Ur illi d urar waya"},{"SurahNumber":86,"VerseNumber":15,"Text":"Atnan, ih, ppandin ticeokep"},{"SurahNumber":86,"VerseNumber":16,"Text":"Ad Andi$ ticeokep"},{"SurahNumber":86,"VerseNumber":17,"Text":"I ijehliyen eoo lmudda, eoo asen cwii lmudda"}]}