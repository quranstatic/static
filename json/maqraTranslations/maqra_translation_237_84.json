{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":87,"VerseNumber":1,"Text":"Forherlige navnet Deres Lord mest Højeste"},{"SurahNumber":87,"VerseNumber":2,"Text":"Han opretter forme"},{"SurahNumber":87,"VerseNumber":3,"Text":"Han formgiver guides"},{"SurahNumber":87,"VerseNumber":4,"Text":"Han producerer græsgangen"},{"SurahNumber":87,"VerseNumber":5,"Text":"Derefter drejer det lyse hø"},{"SurahNumber":87,"VerseNumber":6,"Text":"Vi recitere jer; ikke glemme"},{"SurahNumber":87,"VerseNumber":7,"Text":"Alt er ind overensstemmelse med gud ville Han kender hvad erklæres hvad skjules"},{"SurahNumber":87,"VerseNumber":8,"Text":"Vi forestå jer lettest stien"},{"SurahNumber":87,"VerseNumber":9,"Text":"Derfor du påminde; måske påmindelsen gavne"},{"SurahNumber":87,"VerseNumber":10,"Text":"Ærværdige viljen tager heed"},{"SurahNumber":87,"VerseNumber":11,"Text":"wicked viljen undlader det"},{"SurahNumber":87,"VerseNumber":12,"Text":"Følgelig han lide great Hellfire"},{"SurahNumber":87,"VerseNumber":13,"Text":"Hvori han aldrig omkommer nor ophold alive"},{"SurahNumber":87,"VerseNumber":14,"Text":"Succesrige indeed er en indfrir hans sjælen"},{"SurahNumber":87,"VerseNumber":15,"Text":"Ved mindes navnet hans Lord observing kontakten bønner (Salat)"},{"SurahNumber":87,"VerseNumber":16,"Text":"Indeed du preoccupied den først liv"},{"SurahNumber":87,"VerseNumber":17,"Text":"Jævne skønt Herefter er fjerneste godt everlasting"},{"SurahNumber":87,"VerseNumber":18,"Text":"Den registreres tidligere undervisningerne"},{"SurahNumber":87,"VerseNumber":19,"Text":"Undervisningerne af Abraham Moses"},{"SurahNumber":88,"VerseNumber":1,"Text":"Er I aware af de Overvældende"},{"SurahNumber":88,"VerseNumber":2,"Text":"Ansigter den dag shamed"},{"SurahNumber":88,"VerseNumber":3,"Text":"Laboring og opbrugte"},{"SurahNumber":88,"VerseNumber":4,"Text":"Lidende blussende Hellfire"},{"SurahNumber":88,"VerseNumber":5,"Text":"drikke fra flammende forår"},{"SurahNumber":88,"VerseNumber":6,"Text":"De ikke have mad gavnløse mangfoldigheden"},{"SurahNumber":88,"VerseNumber":7,"Text":"Det aldrig nærer nor tilfredsstille hunger"},{"SurahNumber":88,"VerseNumber":8,"Text":"Øvrige ansigt dag ville være fuld af glæde"},{"SurahNumber":88,"VerseNumber":9,"Text":"Tilfredsstillede deres arbejde"},{"SurahNumber":88,"VerseNumber":10,"Text":"Ind exalted Paradis"},{"SurahNumber":88,"VerseNumber":11,"Text":"Ind det ing nonsens høres"},{"SurahNumber":88,"VerseNumber":12,"Text":"Ind det springe gennemstrømninger"},{"SurahNumber":88,"VerseNumber":13,"Text":"Ind det xxxx er luksuøse furnishings"},{"SurahNumber":88,"VerseNumber":14,"Text":"Drikke stille disponible"},{"SurahNumber":88,"VerseNumber":15,"Text":"Kander ind ro"},{"SurahNumber":88,"VerseNumber":16,"Text":"Og gulvtæppe throughout"},{"SurahNumber":88,"VerseNumber":17,"Text":"Hvorfor de ikke reflekterer kamelerne og hvordan de oprettes"},{"SurahNumber":88,"VerseNumber":18,"Text":"Og himlen og hvordan det rejst"},{"SurahNumber":88,"VerseNumber":19,"Text":"Og bjergene og hvordan de konstrueres"},{"SurahNumber":88,"VerseNumber":20,"Text":"Og jorden og hvordan det bygges"},{"SurahNumber":88,"VerseNumber":21,"Text":"Du påminde Deres mission er afsie den påmindelse"},{"SurahNumber":88,"VerseNumber":22,"Text":"Du ikke haver magt dem"},{"SurahNumber":88,"VerseNumber":23,"Text":"Angående de sving disbelieve"},{"SurahNumber":88,"VerseNumber":24,"Text":"GUD ville FORPLIGTE DEM TIL great GENGÆLDELSEN"},{"SurahNumber":88,"VerseNumber":25,"Text":"Til os er deres ultimate destiny"},{"SurahNumber":88,"VerseNumber":26,"Text":"Derefter vi benævne dem konto"},{"SurahNumber":89,"VerseNumber":1,"Text":"Ved daggryet"},{"SurahNumber":89,"VerseNumber":2,"Text":"Ti nættene"},{"SurahNumber":89,"VerseNumber":3,"Text":"Ved jævne de besynderlige"},{"SurahNumber":89,"VerseNumber":4,"Text":"Ved natten det vedtage"},{"SurahNumber":89,"VerseNumber":5,"Text":"Dybe ed ene som possesses intelligens"},{"SurahNumber":89,"VerseNumber":6,"Text":"Du noterede hvad Deres Lord have gjorde ‘Aad"},{"SurahNumber":89,"VerseNumber":7,"Text":"Erum; byen høje bygninger"},{"SurahNumber":89,"VerseNumber":8,"Text":"der være intet det anywhere"},{"SurahNumber":89,"VerseNumber":9,"Text":"Også Thamoud udskar klipperne deres dal"},{"SurahNumber":89,"VerseNumber":10,"Text":"Pharaoh possessed might"},{"SurahNumber":89,"VerseNumber":11,"Text":"De al transgressed landet"},{"SurahNumber":89,"VerseNumber":12,"Text":"De spreder onde throughout"},{"SurahNumber":89,"VerseNumber":13,"Text":"Følgelig Deres Lord hældte dem piskning gengældelse"},{"SurahNumber":89,"VerseNumber":14,"Text":"Deres Lord er nogensinde vagtsomme"},{"SurahNumber":89,"VerseNumber":15,"Text":"Hvornår menneskelig være tested hans Lord velsignelser glæde han sir Min Lord er gavmilde mod mig"},{"SurahNumber":89,"VerseNumber":16,"Text":"Han prøve ham nedsættelse forråd han sir Min Lord ydmyger mig"},{"SurahNumber":89,"VerseNumber":17,"Text":"forkert Er jer bragte det jer ikke betragte forældreløse"},{"SurahNumber":89,"VerseNumber":18,"Text":"Og ikke advocating velgørenhed fattig"},{"SurahNumber":89,"VerseNumber":19,"Text":"indtage arven hjælpeløse forældreløse"},{"SurahNumber":89,"VerseNumber":20,"Text":"Og elske pengen too mere"},{"SurahNumber":89,"VerseNumber":21,"Text":"Indeed hvornår jorden knuses utterly knuste"},{"SurahNumber":89,"VerseNumber":22,"Text":"Deres Lord kommer sammen englene ind ro efter ro"},{"SurahNumber":89,"VerseNumber":23,"Text":"Den dag Gehenna bringes væk. Den dag menneskelig være mindes - Men hvad remembrance - Det være too sene"},{"SurahNumber":89,"VerseNumber":24,"Text":"Han sie Ohio Jeg ønske jeg tillave min (evige) liv"},{"SurahNumber":89,"VerseNumber":25,"Text":"Den dag ing gengældelse være ond end Hans gengældelse"},{"SurahNumber":89,"VerseNumber":26,"Text":"nej confinement er idet virkningsfulde idet Hans confinement"},{"SurahNumber":89,"VerseNumber":27,"Text":"Angående jer O indehold sjæl"},{"SurahNumber":89,"VerseNumber":28,"Text":"Tilbagekomst til Deres Lord tilfredse behage"},{"SurahNumber":89,"VerseNumber":29,"Text":"Velkommen into Min servants"},{"SurahNumber":89,"VerseNumber":30,"Text":"Velkommen into Min Paradis"}]}