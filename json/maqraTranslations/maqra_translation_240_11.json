{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":100,"VerseNumber":9,"Text":"Afala ya'lamu iza b'uthira ma filquboor"},{"SurahNumber":100,"VerseNumber":10,"Text":"Wa hussila maa fis sudoor"},{"SurahNumber":100,"VerseNumber":11,"Text":"Inna rabbahum bihim yauma 'izil la khabeer"},{"SurahNumber":101,"VerseNumber":1,"Text":"Al qaari'ah"},{"SurahNumber":101,"VerseNumber":2,"Text":"Mal qaariah"},{"SurahNumber":101,"VerseNumber":3,"Text":"Wa maa adraaka mal qaari'ah"},{"SurahNumber":101,"VerseNumber":4,"Text":"Yauma ya koonun naasu kal farashil mabthooth"},{"SurahNumber":101,"VerseNumber":5,"Text":"Wa ta koonul jibalu kal 'ihnil manfoosh"},{"SurahNumber":101,"VerseNumber":6,"Text":"Fa-amma man thaqulat mawa zeenuh"},{"SurahNumber":101,"VerseNumber":7,"Text":"Fahuwa fee 'ishatir raadiyah"},{"SurahNumber":101,"VerseNumber":8,"Text":"Wa amma man khaffat mawa zeenuh"},{"SurahNumber":101,"VerseNumber":9,"Text":"Fa-ummuhu haawiyah"},{"SurahNumber":101,"VerseNumber":10,"Text":"Wa maa adraaka maa hiyah"},{"SurahNumber":101,"VerseNumber":11,"Text":"Naarun hamiyah"},{"SurahNumber":102,"VerseNumber":1,"Text":"Al haaku mut takathur"},{"SurahNumber":102,"VerseNumber":2,"Text":"Hatta zurtumul-maqaabir"},{"SurahNumber":102,"VerseNumber":3,"Text":"Kalla sawfa ta'lamoon"},{"SurahNumber":102,"VerseNumber":4,"Text":"Thumma kalla sawfa ta'lamoon"},{"SurahNumber":102,"VerseNumber":5,"Text":"Kalla law ta'lamoona 'ilmal yaqeen"},{"SurahNumber":102,"VerseNumber":6,"Text":"Latara-wun nal jaheem"},{"SurahNumber":102,"VerseNumber":7,"Text":"Thumma latara wunnaha 'ainal yaqeen"},{"SurahNumber":102,"VerseNumber":8,"Text":"Thumma latus alunna yauma-izin 'anin na'eem"},{"SurahNumber":103,"VerseNumber":1,"Text":"Wal' asr"},{"SurahNumber":103,"VerseNumber":2,"Text":"Innal insaana lafee khusr"},{"SurahNumber":103,"VerseNumber":3,"Text":"Il lal lazeena aamanu wa 'amilus saali haati wa tawa saw bil haqqi wa tawa saw bis sabr"},{"SurahNumber":104,"VerseNumber":1,"Text":"Wai lul-li kulli hu mazatil-lumaza"},{"SurahNumber":104,"VerseNumber":2,"Text":"Al-lazi jama'a maalaw wa'addadah"},{"SurahNumber":104,"VerseNumber":3,"Text":"Yahsabu anna maalahu akhladah"},{"SurahNumber":104,"VerseNumber":4,"Text":"Kalla layum ba zanna fil hutamah"},{"SurahNumber":104,"VerseNumber":5,"Text":"Wa maa adraaka mal-hutamah"},{"SurahNumber":104,"VerseNumber":6,"Text":"Narul laahil-mooqada"},{"SurahNumber":104,"VerseNumber":7,"Text":"Al latee tat tali'u 'alalafidah"},{"SurahNumber":104,"VerseNumber":8,"Text":"Innaha 'alaihim moosada"},{"SurahNumber":104,"VerseNumber":9,"Text":"Fee 'amadim-mu mad dadah"},{"SurahNumber":105,"VerseNumber":1,"Text":"Alam tara kaifa fa'ala rabbuka bi ashaabil feel"},{"SurahNumber":105,"VerseNumber":2,"Text":"Alam yaj'al kai dahum fee tad leel"},{"SurahNumber":105,"VerseNumber":3,"Text":"Wa arsala 'alaihim tairan abaabeel"},{"SurahNumber":105,"VerseNumber":4,"Text":"Tar meehim bi hi jaaratim min sij jeel"},{"SurahNumber":105,"VerseNumber":5,"Text":"Faja 'alahum ka'asfim m'akool"},{"SurahNumber":106,"VerseNumber":1,"Text":"Li-ilaafi quraish"},{"SurahNumber":106,"VerseNumber":2,"Text":"Elaafihim rihlatash shitaa-i wass saif"},{"SurahNumber":106,"VerseNumber":3,"Text":"Fal y'abudu rabba haazal-bait"},{"SurahNumber":106,"VerseNumber":4,"Text":"Allazi at'amahum min ju'inw-wa-aamana hum min khawf"},{"SurahNumber":107,"VerseNumber":1,"Text":"Ara-aital lazee yu kazzibu bid deen"},{"SurahNumber":107,"VerseNumber":2,"Text":"Fa zaalikal lazi yadu'ul-yateem"},{"SurahNumber":107,"VerseNumber":3,"Text":"Wa la ya huddu 'alaa ta'amil miskeen"},{"SurahNumber":107,"VerseNumber":4,"Text":"Fa wai lul-lil mu salleen"},{"SurahNumber":107,"VerseNumber":5,"Text":"Al lazeena hum 'an salaatihim sahoon"},{"SurahNumber":107,"VerseNumber":6,"Text":"Al lazeena hum yuraa-oon"},{"SurahNumber":107,"VerseNumber":7,"Text":"Wa yamna'oonal ma'oon"},{"SurahNumber":108,"VerseNumber":1,"Text":"Innaa a'taina kal kauthar"},{"SurahNumber":108,"VerseNumber":2,"Text":"Fa salli li rabbika wanhar"},{"SurahNumber":108,"VerseNumber":3,"Text":"Inna shani-aka huwal abtar"},{"SurahNumber":109,"VerseNumber":1,"Text":"Qul yaa-ai yuhal kaafiroon"},{"SurahNumber":109,"VerseNumber":2,"Text":"Laa a'budu ma t'abudoon"},{"SurahNumber":109,"VerseNumber":3,"Text":"Wa laa antum 'aabidoona maa a'bud"},{"SurahNumber":109,"VerseNumber":4,"Text":"Wa laa ana 'abidum maa 'abattum"},{"SurahNumber":109,"VerseNumber":5,"Text":"Wa laa antum 'aabidoona ma a'bud"},{"SurahNumber":109,"VerseNumber":6,"Text":"Lakum deenukum wa liya deen"},{"SurahNumber":110,"VerseNumber":1,"Text":"Iza jaa-a nas rullahi walfath"},{"SurahNumber":110,"VerseNumber":2,"Text":"Wa ra-aitan naasa yadkhuloona fee deenil laahi afwajah"},{"SurahNumber":110,"VerseNumber":3,"Text":"Fa sab bih bihamdi rabbika was taghfir, innahu kaana tawwaaba"},{"SurahNumber":111,"VerseNumber":1,"Text":"Tab bat yadaa abee Lahabinw-wa tabb"},{"SurahNumber":111,"VerseNumber":2,"Text":"Maa aghna 'anhu maaluhu wa ma kasab"},{"SurahNumber":111,"VerseNumber":3,"Text":"Sa yas laa naran zaata lahab"},{"SurahNumber":111,"VerseNumber":4,"Text":"Wam ra-atuh hamma latal-hatab"},{"SurahNumber":111,"VerseNumber":5,"Text":"Fee jeediha hab lum mim-masad"},{"SurahNumber":112,"VerseNumber":1,"Text":"Qul huwal laahu ahad"},{"SurahNumber":112,"VerseNumber":2,"Text":"Allah hus-samad"},{"SurahNumber":112,"VerseNumber":3,"Text":"Lam yalid wa lam yoolad"},{"SurahNumber":112,"VerseNumber":4,"Text":"Wa lam yakul-lahu kufuwan ahad"},{"SurahNumber":113,"VerseNumber":1,"Text":"Qul a'uzoo bi rabbil-falaq"},{"SurahNumber":113,"VerseNumber":2,"Text":"Min sharri ma khalaq"},{"SurahNumber":113,"VerseNumber":3,"Text":"Wa min sharri ghasiqin iza waqab"},{"SurahNumber":113,"VerseNumber":4,"Text":"Wa min sharrin-naffaa-thaati fil 'uqad"},{"SurahNumber":113,"VerseNumber":5,"Text":"Wa min shar ri haasidin iza hasad"},{"SurahNumber":114,"VerseNumber":1,"Text":"Qul a'uzu birabbin naas"},{"SurahNumber":114,"VerseNumber":2,"Text":"Malikin naas"},{"SurahNumber":114,"VerseNumber":3,"Text":"Ilaahin naas"},{"SurahNumber":114,"VerseNumber":4,"Text":"Min sharril was waasil khannaas"},{"SurahNumber":114,"VerseNumber":5,"Text":"Al lazee yuwas wisu fee sudoorin naas"},{"SurahNumber":114,"VerseNumber":6,"Text":"Minal jinnati wan naas"}]}