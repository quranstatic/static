{"Id":248,"LanguageCodes":["ml","mal"],"Name":"mal-muhammadkarakun-la","Author":"Muhammad Karakunnu And Vanidas Elayavoor","Language":"Malayalam","Description":"Malayalam - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":94,"VerseNumber":1,"Text":"ninre hrdayam ninakku nam visalamakkiyille"},{"SurahNumber":94,"VerseNumber":2,"Text":"ninre bharam ninnil ninnirakki veccille"},{"SurahNumber":94,"VerseNumber":3,"Text":"ninre mutukine nericcukeantirunna bharam"},{"SurahNumber":94,"VerseNumber":4,"Text":"ninre kirtti nam uyarttittarikayum ceytu"},{"SurahNumber":94,"VerseNumber":5,"Text":"atinal tirccayayum prayasatteateappam eluppavumunt"},{"SurahNumber":94,"VerseNumber":6,"Text":"niscayam, prayasatteateappaman eluppam"},{"SurahNumber":94,"VerseNumber":7,"Text":"atinal onnil ninnealivayal marreannil mulukuka"},{"SurahNumber":94,"VerseNumber":8,"Text":"ninre nathanil pratiksa arppikkukayum ceyyuka"},{"SurahNumber":95,"VerseNumber":1,"Text":"attiyum olivum saksi"},{"SurahNumber":95,"VerseNumber":2,"Text":"sinamala saksi"},{"SurahNumber":95,"VerseNumber":3,"Text":"nirbhitamaya i makkanagaram saksi"},{"SurahNumber":95,"VerseNumber":4,"Text":"tirccayayum manusyane nam mikavurra ghatanayil srsticcu"},{"SurahNumber":95,"VerseNumber":5,"Text":"pinne namavane patitaril patitanakki"},{"SurahNumber":95,"VerseNumber":6,"Text":"satyavisvasam svikariccavareyum salkkarmannal pravartticcavareyumealike. avarkk, arutiyillatta pratiphalamunt"},{"SurahNumber":95,"VerseNumber":7,"Text":"ennittum raksasiksakalute karyattil ninne kallamakkunnatent"},{"SurahNumber":95,"VerseNumber":8,"Text":"vidhikarttakkalil erravum nalla vidhikarttav allahuvallayea"},{"SurahNumber":96,"VerseNumber":1,"Text":"vayikkuka, srsticca ninre nathanre namattil"},{"SurahNumber":96,"VerseNumber":2,"Text":"ottippitikkunnatilninn avan manusyane srsticcu"},{"SurahNumber":96,"VerseNumber":3,"Text":"vayikkuka! ninre nathan atyudaranan"},{"SurahNumber":96,"VerseNumber":4,"Text":"penakeantu pathippiccavan"},{"SurahNumber":96,"VerseNumber":5,"Text":"manusyane avanariyattat avan pathippiccu"},{"SurahNumber":96,"VerseNumber":6,"Text":"sansayamilla; manusyan atikramiyayirikkunnu"},{"SurahNumber":96,"VerseNumber":7,"Text":"tanikkutanpeannavanayi kantatinal"},{"SurahNumber":96,"VerseNumber":8,"Text":"niscayam, matakkam ninre nathankalekkan"},{"SurahNumber":96,"VerseNumber":9,"Text":"tatayunnavane ni kantea"},{"SurahNumber":96,"VerseNumber":10,"Text":"nam'mute dasane, avan namaskarikkumpeal"},{"SurahNumber":96,"VerseNumber":11,"Text":"ni kantea? a atima nervaliyil tanneyan"},{"SurahNumber":96,"VerseNumber":12,"Text":"athava, bhakti upadesikkunnavanan"},{"SurahNumber":96,"VerseNumber":13,"Text":"ni kantea? i tatayunnavan satyatte tallikkalayukayum purantirinnu nilkkukayum ceytavanan"},{"SurahNumber":96,"VerseNumber":14,"Text":"allahu ellam kanunnuvenn avan ariyunnille"},{"SurahNumber":96,"VerseNumber":15,"Text":"sansayam venta; avanit avasanippikkunnillenkil; avanre kutuma nam piticcu valikkuka tanne ceyyum"},{"SurahNumber":96,"VerseNumber":16,"Text":"kallam parayukayum; papam pravarttikkukayum ceyyunna kutuma"},{"SurahNumber":96,"VerseNumber":17,"Text":"appealavan tanre alukale vilikkatte"},{"SurahNumber":96,"VerseNumber":18,"Text":"nam nam'mute siksakaryannalute cumatalakkareyum vilikkam"},{"SurahNumber":96,"VerseNumber":19,"Text":"arut! ni avan valannarut. ni sastangam pranamikkuka! nam'mute samipyam netuka"},{"SurahNumber":97,"VerseNumber":1,"Text":"tirccayayum nam i khur'anine vidhi nirnayaka ravil avatarippiccu"},{"SurahNumber":97,"VerseNumber":2,"Text":"vidhi nirnayaka rav entenn ninakkentariyam"},{"SurahNumber":97,"VerseNumber":3,"Text":"vidhi nirnayaka rav ayiram masattekkal mahattaraman"},{"SurahNumber":97,"VerseNumber":4,"Text":"a ravil malakkukalum jibrilum iranni varunnu. samasta karyannaleyum sambandhicca tannalute nathanre uttaravumayi"},{"SurahNumber":97,"VerseNumber":5,"Text":"pulareali viriyum vare at prasantamayirikkum"},{"SurahNumber":98,"VerseNumber":1,"Text":"vedakkarum bahudaiva visvasikalumaya satyanisedhikal vyaktamaya teliv vannettum vare tannalute valiyil uraccuninnu"},{"SurahNumber":98,"VerseNumber":2,"Text":"allahuvil ninnulla dutan pavitramaya granthattalukal vayiccu kelppikkunnat vare"},{"SurahNumber":98,"VerseNumber":3,"Text":"a granthattalukalil satyanisthamaya pramanannalunt"},{"SurahNumber":98,"VerseNumber":4,"Text":"vedam nalkappettavar bhinniccittilla. avarkku vyaktamaya teliv vannettiya sesamallate"},{"SurahNumber":98,"VerseNumber":5,"Text":"vidheyatvam allahuvinu matramakki avane matram valippett nervaliyil jivikkanallate avareat kalpiccittilla. oppam namaskaram nisthayeate nirvahikkanum sakat nalkanum. atan ceavvaya jivitakramam"},{"SurahNumber":98,"VerseNumber":6,"Text":"tirccayayum vedakkarum bahudaiva visvasikalumaya satyanisedhikal narakattiyilan. avaratil sthiravasikalayirikkum. avaran srstikalilerram nikrstar"},{"SurahNumber":98,"VerseNumber":7,"Text":"ennal satyavisvasam svikarikkukayum salkkarmannal pravarttikkukayum ceytavarea, avaran srstikalilerram sresthar"},{"SurahNumber":98,"VerseNumber":8,"Text":"avarkk avarute nathankal arhamaya pratiphalamunt. talbhagattute arukalealukunna svargiyaramannal. avaratil ekkalavum sthiravasikalayirikkum. allahu avarekkuricc trptanayirikkum. avar allahuvilum santrptarayirikkum. itellam tannalute nathane bhayappetunnavarkkullatan"},{"SurahNumber":99,"VerseNumber":1,"Text":"bhumi atisaktiyayi virakeantal"},{"SurahNumber":99,"VerseNumber":2,"Text":"bhumi atinre bharannal purantalliyal"},{"SurahNumber":99,"VerseNumber":3,"Text":"manusyan ceadikkum: atinentu parri"},{"SurahNumber":99,"VerseNumber":4,"Text":"annalil bhumi atinre vivarameakke parannariyikkum"},{"SurahNumber":99,"VerseNumber":5,"Text":"ninre nathan atinu beadhanam nalkiyatinalanit"},{"SurahNumber":99,"VerseNumber":6,"Text":"annalil janam pala sanghannalayi purappetum; tannalute pravarttanaphalannal neril kanan"},{"SurahNumber":99,"VerseNumber":7,"Text":"atinal, anuttukkam nanma ceytavan at kanum"},{"SurahNumber":99,"VerseNumber":8,"Text":"anuttukkam tinma ceytavan atum kanum"},{"SurahNumber":100,"VerseNumber":1,"Text":"kitacceatunnava saksi"},{"SurahNumber":100,"VerseNumber":2,"Text":"annane kulampurasi tippeari parattunnava saksi"},{"SurahNumber":100,"VerseNumber":3,"Text":"pularcce akramanam natattunnava saksi"},{"SurahNumber":100,"VerseNumber":4,"Text":"annane peatipatalam ilakkivitunnava saksi"},{"SurahNumber":100,"VerseNumber":5,"Text":"satrukkalkku natuvil katannucellunnava saksi"},{"SurahNumber":100,"VerseNumber":6,"Text":"tirccayayum manusyan tanre nathaneat nandiyillattavanan"},{"SurahNumber":100,"VerseNumber":7,"Text":"urappayum avan tanne i nandiketinu saksiyan"},{"SurahNumber":100,"VerseNumber":8,"Text":"dhanatteatulla avanre artti atikathinam tanne"}]}