{"Id":13,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la3","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"","Translations":[{"SurahNumber":37,"VerseNumber":145,"Text":"Fanabathnahu bialAAara-i wahuwa saqeemun"},{"SurahNumber":37,"VerseNumber":146,"Text":"Waanbatna AAalayhi shajaratan min yaqteenin"},{"SurahNumber":37,"VerseNumber":147,"Text":"Waarsalnahu ila mi-ati alfin aw yazeedoona"},{"SurahNumber":37,"VerseNumber":148,"Text":"Faamanoo famattaAAnahum ila heenin"},{"SurahNumber":37,"VerseNumber":149,"Text":"Faistaftihim alirabbika albanatu walahumu albanoona"},{"SurahNumber":37,"VerseNumber":150,"Text":"Am khalaqna almala-ikata inathan wahum shahidoona"},{"SurahNumber":37,"VerseNumber":151,"Text":"Ala innahum min ifkihim layaqooloona"},{"SurahNumber":37,"VerseNumber":152,"Text":"Walada Allahu wa-innahum lakathiboona"},{"SurahNumber":37,"VerseNumber":153,"Text":"Astafa albanati AAala albaneena"},{"SurahNumber":37,"VerseNumber":154,"Text":"Ma lakum kayfa tahkumoona"},{"SurahNumber":37,"VerseNumber":155,"Text":"Afala tathakkaroona"},{"SurahNumber":37,"VerseNumber":156,"Text":"Am lakum sultanun mubeenun"},{"SurahNumber":37,"VerseNumber":157,"Text":"Fa/too bikitabikum in kuntum sadiqeena"},{"SurahNumber":37,"VerseNumber":158,"Text":"WajaAAaloo baynahu wabayna aljinnati nasaban walaqad AAalimati aljinnatu innahum lamuhdaroona"},{"SurahNumber":37,"VerseNumber":159,"Text":"Subhana Allahi AAamma yasifoona"},{"SurahNumber":37,"VerseNumber":160,"Text":"Illa AAibada Allahi almukhlaseena"},{"SurahNumber":37,"VerseNumber":161,"Text":"Fa-innakum wama taAAbudoona"},{"SurahNumber":37,"VerseNumber":162,"Text":"Ma antum AAalayhi bifatineena"},{"SurahNumber":37,"VerseNumber":163,"Text":"Illa man huwa sali aljaheemi"},{"SurahNumber":37,"VerseNumber":164,"Text":"Wama minna illa lahu maqamun maAAloomun"},{"SurahNumber":37,"VerseNumber":165,"Text":"Wa-inna lanahnu alssaffoona"},{"SurahNumber":37,"VerseNumber":166,"Text":"Wa-inna lanahnu almusabbihoona"},{"SurahNumber":37,"VerseNumber":167,"Text":"Wa-in kanoo layaqooloona"},{"SurahNumber":37,"VerseNumber":168,"Text":"Law anna AAindana thikran mina al-awwaleena"},{"SurahNumber":37,"VerseNumber":169,"Text":"Lakunna AAibada Allahi almukhlaseena"},{"SurahNumber":37,"VerseNumber":170,"Text":"Fakafaroo bihi fasawfa yaAAlamoona"},{"SurahNumber":37,"VerseNumber":171,"Text":"Walaqad sabaqat kalimatuna liAAibadina almursaleena"},{"SurahNumber":37,"VerseNumber":172,"Text":"Innahum lahumu almansooroona"},{"SurahNumber":37,"VerseNumber":173,"Text":"Wa-inna jundana lahumu alghaliboona"},{"SurahNumber":37,"VerseNumber":174,"Text":"Fatawalla AAanhum hatta heenin"},{"SurahNumber":37,"VerseNumber":175,"Text":"Waabsirhum fasawfa yubsiroona"},{"SurahNumber":37,"VerseNumber":176,"Text":"AfabiAAathabina yastaAAjiloona"},{"SurahNumber":37,"VerseNumber":177,"Text":"Fa-itha nazala bisahatihim fasaa sabahu almunthareena"},{"SurahNumber":37,"VerseNumber":178,"Text":"Watawalla AAanhum hatta heenin"},{"SurahNumber":37,"VerseNumber":179,"Text":"Waabsir fasawfa yubsiroona"},{"SurahNumber":37,"VerseNumber":180,"Text":"Subhana rabbika rabbi alAAizzati AAamma yasifoona"},{"SurahNumber":37,"VerseNumber":181,"Text":"Wasalamun AAala almursaleena"},{"SurahNumber":37,"VerseNumber":182,"Text":"Waalhamdu lillahi rabbi alAAalameena"},{"SurahNumber":38,"VerseNumber":1,"Text":"Sad waalqur-ani thee alththikri"},{"SurahNumber":38,"VerseNumber":2,"Text":"Bali allatheena kafaroo fee AAizzatin washiqaqin"},{"SurahNumber":38,"VerseNumber":3,"Text":"Kam ahlakna min qablihim min qarnin fanadaw walata heena manasin"},{"SurahNumber":38,"VerseNumber":4,"Text":"WaAAajiboo an jaahum munthirun minhum waqala alkafiroona hatha sahirun kaththabun"},{"SurahNumber":38,"VerseNumber":5,"Text":"AjaAAala al-alihata ilahan wahidan inna hatha lashay-on AAujabun"},{"SurahNumber":38,"VerseNumber":6,"Text":"Waintalaqa almalao minhum ani imshoo waisbiroo AAala alihatikum inna hatha lashay-on yuradu"},{"SurahNumber":38,"VerseNumber":7,"Text":"Ma samiAAna bihatha fee almillati al-akhirati in hatha illa ikhtilaqun"},{"SurahNumber":38,"VerseNumber":8,"Text":"Aonzila AAalayhi alththikru min baynina bal hum fee shakkin min thikree bal lamma yathooqoo AAathabi"},{"SurahNumber":38,"VerseNumber":9,"Text":"Am AAindahum khaza-inu rahmati rabbika alAAazeezi alwahhabi"},{"SurahNumber":38,"VerseNumber":10,"Text":"Am lahum mulku alssamawati waal-ardi wama baynahuma falyartaqoo fee al-asbabi"},{"SurahNumber":38,"VerseNumber":11,"Text":"Jundun ma hunalika mahzoomun mina al-ahzabi"},{"SurahNumber":38,"VerseNumber":12,"Text":"Kaththabat qablahum qawmu noohin waAAadun wafirAAawnu thoo al-awtadi"},{"SurahNumber":38,"VerseNumber":13,"Text":"Wathamoodu waqawmu lootin waas-habu al-aykati ola-ika al-ahzabu"},{"SurahNumber":38,"VerseNumber":14,"Text":"In kullun illa kaththaba alrrusula fahaqqa AAiqabi"},{"SurahNumber":38,"VerseNumber":15,"Text":"Wama yanthuru haola-i illa sayhatan wahidatan ma laha min fawaqin"},{"SurahNumber":38,"VerseNumber":16,"Text":"Waqaloo rabbana AAajjil lana qittana qabla yawmi alhisabi"},{"SurahNumber":38,"VerseNumber":17,"Text":"Isbir AAala ma yaqooloona waothkur AAabdana dawooda tha al-aydi innahu awwabun"},{"SurahNumber":38,"VerseNumber":18,"Text":"Inna sakhkharna aljibala maAAahu yusabbihna bialAAashiyyi waal-ishraqi"},{"SurahNumber":38,"VerseNumber":19,"Text":"Waalttayra mahshooratan kullun lahu awwabun"},{"SurahNumber":38,"VerseNumber":20,"Text":"Washadadna mulkahu waataynahu alhikmata wafasla alkhitabi"}]}