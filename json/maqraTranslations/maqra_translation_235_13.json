{"Id":13,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la3","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"Itha alssamao infatarat"},{"SurahNumber":82,"VerseNumber":2,"Text":"Wa-itha alkawakibu intatharat"},{"SurahNumber":82,"VerseNumber":3,"Text":"Wa-itha albiharu fujjirat"},{"SurahNumber":82,"VerseNumber":4,"Text":"Wa-itha alqubooru buAAthirat"},{"SurahNumber":82,"VerseNumber":5,"Text":"AAalimat nafsun ma qaddamat waakhkharat"},{"SurahNumber":82,"VerseNumber":6,"Text":"Ya ayyuha al-insanu ma gharraka birabbika alkareemi"},{"SurahNumber":82,"VerseNumber":7,"Text":"Allathee khalaqaka fasawwaka faAAadalaka"},{"SurahNumber":82,"VerseNumber":8,"Text":"Fee ayyi sooratin ma shaa rakkabaka"},{"SurahNumber":82,"VerseNumber":9,"Text":"Kalla bal tukaththiboona bialddeeni"},{"SurahNumber":82,"VerseNumber":10,"Text":"Wa-inna AAalaykum lahafitheena"},{"SurahNumber":82,"VerseNumber":11,"Text":"Kiraman katibeena"},{"SurahNumber":82,"VerseNumber":12,"Text":"YaAAlamoona ma tafAAaloona"},{"SurahNumber":82,"VerseNumber":13,"Text":"Inna al-abrara lafee naAAeemin"},{"SurahNumber":82,"VerseNumber":14,"Text":"Wa-inna alfujjara lafee jaheemin"},{"SurahNumber":82,"VerseNumber":15,"Text":"Yaslawnaha yawma alddeeni"},{"SurahNumber":82,"VerseNumber":16,"Text":"Wama hum AAanha bigha-ibeena"},{"SurahNumber":82,"VerseNumber":17,"Text":"Wama adraka ma yawmu alddeeni"},{"SurahNumber":82,"VerseNumber":18,"Text":"Thumma ma adraka ma yawmu alddeeni"},{"SurahNumber":82,"VerseNumber":19,"Text":"Yawma la tamliku nafsun linafsin shay-an waal-amru yawma-ithin lillahi"},{"SurahNumber":83,"VerseNumber":1,"Text":"Waylun lilmutaffifeena"},{"SurahNumber":83,"VerseNumber":2,"Text":"Allatheena itha iktaloo AAala alnnasi yastawfoona"},{"SurahNumber":83,"VerseNumber":3,"Text":"Wa-itha kaloohum aw wazanoohum yukhsiroona"},{"SurahNumber":83,"VerseNumber":4,"Text":"Ala yathunnu ola-ika annahum mabAAoothoona"},{"SurahNumber":83,"VerseNumber":5,"Text":"Liyawmin AAatheemin"},{"SurahNumber":83,"VerseNumber":6,"Text":"Yawma yaqoomu alnnasu lirabbi alAAalameena"},{"SurahNumber":83,"VerseNumber":7,"Text":"Kalla inna kitaba alfujjari lafee sijjeenin"},{"SurahNumber":83,"VerseNumber":8,"Text":"Wama adraka ma sijjeenun"},{"SurahNumber":83,"VerseNumber":9,"Text":"Kitabun marqoomun"},{"SurahNumber":83,"VerseNumber":10,"Text":"Waylun yawma-ithin lilmukaththibeena"},{"SurahNumber":83,"VerseNumber":11,"Text":"Allatheena yukaththiboona biyawmi alddeeni"},{"SurahNumber":83,"VerseNumber":12,"Text":"Wama yukaththibu bihi illa kullu muAAtadin atheemin"},{"SurahNumber":83,"VerseNumber":13,"Text":"Itha tutla AAalayhi ayatuna qala asateeru al-awwaleena"},{"SurahNumber":83,"VerseNumber":14,"Text":"Kalla bal rana AAala quloobihim ma kanoo yaksiboona"},{"SurahNumber":83,"VerseNumber":15,"Text":"Kalla innahum AAan rabbihim yawma-ithin lamahjooboona"},{"SurahNumber":83,"VerseNumber":16,"Text":"Thumma innahum lasaloo aljaheemi"},{"SurahNumber":83,"VerseNumber":17,"Text":"Thumma yuqalu hatha allathee kuntum bihi tukaththiboona"},{"SurahNumber":83,"VerseNumber":18,"Text":"Kalla inna kitaba al-abrari lafee AAilliyyeena"},{"SurahNumber":83,"VerseNumber":19,"Text":"Wama adraka ma AAilliyyoona"},{"SurahNumber":83,"VerseNumber":20,"Text":"Kitabun marqoomun"},{"SurahNumber":83,"VerseNumber":21,"Text":"Yashhaduhu almuqarraboona"},{"SurahNumber":83,"VerseNumber":22,"Text":"Inna al-abrara lafee naAAeemin"},{"SurahNumber":83,"VerseNumber":23,"Text":"AAala al-ara-iki yanthuroona"},{"SurahNumber":83,"VerseNumber":24,"Text":"TaAArifu fee wujoohihim nadrata alnnaAAeemi"},{"SurahNumber":83,"VerseNumber":25,"Text":"Yusqawna min raheeqin makhtoomin"},{"SurahNumber":83,"VerseNumber":26,"Text":"Khitamuhu miskun wafee thalika falyatanafasi almutanafisoona"},{"SurahNumber":83,"VerseNumber":27,"Text":"Wamizajuhu min tasneemin"},{"SurahNumber":83,"VerseNumber":28,"Text":"AAaynan yashrabu biha almuqarraboona"},{"SurahNumber":83,"VerseNumber":29,"Text":"Inna allatheena ajramoo kanoo mina allatheena amanoo yadhakoona"},{"SurahNumber":83,"VerseNumber":30,"Text":"Wa-itha marroo bihim yataghamazoona"},{"SurahNumber":83,"VerseNumber":31,"Text":"Wa-itha inqalaboo ila ahlihimu inqalaboo fakiheena"},{"SurahNumber":83,"VerseNumber":32,"Text":"Wa-itha raawhum qaloo inna haola-i ladalloona"},{"SurahNumber":83,"VerseNumber":33,"Text":"Wama orsiloo AAalayhim hafitheena"},{"SurahNumber":83,"VerseNumber":34,"Text":"Faalyawma allatheena amanoo mina alkuffari yadhakoona"},{"SurahNumber":83,"VerseNumber":35,"Text":"AAala al-ara-iki yanthuroona"},{"SurahNumber":83,"VerseNumber":36,"Text":"Hal thuwwiba alkuffaru ma kanoo yafAAaloona"}]}