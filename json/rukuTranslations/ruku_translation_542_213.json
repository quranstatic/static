{"Id":213,"LanguageCodes":["kn","kan"],"Name":"kan-abdussalamputhi-la","Author":"Abdussalam Puthige","Language":"Kannada","Description":"Kannada - Transliteration","Direction":"ltr","Source":"http://quraninkannada.com , aputhige@gmail.com","Translations":[{"SurahNumber":100,"VerseNumber":1,"Text":"Edusiru bidutta oduva kuduregalane"},{"SurahNumber":100,"VerseNumber":2,"Text":"avu kidi harisuttave"},{"SurahNumber":100,"VerseNumber":3,"Text":"munjavina hottu avu dhali madi biduttave"},{"SurahNumber":100,"VerseNumber":4,"Text":"avu dhulebbisi biduttave"},{"SurahNumber":100,"VerseNumber":5,"Text":"mattu satrugala padegalolage nuggi biduttave"},{"SurahNumber":100,"VerseNumber":6,"Text":"manusyanu khandita tanna odeyanige krtaghnanagiddane"},{"SurahNumber":100,"VerseNumber":7,"Text":"idakke khandita avane saksiyagiddane"},{"SurahNumber":100,"VerseNumber":8,"Text":"avanu sampattannu tumba pritisuttane"},{"SurahNumber":100,"VerseNumber":9,"Text":"avanige tilidillave, gorigalolage iruvavarannella hora tegeyalaguva dinada kuritu"},{"SurahNumber":100,"VerseNumber":10,"Text":"mattu manas'sugalolage iruva ellavannu prakata padisalaguva dinada kuritu"},{"SurahNumber":100,"VerseNumber":11,"Text":"andu avara odeyanu khandita avara kuritu ellavannu ballavanagiruvanu"}]}