{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":56,"VerseNumber":39,"Text":"Mange fra tidlige generationerne"},{"SurahNumber":56,"VerseNumber":40,"Text":"Mange fra senere generationerne"},{"SurahNumber":56,"VerseNumber":41,"Text":"De af venstre være forlade"},{"SurahNumber":56,"VerseNumber":42,"Text":"Ind elendighed inferno"},{"SurahNumber":56,"VerseNumber":43,"Text":"Jævne deres shade er hede"},{"SurahNumber":56,"VerseNumber":44,"Text":"Aldrig køle aldrig tolerable"},{"SurahNumber":56,"VerseNumber":45,"Text":"De brugte være rige"},{"SurahNumber":56,"VerseNumber":46,"Text":"De insisterede great blasfemien"},{"SurahNumber":56,"VerseNumber":47,"Text":"De sagde Efter vi omkommer sving støve bones vi får resurrected"},{"SurahNumber":56,"VerseNumber":48,"Text":"Gøre den medta vore forfædre"},{"SurahNumber":56,"VerseNumber":49,"Text":"Sie tidligere generationerne senere generationerne"},{"SurahNumber":56,"VerseNumber":50,"Text":"summoned møde forudbestemte dag"},{"SurahNumber":56,"VerseNumber":51,"Text":"Derefter I O disbelieving strayers"},{"SurahNumber":56,"VerseNumber":52,"Text":"Ædt træerne bitterhed"},{"SurahNumber":56,"VerseNumber":53,"Text":"Fyldende Deres bellies therefrom"},{"SurahNumber":56,"VerseNumber":54,"Text":"Derefter drikkende det hellish drinks"},{"SurahNumber":56,"VerseNumber":55,"Text":"Derefter addere drinks sand"},{"SurahNumber":56,"VerseNumber":56,"Text":"Such er deres dele Dagen Bedømmelse"},{"SurahNumber":56,"VerseNumber":57,"Text":"Vi opretter jer du kun tro"},{"SurahNumber":56,"VerseNumber":58,"Text":"Du noterede semenet den I have producere"},{"SurahNumber":56,"VerseNumber":59,"Text":"Du opretter det gøre gøre vi"},{"SurahNumber":56,"VerseNumber":60,"Text":"Vi forudbestemmer død jer. Intet stoppe os"},{"SurahNumber":56,"VerseNumber":61,"Text":"Fra substituting nye generationer I sted etablerende hvad du ikke kender"},{"SurahNumber":56,"VerseNumber":62,"Text":"Du kender først anlæggelse. Du ikke mindes"},{"SurahNumber":56,"VerseNumber":63,"Text":"Du noterede crops I have reap"},{"SurahNumber":56,"VerseNumber":64,"Text":"Du voksede dem gøre vi"},{"SurahNumber":56,"VerseNumber":65,"Text":"Vi ville vi dreje det hø. Derefter du begræde"},{"SurahNumber":56,"VerseNumber":66,"Text":"Vi tabte"},{"SurahNumber":56,"VerseNumber":67,"Text":"Vi berøves"},{"SurahNumber":56,"VerseNumber":68,"Text":"Du noterede vandet I have drikke"},{"SurahNumber":56,"VerseNumber":69,"Text":"Du sender det derned skyerne gøre gøre vi"},{"SurahNumber":56,"VerseNumber":70,"Text":"Vi ville vi stille det saltholdige. Du være thankful"},{"SurahNumber":56,"VerseNumber":71,"Text":"Du noterede ilden I have ignite"},{"SurahNumber":56,"VerseNumber":72,"Text":"Du initiate dens træ gøre gøre vi"},{"SurahNumber":56,"VerseNumber":73,"Text":"Vi rendered det påmindelse brugbare værktøj brugerne"},{"SurahNumber":56,"VerseNumber":74,"Text":"Du forherlige navnet Deres Lord de Great"}]}