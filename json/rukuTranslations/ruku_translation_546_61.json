{"Id":61,"LanguageCodes":["bs","bos"],"Name":"bos-besimkorkut-la","Author":"Besim Korkut","Language":"Bosnian","Description":"Bosnian - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":104,"VerseNumber":1,"Text":"Tesko svakom klevetniku-podrugljivcu"},{"SurahNumber":104,"VerseNumber":2,"Text":"koji blago gomila i prebrojava ga"},{"SurahNumber":104,"VerseNumber":3,"Text":"i misli da ce ga blago njegovo besmrtnim uciniti"},{"SurahNumber":104,"VerseNumber":4,"Text":"A ne valja tako! On ce, sigurno, biti bacen u Dzehennem"},{"SurahNumber":104,"VerseNumber":5,"Text":"A znas li ti sta je Dzehennem"},{"SurahNumber":104,"VerseNumber":6,"Text":"Vatra Allahova razbuktana"},{"SurahNumber":104,"VerseNumber":7,"Text":"koja ce do srca dopirati"},{"SurahNumber":104,"VerseNumber":8,"Text":"Ona ce iznad njih biti zatvorena"},{"SurahNumber":104,"VerseNumber":9,"Text":"plamenim stupovima zasvoÄ‘ena"}]}