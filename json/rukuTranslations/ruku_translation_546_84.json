{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":104,"VerseNumber":1,"Text":"Ve til enhvere backbiter bagvasker"},{"SurahNumber":104,"VerseNumber":2,"Text":"Han hoards penge tæller det"},{"SurahNumber":104,"VerseNumber":3,"Text":"Idet dersom hans penge stille ham udødelige"},{"SurahNumber":104,"VerseNumber":4,"Text":"Aldrig; han kastes Devastator"},{"SurahNumber":104,"VerseNumber":5,"Text":"Du kender hvad Devastator gøre være"},{"SurahNumber":104,"VerseNumber":6,"Text":"Gud blussende Hellfire"},{"SurahNumber":104,"VerseNumber":7,"Text":"Det forbrænder dem indvendig ydre"},{"SurahNumber":104,"VerseNumber":8,"Text":"De confined therein"},{"SurahNumber":104,"VerseNumber":9,"Text":"Ind traktere kolonner"}]}