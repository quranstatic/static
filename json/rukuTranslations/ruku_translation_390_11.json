{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":37,"VerseNumber":139,"Text":"Wa inna Yoonusa laminal mursaleen"},{"SurahNumber":37,"VerseNumber":140,"Text":"Iz abaqa ilal fulkil mash hoon"},{"SurahNumber":37,"VerseNumber":141,"Text":"Fasaahama fakaana minal mudhadeen"},{"SurahNumber":37,"VerseNumber":142,"Text":"Faltaqamahul hootu wa huwa muleem"},{"SurahNumber":37,"VerseNumber":143,"Text":"Falaw laaa annahoo kaana minal musabbiheen"},{"SurahNumber":37,"VerseNumber":144,"Text":"Lalabisa fee batniheee ilaa Yawmi yub'asoon"},{"SurahNumber":37,"VerseNumber":145,"Text":"Fanabaznaahu bil'araaa'i wa huwa saqeem"},{"SurahNumber":37,"VerseNumber":146,"Text":"Wa ambatnaa 'alaihi shajaratam mai yaqteen"},{"SurahNumber":37,"VerseNumber":147,"Text":"Wa arsalnaahu ilaa mi'ati alfin aw yazeedoon"},{"SurahNumber":37,"VerseNumber":148,"Text":"Fa aamanoo famatta' naahum ilaa heen"},{"SurahNumber":37,"VerseNumber":149,"Text":"Fastaftihim ali Rabbikal banaatu wa lahumul banoon"},{"SurahNumber":37,"VerseNumber":150,"Text":"Am khalaqnal malaaa'i kata inaasanw wa hm shaahidoon"},{"SurahNumber":37,"VerseNumber":151,"Text":"Alaaa innahum min ifkihim la yaqooloon"},{"SurahNumber":37,"VerseNumber":152,"Text":"Waladal laahu wa innhum lakaaziboon"},{"SurahNumber":37,"VerseNumber":153,"Text":"Astafal banaati 'alal baneen"},{"SurahNumber":37,"VerseNumber":154,"Text":"Maa lakum kaifa tahkumoon"},{"SurahNumber":37,"VerseNumber":155,"Text":"Afalaa tazakkaroon"},{"SurahNumber":37,"VerseNumber":156,"Text":"Am lakum sultaanum mubeen"},{"SurahNumber":37,"VerseNumber":157,"Text":"Faatoo bi Kitaabikum in kuntum saadiqeen"},{"SurahNumber":37,"VerseNumber":158,"Text":"Wa ja'aloo bainahoo wa bainal jinnati nasabaa; wa laqad 'alimatil jinnatu innahum lamuhdaroon"},{"SurahNumber":37,"VerseNumber":159,"Text":"Subhaanal laahi 'ammaa yasifoon"},{"SurahNumber":37,"VerseNumber":160,"Text":"Illaa 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":161,"Text":"Fa innakum wa maa ta'ubdoon"},{"SurahNumber":37,"VerseNumber":162,"Text":"Maaa antum 'alaihi befaaatineen"},{"SurahNumber":37,"VerseNumber":163,"Text":"Illaa man huwa saalil jaheem"},{"SurahNumber":37,"VerseNumber":164,"Text":"Wa maa minnasa illaa lahoo maqaamum ma'loom"},{"SurahNumber":37,"VerseNumber":165,"Text":"Wa innaa llanah nus saaffoon"},{"SurahNumber":37,"VerseNumber":166,"Text":"Wa innaa lanah nul musabbihoon"},{"SurahNumber":37,"VerseNumber":167,"Text":"Wa in kaanoo la yaqooloon"},{"SurahNumber":37,"VerseNumber":168,"Text":"Law anna 'indana zikram minal awwaleen"},{"SurahNumber":37,"VerseNumber":169,"Text":"Lakunna 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":170,"Text":"Fakafaroo bihee fasawfa ya'lamoon"},{"SurahNumber":37,"VerseNumber":171,"Text":"Wa laqad sabaqat Kalimatunaa li'ibaadinal mursa leen"},{"SurahNumber":37,"VerseNumber":172,"Text":"Innaa hum lahumul mansooroon"},{"SurahNumber":37,"VerseNumber":173,"Text":"Wa inna jundana lahumul ghaaliboon"},{"SurahNumber":37,"VerseNumber":174,"Text":"Fatawalla 'anhum hatta heen"},{"SurahNumber":37,"VerseNumber":175,"Text":"Wa absirhum fasawfa yubsiroon"},{"SurahNumber":37,"VerseNumber":176,"Text":"Afabi'azaabinaa yasta'jiloon"},{"SurahNumber":37,"VerseNumber":177,"Text":"Fa izaa nazala bisaahatihim fasaaa'a sabaahul munzareen"},{"SurahNumber":37,"VerseNumber":178,"Text":"Wa tawalla 'anhum hattaa heen"},{"SurahNumber":37,"VerseNumber":179,"Text":"Wa absir fasawfa yubsiroon"},{"SurahNumber":37,"VerseNumber":180,"Text":"Subhaana Rabbika Rabbil 'izzati 'amma yasifoon"},{"SurahNumber":37,"VerseNumber":181,"Text":"Wa salaamun 'alalmursaleen"},{"SurahNumber":37,"VerseNumber":182,"Text":"Walhamdu lillaahi Rabbil 'aalameen"}]}