{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"Izas samaaa'un fatarat"},{"SurahNumber":82,"VerseNumber":2,"Text":"Wa izal kawaakibun tasarat"},{"SurahNumber":82,"VerseNumber":3,"Text":"Wa izal bihaaru fujjirat"},{"SurahNumber":82,"VerseNumber":4,"Text":"Wa izal qubooru bu'sirat"},{"SurahNumber":82,"VerseNumber":5,"Text":"Alimat nafsum maa qaddamat wa akhkharat"},{"SurahNumber":82,"VerseNumber":6,"Text":"Yaaa ayyuhal insaaanu maa gharraka bi Rabbikal kareem"},{"SurahNumber":82,"VerseNumber":7,"Text":"Allazee khalaqaka fasaw waaka fa'adalak"},{"SurahNumber":82,"VerseNumber":8,"Text":"Feee ayye sooratim maa shaaa'a rakkabak"},{"SurahNumber":82,"VerseNumber":9,"Text":"Kalla bal tukazziboona bid deen"},{"SurahNumber":82,"VerseNumber":10,"Text":"Wa inna 'alaikum lahaa fizeen"},{"SurahNumber":82,"VerseNumber":11,"Text":"Kiraaman kaatibeen"},{"SurahNumber":82,"VerseNumber":12,"Text":"Ya'lamoona ma taf'aloon"},{"SurahNumber":82,"VerseNumber":13,"Text":"Innal abraara lafee na'eem"},{"SurahNumber":82,"VerseNumber":14,"Text":"Wa innal fujjaara lafee jaheem"},{"SurahNumber":82,"VerseNumber":15,"Text":"Yaslawnahaa Yawmad Deen"},{"SurahNumber":82,"VerseNumber":16,"Text":"Wa maa hum 'anhaa bighaaa 'ibeen"},{"SurahNumber":82,"VerseNumber":17,"Text":"Wa maaa adraaka maa Yawmud Deen"},{"SurahNumber":82,"VerseNumber":18,"Text":"Summa maaa adraaka maa Yawmud Deen"},{"SurahNumber":82,"VerseNumber":19,"Text":"Yawma laa tamliku nafsul linafsin shai'anw walamru yawma'izil lillaah"}]}