{"Id":419,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":26,"VerseNumber":70,"Text":"iẕ ḳâle liebîhi veḳavmihî mâ ta`büdûn"},{"SurahNumber":26,"VerseNumber":71,"Text":"ḳâlû na`büdü aṣnâmen feneżallü lehâ `âkifîn"},{"SurahNumber":26,"VerseNumber":72,"Text":"ḳâle hel yesme`ûneküm iẕ ted`ûn"},{"SurahNumber":26,"VerseNumber":73,"Text":"ev yenfe`ûneküm ev yeḍurrûn"},{"SurahNumber":26,"VerseNumber":74,"Text":"ḳâlû bel vecednâ âbâenâ keẕâlike yef`alûn"},{"SurahNumber":26,"VerseNumber":75,"Text":"ḳâle eferaeytüm mâ küntüm ta`büdûn"},{"SurahNumber":26,"VerseNumber":76,"Text":"entüm veâbâükümü-l'aḳdemûn"},{"SurahNumber":26,"VerseNumber":77,"Text":"feinnehüm `adüvvül lî illâ rabbe-l`âlemîn"},{"SurahNumber":26,"VerseNumber":78,"Text":"elleẕî ḫaleḳanî fehüve yehdîn"},{"SurahNumber":26,"VerseNumber":79,"Text":"velleẕî hüve yuṭ`imünî veyesḳîn"},{"SurahNumber":26,"VerseNumber":80,"Text":"veiẕâ meriḍtü fehüve yeşfîn"},{"SurahNumber":26,"VerseNumber":81,"Text":"velleẕî yümîtünî ŝümme yuḥyîn"},{"SurahNumber":26,"VerseNumber":82,"Text":"velleẕî aṭme`u ey yagfira lî ḫaṭîetî yevme-ddîn"},{"SurahNumber":26,"VerseNumber":83,"Text":"rabbi heb lî ḥukmev veelḥiḳnî biṣṣâliḥîn"},{"SurahNumber":26,"VerseNumber":84,"Text":"vec`al lî lisâne ṣidḳin fi-l'âḫirîn"},{"SurahNumber":26,"VerseNumber":85,"Text":"vec`alnî miv veraŝeti cenneti-nne`îm"},{"SurahNumber":26,"VerseNumber":86,"Text":"vagfir liebî innehû kâne mine-ḍḍâllîn"},{"SurahNumber":26,"VerseNumber":87,"Text":"velâ tuḫzinî yevme yüb`aŝûn"},{"SurahNumber":26,"VerseNumber":88,"Text":"yevme lâ yenfe`u mâlüv velâ benûn"},{"SurahNumber":26,"VerseNumber":89,"Text":"illâ men ete-llâhe biḳalbin selîm"},{"SurahNumber":26,"VerseNumber":90,"Text":"veüzlifeti-lcennetü lilmütteḳîn"},{"SurahNumber":26,"VerseNumber":91,"Text":"vebürrizeti-lceḥîmü lilgâvîn"},{"SurahNumber":26,"VerseNumber":92,"Text":"veḳîle lehüm eyne mâ küntüm ta`büdûn"},{"SurahNumber":26,"VerseNumber":93,"Text":"min dûni-llâh. hel yenṣurûneküm ev yenteṣirûn"},{"SurahNumber":26,"VerseNumber":94,"Text":"fekübkibû fîhâ hüm velgâvûn"},{"SurahNumber":26,"VerseNumber":95,"Text":"vecünûdü iblîse ecme`ûn"},{"SurahNumber":26,"VerseNumber":96,"Text":"ḳâlû vehüm fîhâ yaḫteṣimûn"},{"SurahNumber":26,"VerseNumber":97,"Text":"tellâhi in künnâ lefî ḍalâlim mübîn"},{"SurahNumber":26,"VerseNumber":98,"Text":"iẕ nüsevvîküm birabbi-l`âlemîn"},{"SurahNumber":26,"VerseNumber":99,"Text":"vemâ eḍallenâ ille-lmücrimûn"},{"SurahNumber":26,"VerseNumber":100,"Text":"femâ lenâ min şâfi`în"},{"SurahNumber":26,"VerseNumber":101,"Text":"velâ ṣadîḳin ḥamîm"},{"SurahNumber":26,"VerseNumber":102,"Text":"felev enne lenâ kerraten fenekûne mine-lmü'minîn"},{"SurahNumber":26,"VerseNumber":103,"Text":"inne fî ẕâlike leâyeh. vemâ kâne ekŝeruhüm mü'minîn"},{"SurahNumber":26,"VerseNumber":104,"Text":"veinne rabbeke lehüve-l`azîzü-rraḥîm"}]}