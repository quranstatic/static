{"Id":285,"LanguageCodes":["pt","por"],"Name":"por-samirelhayek-la","Author":"Samir El Hayek","Language":"Portuguese","Description":"Portuguese - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":102,"VerseNumber":1,"Text":"A cobica vos entretera"},{"SurahNumber":102,"VerseNumber":2,"Text":"Ate que descais aos sepulcros"},{"SurahNumber":102,"VerseNumber":3,"Text":"Qual! Logo o sabereis"},{"SurahNumber":102,"VerseNumber":4,"Text":"Novamente, qual! Logo o sabereis"},{"SurahNumber":102,"VerseNumber":5,"Text":"Qual! Se soubesseis da ciencia certa"},{"SurahNumber":102,"VerseNumber":6,"Text":"Verdadeiramente, entao, havieis de ver a fogueira do inferno"},{"SurahNumber":102,"VerseNumber":7,"Text":"Logo a vereis claramente"},{"SurahNumber":102,"VerseNumber":8,"Text":"Entao, sereis interrogados, nesse dia, a respeito dos prazeres (mundanos)"}]}