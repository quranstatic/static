{"Id":414,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet1-la","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":91,"VerseNumber":1,"Text":"Ves semsi ve duhaha"},{"SurahNumber":91,"VerseNumber":2,"Text":"Vel kameri iza telaha"},{"SurahNumber":91,"VerseNumber":3,"Text":"Ven nehari iza cellaha"},{"SurahNumber":91,"VerseNumber":4,"Text":"Vel leyli iza yagsaha"},{"SurahNumber":91,"VerseNumber":5,"Text":"Ves semai ve ma benaha"},{"SurahNumber":91,"VerseNumber":6,"Text":"Vel ardÄ± ve ma tahaha"},{"SurahNumber":91,"VerseNumber":7,"Text":"Ve nefsin ve ma sevvaha"},{"SurahNumber":91,"VerseNumber":8,"Text":"Fe elhemeha fucureha ve takvaha"},{"SurahNumber":91,"VerseNumber":9,"Text":"Kad efleha men zekkaha"},{"SurahNumber":91,"VerseNumber":10,"Text":"Ve kad habe men dessaha"},{"SurahNumber":91,"VerseNumber":11,"Text":"Kezzebet semudu bi tagvaha"},{"SurahNumber":91,"VerseNumber":12,"Text":"Izin baase eskaha"},{"SurahNumber":91,"VerseNumber":13,"Text":"Fe kale lehum resulullahi nakatallahi ve sukyaha"},{"SurahNumber":91,"VerseNumber":14,"Text":"Fe kezzebuhu fe akaruha fe demdeme aleyhim rabbuhum bi zenbihim fe sevvaha"},{"SurahNumber":91,"VerseNumber":15,"Text":"Ve la yehafu ukbaha"}]}