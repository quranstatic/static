{"Id":61,"LanguageCodes":["bs","bos"],"Name":"bos-besimkorkut-la","Author":"Besim Korkut","Language":"Bosnian","Description":"Bosnian - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"Smak svijeta"},{"SurahNumber":101,"VerseNumber":2,"Text":"Sta je Smak svijeta"},{"SurahNumber":101,"VerseNumber":3,"Text":"i sta ti znas o Smaku svijeta"},{"SurahNumber":101,"VerseNumber":4,"Text":"Toga dana ce ljudi biti kao leptiri rastrkani"},{"SurahNumber":101,"VerseNumber":5,"Text":"a planine kao sarena vuna iscupana"},{"SurahNumber":101,"VerseNumber":6,"Text":"Onaj u koga njegova djela budu teska –"},{"SurahNumber":101,"VerseNumber":7,"Text":"u ugodnu zivotu ce zivjeti"},{"SurahNumber":101,"VerseNumber":8,"Text":"a onaj u koga njegova djela budu lahka –"},{"SurahNumber":101,"VerseNumber":9,"Text":"boraviste ce mu bezdan biti"},{"SurahNumber":101,"VerseNumber":10,"Text":"A znas li ti sta ce to biti? –"},{"SurahNumber":101,"VerseNumber":11,"Text":"Vatra uzarena"}]}