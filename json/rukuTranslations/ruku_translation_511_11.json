{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":74,"VerseNumber":32,"Text":"Kallaa walqamar"},{"SurahNumber":74,"VerseNumber":33,"Text":"Wallaili adbar"},{"SurahNumber":74,"VerseNumber":34,"Text":"Wassub hi izaaa asfar"},{"SurahNumber":74,"VerseNumber":35,"Text":"Innahaa la ihdal kubar"},{"SurahNumber":74,"VerseNumber":36,"Text":"Nazeeral lilbashar"},{"SurahNumber":74,"VerseNumber":37,"Text":"Liman shaaa'a minkum any yataqaddama aw yata akhkhar"},{"SurahNumber":74,"VerseNumber":38,"Text":"Kullu nafsim bim kasabat raheenah"},{"SurahNumber":74,"VerseNumber":39,"Text":"Illaaa as haabal yameen"},{"SurahNumber":74,"VerseNumber":40,"Text":"Fee jannaatiny yata saaa'aloon"},{"SurahNumber":74,"VerseNumber":41,"Text":"Anil mujrimeen"},{"SurahNumber":74,"VerseNumber":42,"Text":"Maa salakakum fee saqar"},{"SurahNumber":74,"VerseNumber":43,"Text":"Qaaloo lam naku minal musalleen"},{"SurahNumber":74,"VerseNumber":44,"Text":"Wa lam naku nut'imul miskeen"},{"SurahNumber":74,"VerseNumber":45,"Text":"Wa kunnaa nakhoodu ma'al khaaa'ideen"},{"SurahNumber":74,"VerseNumber":46,"Text":"Wa kunnaa nukazzibu bi yawmid Deen"},{"SurahNumber":74,"VerseNumber":47,"Text":"Hattaaa ataanal yaqeen"},{"SurahNumber":74,"VerseNumber":48,"Text":"Famaa tanfa'uhum shafaa'atush shaafi'een"},{"SurahNumber":74,"VerseNumber":49,"Text":"Famaa lahum 'anittazkirati mu'rideen"},{"SurahNumber":74,"VerseNumber":50,"Text":"Ka annahum humurum mustanfirah"},{"SurahNumber":74,"VerseNumber":51,"Text":"Farrat min qaswarah"},{"SurahNumber":74,"VerseNumber":52,"Text":"Bal yureedu kullum ri'im minhum any yu'taa suhufam munashsharah"},{"SurahNumber":74,"VerseNumber":53,"Text":"Kallaa bal laa yakhaafoonal aakhirah"},{"SurahNumber":74,"VerseNumber":54,"Text":"Kallaaa innahoo tazkirah"},{"SurahNumber":74,"VerseNumber":55,"Text":"Fa man shaaa'a zakarah"},{"SurahNumber":74,"VerseNumber":56,"Text":"Wa maa yazkuroona illaaa any yashaaa'al laah; Huwa ahlut taqwaa wa ahlul maghfirah"}]}