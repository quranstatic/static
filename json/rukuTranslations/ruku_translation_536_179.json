{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":94,"VerseNumber":1,"Text":"Mbela Min njaacnaani ma ɓernde ma"},{"SurahNumber":94,"VerseNumber":2,"Text":"Min ittan ma bakkatuuji ma"},{"SurahNumber":94,"VerseNumber":3,"Text":"Oon nisnuɗo keeci ma"},{"SurahNumber":94,"VerseNumber":4,"Text":"Min ɓamtan ma Innde ma"},{"SurahNumber":94,"VerseNumber":5,"Text":"Pellet kala satteende na wondi e newaare"},{"SurahNumber":94,"VerseNumber":6,"Text":"Pellet satteende kala na wondi e yaajeende"},{"SurahNumber":94,"VerseNumber":7,"Text":"Si A hantinii tampu"},{"SurahNumber":94,"VerseNumber":8,"Text":"E faade joom Ma reerɗu"}]}