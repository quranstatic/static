{"Id":68,"LanguageCodes":["bs","bos"],"Name":"bos-mustafamlivo","Author":"Mustafa Mlivo","Language":"Bosnian","Description":"Bosnian","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":75,"VerseNumber":31,"Text":"Pa nije vjerovao, ni salat obavljao"},{"SurahNumber":75,"VerseNumber":32,"Text":"Nego je poricao i okretao se"},{"SurahNumber":75,"VerseNumber":33,"Text":"Zatim odlazio porodici svojoj kočoperno"},{"SurahNumber":75,"VerseNumber":34,"Text":"Teško tebi i teško"},{"SurahNumber":75,"VerseNumber":35,"Text":"Ponovo, teško tebi i teško"},{"SurahNumber":75,"VerseNumber":36,"Text":"Misli li čovjek da će se ostaviti besciljnim"},{"SurahNumber":75,"VerseNumber":37,"Text":"Zar nije bio kap sperme izbačene"},{"SurahNumber":75,"VerseNumber":38,"Text":"Zatim je bio zakvačak - pa je stvarao, te usavršio"},{"SurahNumber":75,"VerseNumber":39,"Text":"Pa načinio od njega par - muško i žensko"},{"SurahNumber":75,"VerseNumber":40,"Text":"Zar nije Taj kadar da oživi mrtve"}]}