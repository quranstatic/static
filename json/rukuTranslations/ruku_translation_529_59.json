{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":87,"VerseNumber":1,"Text":"Sbuccaa ism n Mass ik, Aalayan"},{"SurahNumber":87,"VerseNumber":2,"Text":"Ixelqen, Issemsaway"},{"SurahNumber":87,"VerseNumber":3,"Text":"Iwezznen, Inhan"},{"SurahNumber":87,"VerseNumber":4,"Text":"I d Issuff$en takessawt"},{"SurahNumber":87,"VerseNumber":5,"Text":"sinna, Ippara p d asa$uo aberkan"},{"SurahNumber":87,"VerseNumber":6,"Text":"A k Ness$eo, ur teppu"},{"SurahNumber":87,"VerseNumber":7,"Text":"siwa ayen Irad Oebbi. Ih, Neppa, Issen ayen ibanen, akked wayen iffren"},{"SurahNumber":87,"VerseNumber":8,"Text":"A k Nawi ar liser"},{"SurahNumber":87,"VerseNumber":9,"Text":"Smmekti mi infaa usmekti"},{"SurahNumber":87,"VerseNumber":10,"Text":"A d ismmekti win iugaden"},{"SurahNumber":87,"VerseNumber":11,"Text":"Ageswae ijuneb it"},{"SurahNumber":87,"VerseNumber":12,"Text":"Di tmes tabarart mavi ara io"},{"SurahNumber":87,"VerseNumber":13,"Text":"Degs, ur ipmeppat, ur ippidir"},{"SurahNumber":87,"VerseNumber":14,"Text":"Amarezg n win zeddigen"},{"SurahNumber":87,"VerseNumber":15,"Text":"I ismektayen ism n Mass is, d$a ipealla"},{"SurahNumber":87,"VerseNumber":16,"Text":"Maca, tesmenyafem tameddurt n ddunit"},{"SurahNumber":87,"VerseNumber":17,"Text":"Tif ip laxeot, a pdum kteo"},{"SurahNumber":87,"VerseNumber":18,"Text":"Ayagi, illa di tewoeqtin timezwura"},{"SurahNumber":87,"VerseNumber":19,"Text":"Tiwoeqtin n Iboahim akked Musa"}]}