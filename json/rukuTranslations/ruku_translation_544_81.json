{"Id":81,"LanguageCodes":["cs","ces"],"Name":"ces-prekladihrbek-la","Author":"Preklad I. Hrbek","Language":"Czech","Description":"Czech - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":102,"VerseNumber":1,"Text":"Vy radi v rozmnozovani jmeni souperite"},{"SurahNumber":102,"VerseNumber":2,"Text":"pokud hroby nenavstivite"},{"SurahNumber":102,"VerseNumber":3,"Text":"Vsak pozor! Brzy poznate"},{"SurahNumber":102,"VerseNumber":4,"Text":"A znovu pozor! Brzy poznate"},{"SurahNumber":102,"VerseNumber":5,"Text":"Vsak pozor! Kez zretelnym poznanim poznate"},{"SurahNumber":102,"VerseNumber":6,"Text":"a veru ohen planouci uzrite"},{"SurahNumber":102,"VerseNumber":7,"Text":"a potom vskutku okem jistym jej spatrite"},{"SurahNumber":102,"VerseNumber":8,"Text":"a posleze v ten den na blahobyt tazani budete"}]}