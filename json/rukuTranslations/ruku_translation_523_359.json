{"Id":359,"LanguageCodes":["te","tel"],"Name":"tel-abdulraheemmoha-la","Author":"Abdul Raheem Mohammad Moulana","Language":"Telugu","Description":"Telugu - Transliteration","Direction":"ltr","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":81,"VerseNumber":1,"Text":"suryudu (andhakaranlo) cutti poyabadi kantihinudainappudu"},{"SurahNumber":81,"VerseNumber":2,"Text":"mariyu naksatralu (kantini kolpoyi) ralipovunappudu"},{"SurahNumber":81,"VerseNumber":3,"Text":"mariyu parvatalu kadilincabadinappudu"},{"SurahNumber":81,"VerseNumber":4,"Text":"mariyu nindu sudi ontelu, nirapeksanga vadiliveyabadinappudu"},{"SurahNumber":81,"VerseNumber":5,"Text":"Mariyu kruramrgalanni okaceta samakurcabadinappudu"},{"SurahNumber":81,"VerseNumber":6,"Text":"mariyu samudralu uppongipoyi nappudu"},{"SurahNumber":81,"VerseNumber":7,"Text":"mariyu atmalu (sariralato) tirigi kalupabadi nappudu"},{"SurahNumber":81,"VerseNumber":8,"Text":"mariyu sajivanga pati pettabadina balika prasnincabadinappudu"},{"SurahNumber":81,"VerseNumber":9,"Text":"e aparadhaniki tanu hatya ceyabadindani"},{"SurahNumber":81,"VerseNumber":10,"Text":"mariyu karmapatralu teruvabadinappudu"},{"SurahNumber":81,"VerseNumber":11,"Text":"mariyu akasam olici veyabadinappudu"},{"SurahNumber":81,"VerseNumber":12,"Text":"mariyu narakagni mandincabadinappudu"},{"SurahNumber":81,"VerseNumber":13,"Text":"mariyu svargam daggaraku tisukurabadinappudu"},{"SurahNumber":81,"VerseNumber":14,"Text":"prati atma tanu cesi teccina karmalanu telusukuntundi"},{"SurahNumber":81,"VerseNumber":15,"Text":"ala kadu! Nenu tolagipoye naksatrala saksiga cebutunnanu"},{"SurahNumber":81,"VerseNumber":16,"Text":"(evaite) veganga tirugutu kanumarugavutunnayo"},{"SurahNumber":81,"VerseNumber":17,"Text":"mariyu gadaci poye ratri saksiga"},{"SurahNumber":81,"VerseNumber":18,"Text":"mariyu prakasince udayam saksiga"},{"SurahNumber":81,"VerseNumber":19,"Text":"niscayanga, idi (i khur'an) gauravaniyudaina sandesaharudu teccina vakku"},{"SurahNumber":81,"VerseNumber":20,"Text":"atanu (jibril) maha balasali, sinhasana (ars) adhipati sannidhilo unnata sthanam galavadu"},{"SurahNumber":81,"VerseNumber":21,"Text":"atani ajnalu patimpabadatayi mariyu (atanu) visvasaniyudu"},{"SurahNumber":81,"VerseNumber":22,"Text":"mariyu (o prajalara!) Mi sahacarudu piccivadu kadu"},{"SurahNumber":81,"VerseNumber":23,"Text":"mariyu vastavanga, atanu a sandesaharunni (jibril nu) prakasavantamaina dinmandalanlo cusadu"},{"SurahNumber":81,"VerseNumber":24,"Text":"mariyu atanu (muham'mad) agocara jnananni prajala nundi dacevadu kadu"},{"SurahNumber":81,"VerseNumber":25,"Text":"mariyu idi (i khur'an) sapinca (bahiskarinca) badina saitan vakku kadu"},{"SurahNumber":81,"VerseNumber":26,"Text":"mari miru etu potunnaru"},{"SurahNumber":81,"VerseNumber":27,"Text":"idi (i khur'an) sarvalokalaku oka hitopadesam"},{"SurahNumber":81,"VerseNumber":28,"Text":"milo, rjumarganlo nadava dalacukunna prativani koraku"},{"SurahNumber":81,"VerseNumber":29,"Text":"mariyu sarvalokala prabhuvaina allah talacananta varaku, miru talacinanta matrana emi kadu"}]}