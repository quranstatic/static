{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":68,"VerseNumber":1,"Text":"Noon; walqalami wa maa yasturoon"},{"SurahNumber":68,"VerseNumber":2,"Text":"Maa anta bini'mati Rabbika bimajnoon"},{"SurahNumber":68,"VerseNumber":3,"Text":"Wa inna laka la ajran ghaira mamnoon"},{"SurahNumber":68,"VerseNumber":4,"Text":"Wa innaka la'alaa khuluqin 'azeem"},{"SurahNumber":68,"VerseNumber":5,"Text":"Fasatubsiru wa yubsiroon"},{"SurahNumber":68,"VerseNumber":6,"Text":"Bi ayyikumul maftoon"},{"SurahNumber":68,"VerseNumber":7,"Text":"Innaa Rabbaka Huwa a'lamu biman dalla 'an sabeelihee wa Huwa a'lamu bilmuhtadeen"},{"SurahNumber":68,"VerseNumber":8,"Text":"Falaa tuti'il mukazzibeen"},{"SurahNumber":68,"VerseNumber":9,"Text":"Waddoo law tudhinu fa-yudhinoon"},{"SurahNumber":68,"VerseNumber":10,"Text":"Wa laa tuti' kulla hallaa fim maheen"},{"SurahNumber":68,"VerseNumber":11,"Text":"Hammaazim mash shaaa'im binameem"},{"SurahNumber":68,"VerseNumber":12,"Text":"Mannaa'il lilkhairi mu'tadin aseem"},{"SurahNumber":68,"VerseNumber":13,"Text":"Utullim ba'da zaalika zaneem"},{"SurahNumber":68,"VerseNumber":14,"Text":"An kaana zaa maalinw-wa baneen"},{"SurahNumber":68,"VerseNumber":15,"Text":"Izaa tutlaa 'alaihi aayaatunaa qaala asaateerul awwaleen"},{"SurahNumber":68,"VerseNumber":16,"Text":"Sanasimuhoo 'alal khurtoom"},{"SurahNumber":68,"VerseNumber":17,"Text":"Innaa balawnaahum kamaa balawnaaa As-haabal jannati iz 'aqsamoo la-yasri munnahaa musbiheen"},{"SurahNumber":68,"VerseNumber":18,"Text":"Wa laa yastasnoon"},{"SurahNumber":68,"VerseNumber":19,"Text":"Fataafa 'alaihaa taaa'i fum mir rabbika wa hum naaa'imoon"},{"SurahNumber":68,"VerseNumber":20,"Text":"Fa asbahat kassareem"},{"SurahNumber":68,"VerseNumber":21,"Text":"Fatanaadaw musbiheen"},{"SurahNumber":68,"VerseNumber":22,"Text":"Anighdoo 'alaa harsikum in kuntum saarimeen"},{"SurahNumber":68,"VerseNumber":23,"Text":"Fantalaqoo wa hum yatakhaafatoon"},{"SurahNumber":68,"VerseNumber":24,"Text":"Al laa yadkhulannahal yawma 'alaikum miskeen"},{"SurahNumber":68,"VerseNumber":25,"Text":"Wa ghadaw 'alaa hardin qaadireen"},{"SurahNumber":68,"VerseNumber":26,"Text":"Falammaa ra awhaa qaalooo innaa ladaaalloon"},{"SurahNumber":68,"VerseNumber":27,"Text":"Bal nahnu mahroomoon"},{"SurahNumber":68,"VerseNumber":28,"Text":"Qaala awsatuhum alam aqul lakum law laa tusabbihoon"},{"SurahNumber":68,"VerseNumber":29,"Text":"Qaaloo subhaana rabbinaaa innaa kunnaa zaalimeen"},{"SurahNumber":68,"VerseNumber":30,"Text":"Fa aqbala ba'duhum 'alaa ba'diny yatalaawamoon"},{"SurahNumber":68,"VerseNumber":31,"Text":"Qaaloo yaa wailanaaa innaa kunnaa taagheen"},{"SurahNumber":68,"VerseNumber":32,"Text":"Asaa rabbunaaa any yubdilanaa khairam minhaaa innaaa ilaa rabbinaa raaghiboon"},{"SurahNumber":68,"VerseNumber":33,"Text":"Kazaalikal azaab, wa la'azaabul aakhirati akbar; law kaanoo ya'lamoon"}]}