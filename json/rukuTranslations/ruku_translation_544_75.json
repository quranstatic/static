{"Id":75,"LanguageCodes":["ca","cat"],"Name":"cat-yousseflyoussi-la","Author":"Youssef Lyoussi","Language":"Catalan","Description":"Catalan - Transliteration","Direction":"ltr","Source":"","Translations":[{"SurahNumber":102,"VerseNumber":1,"Text":"L'afany de lucre us distreu"},{"SurahNumber":102,"VerseNumber":2,"Text":"fins que visitareu les tombas"},{"SurahNumber":102,"VerseNumber":3,"Text":"No! Ja veureu"},{"SurahNumber":102,"VerseNumber":4,"Text":"No i no! Ja veureu"},{"SurahNumber":102,"VerseNumber":5,"Text":"No! Si sabessiu amb certesa"},{"SurahNumber":102,"VerseNumber":6,"Text":"Veureu, de segur, el Jaheem"},{"SurahNumber":102,"VerseNumber":7,"Text":"Si, ho veureu amb ulls de certesa"},{"SurahNumber":102,"VerseNumber":8,"Text":"Despres, aquest dia, se us preguntara, certament, per la delicia"}]}