{"Id":285,"LanguageCodes":["pt","por"],"Name":"por-samirelhayek-la","Author":"Samir El Hayek","Language":"Portuguese","Description":"Portuguese - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":107,"VerseNumber":1,"Text":"Tens reparado em quem nega a religiao"},{"SurahNumber":107,"VerseNumber":2,"Text":"E quem repele o orfao"},{"SurahNumber":107,"VerseNumber":3,"Text":"E nao estimula (os demais) a alimentacao dos necessitados"},{"SurahNumber":107,"VerseNumber":4,"Text":"Ai, pois, dos praticantes das oracoes"},{"SurahNumber":107,"VerseNumber":5,"Text":"Que sao negligentes em suas oracoes"},{"SurahNumber":107,"VerseNumber":6,"Text":"Que as fazem por ostentacao"},{"SurahNumber":107,"VerseNumber":7,"Text":"Negando-se, contudo, a prestar obsequios"}]}