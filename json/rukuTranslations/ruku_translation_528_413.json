{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":86,"VerseNumber":1,"Text":"Vessemai vettarikı"},{"SurahNumber":86,"VerseNumber":2,"Text":"Ve ma edrake mettariku"},{"SurahNumber":86,"VerseNumber":3,"Text":"Ennecmüssakıbü"},{"SurahNumber":86,"VerseNumber":4,"Text":"İn küllü nefsin lemma ´aleyha hafizun"},{"SurahNumber":86,"VerseNumber":5,"Text":"Felyenzuril´insanü mimme hulika"},{"SurahNumber":86,"VerseNumber":6,"Text":"Hulika min main dafikın"},{"SurahNumber":86,"VerseNumber":7,"Text":"Yahrücü min beynissulbi vetteraibi"},{"SurahNumber":86,"VerseNumber":8,"Text":"İnnehu ´ala rec´ıhı lekadirün"},{"SurahNumber":86,"VerseNumber":9,"Text":"Yevme tüblesserairü"},{"SurahNumber":86,"VerseNumber":10,"Text":"Fema lehu min kuvvetin ve la nasırin"},{"SurahNumber":86,"VerseNumber":11,"Text":"Vessemai zatirrec´ı"},{"SurahNumber":86,"VerseNumber":12,"Text":"Vel´ardı zatissad´ı"},{"SurahNumber":86,"VerseNumber":13,"Text":"İnnehu likavlün faslün"},{"SurahNumber":86,"VerseNumber":14,"Text":"Ve ma hüve bilhezli"},{"SurahNumber":86,"VerseNumber":15,"Text":"İnnehüm yekiydune keyden"},{"SurahNumber":86,"VerseNumber":16,"Text":"Ve ekiydü keyden"},{"SurahNumber":86,"VerseNumber":17,"Text":"Femehhililkafiriyne emhilhüm rüveyden"}]}