{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":53,"VerseNumber":33,"Text":"افرءيت الذي تولى"},{"SurahNumber":53,"VerseNumber":34,"Text":"واعطى قليل ا واكدى"},{"SurahNumber":53,"VerseNumber":35,"Text":"اعنده علم الغيب فهو يرى"},{"SurahNumber":53,"VerseNumber":36,"Text":"ام لم ينبا بما في صحف موسى"},{"SurahNumber":53,"VerseNumber":37,"Text":"وابرهيم الذي وفى"},{"SurahNumber":53,"VerseNumber":38,"Text":"الا تزر وازرة وزر اخرى"},{"SurahNumber":53,"VerseNumber":39,"Text":"وان ليس للانسن الا ما سعى"},{"SurahNumber":53,"VerseNumber":40,"Text":"وان سعيه سوف يرى"},{"SurahNumber":53,"VerseNumber":41,"Text":"ثم يجزىه الجزاء الاوفى"},{"SurahNumber":53,"VerseNumber":42,"Text":"وان الى ربك المنتهى"},{"SurahNumber":53,"VerseNumber":43,"Text":"وانه هو اضحك وابكى"},{"SurahNumber":53,"VerseNumber":44,"Text":"وانه هو امات واحيا"},{"SurahNumber":53,"VerseNumber":45,"Text":"وانه خلق الزوجين الذكر والانثى"},{"SurahNumber":53,"VerseNumber":46,"Text":"من نطفة اذا تمنى"},{"SurahNumber":53,"VerseNumber":47,"Text":"وان عليه النشاة الاخرى"},{"SurahNumber":53,"VerseNumber":48,"Text":"وانه هو اغنى واقنى"},{"SurahNumber":53,"VerseNumber":49,"Text":"وانه هو رب الشعرى"},{"SurahNumber":53,"VerseNumber":50,"Text":"وانه اهلك عادا الاولى"},{"SurahNumber":53,"VerseNumber":51,"Text":"وثمودا فما ابقى"},{"SurahNumber":53,"VerseNumber":52,"Text":"وقوم نوح من قبل انهم كانوا هم اظلم واطغى"},{"SurahNumber":53,"VerseNumber":53,"Text":"والموتفكة اهوى"},{"SurahNumber":53,"VerseNumber":54,"Text":"فغشىها ما غشى"},{"SurahNumber":53,"VerseNumber":55,"Text":"فباي ءالاء ربك تتمارى"},{"SurahNumber":53,"VerseNumber":56,"Text":"هذا نذير من النذر الاولى"},{"SurahNumber":53,"VerseNumber":57,"Text":"ازفت الازفة"},{"SurahNumber":53,"VerseNumber":58,"Text":"ليس لها من دون الله كاشفة"},{"SurahNumber":53,"VerseNumber":59,"Text":"افمن هذا الحديث تعجبون"},{"SurahNumber":53,"VerseNumber":60,"Text":"وتضحكون ولا تبكون"},{"SurahNumber":53,"VerseNumber":61,"Text":"وانتم سمدون"},{"SurahNumber":53,"VerseNumber":62,"Text":"فاسجدوا لله واعبدوا۩"}]}