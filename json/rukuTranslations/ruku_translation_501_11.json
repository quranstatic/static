{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":69,"VerseNumber":38,"Text":"Falaaa uqsimu bimaa tubsiroon"},{"SurahNumber":69,"VerseNumber":39,"Text":"Wa maa laa tubsiroon"},{"SurahNumber":69,"VerseNumber":40,"Text":"Innahoo laqawlu Rasoolin kareem"},{"SurahNumber":69,"VerseNumber":41,"Text":"Wa ma huwa biqawli shaa'ir; qaleelam maa tu'minoon"},{"SurahNumber":69,"VerseNumber":42,"Text":"Wa laa biqawli kaahin; qaleelam maa tazakkaroon"},{"SurahNumber":69,"VerseNumber":43,"Text":"Tanzeelum mir rabbil 'aalameen"},{"SurahNumber":69,"VerseNumber":44,"Text":"Wa law taqawwala 'alainaa ba'dal aqaaweel"},{"SurahNumber":69,"VerseNumber":45,"Text":"La-akhaznaa minhu bilyameen"},{"SurahNumber":69,"VerseNumber":46,"Text":"Summa laqata'naa minhul wateen"},{"SurahNumber":69,"VerseNumber":47,"Text":"Famaa minkum min ahadin'anhu haajizeen"},{"SurahNumber":69,"VerseNumber":48,"Text":"Wa innahoo latazkiratul lilmuttaqeen"},{"SurahNumber":69,"VerseNumber":49,"Text":"Wa inna lana'lamu anna minkum mukazzibeen"},{"SurahNumber":69,"VerseNumber":50,"Text":"Wa innahu lahasratun 'alal kaafireen"},{"SurahNumber":69,"VerseNumber":51,"Text":"Wa innahoo lahaqqul yaqeen"},{"SurahNumber":69,"VerseNumber":52,"Text":"Fasbbih bismi Rabbikal 'Azeem"}]}