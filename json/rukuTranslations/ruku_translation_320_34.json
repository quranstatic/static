{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":70,"Text":"اذ قال لابيه وقومه ما تعبدون"},{"SurahNumber":26,"VerseNumber":71,"Text":"قالوا نعبد اصنام ا فنظل لها عكفين"},{"SurahNumber":26,"VerseNumber":72,"Text":"قال هل يسمعونكم اذ تدعون"},{"SurahNumber":26,"VerseNumber":73,"Text":"او ينفعونكم او يضرون"},{"SurahNumber":26,"VerseNumber":74,"Text":"قالوا بل وجدنا ءاباءنا كذلك يفعلون"},{"SurahNumber":26,"VerseNumber":75,"Text":"قال افرءيتم ما كنتم تعبدون"},{"SurahNumber":26,"VerseNumber":76,"Text":"انتم وءاباوكم الاقدمون"},{"SurahNumber":26,"VerseNumber":77,"Text":"فانهم عدو لي الا رب العلمين"},{"SurahNumber":26,"VerseNumber":78,"Text":"الذي خلقني فهو يهدين"},{"SurahNumber":26,"VerseNumber":79,"Text":"والذي هو يطعمني ويسقين"},{"SurahNumber":26,"VerseNumber":80,"Text":"واذا مرضت فهو يشفين"},{"SurahNumber":26,"VerseNumber":81,"Text":"والذي يميتني ثم يحيين"},{"SurahNumber":26,"VerseNumber":82,"Text":"والذي اطمع ان يغفر لي خطيتي يوم الدين"},{"SurahNumber":26,"VerseNumber":83,"Text":"رب هب لي حكم ا والحقني بالصلحين"},{"SurahNumber":26,"VerseNumber":84,"Text":"واجعل لي لسان صدق في الاخرين"},{"SurahNumber":26,"VerseNumber":85,"Text":"واجعلني من ورثة جنة النعيم"},{"SurahNumber":26,"VerseNumber":86,"Text":"واغفر لابي انه كان من الضالين"},{"SurahNumber":26,"VerseNumber":87,"Text":"ولا تخزني يوم يبعثون"},{"SurahNumber":26,"VerseNumber":88,"Text":"يوم لا ينفع مال ولا بنون"},{"SurahNumber":26,"VerseNumber":89,"Text":"الا من اتى الله بقلب سليم"},{"SurahNumber":26,"VerseNumber":90,"Text":"وازلفت الجنة للمتقين"},{"SurahNumber":26,"VerseNumber":91,"Text":"وبرزت الجحيم للغاوين"},{"SurahNumber":26,"VerseNumber":92,"Text":"وقيل لهم اين ما كنتم تعبدون"},{"SurahNumber":26,"VerseNumber":93,"Text":"من دون الله هل ينصرونكم او ينتصرون"},{"SurahNumber":26,"VerseNumber":94,"Text":"فكبكبوا فيها هم والغاون"},{"SurahNumber":26,"VerseNumber":95,"Text":"وجنود ابليس اجمعون"},{"SurahNumber":26,"VerseNumber":96,"Text":"قالوا وهم فيها يختصمون"},{"SurahNumber":26,"VerseNumber":97,"Text":"تالله ان كنا لفي ضلل مبين"},{"SurahNumber":26,"VerseNumber":98,"Text":"اذ نسويكم برب العلمين"},{"SurahNumber":26,"VerseNumber":99,"Text":"وما اضلنا الا المجرمون"},{"SurahNumber":26,"VerseNumber":100,"Text":"فما لنا من شفعين"},{"SurahNumber":26,"VerseNumber":101,"Text":"ولا صديق حميم"},{"SurahNumber":26,"VerseNumber":102,"Text":"فلو ان لنا كرة فنكون من المومنين"},{"SurahNumber":26,"VerseNumber":103,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":104,"Text":"وان ربك لهو العزيز الرحيم"}]}