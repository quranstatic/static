{"Id":329,"LanguageCodes":["es","spa"],"Name":"spa-juliocortes-la","Author":"Julio Cortes","Language":"Spanish","Description":"Spanish - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":95,"VerseNumber":1,"Text":"¡Por las higueras y los olivos"},{"SurahNumber":95,"VerseNumber":2,"Text":"¡Por el monte Sinai"},{"SurahNumber":95,"VerseNumber":3,"Text":"¡Por esta ciudad segura"},{"SurahNumber":95,"VerseNumber":4,"Text":"Hemos creado al hombre dandole la mejor complexion"},{"SurahNumber":95,"VerseNumber":5,"Text":"Luego, hemos hecho de el el mas abyecto"},{"SurahNumber":95,"VerseNumber":6,"Text":"Excepto quienes crean y obren bien, que recibiran una recompensa ininterrumpida"},{"SurahNumber":95,"VerseNumber":7,"Text":"Como puede aun desmentirse el Juicio"},{"SurahNumber":95,"VerseNumber":8,"Text":"¿No es Ala quien mejor decide"}]}