{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":70,"VerseNumber":1,"Text":"Sa ala saaa'ilum bi'azaa binw-waaqi"},{"SurahNumber":70,"VerseNumber":2,"Text":"Lilkaafireen laisa lahoo daafi"},{"SurahNumber":70,"VerseNumber":3,"Text":"Minal laahi zil ma'aarij"},{"SurahNumber":70,"VerseNumber":4,"Text":"Ta'rujul malaaa'ikatu war Roohu ilaihi fee yawmin kaana miqdaaruhoo khamseena alfa sanah"},{"SurahNumber":70,"VerseNumber":5,"Text":"Fasbir ssabran jameelaa"},{"SurahNumber":70,"VerseNumber":6,"Text":"Inaahum yarawnahoo ba'eedaa"},{"SurahNumber":70,"VerseNumber":7,"Text":"Wa naraahu qareebaa"},{"SurahNumber":70,"VerseNumber":8,"Text":"Yawma takoonus samaaa'u kalmuhl"},{"SurahNumber":70,"VerseNumber":9,"Text":"Wa takoonul jibaalu kal'ihn"},{"SurahNumber":70,"VerseNumber":10,"Text":"Wa laa yas'alu hameemun hameemaa"},{"SurahNumber":70,"VerseNumber":11,"Text":"Yubassaroonahum; ya waddul mujrimu law yaftadee min 'azaabi yawma'izim bibaneeh"},{"SurahNumber":70,"VerseNumber":12,"Text":"Wa saahibatihee wa akheeh"},{"SurahNumber":70,"VerseNumber":13,"Text":"Wa faseelathil latee tu'weeh"},{"SurahNumber":70,"VerseNumber":14,"Text":"Wa man fil ardi jamee'an summa yunjeeh"},{"SurahNumber":70,"VerseNumber":15,"Text":"Kallaa innahaa lazaa"},{"SurahNumber":70,"VerseNumber":16,"Text":"Nazzaa'atal lishshawaa"},{"SurahNumber":70,"VerseNumber":17,"Text":"Tad'oo man adbara wa tawallaa"},{"SurahNumber":70,"VerseNumber":18,"Text":"W jama'a fa aw'aa"},{"SurahNumber":70,"VerseNumber":19,"Text":"Innal insaana khuliqa haloo'aa"},{"SurahNumber":70,"VerseNumber":20,"Text":"Izaa massahush sharru jazoo'aa"},{"SurahNumber":70,"VerseNumber":21,"Text":"Wa izaa massahul khairu manoo'aa"},{"SurahNumber":70,"VerseNumber":22,"Text":"Illal musalleen"},{"SurahNumber":70,"VerseNumber":23,"Text":"Allazeena hum 'alaa Salaatihim daaa'imoon"},{"SurahNumber":70,"VerseNumber":24,"Text":"Wallazeena feee amwaalihim haqqum ma'loom"},{"SurahNumber":70,"VerseNumber":25,"Text":"Lissaaa 'ili walmahroom"},{"SurahNumber":70,"VerseNumber":26,"Text":"Wallazeena yusaddiqoona bi yawmid Deen"},{"SurahNumber":70,"VerseNumber":27,"Text":"Wallazeena hum min 'azaabi Rabbihim mushfiqoon"},{"SurahNumber":70,"VerseNumber":28,"Text":"Inna 'azaaba Rabbihim ghairu maamoon"},{"SurahNumber":70,"VerseNumber":29,"Text":"Wallazeena hum lifuroo jihim haafizoon"},{"SurahNumber":70,"VerseNumber":30,"Text":"Illaa 'alaaa azwaajihim aw maa malakat aymaanuhum fainnahum ghairu maloomeen"},{"SurahNumber":70,"VerseNumber":31,"Text":"Famanib taghaa waraaa'a zaalika fa ulaaa'ika humul 'aadoon"},{"SurahNumber":70,"VerseNumber":32,"Text":"Wallazeena hum li amaa naatihim wa 'ahdihim raa'oon"},{"SurahNumber":70,"VerseNumber":33,"Text":"Wallazeena hum bishahaadaatihim qaaa'imoon"},{"SurahNumber":70,"VerseNumber":34,"Text":"Wallazeena hum 'alaa salaatihim yuhaafizoon"},{"SurahNumber":70,"VerseNumber":35,"Text":"Ulaaa'ika fee jannaatim mukramoon"}]}