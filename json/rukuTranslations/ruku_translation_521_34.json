{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":79,"VerseNumber":27,"Text":"ءانتم اشد خلقا ام السماء بنىها"},{"SurahNumber":79,"VerseNumber":28,"Text":"رفع سمكها فسوىها"},{"SurahNumber":79,"VerseNumber":29,"Text":"واغطش ليلها واخرج ضحىها"},{"SurahNumber":79,"VerseNumber":30,"Text":"والارض بعد ذلك دحىها"},{"SurahNumber":79,"VerseNumber":31,"Text":"اخرج منها ماءها ومرعىها"},{"SurahNumber":79,"VerseNumber":32,"Text":"والجبال ارسىها"},{"SurahNumber":79,"VerseNumber":33,"Text":"متع ا لكم ولانعمكم"},{"SurahNumber":79,"VerseNumber":34,"Text":"فاذا جاءت الطامة الكبرى"},{"SurahNumber":79,"VerseNumber":35,"Text":"يوم يتذكر الانسن ما سعى"},{"SurahNumber":79,"VerseNumber":36,"Text":"وبرزت الجحيم لمن يرى"},{"SurahNumber":79,"VerseNumber":37,"Text":"فاما من طغى"},{"SurahNumber":79,"VerseNumber":38,"Text":"وءاثر الحيوة الدنيا"},{"SurahNumber":79,"VerseNumber":39,"Text":"فان الجحيم هي الماوى"},{"SurahNumber":79,"VerseNumber":40,"Text":"واما من خاف مقام ربه ونهى النفس عن الهوى"},{"SurahNumber":79,"VerseNumber":41,"Text":"فان الجنة هي الماوى"},{"SurahNumber":79,"VerseNumber":42,"Text":"يسلونك عن الساعة ايان مرسىها"},{"SurahNumber":79,"VerseNumber":43,"Text":"فيم انت من ذكرىها"},{"SurahNumber":79,"VerseNumber":44,"Text":"الى ربك منتهىها"},{"SurahNumber":79,"VerseNumber":45,"Text":"انما انت منذر من يخشىها"},{"SurahNumber":79,"VerseNumber":46,"Text":"كانهم يوم يرونها لم يلبثوا الا عشية او ضحىها"}]}