{"Id":10,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":37,"VerseNumber":22,"Text":"Ohshuroo allatheena thalamoo waazwajahum wama kanoo yaAAbudoona"},{"SurahNumber":37,"VerseNumber":23,"Text":"Min dooni Allahi faihdoohum ila sirati aljaheemi"},{"SurahNumber":37,"VerseNumber":24,"Text":"Waqifoohum innahum masooloona"},{"SurahNumber":37,"VerseNumber":25,"Text":"Ma lakum la tanasaroona"},{"SurahNumber":37,"VerseNumber":26,"Text":"Bal humu alyawma mustaslimoona"},{"SurahNumber":37,"VerseNumber":27,"Text":"Waaqbala baAAduhum AAala baAAdin yatasaaloona"},{"SurahNumber":37,"VerseNumber":28,"Text":"Qaloo innakum kuntum tatoonana AAani alyameeni"},{"SurahNumber":37,"VerseNumber":29,"Text":"Qaloo bal lam takoonoo mumineena"},{"SurahNumber":37,"VerseNumber":30,"Text":"Wama kana lana AAalaykum min sultanin bal kuntum qawman tagheena"},{"SurahNumber":37,"VerseNumber":31,"Text":"Fahaqqa AAalayna qawlu rabbina inna lathaiqoona"},{"SurahNumber":37,"VerseNumber":32,"Text":"Faaghwaynakum inna kunna ghaweena"},{"SurahNumber":37,"VerseNumber":33,"Text":"Fainnahum yawmaithin fee alAAathabi mushtarikoona"},{"SurahNumber":37,"VerseNumber":34,"Text":"Inna kathalika nafAAalu bialmujrimeena"},{"SurahNumber":37,"VerseNumber":35,"Text":"Innahum kanoo itha qeela lahum la ilaha illa Allahu yastakbiroona"},{"SurahNumber":37,"VerseNumber":36,"Text":"Wayaqooloona ainna latarikoo alihatina lishaAAirin majnoonin"},{"SurahNumber":37,"VerseNumber":37,"Text":"Bal jaa bialhaqqi wasaddaqa almursaleena"},{"SurahNumber":37,"VerseNumber":38,"Text":"Innakum lathaiqoo alAAathabi alaleemi"},{"SurahNumber":37,"VerseNumber":39,"Text":"Wama tujzawna illa ma kuntum taAAmaloona"},{"SurahNumber":37,"VerseNumber":40,"Text":"Illa AAibada Allahi almukhlaseena"},{"SurahNumber":37,"VerseNumber":41,"Text":"Olaika lahum rizqun maAAloomun"},{"SurahNumber":37,"VerseNumber":42,"Text":"Fawakihu wahum mukramoona"},{"SurahNumber":37,"VerseNumber":43,"Text":"Fee jannati alnnaAAeemi"},{"SurahNumber":37,"VerseNumber":44,"Text":"AAala sururin mutaqabileena"},{"SurahNumber":37,"VerseNumber":45,"Text":"Yutafu AAalayhim bikasin min maAAeenin"},{"SurahNumber":37,"VerseNumber":46,"Text":"Baydaa laththatin lilshsharibeena"},{"SurahNumber":37,"VerseNumber":47,"Text":"La feeha ghawlun wala hum AAanha yunzafoona"},{"SurahNumber":37,"VerseNumber":48,"Text":"WaAAindahum qasiratu alttarfi AAeenun"},{"SurahNumber":37,"VerseNumber":49,"Text":"Kaannahunna baydun maknoonun"},{"SurahNumber":37,"VerseNumber":50,"Text":"Faaqbala baAAduhum AAala baAAdin yatasaaloona"},{"SurahNumber":37,"VerseNumber":51,"Text":"Qala qailun minhum innee kana lee qareenun"},{"SurahNumber":37,"VerseNumber":52,"Text":"Yaqoolu ainnaka lamina almusaddiqeena"},{"SurahNumber":37,"VerseNumber":53,"Text":"Aitha mitna wakunna turaban waAAithaman ainna lamadeenoona"},{"SurahNumber":37,"VerseNumber":54,"Text":"Qala hal antum muttaliAAoona"},{"SurahNumber":37,"VerseNumber":55,"Text":"FaittalaAAa faraahu fee sawai aljaheemi"},{"SurahNumber":37,"VerseNumber":56,"Text":"Qala taAllahi in kidta laturdeeni"},{"SurahNumber":37,"VerseNumber":57,"Text":"Walawla niAAmatu rabbee lakuntu mina almuhdareena"},{"SurahNumber":37,"VerseNumber":58,"Text":"Afama nahnu bimayyiteena"},{"SurahNumber":37,"VerseNumber":59,"Text":"Illa mawtatana aloola wama nahnu bimuAAaththabeena"},{"SurahNumber":37,"VerseNumber":60,"Text":"Inna hatha lahuwa alfawzu alAAatheemu"},{"SurahNumber":37,"VerseNumber":61,"Text":"Limithli hatha falyaAAmali alAAamiloona"},{"SurahNumber":37,"VerseNumber":62,"Text":"Athalika khayrun nuzulan am shajaratu alzzaqqoomi"},{"SurahNumber":37,"VerseNumber":63,"Text":"Inna jaAAalnaha fitnatan lilththalimeena"},{"SurahNumber":37,"VerseNumber":64,"Text":"Innaha shajaratun takhruju fee asli aljaheemi"},{"SurahNumber":37,"VerseNumber":65,"Text":"TalAAuha kaannahu ruoosu alshshayateeni"},{"SurahNumber":37,"VerseNumber":66,"Text":"Fainnahum laakiloona minha famalioona minha albutoona"},{"SurahNumber":37,"VerseNumber":67,"Text":"Thumma inna lahum AAalayha lashawban min hameemin"},{"SurahNumber":37,"VerseNumber":68,"Text":"Thumma inna marjiAAahum laila aljaheemi"},{"SurahNumber":37,"VerseNumber":69,"Text":"Innahum alfaw abaahum dalleena"},{"SurahNumber":37,"VerseNumber":70,"Text":"Fahum AAala atharihim yuhraAAoona"},{"SurahNumber":37,"VerseNumber":71,"Text":"Walaqad dalla qablahum aktharu alawwaleena"},{"SurahNumber":37,"VerseNumber":72,"Text":"Walaqad arsalna feehim munthireena"},{"SurahNumber":37,"VerseNumber":73,"Text":"Faonthur kayfa kana AAaqibatu almunthareena"},{"SurahNumber":37,"VerseNumber":74,"Text":"Illa AAibada Allahi almukhlaseena"}]}