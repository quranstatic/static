{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":85,"VerseNumber":1,"Text":"Wassamaaa'i zaatil burooj"},{"SurahNumber":85,"VerseNumber":2,"Text":"Wal yawmil maw'ood"},{"SurahNumber":85,"VerseNumber":3,"Text":"Wa shaahidinw wa mashhood"},{"SurahNumber":85,"VerseNumber":4,"Text":"Qutila as haabul ukhdood"},{"SurahNumber":85,"VerseNumber":5,"Text":"Annaari zaatil waqood"},{"SurahNumber":85,"VerseNumber":6,"Text":"Iz hum 'alaihaa qu'ood"},{"SurahNumber":85,"VerseNumber":7,"Text":"Wa hum 'alaa maa yaf'aloona bilmu 'mineena shuhood"},{"SurahNumber":85,"VerseNumber":8,"Text":"Wa maa naqamoo minhum illaaa aiyu'minoo billaahil 'azeezil Hameed"},{"SurahNumber":85,"VerseNumber":9,"Text":"Allazee lahoo mulkus samaawaati wal ard; wallaahu 'alaa kulli shai 'in Shaheed"},{"SurahNumber":85,"VerseNumber":10,"Text":"Innal lazeena fatanul mu'mineena wal mu'minaati summa lam yatooboo falahum 'azaabu Jahannama wa lahum 'azaabul hareeq"},{"SurahNumber":85,"VerseNumber":11,"Text":"Innal lazeena aamanoo wa 'amilus saalihaati lahum Jannaatun tajree min tahtihal anhaar; zaalikal fawzul kabeer"},{"SurahNumber":85,"VerseNumber":12,"Text":"Inna batsha Rabbika lashadeed"},{"SurahNumber":85,"VerseNumber":13,"Text":"Innahoo Huwa yubdi'u wa yu'eed"},{"SurahNumber":85,"VerseNumber":14,"Text":"Wa Huwal Ghafoorul Wadood"},{"SurahNumber":85,"VerseNumber":15,"Text":"Zul 'Arshil Majeed"},{"SurahNumber":85,"VerseNumber":16,"Text":"Fa' 'aalul limaa yureed"},{"SurahNumber":85,"VerseNumber":17,"Text":"Hal ataaka hadeesul junood"},{"SurahNumber":85,"VerseNumber":18,"Text":"Fir'awna wa Samood"},{"SurahNumber":85,"VerseNumber":19,"Text":"Balil lazeena kafaroo fee takzeeb"},{"SurahNumber":85,"VerseNumber":20,"Text":"Wallaahu minw waraaa'ihim muheet"},{"SurahNumber":85,"VerseNumber":21,"Text":"Bal huwa Quraanum Majeed"},{"SurahNumber":85,"VerseNumber":22,"Text":"Fee Lawhim Mahfooz"}]}