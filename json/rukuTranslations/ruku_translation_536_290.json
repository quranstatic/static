{"Id":290,"LanguageCodes":["ro","ron"],"Name":"ron-georgegrigore-la","Author":"George Grigore","Language":"Romanian","Description":"Romanian - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":94,"VerseNumber":1,"Text":"Nu ti-am deschis Noi oare pieptul"},{"SurahNumber":94,"VerseNumber":2,"Text":"Nu ti-am luat povara"},{"SurahNumber":94,"VerseNumber":3,"Text":"care-ti frangea spatele"},{"SurahNumber":94,"VerseNumber":4,"Text":"N-am inaltat Noi oare amintirea ta"},{"SurahNumber":94,"VerseNumber":5,"Text":"Cu greul vine usorul"},{"SurahNumber":94,"VerseNumber":6,"Text":"Da, cu greul vine usorul"},{"SurahNumber":94,"VerseNumber":7,"Text":"Cand ai ispravit munca, ridica-te"},{"SurahNumber":94,"VerseNumber":8,"Text":"si catre Domnul tau cauta cu dor"}]}