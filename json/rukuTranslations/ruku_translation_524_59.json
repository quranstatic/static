{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"Igenni, ma iqlaa"},{"SurahNumber":82,"VerseNumber":2,"Text":"itran, ma mebruzzaen"},{"SurahNumber":82,"VerseNumber":3,"Text":"ilellen, ma falen"},{"SurahNumber":82,"VerseNumber":4,"Text":"ieekwan, ma nneqlaben"},{"SurahNumber":82,"VerseNumber":5,"Text":"ad ieeo yiman ayen d ibbwi, akked wayen iooa"},{"SurahNumber":82,"VerseNumber":6,"Text":"A yamdan, d acu k i$uooen, $ef Mass ik Ukyis"},{"SurahNumber":82,"VerseNumber":7,"Text":"ik Ixelqen, ik Imesslen, ik Iwezznen"},{"SurahNumber":82,"VerseNumber":8,"Text":"Ifka yak udem i aS iwulmen"},{"SurahNumber":82,"VerseNumber":9,"Text":"Xaii! Teskiddibem ddin"},{"SurahNumber":82,"VerseNumber":10,"Text":"Ni$ $urwen imearaben"},{"SurahNumber":82,"VerseNumber":11,"Text":"D imura ukyisen"},{"SurahNumber":82,"VerseNumber":12,"Text":"eoan ayen txeddmem"},{"SurahNumber":82,"VerseNumber":13,"Text":"Ih, di ssaad, ara ilin iaaodiyen"},{"SurahNumber":82,"VerseNumber":14,"Text":"Ih, di tmessi, ara ilin ucmiten"},{"SurahNumber":82,"VerseNumber":15,"Text":"A p kecmen, ass iqeiiiyen"},{"SurahNumber":82,"VerseNumber":16,"Text":"Tuff$a segs, ur zmiren"},{"SurahNumber":82,"VerseNumber":17,"Text":"Anwa ara k imlen, d acu t wass iqeiiiyen"},{"SurahNumber":82,"VerseNumber":18,"Text":"Ih! Anwa ara k imlen, d acu t wass iqeiiiyen"},{"SurahNumber":82,"VerseNumber":19,"Text":"D ass ideg yiwen ur izmir ad ifukk yiwen, s wacemma. Assen, lameo i Oebbi"}]}