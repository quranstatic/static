{"Id":248,"LanguageCodes":["ml","mal"],"Name":"mal-muhammadkarakun-la","Author":"Muhammad Karakunnu And Vanidas Elayavoor","Language":"Malayalam","Description":"Malayalam - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":96,"VerseNumber":1,"Text":"vayikkuka, srsticca ninre nathanre namattil"},{"SurahNumber":96,"VerseNumber":2,"Text":"ottippitikkunnatilninn avan manusyane srsticcu"},{"SurahNumber":96,"VerseNumber":3,"Text":"vayikkuka! ninre nathan atyudaranan"},{"SurahNumber":96,"VerseNumber":4,"Text":"penakeantu pathippiccavan"},{"SurahNumber":96,"VerseNumber":5,"Text":"manusyane avanariyattat avan pathippiccu"},{"SurahNumber":96,"VerseNumber":6,"Text":"sansayamilla; manusyan atikramiyayirikkunnu"},{"SurahNumber":96,"VerseNumber":7,"Text":"tanikkutanpeannavanayi kantatinal"},{"SurahNumber":96,"VerseNumber":8,"Text":"niscayam, matakkam ninre nathankalekkan"},{"SurahNumber":96,"VerseNumber":9,"Text":"tatayunnavane ni kantea"},{"SurahNumber":96,"VerseNumber":10,"Text":"nam'mute dasane, avan namaskarikkumpeal"},{"SurahNumber":96,"VerseNumber":11,"Text":"ni kantea? a atima nervaliyil tanneyan"},{"SurahNumber":96,"VerseNumber":12,"Text":"athava, bhakti upadesikkunnavanan"},{"SurahNumber":96,"VerseNumber":13,"Text":"ni kantea? i tatayunnavan satyatte tallikkalayukayum purantirinnu nilkkukayum ceytavanan"},{"SurahNumber":96,"VerseNumber":14,"Text":"allahu ellam kanunnuvenn avan ariyunnille"},{"SurahNumber":96,"VerseNumber":15,"Text":"sansayam venta; avanit avasanippikkunnillenkil; avanre kutuma nam piticcu valikkuka tanne ceyyum"},{"SurahNumber":96,"VerseNumber":16,"Text":"kallam parayukayum; papam pravarttikkukayum ceyyunna kutuma"},{"SurahNumber":96,"VerseNumber":17,"Text":"appealavan tanre alukale vilikkatte"},{"SurahNumber":96,"VerseNumber":18,"Text":"nam nam'mute siksakaryannalute cumatalakkareyum vilikkam"},{"SurahNumber":96,"VerseNumber":19,"Text":"arut! ni avan valannarut. ni sastangam pranamikkuka! nam'mute samipyam netuka"}]}