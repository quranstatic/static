{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"S wavu iwezzaen"},{"SurahNumber":51,"VerseNumber":2,"Text":"I ibubben taakkwemt"},{"SurahNumber":51,"VerseNumber":3,"Text":"Wid ippazalen s shala"},{"SurahNumber":51,"VerseNumber":4,"Text":"Tid ifeoqen lameo"},{"SurahNumber":51,"VerseNumber":5,"Text":"Ar ayen i wen ippuaahden, d tidep"},{"SurahNumber":51,"VerseNumber":6,"Text":"Aqeiii, ih, ad iuao"},{"SurahNumber":51,"VerseNumber":7,"Text":"S igenni bu idurra"},{"SurahNumber":51,"VerseNumber":8,"Text":"Kunwi temgarradem $ef wawal"},{"SurahNumber":51,"VerseNumber":9,"Text":"Iooa t win iooan"},{"SurahNumber":51,"VerseNumber":10,"Text":"Tameppant i ikeddaben"},{"SurahNumber":51,"VerseNumber":11,"Text":"Nitni, di maylellif, stehzan"},{"SurahNumber":51,"VerseNumber":12,"Text":"steqsayen: \"anda t wass uqeiii"},{"SurahNumber":51,"VerseNumber":13,"Text":"Ass ideg, ara ppuhewlen di tmes"},{"SurahNumber":51,"VerseNumber":14,"Text":"Jeoobet ahewwel nnwen agi i$ef tearem"},{"SurahNumber":51,"VerseNumber":15,"Text":"Ih, wid ipeezziben, di leonanat akked tliwa"},{"SurahNumber":51,"VerseNumber":16,"Text":"Iifen ayen i sen Ifka Mass nnsen. Ih, nitni llan, iu$ leal, d lauuam"},{"SurahNumber":51,"VerseNumber":17,"Text":"Deg iv, cwii ay gganen"},{"SurahNumber":51,"VerseNumber":18,"Text":"Di tafrara, nitni ste$fioen"},{"SurahNumber":51,"VerseNumber":19,"Text":"deg wayla nnsen: amur i umappar akked ume$bun"},{"SurahNumber":51,"VerseNumber":20,"Text":"U di tmurt, issekniyen i wid ipputebbten"},{"SurahNumber":51,"VerseNumber":21,"Text":"Llant deg iman nnwen. Aani ur tepwalim ara"},{"SurahNumber":51,"VerseNumber":22,"Text":"U deg igenni, ooeeq nnwen, akked wayen i wen ippuwaaaden"},{"SurahNumber":51,"VerseNumber":23,"Text":"S Mass n tmurt akked igenwan, ar neppa d tidep, am akken kunwi tepmeslayem"}]}