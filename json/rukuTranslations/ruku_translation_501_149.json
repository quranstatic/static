{"Id":149,"LanguageCodes":["eo","epo"],"Name":"epo-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Esperanto","Description":"Esperanto","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":69,"VerseNumber":38,"Text":"Mi jxur ki vi seg"},{"SurahNumber":69,"VerseNumber":39,"Text":"Kaj ki vi ne seg"},{"SurahNumber":69,"VerseNumber":40,"Text":"This est utterance honorable messenger"},{"SurahNumber":69,"VerseNumber":41,"Text":"Ne utterance poet; rarely do vi kred"},{"SurahNumber":69,"VerseNumber":42,"Text":"Nor utterance soothsayer; rarely do vi pren heed"},{"SurahNumber":69,"VerseNumber":43,"Text":"revelation el Lord univers"},{"SurahNumber":69,"VerseNumber":44,"Text":"Hav li uttered any ali teachings"},{"SurahNumber":69,"VerseNumber":45,"Text":"Ni pun him"},{"SurahNumber":69,"VerseNumber":46,"Text":"Ni stop revelations him"},{"SurahNumber":69,"VerseNumber":47,"Text":"Neniom da vi help him"},{"SurahNumber":69,"VerseNumber":48,"Text":"This est reminder por des righteous"},{"SurahNumber":69,"VerseNumber":49,"Text":"Ni kon; kelk da vi est rejectors"},{"SurahNumber":69,"VerseNumber":50,"Text":"gxi est sorrow disbelievers"},{"SurahNumber":69,"VerseNumber":51,"Text":"Gxi est absolut truth"},{"SurahNumber":69,"VerseNumber":52,"Text":"Do vi glor nom your Lord Great"}]}