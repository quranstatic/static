{"Id":67,"LanguageCodes":["bs","bos"],"Name":"bos-mustafamlivo-la","Author":"Mustafa Mlivo","Language":"Bosnian","Description":"Bosnian - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"Udar"},{"SurahNumber":101,"VerseNumber":2,"Text":"Sta je udar"},{"SurahNumber":101,"VerseNumber":3,"Text":"I sta znas ti sta je udar"},{"SurahNumber":101,"VerseNumber":4,"Text":"Dan kad budu ljudi kao leptiri rastrkani"},{"SurahNumber":101,"VerseNumber":5,"Text":"I budu brda kao vuna rascupana"},{"SurahNumber":101,"VerseNumber":6,"Text":"Pa onaj cije mjere budu teske"},{"SurahNumber":101,"VerseNumber":7,"Text":"Pa on ce u zivotu zadovoljstva biti"},{"SurahNumber":101,"VerseNumber":8,"Text":"A onaj cije mjere budu lahke"},{"SurahNumber":101,"VerseNumber":9,"Text":"Pa majka njegova bice hawija"},{"SurahNumber":101,"VerseNumber":10,"Text":"A sta znas ti sta je ona"},{"SurahNumber":101,"VerseNumber":11,"Text":"Vatra zestoka"}]}