{"Id":10,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":15,"VerseNumber":61,"Text":"Falamma jaa ala lootin almursaloona"},{"SurahNumber":15,"VerseNumber":62,"Text":"Qala innakum qawmun munkaroona"},{"SurahNumber":15,"VerseNumber":63,"Text":"Qaloo bal jinaka bima kanoo feehi yamtaroona"},{"SurahNumber":15,"VerseNumber":64,"Text":"Waataynaka bialhaqqi wainna lasadiqoona"},{"SurahNumber":15,"VerseNumber":65,"Text":"Faasri biahlika biqitAAin mina allayli waittabiAA adbarahum wala yaltafit minkum ahadun waimdoo haythu tumaroona"},{"SurahNumber":15,"VerseNumber":66,"Text":"Waqadayna ilayhi thalika alamra anna dabira haolai maqtooAAun musbiheena"},{"SurahNumber":15,"VerseNumber":67,"Text":"Wajaa ahlu almadeenati yastabshiroona"},{"SurahNumber":15,"VerseNumber":68,"Text":"Qala inna haolai dayfee fala tafdahooni"},{"SurahNumber":15,"VerseNumber":69,"Text":"Waittaqoo Allaha wala tukhzooni"},{"SurahNumber":15,"VerseNumber":70,"Text":"Qaloo awalam nanhaka AAani alAAalameena"},{"SurahNumber":15,"VerseNumber":71,"Text":"Qala haolai banatee in kuntum faAAileena"},{"SurahNumber":15,"VerseNumber":72,"Text":"LaAAamruka innahum lafee sakratihim yaAAmahoona"},{"SurahNumber":15,"VerseNumber":73,"Text":"Faakhathathumu alssayhatu mushriqeena"},{"SurahNumber":15,"VerseNumber":74,"Text":"FajaAAalna AAaliyaha safilaha waamtarna AAalayhim hijaratan min sijjeelin"},{"SurahNumber":15,"VerseNumber":75,"Text":"Inna fee thalika laayatin lilmutawassimeena"},{"SurahNumber":15,"VerseNumber":76,"Text":"Wainnaha labisabeelin muqeemin"},{"SurahNumber":15,"VerseNumber":77,"Text":"Inna fee thalika laayatan lilmumineena"},{"SurahNumber":15,"VerseNumber":78,"Text":"Wain kana ashabu alaykati lathalimeena"},{"SurahNumber":15,"VerseNumber":79,"Text":"Faintaqamna minhum wainnahuma labiimamin mubeenin"}]}