{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":112,"VerseNumber":1,"Text":"Maaku ko Kanko woni Alla gooto"},{"SurahNumber":112,"VerseNumber":2,"Text":"Alla paandateeɗo"},{"SurahNumber":112,"VerseNumber":3,"Text":"O jibinaani O jinaaka"},{"SurahNumber":112,"VerseNumber":4,"Text":"O alah pasa hay gooto"},{"SurahNumber":113,"VerseNumber":1,"Text":"Maaku mbɗa moolo e jom subaka"},{"SurahNumber":113,"VerseNumber":2,"Text":"E bone ko o tagi"},{"SurahNumber":113,"VerseNumber":3,"Text":"E bone jemma niɓɓa kunu"},{"SurahNumber":113,"VerseNumber":4,"Text":"E bone wuttooɓe e piɓlee. @Corrected"},{"SurahNumber":113,"VerseNumber":5,"Text":"E bone haaside si o haasidiima"},{"SurahNumber":114,"VerseNumber":1,"Text":"Maaku mbeɗa moolo e jom yimɓe"},{"SurahNumber":114,"VerseNumber":2,"Text":"Laamɗo yimɓe"},{"SurahNumber":114,"VerseNumber":3,"Text":"Deweteeɗo yimɓe"},{"SurahNumber":114,"VerseNumber":4,"Text":"E bone seytaaneeli cikkinooji"},{"SurahNumber":114,"VerseNumber":5,"Text":"Oon cikkinoowo e ɓerɗe yimɓeɓɗj"},{"SurahNumber":114,"VerseNumber":6,"Text":"Ummaade e jinneji e yimɓe"}]}