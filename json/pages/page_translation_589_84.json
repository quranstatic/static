{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":83,"VerseNumber":35,"Text":"På luksuøse furnishings de iagttager"},{"SurahNumber":83,"VerseNumber":36,"Text":"Fleste assuredly tvivlerne requited hvad de gjorde"},{"SurahNumber":84,"VerseNumber":1,"Text":"tid komme hvornår himlen ruptured"},{"SurahNumber":84,"VerseNumber":2,"Text":"Det forelægge dens Lord udløbe"},{"SurahNumber":84,"VerseNumber":3,"Text":"Jorden leveled"},{"SurahNumber":84,"VerseNumber":4,"Text":"Det udstøde dens indehold det erupts"},{"SurahNumber":84,"VerseNumber":5,"Text":"Det forelægge dens Lord udløbe"},{"SurahNumber":84,"VerseNumber":6,"Text":"O mennesker I er irreversibly heading for møde Deres Lord"},{"SurahNumber":84,"VerseNumber":7,"Text":"Angående ene som modta hans registrere hans rigtig hånd"},{"SurahNumber":84,"VerseNumber":8,"Text":"Han reckoning være lette"},{"SurahNumber":84,"VerseNumber":9,"Text":"Han returnere hans folk joyfully"},{"SurahNumber":84,"VerseNumber":10,"Text":"Angående ene som modta hans registrere bagefter hans tilbage"},{"SurahNumber":84,"VerseNumber":11,"Text":"Han rides samvittighedsnag"},{"SurahNumber":84,"VerseNumber":12,"Text":"Forbrænde ind Helvede"},{"SurahNumber":84,"VerseNumber":13,"Text":"Han brugte optræde arrogant hans folk"},{"SurahNumber":84,"VerseNumber":14,"Text":"Han tanke han aldrig hedder konto"},{"SurahNumber":84,"VerseNumber":15,"Text":"Ja indeed hans Lord var Seer af ham"},{"SurahNumber":84,"VerseNumber":16,"Text":"Jeg solemnly sværger rosy mørkningen"},{"SurahNumber":84,"VerseNumber":17,"Text":"Og natten det spreder"},{"SurahNumber":84,"VerseNumber":18,"Text":"Månen dens faser"},{"SurahNumber":84,"VerseNumber":19,"Text":"Du flytte stage stage"},{"SurahNumber":84,"VerseNumber":20,"Text":"Hvorfor de ikke tror"},{"SurahNumber":84,"VerseNumber":21,"Text":"Og hvornår Quran reciteres dem de ikke falder prostrate"},{"SurahNumber":84,"VerseNumber":22,"Text":"Den er de disbelieved forkaste (Quran)"},{"SurahNumber":84,"VerseNumber":23,"Text":"GUD ER fully aware af DERES INDERSTE TANKER"},{"SurahNumber":84,"VerseNumber":24,"Text":"Løfte dem smertelige gengældelse"},{"SurahNumber":84,"VerseNumber":25,"Text":"Angående de tro ledte righteous liv de modtage recompense er well-deserved"}]}