{"Id":238,"LanguageCodes":["la","lat"],"Name":"lat-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Latin","Description":"Latin","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":112,"VerseNumber":1,"Text":"Proclaim He est 1 unus DEUS"},{"SurahNumber":112,"VerseNumber":2,"Text":"Absolute DEUS"},{"SurahNumber":112,"VerseNumber":3,"Text":"Nunquam perfecit He genitus est. Nor He genitus est"},{"SurahNumber":112,"VerseNumber":4,"Text":"Nullus equalis Eum"},{"SurahNumber":113,"VerseNumber":1,"Text":"Dictus ego seek refuge Dominus daybreak"},{"SurahNumber":113,"VerseNumber":2,"Text":"Ex malus His creations"},{"SurahNumber":113,"VerseNumber":3,"Text":"Ex malus darkness it falls"},{"SurahNumber":113,"VerseNumber":4,"Text":"Ex malus troublemakers"},{"SurahNumber":113,"VerseNumber":5,"Text":"Ex malus de envious when they envy"},{"SurahNumber":114,"VerseNumber":1,"Text":"dictus ego seek refuge Dominus people"},{"SurahNumber":114,"VerseNumber":2,"Text":"Rex people"},{"SurahNumber":114,"VerseNumber":3,"Text":"Deus people"},{"SurahNumber":114,"VerseNumber":4,"Text":"Ex malus sneaky whisperers"},{"SurahNumber":114,"VerseNumber":5,"Text":"Qui whisper chests people"},{"SurahNumber":114,"VerseNumber":6,"Text":"Est they de jinns people"}]}