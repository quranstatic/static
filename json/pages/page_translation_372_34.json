{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":112,"Text":"قال وما علمي بما كانوا يعملون"},{"SurahNumber":26,"VerseNumber":113,"Text":"ان حسابهم الا على ربي لو تشعرون"},{"SurahNumber":26,"VerseNumber":114,"Text":"وما انا بطارد المومنين"},{"SurahNumber":26,"VerseNumber":115,"Text":"ان انا الا نذير مبين"},{"SurahNumber":26,"VerseNumber":116,"Text":"قالوا لين لم تنته ينوح لتكونن من المرجومين"},{"SurahNumber":26,"VerseNumber":117,"Text":"قال رب ان قومي كذبون"},{"SurahNumber":26,"VerseNumber":118,"Text":"فافتح بيني وبينهم فتح ا ونجني ومن معي من المومنين"},{"SurahNumber":26,"VerseNumber":119,"Text":"فانجينه ومن معه في الفلك المشحون"},{"SurahNumber":26,"VerseNumber":120,"Text":"ثم اغرقنا بعد الباقين"},{"SurahNumber":26,"VerseNumber":121,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":122,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":123,"Text":"كذبت عاد المرسلين"},{"SurahNumber":26,"VerseNumber":124,"Text":"اذ قال لهم اخوهم هود الا تتقون"},{"SurahNumber":26,"VerseNumber":125,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":126,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":127,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":128,"Text":"اتبنون بكل ريع ءاية تعبثون"},{"SurahNumber":26,"VerseNumber":129,"Text":"وتتخذون مصانع لعلكم تخلدون"},{"SurahNumber":26,"VerseNumber":130,"Text":"واذا بطشتم بطشتم جبارين"},{"SurahNumber":26,"VerseNumber":131,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":132,"Text":"واتقوا الذي امدكم بما تعلمون"},{"SurahNumber":26,"VerseNumber":133,"Text":"امدكم بانعم وبنين"},{"SurahNumber":26,"VerseNumber":134,"Text":"وجنت وعيون"},{"SurahNumber":26,"VerseNumber":135,"Text":"اني اخاف عليكم عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":136,"Text":"قالوا سواء علينا اوعظت ام لم تكن من الوعظين"}]}