{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":77,"VerseNumber":20,"Text":"الم نخلقكم من ماء مهين"},{"SurahNumber":77,"VerseNumber":21,"Text":"فجعلنه في قرار مكين"},{"SurahNumber":77,"VerseNumber":22,"Text":"الى قدر معلوم"},{"SurahNumber":77,"VerseNumber":23,"Text":"فقدرنا فنعم القدرون"},{"SurahNumber":77,"VerseNumber":24,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":25,"Text":"الم نجعل الارض كفاتا"},{"SurahNumber":77,"VerseNumber":26,"Text":"احياء واموت ا"},{"SurahNumber":77,"VerseNumber":27,"Text":"وجعلنا فيها روسي شمخت واسقينكم ماء فرات ا"},{"SurahNumber":77,"VerseNumber":28,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":29,"Text":"انطلقوا الى ما كنتم به تكذبون"},{"SurahNumber":77,"VerseNumber":30,"Text":"انطلقوا الى ظل ذي ثلث شعب"},{"SurahNumber":77,"VerseNumber":31,"Text":"لا ظليل ولا يغني من اللهب"},{"SurahNumber":77,"VerseNumber":32,"Text":"انها ترمي بشرر كالقصر"},{"SurahNumber":77,"VerseNumber":33,"Text":"كانه جملت صفر"},{"SurahNumber":77,"VerseNumber":34,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":35,"Text":"هذا يوم لا ينطقون"},{"SurahNumber":77,"VerseNumber":36,"Text":"ولا يوذن لهم فيعتذرون"},{"SurahNumber":77,"VerseNumber":37,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":38,"Text":"هذا يوم الفصل جمعنكم والاولين"},{"SurahNumber":77,"VerseNumber":39,"Text":"فان كان لكم كيد فكيدون"},{"SurahNumber":77,"VerseNumber":40,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":41,"Text":"ان المتقين في ظلل وعيون"},{"SurahNumber":77,"VerseNumber":42,"Text":"وفوكه مما يشتهون"},{"SurahNumber":77,"VerseNumber":43,"Text":"كلوا واشربوا هنيا بما كنتم تعملون"},{"SurahNumber":77,"VerseNumber":44,"Text":"انا كذلك نجزي المحسنين"},{"SurahNumber":77,"VerseNumber":45,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"Verse