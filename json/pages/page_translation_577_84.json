{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":74,"VerseNumber":48,"Text":"intercession intercessors aldrig hjælpes dem"},{"SurahNumber":74,"VerseNumber":49,"Text":"Hvorfor være de så averse til den påmindelse"},{"SurahNumber":74,"VerseNumber":50,"Text":"Løbende zebraer"},{"SurahNumber":74,"VerseNumber":51,"Text":"Som fleeing løven"},{"SurahNumber":74,"VerseNumber":52,"Text":"Hvert en af dem vil gøre modta scripture personally"},{"SurahNumber":74,"VerseNumber":53,"Text":"Indeed de ikke frygter Herefter"},{"SurahNumber":74,"VerseNumber":54,"Text":"Indeed den er påmindelse"},{"SurahNumber":74,"VerseNumber":55,"Text":"De ønsker tage heed"},{"SurahNumber":74,"VerseNumber":56,"Text":"De ikke tage heed imod gud ville Han er kilden retmæssighed; Han er kilden tilgivelse"},{"SurahNumber":75,"VerseNumber":1,"Text":"Jeg sværger Dagen Opstandelse"},{"SurahNumber":75,"VerseNumber":2,"Text":"Jeg sværger blaming sjælen"},{"SurahNumber":75,"VerseNumber":3,"Text":"menneskelig være tro vi ikke rekonstruere hans bones"},{"SurahNumber":75,"VerseNumber":4,"Text":"Ja indeed; vi er able til rekonstruere hans finger tip"},{"SurahNumber":75,"VerseNumber":5,"Text":"menneskelig være tenderer tro kun hvad han ser ham"},{"SurahNumber":75,"VerseNumber":6,"Text":"Han tvivler Dagen Opstandelse"},{"SurahNumber":75,"VerseNumber":7,"Text":"Tidligere visionen skærpes"},{"SurahNumber":75,"VerseNumber":8,"Text":"Og månen eclipsed"},{"SurahNumber":75,"VerseNumber":9,"Text":"Solen månen styrter ene another"},{"SurahNumber":75,"VerseNumber":10,"Text":"menneskelig være sie dag er flugten"},{"SurahNumber":75,"VerseNumber":11,"Text":"Absolut xxxx ikke er flygte"},{"SurahNumber":75,"VerseNumber":12,"Text":"Til Deres Lord på den dag er endelige destiny"},{"SurahNumber":75,"VerseNumber":13,"Text":"menneskelig være være meddele på dag af alt han gøre avancere alt han gøre regress"},{"SurahNumber":75,"VerseNumber":14,"Text":"menneskelig være være hans egen dommer"},{"SurahNumber":75,"VerseNumber":15,"Text":"Nej undskyldning accepteres"},{"SurahNumber":75,"VerseNumber":16,"Text":"Ikke flytte Deres tunge fremskynde det"},{"SurahNumber":75,"VerseNumber":17,"Text":"Være vi indsamle det Quran"},{"SurahNumber":75,"VerseNumber":18,"Text":"Tidligere vi reciterer det du følge such Quran"},{"SurahNumber":75,"VerseNumber":19,"Text":"Derefter det er vi forklare det"}]}