{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":81,"VerseNumber":1,"Text":"Hvornår solen rulles"},{"SurahNumber":81,"VerseNumber":2,"Text":"Stjernerne styrtes into hvert øvrig"},{"SurahNumber":81,"VerseNumber":3,"Text":"Bjergene wiped ydre"},{"SurahNumber":81,"VerseNumber":4,"Text":"Reproduktionen halted"},{"SurahNumber":81,"VerseNumber":5,"Text":"beasts summoned"},{"SurahNumber":81,"VerseNumber":6,"Text":"Oceanerne sættes aflame"},{"SurahNumber":81,"VerseNumber":7,"Text":"Sjælene restitueres deres kroppe"},{"SurahNumber":81,"VerseNumber":8,"Text":"Pigen begravedes alive spørges"},{"SurahNumber":81,"VerseNumber":9,"Text":"I hvad forbrydelse hun dræbtes"},{"SurahNumber":81,"VerseNumber":10,"Text":"Rekorderne stilles kendt"},{"SurahNumber":81,"VerseNumber":11,"Text":"Himlen flyttes"},{"SurahNumber":81,"VerseNumber":12,"Text":"Helvede ignited"},{"SurahNumber":81,"VerseNumber":13,"Text":"Paradis præsenteres"},{"SurahNumber":81,"VerseNumber":14,"Text":"Enhvere sjæl kende alt det bragte"},{"SurahNumber":81,"VerseNumber":15,"Text":"Jeg solemnly sværger mælkevejene"},{"SurahNumber":81,"VerseNumber":16,"Text":"Netop løbende deres omløbsbaner"},{"SurahNumber":81,"VerseNumber":17,"Text":"Ved natten det falde"},{"SurahNumber":81,"VerseNumber":18,"Text":"Og morn det ånder"},{"SurahNumber":81,"VerseNumber":19,"Text":"Den er utterance ærværdige bud"},{"SurahNumber":81,"VerseNumber":20,"Text":"Autoriserede Possessor Tronen fully understøttede"},{"SurahNumber":81,"VerseNumber":21,"Text":"Han adlydes stolede"},{"SurahNumber":81,"VerseNumber":22,"Text":"Deres kammerat (Rashad) ikke er crazy"},{"SurahNumber":81,"VerseNumber":23,"Text":"Han så ham høje horisonten"},{"SurahNumber":81,"VerseNumber":24,"Text":"Han ikke er holde tilbage nogen nyhed"},{"SurahNumber":81,"VerseNumber":25,"Text":"Det ikke er snakken forkaste djævel"},{"SurahNumber":81,"VerseNumber":26,"Text":"omgående derefter hvor du gå"},{"SurahNumber":81,"VerseNumber":27,"Text":"Den er meddelelse al folkene"},{"SurahNumber":81,"VerseNumber":28,"Text":"De ønsker gå lige"},{"SurahNumber":81,"VerseNumber":29,"Text":"Whatever du gøre det er ind overensstemmelse med viljen GUD Lord universet"}]}