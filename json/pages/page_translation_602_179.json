{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":106,"VerseNumber":1,"Text":"Sabu woowde Quraysi en"},{"SurahNumber":106,"VerseNumber":2,"Text":"Goowgol ɓe ɗatngal dabbunde e ceeɗu"},{"SurahNumber":106,"VerseNumber":3,"Text":"Yo ɓe ndew jom ndu suudu"},{"SurahNumber":106,"VerseNumber":4,"Text":"Oon ñamminɗo ɓe saanga heege O hoolni ɓe e kulol"},{"SurahNumber":107,"VerseNumber":1,"Text":"Mbela a anndii oon pennoowo njoɓdi ñande daranga"},{"SurahNumber":107,"VerseNumber":2,"Text":"E oon duñoowo alyatiime"},{"SurahNumber":107,"VerseNumber":3,"Text":"O reerɗiŋtaa e ñammiingol miskiino"},{"SurahNumber":107,"VerseNumber":4,"Text":"Halkaare woodanii juulooɓe"},{"SurahNumber":107,"VerseNumber":5,"Text":"Ɓen welsandiiɓe nder juulde maɓɓe"},{"SurahNumber":107,"VerseNumber":6,"Text":"ɓeen yiinatooɓe"},{"SurahNumber":107,"VerseNumber":7,"Text":"Hoɓe kaɗa ngenndukon"},{"SurahNumber":108,"VerseNumber":1,"Text":"Min tottiima AlKawsara. @Corrected"},{"SurahNumber":108,"VerseNumber":2,"Text":"juulan joom Ma mboroɗa"},{"SurahNumber":108,"VerseNumber":3,"Text":"Gañɗo ma o woni taƴiiɗo"}]}