{"Id":150,"LanguageCodes":["fa","fas"],"Name":"fas-abdolmohammaday","Author":"Abdolmohammad Ayati","Language":"Persian","Description":"Persian","Direction":"rtl","Source":"http://tanzil.net","Translations":[{"SurahNumber":112,"VerseNumber":1,"Text":"بگو: اوست خداى يكتا،"},{"SurahNumber":112,"VerseNumber":2,"Text":"خداى كه در حاجت به او رو كنند،"},{"SurahNumber":112,"VerseNumber":3,"Text":"نه زاده است و نه زاده شده،"},{"SurahNumber":112,"VerseNumber":4,"Text":"و نه هيچ كس همتاى اوست"},{"SurahNumber":113,"VerseNumber":1,"Text":"بگو: به پروردگار صبحگاه پناه مى‌برم،"},{"SurahNumber":113,"VerseNumber":2,"Text":"از شر آنچه بيافريده است،"},{"SurahNumber":113,"VerseNumber":3,"Text":"و از شر شب چون درآيد،"},{"SurahNumber":113,"VerseNumber":4,"Text":"و از شر جادوگرانى كه در گره‌ها افسون مى‌دمند،"},{"SurahNumber":113,"VerseNumber":5,"Text":"از شر حسود چون رشك مى‌ورزد"},{"SurahNumber":114,"VerseNumber":1,"Text":"بگو: به پروردگار مردم پناه مى‌برم،"},{"SurahNumber":114,"VerseNumber":2,"Text":"فرمانرواى مردم،"},{"SurahNumber":114,"VerseNumber":3,"Text":"خداى مردم،"},{"SurahNumber":114,"VerseNumber":4,"Text":"از شر وسوسه وسوسه‌گر نهانى،"},{"SurahNumber":114,"VerseNumber":5,"Text":"آن كه در دلهاى مردم وسوسه مى‌كند،"},{"SurahNumber":114,"VerseNumber":6,"Text":"خواه از جنيان باشد يا از آدميان"}]}