{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":100,"VerseNumber":10,"Text":"Ve hussıle ma fis sudur"},{"SurahNumber":100,"VerseNumber":11,"Text":"İnne rabbehüm bihim yevmeizin le habir"},{"SurahNumber":101,"VerseNumber":1,"Text":"El kariah"},{"SurahNumber":101,"VerseNumber":2,"Text":"Mel kariah"},{"SurahNumber":101,"VerseNumber":3,"Text":"Ve ma edrake mel kariah"},{"SurahNumber":101,"VerseNumber":4,"Text":"Yevme yekunün nasü kelferaşil mebsus"},{"SurahNumber":101,"VerseNumber":5,"Text":"Ve tekunül cibalü kelıhnil menfuş"},{"SurahNumber":101,"VerseNumber":6,"Text":"Fe emma men sekulet mevazınüh"},{"SurahNumber":101,"VerseNumber":7,"Text":"Fe hüve fi ıyşetir radıyeh"},{"SurahNumber":101,"VerseNumber":8,"Text":"Ve emma men haffet mevazınüh"},{"SurahNumber":101,"VerseNumber":9,"Text":"Fe ümmühu havıyeh"},{"SurahNumber":101,"VerseNumber":10,"Text":"Ve ma edrake mahiyeh"},{"SurahNumber":101,"VerseNumber":11,"Text":"Narun hamiyeh"},{"SurahNumber":102,"VerseNumber":1,"Text":"Elhakümüt tekasür"},{"SurahNumber":102,"VerseNumber":2,"Text":"Hatta zürtümülmekabir"},{"SurahNumber":102,"VerseNumber":3,"Text":"Kella sevfe ta´lemun"},{"SurahNumber":102,"VerseNumber":4,"Text":"Sümme kella sevfe ta´lemun"},{"SurahNumber":102,"VerseNumber":5,"Text":"Kella lev ta´lemune ılmel yekıyn"},{"SurahNumber":102,"VerseNumber":6,"Text":"Le teravünnelcehıym"},{"SurahNumber":102,"VerseNumber":7,"Text":"Sümme leteravünneha aynelyakıyn"},{"SurahNumber":102,"VerseNumber":8,"Text":"Sümme le tüs´elünne yevmeizin anin neıym"}]}