{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":56,"VerseNumber":17,"Text":"Sukaaɓe duuminaaɓe hino wanngoo hakkunde maɓɓe"},{"SurahNumber":56,"VerseNumber":18,"Text":"wonndude e ñedduɗe, e cataloy, e koroy e [hebbiniraakoy] beere ilaynde"},{"SurahNumber":56,"VerseNumber":19,"Text":"ɓe muusetaake ko*e nder ton, ɓe siwritaa"},{"SurahNumber":56,"VerseNumber":20,"Text":"e muuyanteeɗi ɗi ɓe suɓotoo"},{"SurahNumber":56,"VerseNumber":21,"Text":"e teewu colli ngu ɓe tuuyatee"},{"SurahNumber":56,"VerseNumber":22,"Text":"e huurul'ayniiɓe -yaaja gite en"},{"SurahNumber":56,"VerseNumber":23,"Text":"nannduɓe e kaŋŋe lu'lu'u maraaɗe"},{"SurahNumber":56,"VerseNumber":24,"Text":"njoɓdi ko ɓe gollaynoo kon"},{"SurahNumber":56,"VerseNumber":25,"Text":"Ɓe nanataa nder ton haala meere, wanaa ko bakkondiŋta"},{"SurahNumber":56,"VerseNumber":26,"Text":"si wanaa wi'aneede : \"Salaaman! Salaaman!\" [Saɗi mon! saɗi mon]"},{"SurahNumber":56,"VerseNumber":27,"Text":"E wontirooɓe ñaamo ɓen, ko woni ɓen yimɓe ñaamo"},{"SurahNumber":56,"VerseNumber":28,"Text":"[Ɓe wonay] e leɗɗe jaaɓe newɗuɗe"},{"SurahNumber":56,"VerseNumber":29,"Text":"e banaanaje fawandirɗe"},{"SurahNumber":56,"VerseNumber":30,"Text":"e ɗowdi duumiindi"},{"SurahNumber":56,"VerseNumber":31,"Text":"e ndiyam juurayɗam"},{"SurahNumber":56,"VerseNumber":32,"Text":"e ɓiɗɓe leɗɗe ɗuuɗuɗe"},{"SurahNumber":56,"VerseNumber":33,"Text":"ɗe taƴondirtaa ɗe haɗetaake"},{"SurahNumber":56,"VerseNumber":34,"Text":"e danɗe ɓamtaaɗe"},{"SurahNumber":56,"VerseNumber":35,"Text":"Ko Menen tagi fuɗɗi ɓe"},{"SurahNumber":56,"VerseNumber":36,"Text":"Men waɗi ɓe curbaaɓe-boomi"},{"SurahNumber":56,"VerseNumber":37,"Text":"yiɗuɓe worɓe mun, goreeɓe"},{"SurahNumber":56,"VerseNumber":38,"Text":"[Men marani ɓe] yimɓe ñaamo ɓen"},{"SurahNumber":56,"VerseNumber":39,"Text":"Dental jeyaaɓe e [mofte] adinooɓe ɓen"},{"SurahNumber":56,"VerseNumber":40,"Text":"e seeɗaaɓe jeyaaɓe e [mofte] sakkitiiɓe ɓen"},{"SurahNumber":56,"VerseNumber":41,"Text":"E wontirooɓe nano ɓen. Ko homɓe woni ɓen yimɓe nano"},{"SurahNumber":56,"VerseNumber":42,"Text":"[Ɓen] no e nguleendi e fatayɗam"},{"SurahNumber":56,"VerseNumber":43,"Text":"e ɗowdi curki ɓawluki"},{"SurahNumber":56,"VerseNumber":44,"Text":"wonaa ɓuuɓundi, wanaa teddundi"},{"SurahNumber":56,"VerseNumber":45,"Text":"Ado ɗum, hari ko ɓe neeminaaɓe"},{"SurahNumber":56,"VerseNumber":46,"Text":"ɓe laatino hiɓe deppitoo e bakkaat mawɗi ɗin"},{"SurahNumber":56,"VerseNumber":47,"Text":"hiɓe wi'aynoo: \"Enee, si men maayii, men wontii mbullaari e ƴi'e, hara ko men immintinoyteeɓe"},{"SurahNumber":56,"VerseNumber":48,"Text":"hay baabiraaɓe amen adinooɓe ɓen"},{"SurahNumber":56,"VerseNumber":49,"Text":"Maaku : \"Pellet, aramɓe e sakkitiiɓe"},{"SurahNumber":56,"VerseNumber":50,"Text":"ko mooɓoyteeɓe e ñalaande anndaande waqutu mun"}]}