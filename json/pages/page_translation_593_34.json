{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":89,"VerseNumber":1,"Text":"والفجر"},{"SurahNumber":89,"VerseNumber":2,"Text":"وليال عشر"},{"SurahNumber":89,"VerseNumber":3,"Text":"والشفع والوتر"},{"SurahNumber":89,"VerseNumber":4,"Text":"واليل اذا يسر"},{"SurahNumber":89,"VerseNumber":5,"Text":"هل في ذلك قسم لذي حجر"},{"SurahNumber":89,"VerseNumber":6,"Text":"الم تر كيف فعل ربك بعاد"},{"SurahNumber":89,"VerseNumber":7,"Text":"ارم ذات العماد"},{"SurahNumber":89,"VerseNumber":8,"Text":"التي لم يخلق مثلها في البلد"},{"SurahNumber":89,"VerseNumber":9,"Text":"وثمود الذين جابوا الصخر بالواد"},{"SurahNumber":89,"VerseNumber":10,"Text":"وفرعون ذي الاوتاد"},{"SurahNumber":89,"VerseNumber":11,"Text":"الذين طغوا في البلد"},{"SurahNumber":89,"VerseNumber":12,"Text":"فاكثروا فيها الفساد"},{"SurahNumber":89,"VerseNumber":13,"Text":"فصب عليهم ربك سوط عذاب"},{"SurahNumber":89,"VerseNumber":14,"Text":"ان ربك لبالمرصاد"},{"SurahNumber":89,"VerseNumber":15,"Text":"فاما الانسن اذا ما ابتلىه ربه فاكرمه ونعمه فيقول ربي اكرمن"},{"SurahNumber":89,"VerseNumber":16,"Text":"واما اذا ما ابتلىه فقدر عليه رزقه فيقول ربي اهنن"},{"SurahNumber":89,"VerseNumber":17,"Text":"كلا بل لا تكرمون اليتيم"},{"SurahNumber":89,"VerseNumber":18,"Text":"ولا تحضون على طعام المسكين"},{"SurahNumber":89,"VerseNumber":19,"Text":"وتاكلون التراث اكل ا لم ا"},{"SurahNumber":89,"VerseNumber":20,"Text":"وتحبون المال حب ا جم ا"},{"SurahNumber":89,"VerseNumber":21,"Text":"كلا اذا دكت الارض دك ا دك ا"},{"SurahNumber":89,"VerseNumber":22,"Text":"وجاء ربك والملك صف ا صف ا"},{"SurahNumber":89,"VerseNumber":23,"Text":"وجايء يوميذ بجهنم يوميذ يتذكر الانسن وانى له الذكرى"}]}