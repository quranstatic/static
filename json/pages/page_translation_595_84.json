{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":91,"VerseNumber":1,"Text":"Ved solen dens brightness"},{"SurahNumber":91,"VerseNumber":2,"Text":"Månen følger det"},{"SurahNumber":91,"VerseNumber":3,"Text":"Dagen afslører"},{"SurahNumber":91,"VerseNumber":4,"Text":"Natten bedækker"},{"SurahNumber":91,"VerseNumber":5,"Text":"Himlen Ham byggede det"},{"SurahNumber":91,"VerseNumber":6,"Text":"Jorden Ham sustains det"},{"SurahNumber":91,"VerseNumber":7,"Text":"Sjælen Ham oprettede det"},{"SurahNumber":91,"VerseNumber":8,"Text":"Derefter udfoldede det hvad er onde hvad er gode"},{"SurahNumber":91,"VerseNumber":9,"Text":"Succesrige er en indfrir det"},{"SurahNumber":91,"VerseNumber":10,"Text":"mislykkes er en forsømmer det"},{"SurahNumber":91,"VerseNumber":11,"Text":"Thamoud's vantro voldte dem transgress"},{"SurahNumber":91,"VerseNumber":12,"Text":"De fulgte værst hos dem"},{"SurahNumber":91,"VerseNumber":13,"Text":"Gud bud sie til dem Den er gud kamel; lade hendes drink"},{"SurahNumber":91,"VerseNumber":14,"Text":"De disbelieved ham slaughtered hende! Deres Lord derefter requited dem i deres synd annihilated dem"},{"SurahNumber":91,"VerseNumber":15,"Text":"men de som kom efter dem vedvare heedless"},{"SurahNumber":92,"VerseNumber":1,"Text":"Ved natten det bedække"},{"SurahNumber":92,"VerseNumber":2,"Text":"Dagen det afslører"},{"SurahNumber":92,"VerseNumber":3,"Text":"Ham oprettede mandlig kvindelig"},{"SurahNumber":92,"VerseNumber":4,"Text":"Deres arbejder er af adskillige arter"},{"SurahNumber":92,"VerseNumber":5,"Text":"Angående ham giver velgørenhed vedligeholde retmæssighed"},{"SurahNumber":92,"VerseNumber":6,"Text":"Og upholds scripture"},{"SurahNumber":92,"VerseNumber":7,"Text":"Vi forestå ham lykke"},{"SurahNumber":92,"VerseNumber":8,"Text":"Han som er nærige han er rige"},{"SurahNumber":92,"VerseNumber":9,"Text":"Og disbelieves scripture"},{"SurahNumber":92,"VerseNumber":10,"Text":"Vi forestå ham elendighed"},{"SurahNumber":92,"VerseNumber":11,"Text":"Hans penge ikke hjælpes ham hvornår han falder"},{"SurahNumber":92,"VerseNumber":12,"Text":"Vi forsyner vejledningen"},{"SurahNumber":92,"VerseNumber":13,"Text":"Vi kontrollerer Herefter også ligesom den liv"},{"SurahNumber":92,"VerseNumber":14,"Text":"Jeg advarer jer blussende Hellfire"}]}