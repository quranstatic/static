{"Id":82,"LanguageCodes":["cs","ces"],"Name":"ces-prekladihrbek","Author":"Preklad I. Hrbek","Language":"Czech","Description":"Czech","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":112,"VerseNumber":1,"Text":"Rci: \"On Bůh je jedinečný"},{"SurahNumber":112,"VerseNumber":2,"Text":"Bůh sám o sobě věčný"},{"SurahNumber":112,"VerseNumber":3,"Text":"Neplodil a nebyl zplozen"},{"SurahNumber":112,"VerseNumber":4,"Text":"a není nikoho, kdo je mu roven"},{"SurahNumber":113,"VerseNumber":1,"Text":"Rci: \"Utíkám se v ochranu Pána záře jitřní"},{"SurahNumber":113,"VerseNumber":2,"Text":"před zlem Jeho stvoření"},{"SurahNumber":113,"VerseNumber":3,"Text":"před zlem temnoty, když se šíří"},{"SurahNumber":113,"VerseNumber":4,"Text":"před zlem žen, jež do uzlů prskají"},{"SurahNumber":113,"VerseNumber":5,"Text":"před zlem závistníka, když závidí"},{"SurahNumber":114,"VerseNumber":1,"Text":"Rci: \"Utíkám se v ochranu Pána lidí"},{"SurahNumber":114,"VerseNumber":2,"Text":"vládce lidí"},{"SurahNumber":114,"VerseNumber":3,"Text":"Boha lidí"},{"SurahNumber":114,"VerseNumber":4,"Text":"před zlem našeptavače pokradmého"},{"SurahNumber":114,"VerseNumber":5,"Text":"jenž našeptává do hrudi lidí"},{"SurahNumber":114,"VerseNumber":6,"Text":"ať již je z džinů či z lidí"}]}