{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":106,"VerseNumber":1,"Text":"Li iylafi kurayş"},{"SurahNumber":106,"VerseNumber":2,"Text":"İylafihim rıhleteş şitai ves sayf"},{"SurahNumber":106,"VerseNumber":3,"Text":"Felya´büdu rabbe hazelbeyt"},{"SurahNumber":106,"VerseNumber":4,"Text":"Ellezı at´amehüm min cuıv ve amenehüm min havf"},{"SurahNumber":107,"VerseNumber":1,"Text":"E raeytellezi yükezzibü bid din"},{"SurahNumber":107,"VerseNumber":2,"Text":"Fe zalikellezi yedu´ul yetim"},{"SurahNumber":107,"VerseNumber":3,"Text":"Ve la yehuddu ala taamil miskin"},{"SurahNumber":107,"VerseNumber":4,"Text":"Fe veylün lil müsallin"},{"SurahNumber":107,"VerseNumber":5,"Text":"Ellezine hüm an salatihim sahun"},{"SurahNumber":107,"VerseNumber":6,"Text":"Ellezine hüm yüraun"},{"SurahNumber":107,"VerseNumber":7,"Text":"Ve yemneunel maun"},{"SurahNumber":108,"VerseNumber":1,"Text":"İnna a´taynakel kevser"},{"SurahNumber":108,"VerseNumber":2,"Text":"Fe salli li rabbike venhar"},{"SurahNumber":108,"VerseNumber":3,"Text":"İnne şanieke hüvel´ebter"}]}