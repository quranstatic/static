{"Id":349,"LanguageCodes":["sw","swa"],"Name":"swa-alimuhsinalbarw","Author":"Ali Muhsin Al Barwani","Language":"Swahili","Description":"Swahili","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":80,"VerseNumber":1,"Text":"Alikunja kipaji na akageuka"},{"SurahNumber":80,"VerseNumber":2,"Text":"Kwa sababu alimjia kipofu"},{"SurahNumber":80,"VerseNumber":3,"Text":"Na nini kitakujuulisha pengine yeye atatakasika"},{"SurahNumber":80,"VerseNumber":4,"Text":"Au atawaidhika, na mawaidha yamfae"},{"SurahNumber":80,"VerseNumber":5,"Text":"Ama ajionaye hana haja"},{"SurahNumber":80,"VerseNumber":6,"Text":"Wewe ndio unamshughulikia"},{"SurahNumber":80,"VerseNumber":7,"Text":"Na si juu yako kama hakutakasika"},{"SurahNumber":80,"VerseNumber":8,"Text":"Ama anaye kujia kwa juhudi"},{"SurahNumber":80,"VerseNumber":9,"Text":"Naye anaogopa"},{"SurahNumber":80,"VerseNumber":10,"Text":"Ndio wewe unampuuza"},{"SurahNumber":80,"VerseNumber":11,"Text":"Sivyo hivyo! Huku ni kukumbushana"},{"SurahNumber":80,"VerseNumber":12,"Text":"Basi anaye penda akumbuke"},{"SurahNumber":80,"VerseNumber":13,"Text":"Yamo katika kurasa zilizo hishimiwa"},{"SurahNumber":80,"VerseNumber":14,"Text":"Zilizo inuliwa, zilizo takaswa"},{"SurahNumber":80,"VerseNumber":15,"Text":"Zimo mikononi mwa Malaika waandishi"},{"SurahNumber":80,"VerseNumber":16,"Text":"Watukufu, wema"},{"SurahNumber":80,"VerseNumber":17,"Text":"Ameangamia mwanaadamu! Nini kinacho mkufurisha"},{"SurahNumber":80,"VerseNumber":18,"Text":"Kwa kitu gani amemuumba"},{"SurahNumber":80,"VerseNumber":19,"Text":"Kwa tone la manii, akamuumba na akamkadiria"},{"SurahNumber":80,"VerseNumber":20,"Text":"Kisha akamsahilishia njia"},{"SurahNumber":80,"VerseNumber":21,"Text":"Kisha akamfisha, akamtia kaburini"},{"SurahNumber":80,"VerseNumber":22,"Text":"Kisha apendapo atamfufua"},{"SurahNumber":80,"VerseNumber":23,"Text":"La! Hajamaliza aliyo muamuru"},{"SurahNumber":80,"VerseNumber":24,"Text":"Hebu mtu na atazame chakula chake"},{"SurahNumber":80,"VerseNumber":25,"Text":"Hakika Sisi tumemimina maji kwa nguvu"},{"SurahNumber":80,"VerseNumber":26,"Text":"Tena tukaipasua pasua ardhi kwa nguvu"},{"SurahNumber":80,"VerseNumber":27,"Text":"Kisha tukaotesha humo nafaka"},{"SurahNumber":80,"VerseNumber":28,"Text":"Na zabibu, na mimea ya majani"},{"SurahNumber":80,"VerseNumber":29,"Text":"Na mizaituni, na mitende"},{"SurahNumber":80,"VerseNumber":30,"Text":"Na bustani zenye miti iliyo songana baraabara"},{"SurahNumber":80,"VerseNumber":31,"Text":"Na matunda, na malisho ya wanyama"},{"SurahNumber":80,"VerseNumber":32,"Text":"Kwa manufaa yenu na mifugo yenu"},{"SurahNumber":80,"VerseNumber":33,"Text":"Basi utakapo kuja ukelele"},{"SurahNumber":80,"VerseNumber":34,"Text":"Siku ambayo mtu atamkimbia nduguye"},{"SurahNumber":80,"VerseNumber":35,"Text":"Na mamaye na babaye"},{"SurahNumber":80,"VerseNumber":36,"Text":"Na mkewe na wanawe"},{"SurahNumber":80,"VerseNumber":37,"Text":"Kila mtu miongoni mwao siku hiyo atakuwa na lake la kumtosha"},{"SurahNumber":80,"VerseNumber":38,"Text":"Siku hiyo ziko nyuso zitazo nawiri"},{"SurahNumber":80,"VerseNumber":39,"Text":"Zitacheka, zitachangamka"},{"SurahNumber":80,"VerseNumber":40,"Text":"Na nyuso siku hiyo zitakuwa na mavumbi"},{"SurahNumber":80,"VerseNumber":41,"Text":"Giza totoro litazifunika"},{"SurahNumber":80,"VerseNumber":42,"Text":"Hao ndio makafiri watenda maovu"}]}