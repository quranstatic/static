{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"Hvornår himlen splintres"},{"SurahNumber":82,"VerseNumber":2,"Text":"Planetterne scattered"},{"SurahNumber":82,"VerseNumber":3,"Text":"Oceanerne springes"},{"SurahNumber":82,"VerseNumber":4,"Text":"Gravene åbnes"},{"SurahNumber":82,"VerseNumber":5,"Text":"Enhvere sjæl ville grundlægge ydre hvad voldte det avancere hvad voldte det regress"},{"SurahNumber":82,"VerseNumber":6,"Text":"O I menneskelig være hvad bortledte I fra Deres Lord Ærværdigest"},{"SurahNumber":82,"VerseNumber":7,"Text":"En oprettede du formgav du perfektionerede I"},{"SurahNumber":82,"VerseNumber":8,"Text":"Ind whatever konstruktion Han udvalgte Han konstruerede det"},{"SurahNumber":82,"VerseNumber":9,"Text":"Indeed du disbelieve religionen"},{"SurahNumber":82,"VerseNumber":10,"Text":"Oblivious til at xxxx er (usynlige) vogtere ca. jer"},{"SurahNumber":82,"VerseNumber":11,"Text":"De er ærlige optagere"},{"SurahNumber":82,"VerseNumber":12,"Text":"De registrerer alt du gør"},{"SurahNumber":82,"VerseNumber":13,"Text":"Sikkert pious trænge fryd"},{"SurahNumber":82,"VerseNumber":14,"Text":"Mens wicked trænge Helvede"},{"SurahNumber":82,"VerseNumber":15,"Text":"incur det Dagen Bedømmelse"},{"SurahNumber":82,"VerseNumber":16,"Text":"De aldrig forlader det"},{"SurahNumber":82,"VerseNumber":17,"Text":"Awesome er Dagen Bedømmelse"},{"SurahNumber":82,"VerseNumber":18,"Text":"Hvad dag; Dagen Bedømmelse"},{"SurahNumber":82,"VerseNumber":19,"Text":"Den er dagen hvornår ing sjæl hjælpes another sjæl al bestemmelser den dag ville belong til GUD"},{"SurahNumber":83,"VerseNumber":1,"Text":"Ve til snyderne"},{"SurahNumber":83,"VerseNumber":2,"Text":"Som fordre fulde måle hvornår modtagende folkene"},{"SurahNumber":83,"VerseNumber":3,"Text":"Hvornår give dem måle vægt de snyde"},{"SurahNumber":83,"VerseNumber":4,"Text":"De ikke kender at de resurrected"},{"SurahNumber":83,"VerseNumber":5,"Text":"På tremendous dag"},{"SurahNumber":83,"VerseNumber":6,"Text":"Den er dagen hvornår al folk stå Lord universet"}]}