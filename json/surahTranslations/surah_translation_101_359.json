{"Id":359,"LanguageCodes":["te","tel"],"Name":"tel-abdulraheemmoha-la","Author":"Abdul Raheem Mohammad Moulana","Language":"Telugu","Description":"Telugu - Transliteration","Direction":"ltr","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"a! Adaragotte maha upadravam"},{"SurahNumber":101,"VerseNumber":2,"Text":"emita adaragotte maha upadravam"},{"SurahNumber":101,"VerseNumber":3,"Text":"mariyu a adaragotte maha upadravam, ante emito nikem telusu"},{"SurahNumber":101,"VerseNumber":4,"Text":"a roju manavulu cellaceduraina cim'metala vale ayipotaru"},{"SurahNumber":101,"VerseNumber":5,"Text":"mariyu parvatalu rangu rangula ekina dudi vale ayi potayi"},{"SurahNumber":101,"VerseNumber":6,"Text":"appudu evadi trasupallalu (satkaryalato) baruvuga untayo"},{"SurahNumber":101,"VerseNumber":7,"Text":"atadu (svarganlo) sukhavantamaina jivitam gaduputadu"},{"SurahNumber":101,"VerseNumber":8,"Text":"mariyu evadi (satkaryala) trasu pallalu telikaga untayo"},{"SurahNumber":101,"VerseNumber":9,"Text":"atani nivasam adhah patalame"},{"SurahNumber":101,"VerseNumber":10,"Text":"mariyu adi emito nikem telusu"},{"SurahNumber":101,"VerseNumber":11,"Text":"adoka bhagabhagamande agni (gundam)"}]}