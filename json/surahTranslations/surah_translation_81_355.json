{"Id":355,"LanguageCodes":["ta","tam"],"Name":"tam-janturstfoundat-la","Author":"Jan Turst Foundation","Language":"Tamil","Description":"Tamil - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":81,"VerseNumber":1,"Text":"curiyan (oliyillatatakac) curuttappatum potu"},{"SurahNumber":81,"VerseNumber":2,"Text":"natcattirankal (oliyilantu) utirntu vilumpotu"},{"SurahNumber":81,"VerseNumber":3,"Text":"malaikal peyarkkappatum potu"},{"SurahNumber":81,"VerseNumber":4,"Text":"cul nirainta ottakaikal (kavanippararru) vitappatum potu"},{"SurahNumber":81,"VerseNumber":5,"Text":"kattu mirukankal (manitarkalutanum, itara piranikalutanum) onru cerkkappatumpotu"},{"SurahNumber":81,"VerseNumber":6,"Text":"katalkal ti muttappatumpotu"},{"SurahNumber":81,"VerseNumber":7,"Text":"uyirkal onrinaikkappatum potu"},{"SurahNumber":81,"VerseNumber":8,"Text":"uyirutan putaikkappatta pen (kulantai) vinavappatum potu"},{"SurahNumber":81,"VerseNumber":9,"Text":"entak kurrattirkaka atu kollappattatu?\" Enru"},{"SurahNumber":81,"VerseNumber":10,"Text":"pattolaikal virikkappatum potu"},{"SurahNumber":81,"VerseNumber":11,"Text":"vanam akarrappatum potu"},{"SurahNumber":81,"VerseNumber":12,"Text":"narakam koluntuvittu eriyumaru ceyyappatum potu"},{"SurahNumber":81,"VerseNumber":13,"Text":"cuvarkkam camipamaka kontu varappatumpotu"},{"SurahNumber":81,"VerseNumber":14,"Text":"ovvor atmavum, tan kontu vantatai arintu kollum"},{"SurahNumber":81,"VerseNumber":15,"Text":"enave, pinne vilakic celpavai (kirakankalin) mitu cattiyamaka"},{"SurahNumber":81,"VerseNumber":16,"Text":"mun cenru kontiruppavai maraipavai (mitum)"},{"SurahNumber":81,"VerseNumber":17,"Text":"pinvankic cellum iravin mitum"},{"SurahNumber":81,"VerseNumber":18,"Text":"muccuvittuk kontelum vaikaraiyin mitum cattiyamaka"},{"SurahNumber":81,"VerseNumber":19,"Text":"niccayamaka (ikkur'an) mikavum kanniyamikka oru tutuvar (jiprayil mulam vanta) collakum"},{"SurahNumber":81,"VerseNumber":20,"Text":"(avar) caktimikkavar; arsukkutaiyavanitam perum pataviyutaiyavar"},{"SurahNumber":81,"VerseNumber":21,"Text":"(vanavar tam) talaivar; anriyum nampikkaikkuriyavar"},{"SurahNumber":81,"VerseNumber":22,"Text":"melum unkal tolar paittiyakkarar allar"},{"SurahNumber":81,"VerseNumber":23,"Text":"avar tittamaka avarai (jiprayilai) telivana ativanattil kantar"},{"SurahNumber":81,"VerseNumber":24,"Text":"melum, avar maraivana ceytikalai kuruvatil ulopittanam ceypavarallar"},{"SurahNumber":81,"VerseNumber":25,"Text":"anriyum, itu virattappatta saittanin vakkalla"},{"SurahNumber":81,"VerseNumber":26,"Text":"enave, (nervaliyai vittum) ninkal enke celkinrirkal"},{"SurahNumber":81,"VerseNumber":27,"Text":"itu, akilattarukkellam upatecamakum"},{"SurahNumber":81,"VerseNumber":28,"Text":"Unkalil ninrum yar nervaliyai virumpukiraro, avarukku (nallupatecamakum)"},{"SurahNumber":81,"VerseNumber":29,"Text":"ayinum, akilankalukkellam iraivanakiya allah natinalanri ninkal (nallupatecam pera) natamattirkal"}]}