{"Id":349,"LanguageCodes":["sw","swa"],"Name":"swa-alimuhsinalbarw","Author":"Ali Muhsin Al Barwani","Language":"Swahili","Description":"Swahili","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":102,"VerseNumber":1,"Text":"Kumekushughulisheni kutafuta wingi"},{"SurahNumber":102,"VerseNumber":2,"Text":"Mpaka mje makaburini"},{"SurahNumber":102,"VerseNumber":3,"Text":"Sivyo hivyo! Mtakuja jua"},{"SurahNumber":102,"VerseNumber":4,"Text":"Tena sivyo hivyo! Mtakuja jua"},{"SurahNumber":102,"VerseNumber":5,"Text":"Sivyo hivyo! Lau mngeli jua kwa ujuzi wa yakini"},{"SurahNumber":102,"VerseNumber":6,"Text":"Basi bila ya shaka mtaiona Jahannamu"},{"SurahNumber":102,"VerseNumber":7,"Text":"Tena, bila ya shaka, mtaiona kwa jicho la yakini"},{"SurahNumber":102,"VerseNumber":8,"Text":"Tena bila ya shaka mtaulizwa siku hiyo juu ya neema"}]}