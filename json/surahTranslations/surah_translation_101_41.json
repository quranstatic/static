{"Id":41,"LanguageCodes":["az","aze"],"Name":"aze-alikhanmusayev-la","Author":"Alikhan Musayev","Language":"Azerbaijani","Description":"Azerbaijani - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"Qəlbləri Qorxuya salan"},{"SurahNumber":101,"VerseNumber":2,"Text":"Qəlbləri Qorxuya salan nədir"},{"SurahNumber":101,"VerseNumber":3,"Text":"Sən haradan biləsən ki, qəlbləri Qorxuya salan nədir"},{"SurahNumber":101,"VerseNumber":4,"Text":"O gun insanlar ətrafa səpələnmis pərvanələr tək olacaqlar"},{"SurahNumber":101,"VerseNumber":5,"Text":"Daglar isə didilmis yun gorkəmi alacaqdır"},{"SurahNumber":101,"VerseNumber":6,"Text":"O gun kimin tərəziləri agır gələrsə"},{"SurahNumber":101,"VerseNumber":7,"Text":"xosbəxt guzəran surəcək"},{"SurahNumber":101,"VerseNumber":8,"Text":"Kimin tərəziləri yungul gələrsə"},{"SurahNumber":101,"VerseNumber":9,"Text":"məskəni Haviyə olacaqdır"},{"SurahNumber":101,"VerseNumber":10,"Text":"Sən haradan biləsən ki, o nədir"},{"SurahNumber":101,"VerseNumber":11,"Text":"O, cox qızmar bir oddur"}]}