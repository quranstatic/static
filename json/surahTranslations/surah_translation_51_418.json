{"Id":418,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay-la","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":51,"VerseNumber":1,"Text":"vezzariyati zerva"},{"SurahNumber":51,"VerseNumber":2,"Text":"felhamilati vikra"},{"SurahNumber":51,"VerseNumber":3,"Text":"felcariyati yusra"},{"SurahNumber":51,"VerseNumber":4,"Text":"felmukassimati emra"},{"SurahNumber":51,"VerseNumber":5,"Text":"innema tu`adune lesadik"},{"SurahNumber":51,"VerseNumber":6,"Text":"veinne-ddine levaki"},{"SurahNumber":51,"VerseNumber":7,"Text":"vessemai zati-lhubuk"},{"SurahNumber":51,"VerseNumber":8,"Text":"innekum lefi kavlim muhtelif"},{"SurahNumber":51,"VerseNumber":9,"Text":"yu'feku `anhu men ufik"},{"SurahNumber":51,"VerseNumber":10,"Text":"kutile-lharrasun"},{"SurahNumber":51,"VerseNumber":11,"Text":"ellezine hum fi gamratin sahun"},{"SurahNumber":51,"VerseNumber":12,"Text":"yes'elune eyyane yevmu-ddin"},{"SurahNumber":51,"VerseNumber":13,"Text":"yevme hum `ale-nnari yuftenun"},{"SurahNumber":51,"VerseNumber":14,"Text":"zuku fitnetekum. haze-llezi kuntum bihi testa`cilun"},{"SurahNumber":51,"VerseNumber":15,"Text":"inne-lmuttekine fi cennativ ve`uyun"},{"SurahNumber":51,"VerseNumber":16,"Text":"ahizine ma atahum rabbuhum. innehum kanu kable zalike muhsinin"},{"SurahNumber":51,"VerseNumber":17,"Text":"kanu kalilem mine-lleyli ma yehce`un"},{"SurahNumber":51,"VerseNumber":18,"Text":"vebil'eshari hum yestagfirun"},{"SurahNumber":51,"VerseNumber":19,"Text":"vefi emvalihim hakkul lissaili velmahrum"},{"SurahNumber":51,"VerseNumber":20,"Text":"vefi-l'ardi ayatul lilmukinin"},{"SurahNumber":51,"VerseNumber":21,"Text":"vefi enfusikum. efela tubsirun"},{"SurahNumber":51,"VerseNumber":22,"Text":"vefi-ssemai rizkukum vema tu`adun"},{"SurahNumber":51,"VerseNumber":23,"Text":"feverabbi-ssemai vel'ardi innehu lehakkum misle ma ennekum tentikun"},{"SurahNumber":51,"VerseNumber":24,"Text":"hel etake hadisu dayfi ibrahime-lmukramin"},{"SurahNumber":51,"VerseNumber":25,"Text":"iz dehalu `aleyhi fekalu selama. kale selam. kavmum munkerun"},{"SurahNumber":51,"VerseNumber":26,"Text":"feraga ila ehlihi fecae bi`iclin semin"},{"SurahNumber":51,"VerseNumber":27,"Text":"fekarrabehu ileyhim kale ela te'kulun"},{"SurahNumber":51,"VerseNumber":28,"Text":"feevcese minhum hifeh. kalu la tehaf. vebesseruhu bigulamin `alim"},{"SurahNumber":51,"VerseNumber":29,"Text":"feakbeleti-mraetuhu fi sarratin fesakket vecheha vekalet `acuzun `akim"},{"SurahNumber":51,"VerseNumber":30,"Text":"kalu kezaliki kale rabbuk. innehu huve-lhakimu-l`alim"},{"SurahNumber":51,"VerseNumber":31,"Text":"kale fema hatbukum eyyuhe-lmurselun"},{"SurahNumber":51,"VerseNumber":32,"Text":"kalu inna ursilna ila kavmim mucrimin"},{"SurahNumber":51,"VerseNumber":33,"Text":"linursile `aleyhim hicaratem min tin"},{"SurahNumber":51,"VerseNumber":34,"Text":"musevvemeten `inde rabbike lilmusrifin"},{"SurahNumber":51,"VerseNumber":35,"Text":"feahracna men kane fiha mine-lmu'minin"},{"SurahNumber":51,"VerseNumber":36,"Text":"fema vecedna fiha gayra beytim mine-lmuslimin"},{"SurahNumber":51,"VerseNumber":37,"Text":"veterakna fiha ayetel lillezine yehafune-l`azabe-l'elim"},{"SurahNumber":51,"VerseNumber":38,"Text":"vefi musa iz erselnahu ila fir`avne bisultanim mubin"},{"SurahNumber":51,"VerseNumber":39,"Text":"fetevella biruknihi vekale sahirun ev mecnun"},{"SurahNumber":51,"VerseNumber":40,"Text":"feehaznahu vecunudehu fenebeznahum fi-lyemmi vehuve mulim"},{"SurahNumber":51,"VerseNumber":41,"Text":"vefi `adin iz erselna `aleyhimu-rriha-l`akim"},{"SurahNumber":51,"VerseNumber":42,"Text":"ma tezeru min sey'in etet `aleyhi illa ce`alethu kelramim"},{"SurahNumber":51,"VerseNumber":43,"Text":"vefi semude iz kile lehum temette`u hatta hin"},{"SurahNumber":51,"VerseNumber":44,"Text":"fe`atev `an emri rabbihim feehazethumu-ssa`ikatu vehum yenzurun"},{"SurahNumber":51,"VerseNumber":45,"Text":"feme-steta`u min kiyamiv vema kanu muntesirin"},{"SurahNumber":51,"VerseNumber":46,"Text":"vekavme nuhim min kabl. innehum kanu kavmen fasikin"},{"SurahNumber":51,"VerseNumber":47,"Text":"vessemae beneynaha bieydiv veinna lemusi`un"},{"SurahNumber":51,"VerseNumber":48,"Text":"vel'arda ferasnaha feni`me-lmahidun"},{"SurahNumber":51,"VerseNumber":49,"Text":"vemin kulli sey'in halakna zevceyni le`allekum tezekkerun"},{"SurahNumber":51,"VerseNumber":50,"Text":"fefirru ile-llah. inni lekum minhu nezirum mubin"},{"SurahNumber":51,"VerseNumber":51,"Text":"vela tec`alu me`a-llahi ilahen ahar. inni lekum minhu nezirum mubin"},{"SurahNumber":51,"VerseNumber":52,"Text":"kezalike ma ete-llezine min kablihim mir rasulin illa kalu sahirun ev mecnun"},{"SurahNumber":51,"VerseNumber":53,"Text":"etevasav bih. bel hum kavmun tagun"},{"SurahNumber":51,"VerseNumber":54,"Text":"fetevelle `anhum fema ente bimelum"},{"SurahNumber":51,"VerseNumber":55,"Text":"vezekkir feinne-zzikra tenfe`u-lmu'minin"},{"SurahNumber":51,"VerseNumber":56,"Text":"vema halaktu-lcinne vel'inse illa liya`budun"},{"SurahNumber":51,"VerseNumber":57,"Text":"ma uridu minhum mir rizkiv vema uridu ey yut`imun"},{"SurahNumber":51,"VerseNumber":58,"Text":"inne-llahe huve-rrazzaku zu-lkuvveti-lmetin"},{"SurahNumber":51,"VerseNumber":59,"Text":"feinne lillezine zalemu zenubem misle zenubi ashabihim fela yesta`cilun"},{"SurahNumber":51,"VerseNumber":60,"Text":"feveylul lillezine keferu miy yevmihimu-llezi yu`adun"}]}