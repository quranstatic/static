{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":83,"VerseNumber":1,"Text":"Wailul lil mutaffifeen"},{"SurahNumber":83,"VerseNumber":2,"Text":"Allazeena izak taaloo 'alan naasi yastawfoon"},{"SurahNumber":83,"VerseNumber":3,"Text":"Wa izaa kaaloohum aw wazanoohum yukhsiroon"},{"SurahNumber":83,"VerseNumber":4,"Text":"Alaa yazunnu ulaaa'ika annahum mab'oosoon"},{"SurahNumber":83,"VerseNumber":5,"Text":"Li Yawmin 'Azeem"},{"SurahNumber":83,"VerseNumber":6,"Text":"Yawma yaqoomun naasu li Rabbil 'aalameen"},{"SurahNumber":83,"VerseNumber":7,"Text":"Kallaaa inna kitaabal fujjaari lafee Sijjeen"},{"SurahNumber":83,"VerseNumber":8,"Text":"Wa maa adraaka maa Sijjeen"},{"SurahNumber":83,"VerseNumber":9,"Text":"Kitaabum marqoom"},{"SurahNumber":83,"VerseNumber":10,"Text":"Wailuny yawma'izil lil mukazzibeen"},{"SurahNumber":83,"VerseNumber":11,"Text":"Allazeena yukazziboona bi yawmid deen"},{"SurahNumber":83,"VerseNumber":12,"Text":"Wa maa yukazzibu biheee illaa kullu mu'tadin aseem"},{"SurahNumber":83,"VerseNumber":13,"Text":"Izaa tutlaa'alaihi aayaatunaa qaala asaateerul awwaleen"},{"SurahNumber":83,"VerseNumber":14,"Text":"Kallaa bal raana 'alaa quloobihim maa kaanoo yaksiboon"},{"SurahNumber":83,"VerseNumber":15,"Text":"Kallaaa innahum 'ar Rabbihim yawma'izil lamah jooboon"},{"SurahNumber":83,"VerseNumber":16,"Text":"Summa innahum lasaa lul jaheem"},{"SurahNumber":83,"VerseNumber":17,"Text":"Summa yuqaalu haazal lazee kuntum bihee tukazziboon"},{"SurahNumber":83,"VerseNumber":18,"Text":"Kallaaa inna kitaabal abraari lafee'Illiyyeen"},{"SurahNumber":83,"VerseNumber":19,"Text":"Wa maaa adraaka maa 'Illiyyoon"},{"SurahNumber":83,"VerseNumber":20,"Text":"Kitaabum marqoom"},{"SurahNumber":83,"VerseNumber":21,"Text":"Yashhadu hul muqarra boon"},{"SurahNumber":83,"VerseNumber":22,"Text":"Innal abraara lafee Na'eem"},{"SurahNumber":83,"VerseNumber":23,"Text":"Alal araaa'iki yanzuroon"},{"SurahNumber":83,"VerseNumber":24,"Text":"Ta'rifu fee wujoohihim nadratan na'eem"},{"SurahNumber":83,"VerseNumber":25,"Text":"Yusqawna mir raheeqim makhtoom"},{"SurahNumber":83,"VerseNumber":26,"Text":"Khitaamuhoo misk; wa fee zaalika falyatanaafasil Mutanaafisoon"},{"SurahNumber":83,"VerseNumber":27,"Text":"Wa mizaajuhoo min Tasneem"},{"SurahNumber":83,"VerseNumber":28,"Text":"Ainaiy yashrabu bihal muqarraboon"},{"SurahNumber":83,"VerseNumber":29,"Text":"Innal lazeena ajramoo kaanoo minal lazeena aamanoo yadhakoon"},{"SurahNumber":83,"VerseNumber":30,"Text":"Wa izaa marroo bihim yataghaamazoon"},{"SurahNumber":83,"VerseNumber":31,"Text":"Wa izan qalabooo ilaaa ahlihimun qalaboo fakiheen"},{"SurahNumber":83,"VerseNumber":32,"Text":"Wa izaa ra awhum qaalooo inna haaa'ulaaa'i ladaaal loon"},{"SurahNumber":83,"VerseNumber":33,"Text":"Wa maaa ursiloo 'alaihim haafizeen"},{"SurahNumber":83,"VerseNumber":34,"Text":"Fal yawmal lazeena aamanoo minal kuffaari yadhakoon"},{"SurahNumber":83,"VerseNumber":35,"Text":"Alal araaa'iki yanzuroon"},{"SurahNumber":83,"VerseNumber":36,"Text":"Hal suwwibal kuffaaru maa kaanoo yaf'aloon"}]}