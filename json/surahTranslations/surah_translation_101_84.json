{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":101,"VerseNumber":1,"Text":"Shocker"},{"SurahNumber":101,"VerseNumber":2,"Text":"Hvad shocker"},{"SurahNumber":101,"VerseNumber":3,"Text":"I gøre nogen ide hvad Shocker have være"},{"SurahNumber":101,"VerseNumber":4,"Text":"Den er dagen hvornår folkene komme sværme sommerfugle"},{"SurahNumber":101,"VerseNumber":5,"Text":"Bjergene være dunede uld"},{"SurahNumber":101,"VerseNumber":6,"Text":"Angående ham vægte er tunge"},{"SurahNumber":101,"VerseNumber":7,"Text":"Han lede glade (evige) liv"},{"SurahNumber":101,"VerseNumber":8,"Text":"Angående ham vægte er lyser"},{"SurahNumber":101,"VerseNumber":9,"Text":"Hans destiny er lowly"},{"SurahNumber":101,"VerseNumber":10,"Text":"Du kender hvad det gøre være"},{"SurahNumber":101,"VerseNumber":11,"Text":"Blussende Hellfire"}]}