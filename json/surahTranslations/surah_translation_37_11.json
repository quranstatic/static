{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":37,"VerseNumber":1,"Text":"Wassaaaffaati saffaa"},{"SurahNumber":37,"VerseNumber":2,"Text":"Fazzaajiraati zajraa"},{"SurahNumber":37,"VerseNumber":3,"Text":"Fattaaliyaati Zikra"},{"SurahNumber":37,"VerseNumber":4,"Text":"Inna Illaahakum la Waahid"},{"SurahNumber":37,"VerseNumber":5,"Text":"Rabbus samaawaati wal ardi wa maa bainahumaa wa Rabbul mashaariq"},{"SurahNumber":37,"VerseNumber":6,"Text":"Innaa zaiyannas samaaa 'ad dunyaa bizeenatinil kawaakib"},{"SurahNumber":37,"VerseNumber":7,"Text":"Wa hifzam min kulli Shaitaanim maarid"},{"SurahNumber":37,"VerseNumber":8,"Text":"Laa yassamma 'oona ilal mala il a'alaa wa yuqzafoona min kulli jaanib"},{"SurahNumber":37,"VerseNumber":9,"Text":"Duhooranw wa lahum 'azaabunw waasib"},{"SurahNumber":37,"VerseNumber":10,"Text":"Illaa man khatifal khatfata fa atba'ahoo shihaabun saaqib"},{"SurahNumber":37,"VerseNumber":11,"Text":"Fastaftihim ahum ashaddu khalqan am man khalaqnaa; innaa khalaqnaahum min teenil laazib"},{"SurahNumber":37,"VerseNumber":12,"Text":"Bal'ajibta wa yaskharoon"},{"SurahNumber":37,"VerseNumber":13,"Text":"Wa izaa zukkiroo laa yazkuroon"},{"SurahNumber":37,"VerseNumber":14,"Text":"Wa izaa ra aw Aayatinw yastaskhiroon"},{"SurahNumber":37,"VerseNumber":15,"Text":"Wa qaalooo in haazaa illaa sihrum mubeen"},{"SurahNumber":37,"VerseNumber":16,"Text":"A-izaa mitnaa wa kunnaa turaabanw wa 'izaaman 'ainnaa lamab'oosoon"},{"SurahNumber":37,"VerseNumber":17,"Text":"Awa aabaa'unal awwaloon"},{"SurahNumber":37,"VerseNumber":18,"Text":"Qul na'am wa antum daakhiroon"},{"SurahNumber":37,"VerseNumber":19,"Text":"Fa innamaa hiya zajra tunw waahidatun fa izaa hum yanzuroon"},{"SurahNumber":37,"VerseNumber":20,"Text":"Qa qaaloo yaa wailanaa haazaa Yawmud-Deen"},{"SurahNumber":37,"VerseNumber":21,"Text":"Haazaa Yawmul Faslil lazee kuntum bihee tukaziboon"},{"SurahNumber":37,"VerseNumber":22,"Text":"Uhshurul lazeena zalamoo wa azwaajahum wa maa kaanoo ya'budoon"},{"SurahNumber":37,"VerseNumber":23,"Text":"Min doonil laahi fahdoohum ilaa siraatil Jaheem"},{"SurahNumber":37,"VerseNumber":24,"Text":"Wa qifoohum innahum mas'ooloon"},{"SurahNumber":37,"VerseNumber":25,"Text":"Maa lakum laa tanaasaroon"},{"SurahNumber":37,"VerseNumber":26,"Text":"Bal humul Yawma mustaslimoon"},{"SurahNumber":37,"VerseNumber":27,"Text":"Wa aqbala ba'duhum 'alaa ba'diny yatasaaa'aloon"},{"SurahNumber":37,"VerseNumber":28,"Text":"Qaalooo innakum kuntum taatoonanaa 'anil yameen"},{"SurahNumber":37,"VerseNumber":29,"Text":"Qaaloo bal lam takoonoo mu'mineen"},{"SurahNumber":37,"VerseNumber":30,"Text":"Wa maa kaana lanaa 'alaikum min sultaanim bal kuntum qawman taagheen"},{"SurahNumber":37,"VerseNumber":31,"Text":"Fahaqqa 'alainaa qawlu Rabbinaaa innaa lazaaa'iqoon"},{"SurahNumber":37,"VerseNumber":32,"Text":"Fa aghwainaakum innaa kunnaa ghaaween"},{"SurahNumber":37,"VerseNumber":33,"Text":"Fa innahum Yawma'izin fil'azaabi mushtarikoon"},{"SurahNumber":37,"VerseNumber":34,"Text":"Innaa kazaalika naf'alu bil mujrimeen"},{"SurahNumber":37,"VerseNumber":35,"Text":"Innahum kaanooo izaa qeela lahum laaa ilaaha illal laahu yastakbiroon"},{"SurahNumber":37,"VerseNumber":36,"Text":"Wa yaqooloona a'innaa lataarikooo aalihatinaa lishaa'irim majnoon"},{"SurahNumber":37,"VerseNumber":37,"Text":"bal jaaa'a bilhaqqi wa saddaqal mursaleen"},{"SurahNumber":37,"VerseNumber":38,"Text":"Innakum lazaaa'iqul 'azaabil aleem"},{"SurahNumber":37,"VerseNumber":39,"Text":"Wa maa tujzawna illaa maa kuntum ta'maloon"},{"SurahNumber":37,"VerseNumber":40,"Text":"Illaa 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":41,"Text":"Ulaaa'ika lahum rizqum ma'loom"},{"SurahNumber":37,"VerseNumber":42,"Text":"Fa waakihu wa hum mukramoon"},{"SurahNumber":37,"VerseNumber":43,"Text":"Fee jannaatin Na'eem"},{"SurahNumber":37,"VerseNumber":44,"Text":"Alaa sururim mutaqaa bileen"},{"SurahNumber":37,"VerseNumber":45,"Text":"Yutaafu 'alaihim bikaasim mim ma'een"},{"SurahNumber":37,"VerseNumber":46,"Text":"Baidaaa'a laz zatil lish shaaribeen"},{"SurahNumber":37,"VerseNumber":47,"Text":"Laa feehaa ghawlunw wa laa hum 'anhaa yunzafoon"},{"SurahNumber":37,"VerseNumber":48,"Text":"Wa 'indahum qaasiraatut tarfi 'een"},{"SurahNumber":37,"VerseNumber":49,"Text":"Ka annahunna baidum maknoon"},{"SurahNumber":37,"VerseNumber":50,"Text":"Fa aqbala ba'duhum 'alaa badiny yatasaaa 'aloon"},{"SurahNumber":37,"VerseNumber":51,"Text":"Qaala qaaa'ilum minhum innee kaana lee qareen"},{"SurahNumber":37,"VerseNumber":52,"Text":"Yaqoolu a'innnaka laminal musaddiqeen"},{"SurahNumber":37,"VerseNumber":53,"Text":"A-izaa mitnaa wa kunnaa turaabanw wa 'izaaman 'ainnaa lamadeenoon"},{"SurahNumber":37,"VerseNumber":54,"Text":"Qaala hal antum muttali'oon"},{"SurahNumber":37,"VerseNumber":55,"Text":"Fattala'a fara aahu fee sawaaa'il Jaheem"},{"SurahNumber":37,"VerseNumber":56,"Text":"Qaala tallaahi in kitta laturdeen"},{"SurahNumber":37,"VerseNumber":57,"Text":"Wa law laa ni'matu Rabbee lakuntu minal muhdareen"},{"SurahNumber":37,"VerseNumber":58,"Text":"Afamaa nahnu bimaiyiteen"},{"SurahNumber":37,"VerseNumber":59,"Text":"Illa mawtatanal oola wa maa nahnu bimu'azzabeen"},{"SurahNumber":37,"VerseNumber":60,"Text":"Inna haazaa falya'ma lil'aamiloon"},{"SurahNumber":37,"VerseNumber":61,"Text":"Limisli haaza falya'ma lil 'aamiloon"},{"SurahNumber":37,"VerseNumber":62,"Text":"Azaalika khairun nuzulan am shajaratuz Zaqqom"},{"SurahNumber":37,"VerseNumber":63,"Text":"Innaa ja'alnaahaa fitnatal lizzaalimeen"},{"SurahNumber":37,"VerseNumber":64,"Text":"Innahaa shajaratun takhruju feee aslil Jaheem"},{"SurahNumber":37,"VerseNumber":65,"Text":"Tal'uhaa ka annahoo ru'oosush Shayaateen"},{"SurahNumber":37,"VerseNumber":66,"Text":"Fa innahum la aakiloona minhaa famaali'oona minhal butoon"},{"SurahNumber":37,"VerseNumber":67,"Text":"Summa inna lahum 'alaihaa lashawbam min hameem"},{"SurahNumber":37,"VerseNumber":68,"Text":"Summa inna marji'ahum la ilal Jaheem"},{"SurahNumber":37,"VerseNumber":69,"Text":"Innahum alfaw aabaaa'ahum daaalleen"},{"SurahNumber":37,"VerseNumber":70,"Text":"Fahum 'alaa aasaarihim yuhra'oon"},{"SurahNumber":37,"VerseNumber":71,"Text":"Wa laqad dalla qablahum aksarul awwaleen"},{"SurahNumber":37,"VerseNumber":72,"Text":"Wa laqad arsalnaa feehim munzireen"},{"SurahNumber":37,"VerseNumber":73,"Text":"Fanzur kaifa kaana 'aaqibatul munzareen"},{"SurahNumber":37,"VerseNumber":74,"Text":"Illaa 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":75,"Text":"Wa laqad naadaanaa Noohun falani'mal mujeeboon"},{"SurahNumber":37,"VerseNumber":76,"Text":"Wa jajainaahu wa ahlahoo minal karbil 'azeem"},{"SurahNumber":37,"VerseNumber":77,"Text":"Wa ja'alnaa zurriyyatahoo hummul baaqeen"},{"SurahNumber":37,"VerseNumber":78,"Text":"Wa taraknaa 'alaihi fil aakhireen"},{"SurahNumber":37,"VerseNumber":79,"Text":"Salaamun 'alaa Noohin fil 'aalameen"},{"SurahNumber":37,"VerseNumber":80,"Text":"Innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":81,"Text":"Innahoo min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":82,"Text":"Summa aghraqnal aakhareen"},{"SurahNumber":37,"VerseNumber":83,"Text":"Wa ina min shee'atihee la Ibraaheem"},{"SurahNumber":37,"VerseNumber":84,"Text":"Iz jaaa'a Rabbahoo bi qalbin saleem"},{"SurahNumber":37,"VerseNumber":85,"Text":"Iz qaala li abeehi wa qawmihee maazaa ta'budoon"},{"SurahNumber":37,"VerseNumber":86,"Text":"A'ifkan aalihatan doonal laahi tureedoon"},{"SurahNumber":37,"VerseNumber":87,"Text":"Famaa zannukum bi Rabbil'aalameen"},{"SurahNumber":37,"VerseNumber":88,"Text":"Fanazara nazratan finnujoom"},{"SurahNumber":37,"VerseNumber":89,"Text":"Faqaala inee saqeem"},{"SurahNumber":37,"VerseNumber":90,"Text":"Fatawallaw 'anhu mudbireen"},{"SurahNumber":37,"VerseNumber":91,"Text":"Faraagha ilaaa aalihatihim faqaala alaa taakuloon"},{"SurahNumber":37,"VerseNumber":92,"Text":"Maa lakum laa tantiqoon"},{"SurahNumber":37,"VerseNumber":93,"Text":"Faraagha 'alaihim darbam bilyameen"},{"SurahNumber":37,"VerseNumber":94,"Text":"Fa aqbalooo ilaihi yaziffoon"},{"SurahNumber":37,"VerseNumber":95,"Text":"Qaala ata'budoona maa tanhitoon"},{"SurahNumber":37,"VerseNumber":96,"Text":"Wallaahu khalaqakum wa maa ta'maloon"},{"SurahNumber":37,"VerseNumber":97,"Text":"Qaalub noo lahoo bun yaanan fa alqoohu fil jaheem"},{"SurahNumber":37,"VerseNumber":98,"Text":"Fa araadoo bihee kaidan faja 'alnaahumul asfaleen"},{"SurahNumber":37,"VerseNumber":99,"Text":"Wa qaala innee zaahibun ilaa Rabbee sa yahdeen"},{"SurahNumber":37,"VerseNumber":100,"Text":"Rabbi hab lee minas saaliheen"},{"SurahNumber":37,"VerseNumber":101,"Text":"Fabashsharnaahu bighulaamin haleem"},{"SurahNumber":37,"VerseNumber":102,"Text":"Falamma balagha ma'a hus sa'ya qaala yaa buniya inneee araa fil manaami anneee azbahuka fanzur maazaa taraa; qaala yaaa abatif 'al maa tu'maru satajidunee in shaaa'allaahu minas saabireen"},{"SurahNumber":37,"VerseNumber":103,"Text":"Falammaaa aslamaa wa tallahoo liljabeen"},{"SurahNumber":37,"VerseNumber":104,"Text":"Wa naadainaahu ai yaaaa Ibraheem"},{"SurahNumber":37,"VerseNumber":105,"Text":"Qad saddaqtar ru'yaa; innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":106,"Text":"Inna haazaa lahuwal balaaa'ul mubeen"},{"SurahNumber":37,"VerseNumber":107,"Text":"Wa fadainaahu bizibhin 'azeem"},{"SurahNumber":37,"VerseNumber":108,"Text":"Wa taraknaa 'alaihi fil aakhireen"},{"SurahNumber":37,"VerseNumber":109,"Text":"Salaamun 'alaaa Ibraaheem"},{"SurahNumber":37,"VerseNumber":110,"Text":"Kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":111,"Text":"Innahoo min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":112,"Text":"Wa bashsharnaahu bi Ishaaqa Nabiyayam minas saaliheen"},{"SurahNumber":37,"VerseNumber":113,"Text":"Wa baaraknaa 'alaihi wa 'alaaa Ishaaq; wa min zurriyya tihimaa muhsinunw wa zaalimul linafshihee mubeen"},{"SurahNumber":37,"VerseNumber":114,"Text":"Wa laqad mananna alaa Moosaa wa Haaroon"},{"SurahNumber":37,"VerseNumber":115,"Text":"Wa najjainaahumaa wa qawmahumaa minal karbil 'azeem"},{"SurahNumber":37,"VerseNumber":116,"Text":"Wa nasarnaahum fakaanoo humul ghaalibeen"},{"SurahNumber":37,"VerseNumber":117,"Text":"Wa aatainaahumal Kitaabal mustabeen"},{"SurahNumber":37,"VerseNumber":118,"Text":"Wa hadainaahumus Siraatal Mustaqeem"},{"SurahNumber":37,"VerseNumber":119,"Text":"Wa taraknaa 'alaihimaa fil aakhireen"},{"SurahNumber":37,"VerseNumber":120,"Text":"Salaamun 'alaa Moosaa wa Haaroon"},{"SurahNumber":37,"VerseNumber":121,"Text":"Innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":122,"Text":"Innahumaa min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":123,"Text":"Wa inna Ilyaasa laminal mursaleen"},{"SurahNumber":37,"VerseNumber":124,"Text":"Iz qaala liqawmiheee alaa tattaqoon"},{"SurahNumber":37,"VerseNumber":125,"Text":"Atad'oona Ba'lanw wa tazaroona ahsanal khaaliqeen"},{"SurahNumber":37,"VerseNumber":126,"Text":"Allaaha Rabbakum wa Rabba aabaaa'ikumul awwaleen"},{"SurahNumber":37,"VerseNumber":127,"Text":"Fakazzaboohu fa inna hum lamuhdaroon"},{"SurahNumber":37,"VerseNumber":128,"Text":"Illaa 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":129,"Text":"Wa taraknaa 'alaihi fil aakhireen"},{"SurahNumber":37,"VerseNumber":130,"Text":"Salaamun 'alaaa Ilyaaseen"},{"SurahNumber":37,"VerseNumber":131,"Text":"Innaa kazaalika najzil muhsineen"},{"SurahNumber":37,"VerseNumber":132,"Text":"Innahoo min 'ibaadinal mu'mineen"},{"SurahNumber":37,"VerseNumber":133,"Text":"Wa inna Lootal laminal mursaleen"},{"SurahNumber":37,"VerseNumber":134,"Text":"Iz najjainaahu wa ahlahooo ajma'een"},{"SurahNumber":37,"VerseNumber":135,"Text":"Illaa 'ajoozan fil ghaabireen"},{"SurahNumber":37,"VerseNumber":136,"Text":"Summa dammarnal aakhareen"},{"SurahNumber":37,"VerseNumber":137,"Text":"Wa innakum latamurroona 'alaihim musbiheen"},{"SurahNumber":37,"VerseNumber":138,"Text":"Wa billail; afalaa ta'qiloon"},{"SurahNumber":37,"VerseNumber":139,"Text":"Wa inna Yoonusa laminal mursaleen"},{"SurahNumber":37,"VerseNumber":140,"Text":"Iz abaqa ilal fulkil mash hoon"},{"SurahNumber":37,"VerseNumber":141,"Text":"Fasaahama fakaana minal mudhadeen"},{"SurahNumber":37,"VerseNumber":142,"Text":"Faltaqamahul hootu wa huwa muleem"},{"SurahNumber":37,"VerseNumber":143,"Text":"Falaw laaa annahoo kaana minal musabbiheen"},{"SurahNumber":37,"VerseNumber":144,"Text":"Lalabisa fee batniheee ilaa Yawmi yub'asoon"},{"SurahNumber":37,"VerseNumber":145,"Text":"Fanabaznaahu bil'araaa'i wa huwa saqeem"},{"SurahNumber":37,"VerseNumber":146,"Text":"Wa ambatnaa 'alaihi shajaratam mai yaqteen"},{"SurahNumber":37,"VerseNumber":147,"Text":"Wa arsalnaahu ilaa mi'ati alfin aw yazeedoon"},{"SurahNumber":37,"VerseNumber":148,"Text":"Fa aamanoo famatta' naahum ilaa heen"},{"SurahNumber":37,"VerseNumber":149,"Text":"Fastaftihim ali Rabbikal banaatu wa lahumul banoon"},{"SurahNumber":37,"VerseNumber":150,"Text":"Am khalaqnal malaaa'i kata inaasanw wa hm shaahidoon"},{"SurahNumber":37,"VerseNumber":151,"Text":"Alaaa innahum min ifkihim la yaqooloon"},{"SurahNumber":37,"VerseNumber":152,"Text":"Waladal laahu wa innhum lakaaziboon"},{"SurahNumber":37,"VerseNumber":153,"Text":"Astafal banaati 'alal baneen"},{"SurahNumber":37,"VerseNumber":154,"Text":"Maa lakum kaifa tahkumoon"},{"SurahNumber":37,"VerseNumber":155,"Text":"Afalaa tazakkaroon"},{"SurahNumber":37,"VerseNumber":156,"Text":"Am lakum sultaanum mubeen"},{"SurahNumber":37,"VerseNumber":157,"Text":"Faatoo bi Kitaabikum in kuntum saadiqeen"},{"SurahNumber":37,"VerseNumber":158,"Text":"Wa ja'aloo bainahoo wa bainal jinnati nasabaa; wa laqad 'alimatil jinnatu innahum lamuhdaroon"},{"SurahNumber":37,"VerseNumber":159,"Text":"Subhaanal laahi 'ammaa yasifoon"},{"SurahNumber":37,"VerseNumber":160,"Text":"Illaa 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":161,"Text":"Fa innakum wa maa ta'ubdoon"},{"SurahNumber":37,"VerseNumber":162,"Text":"Maaa antum 'alaihi befaaatineen"},{"SurahNumber":37,"VerseNumber":163,"Text":"Illaa man huwa saalil jaheem"},{"SurahNumber":37,"VerseNumber":164,"Text":"Wa maa minnasa illaa lahoo maqaamum ma'loom"},{"SurahNumber":37,"VerseNumber":165,"Text":"Wa innaa llanah nus saaffoon"},{"SurahNumber":37,"VerseNumber":166,"Text":"Wa innaa lanah nul musabbihoon"},{"SurahNumber":37,"VerseNumber":167,"Text":"Wa in kaanoo la yaqooloon"},{"SurahNumber":37,"VerseNumber":168,"Text":"Law anna 'indana zikram minal awwaleen"},{"SurahNumber":37,"VerseNumber":169,"Text":"Lakunna 'ibaadal laahil mukhlaseen"},{"SurahNumber":37,"VerseNumber":170,"Text":"Fakafaroo bihee fasawfa ya'lamoon"},{"SurahNumber":37,"VerseNumber":171,"Text":"Wa laqad sabaqat Kalimatunaa li'ibaadinal mursa leen"},{"SurahNumber":37,"VerseNumber":172,"Text":"Innaa hum lahumul mansooroon"},{"SurahNumber":37,"VerseNumber":173,"Text":"Wa inna jundana lahumul ghaaliboon"},{"SurahNumber":37,"VerseNumber":174,"Text":"Fatawalla 'anhum hatta heen"},{"SurahNumber":37,"VerseNumber":175,"Text":"Wa absirhum fasawfa yubsiroon"},{"SurahNumber":37,"VerseNumber":176,"Text":"Afabi'azaabinaa yasta'jiloon"},{"SurahNumber":37,"VerseNumber":177,"Text":"Fa izaa nazala bisaahatihim fasaaa'a sabaahul munzareen"},{"SurahNumber":37,"VerseNumber":178,"Text":"Wa tawalla 'anhum hattaa heen"},{"SurahNumber":37,"VerseNumber":179,"Text":"Wa absir fasawfa yubsiroon"},{"SurahNumber":37,"VerseNumber":180,"Text":"Subhaana Rabbika Rabbil 'izzati 'amma yasifoon"},{"SurahNumber":37,"VerseNumber":181,"Text":"Wa salaamun 'alalmursaleen"},{"SurahNumber":37,"VerseNumber":182,"Text":"Walhamdu lillaahi Rabbil 'aalameen"}]}