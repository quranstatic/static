{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":93,"VerseNumber":1,"Text":"S tafat n tuebeit"},{"SurahNumber":93,"VerseNumber":2,"Text":"S yiv, ma irked"},{"SurahNumber":93,"VerseNumber":3,"Text":"Ur k Iooi Mass ik, ur k Ikrih"},{"SurahNumber":93,"VerseNumber":4,"Text":"Tudert taneggarut ik, a ppif tamezwarut ik"},{"SurahNumber":93,"VerseNumber":5,"Text":"A k toavi tikci, deqqal, n Mass ik"},{"SurahNumber":93,"VerseNumber":6,"Text":"Day ur k id Iufa ara d agujil, Isduri k"},{"SurahNumber":93,"VerseNumber":7,"Text":"Iufa k id d utlif, Inha k"},{"SurahNumber":93,"VerseNumber":8,"Text":"Iufa k id d igellil, I$at ik"},{"SurahNumber":93,"VerseNumber":9,"Text":"Ihi, ur qehheo agujil"},{"SurahNumber":93,"VerseNumber":10,"Text":"Ur ppara amappar"},{"SurahNumber":93,"VerseNumber":11,"Text":"Wanag, $ef l$it n Mass ik, xas eeku"}]}