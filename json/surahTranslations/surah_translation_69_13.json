{"Id":13,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la3","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"","Translations":[{"SurahNumber":69,"VerseNumber":1,"Text":"Alhaqqatu"},{"SurahNumber":69,"VerseNumber":2,"Text":"Ma alhaqqatu"},{"SurahNumber":69,"VerseNumber":3,"Text":"Wama adraka ma alhaqqatu"},{"SurahNumber":69,"VerseNumber":4,"Text":"Kaththabat thamoodu waAAadun bialqariAAati"},{"SurahNumber":69,"VerseNumber":5,"Text":"Faamma thamoodu faohlikoo bialttaghiyati"},{"SurahNumber":69,"VerseNumber":6,"Text":"Waamma AAadun faohlikoo bireehin sarsarin AAatiyatin"},{"SurahNumber":69,"VerseNumber":7,"Text":"Sakhkharaha AAalayhim sabAAa layalin wathamaniyata ayyamin husooman fatara alqawma feeha sarAAa kaannahum aAAjazu nakhlin khawiyatun"},{"SurahNumber":69,"VerseNumber":8,"Text":"Fahal tara lahum min baqiyatin"},{"SurahNumber":69,"VerseNumber":9,"Text":"Wajaa firAAawnu waman qablahu waalmu/tafikatu bialkhati-ati"},{"SurahNumber":69,"VerseNumber":10,"Text":"FaAAasaw rasoola rabbihim faakhathahum akhthatan rabiyatan"},{"SurahNumber":69,"VerseNumber":11,"Text":"Inna lamma tagha almao hamalnakum fee aljariyati"},{"SurahNumber":69,"VerseNumber":12,"Text":"LinajAAalaha lakum tathkiratan wataAAiyaha othunun waAAiyatun"},{"SurahNumber":69,"VerseNumber":13,"Text":"Fa-itha nufikha fee alssoori nafkhatun wahidatun"},{"SurahNumber":69,"VerseNumber":14,"Text":"Wahumilati al-ardu waaljibalu fadukkata dakkatan wahidatan"},{"SurahNumber":69,"VerseNumber":15,"Text":"Fayawma-ithin waqaAAati alwaqiAAatu"},{"SurahNumber":69,"VerseNumber":16,"Text":"Wainshaqqati alssamao fahiya yawma-ithin wahiyatun"},{"SurahNumber":69,"VerseNumber":17,"Text":"Waalmalaku AAala arja-iha wayahmilu AAarsha rabbika fawqahum yawma-ithin thamaniyatun"},{"SurahNumber":69,"VerseNumber":18,"Text":"Yawma-ithin tuAAradoona la takhfa minkum khafiyatun"},{"SurahNumber":69,"VerseNumber":19,"Text":"Faamma man ootiya kitabahu biyameenihi fayaqoolu haomu iqraoo kitabiyah"},{"SurahNumber":69,"VerseNumber":20,"Text":"Innee thanantu annee mulaqin hisabiyah"},{"SurahNumber":69,"VerseNumber":21,"Text":"Fahuwa fee AAeeshatin radiyatin"},{"SurahNumber":69,"VerseNumber":22,"Text":"Fee jannatin AAaliyatin"},{"SurahNumber":69,"VerseNumber":23,"Text":"Qutoofuha daniyatun"},{"SurahNumber":69,"VerseNumber":24,"Text":"Kuloo waishraboo hanee-an bima aslaftum fee al-ayyami alkhaliyati"},{"SurahNumber":69,"VerseNumber":25,"Text":"Waamma man ootiya kitabahu bishimalihi fayaqoolu ya laytanee lam oota kitabiyah"},{"SurahNumber":69,"VerseNumber":26,"Text":"Walam adri ma hisabiyah"},{"SurahNumber":69,"VerseNumber":27,"Text":"Ya laytaha kanati alqadiyatu"},{"SurahNumber":69,"VerseNumber":28,"Text":"Ma aghna AAannee maliyah"},{"SurahNumber":69,"VerseNumber":29,"Text":"Halaka AAannee sultaniyah"},{"SurahNumber":69,"VerseNumber":30,"Text":"Khuthoohu faghulloohu"},{"SurahNumber":69,"VerseNumber":31,"Text":"Thumma aljaheema salloohu"},{"SurahNumber":69,"VerseNumber":32,"Text":"Thumma fee silsilatin tharAAuha sabAAoona thiraAAan faoslukoohu"},{"SurahNumber":69,"VerseNumber":33,"Text":"Innahu kana la yu/minu biAllahi alAAatheemi"},{"SurahNumber":69,"VerseNumber":34,"Text":"Wala yahuddu AAala taAAami almiskeeni"},{"SurahNumber":69,"VerseNumber":35,"Text":"Falaysa lahu alyawma hahuna hameemun"},{"SurahNumber":69,"VerseNumber":36,"Text":"Wala taAAamun illa min ghisleenin"},{"SurahNumber":69,"VerseNumber":37,"Text":"La ya/kuluhu illa alkhati-oona"},{"SurahNumber":69,"VerseNumber":38,"Text":"Fala oqsimu bima tubsiroona"},{"SurahNumber":69,"VerseNumber":39,"Text":"Wama la tubsiroona"},{"SurahNumber":69,"VerseNumber":40,"Text":"Innahu laqawlu rasoolin kareemin"},{"SurahNumber":69,"VerseNumber":41,"Text":"Wama huwa biqawli shaAAirin qaleelan ma tu/minoona"},{"SurahNumber":69,"VerseNumber":42,"Text":"Wala biqawli kahinin qaleelan ma tathakkaroona"},{"SurahNumber":69,"VerseNumber":43,"Text":"Tanzeelun min rabbi alAAalameena"},{"SurahNumber":69,"VerseNumber":44,"Text":"Walaw taqawwala AAalayna baAAda al-aqaweeli"},{"SurahNumber":69,"VerseNumber":45,"Text":"Laakhathna minhu bialyameeni"},{"SurahNumber":69,"VerseNumber":46,"Text":"Thumma laqataAAna minhu alwateena"},{"SurahNumber":69,"VerseNumber":47,"Text":"Fama minkum min ahadin AAanhu hajizeena"},{"SurahNumber":69,"VerseNumber":48,"Text":"Wa-innahu latathkiratun lilmuttaqeena"},{"SurahNumber":69,"VerseNumber":49,"Text":"Wa-inna lanaAAlamu anna minkum mukaththibeena"},{"SurahNumber":69,"VerseNumber":50,"Text":"Wa-innahu lahasratun AAala alkafireena"},{"SurahNumber":69,"VerseNumber":51,"Text":"Wa-innahu lahaqqu alyaqeeni"},{"SurahNumber":69,"VerseNumber":52,"Text":"Fasabbih biismi rabbika alAAatheemi"}]}