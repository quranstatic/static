{"Id":248,"LanguageCodes":["ml","mal"],"Name":"mal-muhammadkarakun-la","Author":"Muhammad Karakunnu And Vanidas Elayavoor","Language":"Malayalam","Description":"Malayalam - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":79,"VerseNumber":1,"Text":"munniccenn uriyetukkunnava satyam"},{"SurahNumber":79,"VerseNumber":2,"Text":"semyamayi purattekketukkunnava satyam"},{"SurahNumber":79,"VerseNumber":3,"Text":"saktiyayi nintunnava satyam"},{"SurahNumber":79,"VerseNumber":4,"Text":"ennitt munneattu kutikkunnava satyam"},{"SurahNumber":79,"VerseNumber":5,"Text":"karyannal niyantrikkunnava satyam"},{"SurahNumber":79,"VerseNumber":6,"Text":"ghearasambhavam prakampanam srstikkum dinam"},{"SurahNumber":79,"VerseNumber":7,"Text":"atinre pirake marrearu prakampanavumuntakum"},{"SurahNumber":79,"VerseNumber":8,"Text":"annu cila hrdayannal pitayunnavayayirikkum"},{"SurahNumber":79,"VerseNumber":9,"Text":"avarute kannukal peticcarantirikkum"},{"SurahNumber":79,"VerseNumber":10,"Text":"avar ceadikkunnu: \"urappayum nam purvavasthayilekk matakkappetumennea"},{"SurahNumber":79,"VerseNumber":11,"Text":"nam nurumpiya ellukalaya sesavum?”"},{"SurahNumber":79,"VerseNumber":12,"Text":"avar gheasikkunnu: \"enkilatearu tulanna tiriccu peakku tanne.”"},{"SurahNumber":79,"VerseNumber":13,"Text":"ennal atearu gheara sabdam matramayirikkum"},{"SurahNumber":79,"VerseNumber":14,"Text":"appealekkum avar bhutalattilettiyirikkum"},{"SurahNumber":79,"VerseNumber":15,"Text":"musayute varttamanam ninakk vannettiyea"},{"SurahNumber":79,"VerseNumber":16,"Text":"visud'dhamaya tvuva talvarayil vecc tanre nathan addehatte viliccu kalpiccatearkkuka"},{"SurahNumber":79,"VerseNumber":17,"Text":"ni pharaveanre atuttekk peavuka. avan atikramiyayirikkunnu"},{"SurahNumber":79,"VerseNumber":18,"Text":"ennitt ayaleat ceadikkuka: “ni visud'dhi varikkan tayyaruntea"},{"SurahNumber":79,"VerseNumber":19,"Text":"“nan ninne ninre nathanilekku valinatattanum annane ninakku daivabhaktanakanum?”"},{"SurahNumber":79,"VerseNumber":20,"Text":"musa ayalkk mahattaya oratayalam kaniccukeatuttu"},{"SurahNumber":79,"VerseNumber":21,"Text":"appeal ayalatine kalavakkukayum dhikkarikkukayum ceytu"},{"SurahNumber":79,"VerseNumber":22,"Text":"pinnit ayal etirsramannalkkayi tirinnu natannu"},{"SurahNumber":79,"VerseNumber":23,"Text":"annane janannale orumiccukutti innane vilambaram ceytu"},{"SurahNumber":79,"VerseNumber":24,"Text":"avan prakhyapiccu: nanan ninnalute parameannata nathan"},{"SurahNumber":79,"VerseNumber":25,"Text":"appeal allahu avane pitikuti. maruleakatteyum ileakatteyum siksakkirayakkan"},{"SurahNumber":79,"VerseNumber":26,"Text":"niscayamayum daivabhayamullavarkk itil gunapathamunt"},{"SurahNumber":79,"VerseNumber":27,"Text":"ninnale srstikkunnatea akasatte srstikkunnatea etan kututal prayasakaram? avan atuntakki"},{"SurahNumber":79,"VerseNumber":28,"Text":"atinre vitanam uyarttukayum annane atine kurramarratakkukayum ceytu"},{"SurahNumber":79,"VerseNumber":29,"Text":"atile ravine avan irulullatakki. pakaline irulilninn purattetukkukayum ceytu"},{"SurahNumber":79,"VerseNumber":30,"Text":"atinusesam bhumiye paratti vitartti"},{"SurahNumber":79,"VerseNumber":31,"Text":"bhumiyilninn atinre vellavum sasyannalum purattukeantuvannu"},{"SurahNumber":79,"VerseNumber":32,"Text":"malakale urappiccu nirtti"},{"SurahNumber":79,"VerseNumber":33,"Text":"ninnalkkum ninnalute kannukalikalkkum vibhavamayi"},{"SurahNumber":79,"VerseNumber":34,"Text":"ennal a gheara vipatt vannettiyal"},{"SurahNumber":79,"VerseNumber":35,"Text":"manusyan tan prayatniccu netiyatinekkuriccearkkunna dinam"},{"SurahNumber":79,"VerseNumber":36,"Text":"kalcakkarkkayi narakam velippetuttum nal"},{"SurahNumber":79,"VerseNumber":37,"Text":"appeal; ar atikramam kanikkukayum"},{"SurahNumber":79,"VerseNumber":38,"Text":"i leaka jivitattin alavarra pradhan'yam nalkukayum ceytuvea"},{"SurahNumber":79,"VerseNumber":39,"Text":"avanre sanketam kattikkalunna narakattiyan; tircca"},{"SurahNumber":79,"VerseNumber":40,"Text":"ennal ar tanre nathanre padaviye petikkukayum atmavi ne saririkechakalil ninn vilakki nirttukayum ceytuvea"},{"SurahNumber":79,"VerseNumber":41,"Text":"urappayum avanre matakkasthanam svargaman"},{"SurahNumber":79,"VerseNumber":42,"Text":"a antya samayatte sambandhicc avar ninneat ceadikkunnu. ateppealanuntavukayenn"},{"SurahNumber":79,"VerseNumber":43,"Text":"ni atekkuricc entuparayanan"},{"SurahNumber":79,"VerseNumber":44,"Text":"atekkuricc antimamaya ariv ninre nathankal matramatre"},{"SurahNumber":79,"VerseNumber":45,"Text":"ni atine bhayakkunnavarkkulla takkitukaran matram"},{"SurahNumber":79,"VerseNumber":46,"Text":"atine avar kanum nal, ivite oru sayahnamea prabhatamea allate tamasiccittillenn avarkk teannippeakum"}]}