{"Id":329,"LanguageCodes":["es","spa"],"Name":"spa-juliocortes-la","Author":"Julio Cortes","Language":"Spanish","Description":"Spanish - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":107,"VerseNumber":1,"Text":"¿Que te parece el que desmiente el Juicio"},{"SurahNumber":107,"VerseNumber":2,"Text":"Es el mismo que rechaza violentamente al huerfano"},{"SurahNumber":107,"VerseNumber":3,"Text":"Y no anima a dar de comer al pobre"},{"SurahNumber":107,"VerseNumber":4,"Text":"¡Ay de los que oran"},{"SurahNumber":107,"VerseNumber":5,"Text":"distraidamente"},{"SurahNumber":107,"VerseNumber":6,"Text":"para ser vistos"},{"SurahNumber":107,"VerseNumber":7,"Text":"Y niegan la ayuda"}]}