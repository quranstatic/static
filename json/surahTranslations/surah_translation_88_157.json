{"Id":157,"LanguageCodes":["fa","fas"],"Name":"fas-mohammadkazemmo","Author":"Mohammad Kazem Moezzi","Language":"Persian","Description":"Persian","Direction":"rtl","Source":"http://tanzil.net","Translations":[{"SurahNumber":88,"VerseNumber":1,"Text":"آیا بیامدت داستان فراگیرنده‌"},{"SurahNumber":88,"VerseNumber":2,"Text":"رویهائی است در آن روز سرافکنده‌"},{"SurahNumber":88,"VerseNumber":3,"Text":"کارکننده‌اند رنجبرنده‌"},{"SurahNumber":88,"VerseNumber":4,"Text":"چشند آتشی سوزان‌"},{"SurahNumber":88,"VerseNumber":5,"Text":"نوشانیده شوند از چشمه جوشان‌"},{"SurahNumber":88,"VerseNumber":6,"Text":"نیستشان خوراکی جز از خار"},{"SurahNumber":88,"VerseNumber":7,"Text":"نه فربه سازد و نه بی‌نیاز کند از گرسنگی‌"},{"SurahNumber":88,"VerseNumber":8,"Text":"رویهائی است در آن روز شاداب‌"},{"SurahNumber":88,"VerseNumber":9,"Text":"از کوشش خویشند خوشنود"},{"SurahNumber":88,"VerseNumber":10,"Text":"در بهشتی فرازنده‌"},{"SurahNumber":88,"VerseNumber":11,"Text":"نشنوند در آن یاوه‌ای‌"},{"SurahNumber":88,"VerseNumber":12,"Text":"در آن است چشمه روان‌"},{"SurahNumber":88,"VerseNumber":13,"Text":"در آن است تختهائی افراشته‌"},{"SurahNumber":88,"VerseNumber":14,"Text":"و جامهائی نهاده‌"},{"SurahNumber":88,"VerseNumber":15,"Text":"و بالشهائی چیده (صف آورده)"},{"SurahNumber":88,"VerseNumber":16,"Text":"و پرده‌های گسترده‌"},{"SurahNumber":88,"VerseNumber":17,"Text":"آیا ننگرند بسوی اشتر چگونه آفریده شد"},{"SurahNumber":88,"VerseNumber":18,"Text":"و بسوی آسمان چگونه افراشته شد"},{"SurahNumber":88,"VerseNumber":19,"Text":"و بسوی کوه‌ها چگونه نشانده شد"},{"SurahNumber":88,"VerseNumber":20,"Text":"و بسوی زمین چگونه گسترده شد"},{"SurahNumber":88,"VerseNumber":21,"Text":"پس یادآوری کن جز این نیست توئی یادآورنده‌"},{"SurahNumber":88,"VerseNumber":22,"Text":"نیستی بر ایشان فرمانده‌"},{"SurahNumber":88,"VerseNumber":23,"Text":"مگر آنکه روی برتافت و کفر ورزید"},{"SurahNumber":88,"VerseNumber":24,"Text":"که عذاب کندش خدا آن عذاب بزرگتر"},{"SurahNumber":88,"VerseNumber":25,"Text":"همانا بسوی ماست بازگشتشان‌"},{"SurahNumber":88,"VerseNumber":26,"Text":"سپس همانا بر ماست حسابشان‌"}]}