{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":69,"VerseNumber":1,"Text":"Al haaaqqah"},{"SurahNumber":69,"VerseNumber":2,"Text":"Mal haaaqqah"},{"SurahNumber":69,"VerseNumber":3,"Text":"Wa maaa adraaka mal haaaqqah"},{"SurahNumber":69,"VerseNumber":4,"Text":"Kazzabat samoodu wa 'Aadum bil qaari'ah"},{"SurahNumber":69,"VerseNumber":5,"Text":"Fa-ammaa Samoodu fauhlikoo bittaaghiyah"},{"SurahNumber":69,"VerseNumber":6,"Text":"Wa ammaa 'Aadun fa uhlikoo bireehin sarsarin 'aatiyah"},{"SurahNumber":69,"VerseNumber":7,"Text":"Sakhkharahaa 'alaihim sab'a la yaalinw wa samaaniyata ayyaamin husooman fataral qawma feehaa sar'aa ka annahum a'jaazu nakhlin khaawiyah"},{"SurahNumber":69,"VerseNumber":8,"Text":"Fahal taraa lahum mim baaqiyah"},{"SurahNumber":69,"VerseNumber":9,"Text":"Wa jaaa'a Firawnu wa man qablahoo wal mu'tafikaatu bilhaati'ah"},{"SurahNumber":69,"VerseNumber":10,"Text":"Fa'ansaw Rasoola Rabbihim fa akhazahum akhzatar raabiyah"},{"SurahNumber":69,"VerseNumber":11,"Text":"Innaa lammaa taghal maaa'u hamalnaakum fil jaariyah"},{"SurahNumber":69,"VerseNumber":12,"Text":"Linaj'alahaa lakum tazki ratanw-wa ta'iyahaa unzununw waa'iyah"},{"SurahNumber":69,"VerseNumber":13,"Text":"Fa izaa nufikha fis soori nafkhatunw waahidah"},{"SurahNumber":69,"VerseNumber":14,"Text":"Wa humilatil ardu wal jibaalu fadukkataa dakkatanw waahidah"},{"SurahNumber":69,"VerseNumber":15,"Text":"Fa yawma'izinw waqa'atil waaqi'ah"},{"SurahNumber":69,"VerseNumber":16,"Text":"Wanshaqqatis samaaa'u fahiya yawma 'izinw-waahiyah"},{"SurahNumber":69,"VerseNumber":17,"Text":"Wal malaku 'alaaa arjaaa'ihaa; wa yahmilu 'Arsha Rabbika fawqahum yawma'izin samaaniyah"},{"SurahNumber":69,"VerseNumber":18,"Text":"Yawma'izin tu'radoona laa takhfaa min kum khaafiyah"},{"SurahNumber":69,"VerseNumber":19,"Text":"Fa ammaa man ootiya kitaabahoo biyameenihee fa yaqoolu haaa'umuq ra'oo kitaabiyah"},{"SurahNumber":69,"VerseNumber":20,"Text":"Innee zannantu annee mulaaqin hisaabiyah"},{"SurahNumber":69,"VerseNumber":21,"Text":"Fahuwa fee 'eeshatir raadiyah"},{"SurahNumber":69,"VerseNumber":22,"Text":"Fee jannnatin 'aaliyah"},{"SurahNumber":69,"VerseNumber":23,"Text":"Qutoofuhaa daaniyah"},{"SurahNumber":69,"VerseNumber":24,"Text":"Kuloo washraboo haneee'am bimaaa aslaftum fil ayyaamil khaliyah"},{"SurahNumber":69,"VerseNumber":25,"Text":"Wa ammaa man ootiya kitaabahoo bishimaalihee fa yaqoolu yaalaitanee lam oota kitaaabiyah"},{"SurahNumber":69,"VerseNumber":26,"Text":"Wa lam adri maa hisaabiyah"},{"SurahNumber":69,"VerseNumber":27,"Text":"Yaa laitahaa kaanatil qaadiyah"},{"SurahNumber":69,"VerseNumber":28,"Text":"Maaa aghnaa 'annee maaliyah"},{"SurahNumber":69,"VerseNumber":29,"Text":"Halaka 'annee sultaaniyah"},{"SurahNumber":69,"VerseNumber":30,"Text":"Khuzoohu faghullooh"},{"SurahNumber":69,"VerseNumber":31,"Text":"Summal Jaheema sallooh"},{"SurahNumber":69,"VerseNumber":32,"Text":"Summa fee silsilatin zar'uhaa sab'oona ziraa'an faslukooh"},{"SurahNumber":69,"VerseNumber":33,"Text":"Innahoo kaana laa yu'minubillaahil 'Azeem"},{"SurahNumber":69,"VerseNumber":34,"Text":"wa laa yahuddu 'alaa ta'aamil miskeen"},{"SurahNumber":69,"VerseNumber":35,"Text":"Falaysa lahul yawma haahunaa hameem"},{"SurahNumber":69,"VerseNumber":36,"Text":"Wa laa ta'aamun illaa min ghisleen"},{"SurahNumber":69,"VerseNumber":37,"Text":"Laa yaakuluhooo illal khati'oon"},{"SurahNumber":69,"VerseNumber":38,"Text":"Falaaa uqsimu bimaa tubsiroon"},{"SurahNumber":69,"VerseNumber":39,"Text":"Wa maa laa tubsiroon"},{"SurahNumber":69,"VerseNumber":40,"Text":"Innahoo laqawlu Rasoolin kareem"},{"SurahNumber":69,"VerseNumber":41,"Text":"Wa ma huwa biqawli shaa'ir; qaleelam maa tu'minoon"},{"SurahNumber":69,"VerseNumber":42,"Text":"Wa laa biqawli kaahin; qaleelam maa tazakkaroon"},{"SurahNumber":69,"VerseNumber":43,"Text":"Tanzeelum mir rabbil 'aalameen"},{"SurahNumber":69,"VerseNumber":44,"Text":"Wa law taqawwala 'alainaa ba'dal aqaaweel"},{"SurahNumber":69,"VerseNumber":45,"Text":"La-akhaznaa minhu bilyameen"},{"SurahNumber":69,"VerseNumber":46,"Text":"Summa laqata'naa minhul wateen"},{"SurahNumber":69,"VerseNumber":47,"Text":"Famaa minkum min ahadin'anhu haajizeen"},{"SurahNumber":69,"VerseNumber":48,"Text":"Wa innahoo latazkiratul lilmuttaqeen"},{"SurahNumber":69,"VerseNumber":49,"Text":"Wa inna lana'lamu anna minkum mukazzibeen"},{"SurahNumber":69,"VerseNumber":50,"Text":"Wa innahu lahasratun 'alal kaafireen"},{"SurahNumber":69,"VerseNumber":51,"Text":"Wa innahoo lahaqqul yaqeen"},{"SurahNumber":69,"VerseNumber":52,"Text":"Fasbbih bismi Rabbikal 'Azeem"}]}