{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":1,"Text":"طسم"},{"SurahNumber":26,"VerseNumber":2,"Text":"تلك ءايت الكتب المبين"},{"SurahNumber":26,"VerseNumber":3,"Text":"لعلك بخع نفسك الا يكونوا مومنين"},{"SurahNumber":26,"VerseNumber":4,"Text":"ان نشا ننزل عليهم من السماء ءاية فظلت اعنقهم لها خضعين"},{"SurahNumber":26,"VerseNumber":5,"Text":"وما ياتيهم من ذكر من الرحمن محدث الا كانوا عنه معرضين"},{"SurahNumber":26,"VerseNumber":6,"Text":"فقد كذبوا فسياتيهم انبوا ما كانوا به يستهزءون"},{"SurahNumber":26,"VerseNumber":7,"Text":"اولم يروا الى الارض كم انبتنا فيها من كل زوج كريم"},{"SurahNumber":26,"VerseNumber":8,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":9,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":10,"Text":"واذ نادى ربك موسى ان ايت القوم الظلمين"},{"SurahNumber":26,"VerseNumber":11,"Text":"قوم فرعون الا يتقون"},{"SurahNumber":26,"VerseNumber":12,"Text":"قال رب اني اخاف ان يكذبون"},{"SurahNumber":26,"VerseNumber":13,"Text":"ويضيق صدري ولا ينطلق لساني فارسل الى هرون"},{"SurahNumber":26,"VerseNumber":14,"Text":"ولهم علي ذنب فاخاف ان يقتلون"},{"SurahNumber":26,"VerseNumber":15,"Text":"قال كلا فاذهبا بايتنا انا معكم مستمعون"},{"SurahNumber":26,"VerseNumber":16,"Text":"فاتيا فرعون فقولا انا رسول رب العلمين"},{"SurahNumber":26,"VerseNumber":17,"Text":"ان ارسل معنا بني اسرءيل"},{"SurahNumber":26,"VerseNumber":18,"Text":"قال الم نربك فينا وليد ا ولبثت فينا من عمرك سنين"},{"SurahNumber":26,"VerseNumber":19,"Text":"وفعلت فعلتك التي فعلت وانت من الكفرين"},{"SurahNumber":26,"VerseNumber":20,"Text":"قال فعلتها اذ ا وانا من الضالين"},{"SurahNumber":26,"VerseNumber":21,"Text":"ففررت منكم لما خفتكم فوهب لي ربي حكم ا وجعلني من المرسلين"},{"SurahNumber":26,"VerseNumber":22,"Text":"وتلك نعمة تمنها علي ان عبدت بني اسرءيل"},{"SurahNumber":26,"VerseNumber":23,"Text":"قال فرعون وما رب العلمين"},{"SurahNumber":26,"VerseNumber":24,"Text":"قال رب السموت والارض وما بينهما ان كنتم موقنين"},{"SurahNumber":26,"VerseNumber":25,"Text":"قال لمن حوله الا تستمعون"},{"SurahNumber":26,"VerseNumber":26,"Text":"قال ربكم ورب ءابايكم الاولين"},{"SurahNumber":26,"VerseNumber":27,"Text":"قال ان رسولكم الذي ارسل اليكم لمجنون"},{"SurahNumber":26,"VerseNumber":28,"Text":"قال رب المشرق والمغرب وما بينهما ان كنتم تعقلون"},{"SurahNumber":26,"VerseNumber":29,"Text":"قال لين اتخذت الها غيري لاجعلنك من المسجونين"},{"SurahNumber":26,"VerseNumber":30,"Text":"قال اولو جيتك بشيء مبين"},{"SurahNumber":26,"VerseNumber":31,"Text":"قال فات به ان كنت من الصدقين"},{"SurahNumber":26,"VerseNumber":32,"Text":"فالقى عصاه فاذا هي ثعبان مبين"},{"SurahNumber":26,"VerseNumber":33,"Text":"ونزع يده فاذا هي بيضاء للنظرين"},{"SurahNumber":26,"VerseNumber":34,"Text":"قال للملا حوله ان هذا لسحر عليم"},{"SurahNumber":26,"VerseNumber":35,"Text":"يريد ان يخرجكم من ارضكم بسحره فماذا تامرون"},{"SurahNumber":26,"VerseNumber":36,"Text":"قالوا ارجه واخاه وابعث في المداين حشرين"},{"SurahNumber":26,"VerseNumber":37,"Text":"ياتوك بكل سحار عليم"},{"SurahNumber":26,"VerseNumber":38,"Text":"فجمع السحرة لميقت يوم معلوم"},{"SurahNumber":26,"VerseNumber":39,"Text":"وقيل للناس هل انتم مجتمعون"},{"SurahNumber":26,"VerseNumber":40,"Text":"لعلنا نتبع السحرة ان كانوا هم الغلبين"},{"SurahNumber":26,"VerseNumber":41,"Text":"فلما جاء السحرة قالوا لفرعون اين لنا لاجرا ان كنا نحن الغلبين"},{"SurahNumber":26,"VerseNumber":42,"Text":"قال نعم وانكم اذ ا لمن المقربين"},{"SurahNumber":26,"VerseNumber":43,"Text":"قال لهم موسى القوا ما انتم ملقون"},{"SurahNumber":26,"VerseNumber":44,"Text":"فالقوا حبالهم وعصيهم وقالوا بعزة فرعون انا لنحن الغلبون"},{"SurahNumber":26,"VerseNumber":45,"Text":"فالقى موسى عصاه فاذا هي تلقف ما يافكون"},{"SurahNumber":26,"VerseNumber":46,"Text":"فالقي السحرة سجدين"},{"SurahNumber":26,"VerseNumber":47,"Text":"قالوا ءامنا برب العلمين"},{"SurahNumber":26,"VerseNumber":48,"Text":"رب موسى وهرون"},{"SurahNumber":26,"VerseNumber":49,"Text":"قال ءامنتم له قبل ان ءاذن لكم انه لكبيركم الذي علمكم السحر فلسوف تعلمون لاقطعن ايديكم وارجلكم من خلف ولاصلبنكم اجمعين"},{"SurahNumber":26,"VerseNumber":50,"Text":"قالوا لا ضير انا الى ربنا منقلبون"},{"SurahNumber":26,"VerseNumber":51,"Text":"انا نطمع ان يغفر لنا ربنا خطينا ان كنا اول المومنين"},{"SurahNumber":26,"VerseNumber":52,"Text":"۞واوحينا الى موسى ان اسر بعبادي انكم متبعون"},{"SurahNumber":26,"VerseNumber":53,"Text":"فارسل فرعون في المداين حشرين"},{"SurahNumber":26,"VerseNumber":54,"Text":"ان هولاء لشرذمة قليلون"},{"SurahNumber":26,"VerseNumber":55,"Text":"وانهم لنا لغايظون"},{"SurahNumber":26,"VerseNumber":56,"Text":"وانا لجميع حذرون"},{"SurahNumber":26,"VerseNumber":57,"Text":"فاخرجنهم من جنت وعيون"},{"SurahNumber":26,"VerseNumber":58,"Text":"وكنوز ومقام كريم"},{"SurahNumber":26,"VerseNumber":59,"Text":"كذلك واورثنها بني اسرءيل"},{"SurahNumber":26,"VerseNumber":60,"Text":"فاتبعوهم مشرقين"},{"SurahNumber":26,"VerseNumber":61,"Text":"فلما ترءا الجمعان قال اصحب موسى انا لمدركون"},{"SurahNumber":26,"VerseNumber":62,"Text":"قال كلا ان معي ربي سيهدين"},{"SurahNumber":26,"VerseNumber":63,"Text":"فاوحينا الى موسى ان اضرب بعصاك البحر فانفلق فكان كل فرق كالطود العظيم"},{"SurahNumber":26,"VerseNumber":64,"Text":"وازلفنا ثم الاخرين"},{"SurahNumber":26,"VerseNumber":65,"Text":"وانجينا موسى ومن معه اجمعين"},{"SurahNumber":26,"VerseNumber":66,"Text":"ثم اغرقنا الاخرين"},{"SurahNumber":26,"VerseNumber":67,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":68,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":69,"Text":"واتل عليهم نبا ابرهيم"},{"SurahNumber":26,"VerseNumber":70,"Text":"اذ قال لابيه وقومه ما تعبدون"},{"SurahNumber":26,"VerseNumber":71,"Text":"قالوا نعبد اصنام ا فنظل لها عكفين"},{"SurahNumber":26,"VerseNumber":72,"Text":"قال هل يسمعونكم اذ تدعون"},{"SurahNumber":26,"VerseNumber":73,"Text":"او ينفعونكم او يضرون"},{"SurahNumber":26,"VerseNumber":74,"Text":"قالوا بل وجدنا ءاباءنا كذلك يفعلون"},{"SurahNumber":26,"VerseNumber":75,"Text":"قال افرءيتم ما كنتم تعبدون"},{"SurahNumber":26,"VerseNumber":76,"Text":"انتم وءاباوكم الاقدمون"},{"SurahNumber":26,"VerseNumber":77,"Text":"فانهم عدو لي الا رب العلمين"},{"SurahNumber":26,"VerseNumber":78,"Text":"الذي خلقني فهو يهدين"},{"SurahNumber":26,"VerseNumber":79,"Text":"والذي هو يطعمني ويسقين"},{"SurahNumber":26,"VerseNumber":80,"Text":"واذا مرضت فهو يشفين"},{"SurahNumber":26,"VerseNumber":81,"Text":"والذي يميتني ثم يحيين"},{"SurahNumber":26,"VerseNumber":82,"Text":"والذي اطمع ان يغفر لي خطيتي يوم الدين"},{"SurahNumber":26,"VerseNumber":83,"Text":"رب هب لي حكم ا والحقني بالصلحين"},{"SurahNumber":26,"VerseNumber":84,"Text":"واجعل لي لسان صدق في الاخرين"},{"SurahNumber":26,"VerseNumber":85,"Text":"واجعلني من ورثة جنة النعيم"},{"SurahNumber":26,"VerseNumber":86,"Text":"واغفر لابي انه كان من الضالين"},{"SurahNumber":26,"VerseNumber":87,"Text":"ولا تخزني يوم يبعثون"},{"SurahNumber":26,"VerseNumber":88,"Text":"يوم لا ينفع مال ولا بنون"},{"SurahNumber":26,"VerseNumber":89,"Text":"الا من اتى الله بقلب سليم"},{"SurahNumber":26,"VerseNumber":90,"Text":"وازلفت الجنة للمتقين"},{"SurahNumber":26,"VerseNumber":91,"Text":"وبرزت الجحيم للغاوين"},{"SurahNumber":26,"VerseNumber":92,"Text":"وقيل لهم اين ما كنتم تعبدون"},{"SurahNumber":26,"VerseNumber":93,"Text":"من دون الله هل ينصرونكم او ينتصرون"},{"SurahNumber":26,"VerseNumber":94,"Text":"فكبكبوا فيها هم والغاون"},{"SurahNumber":26,"VerseNumber":95,"Text":"وجنود ابليس اجمعون"},{"SurahNumber":26,"VerseNumber":96,"Text":"قالوا وهم فيها يختصمون"},{"SurahNumber":26,"VerseNumber":97,"Text":"تالله ان كنا لفي ضلل مبين"},{"SurahNumber":26,"VerseNumber":98,"Text":"اذ نسويكم برب العلمين"},{"SurahNumber":26,"VerseNumber":99,"Text":"وما اضلنا الا المجرمون"},{"SurahNumber":26,"VerseNumber":100,"Text":"فما لنا من شفعين"},{"SurahNumber":26,"VerseNumber":101,"Text":"ولا صديق حميم"},{"SurahNumber":26,"VerseNumber":102,"Text":"فلو ان لنا كرة فنكون من المومنين"},{"SurahNumber":26,"VerseNumber":103,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":104,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":105,"Text":"كذبت قوم نوح المرسلين"},{"SurahNumber":26,"VerseNumber":106,"Text":"اذ قال لهم اخوهم نوح الا تتقون"},{"SurahNumber":26,"VerseNumber":107,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":108,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":109,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":110,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":111,"Text":"۞قالوا انومن لك واتبعك الارذلون"},{"SurahNumber":26,"VerseNumber":112,"Text":"قال وما علمي بما كانوا يعملون"},{"SurahNumber":26,"VerseNumber":113,"Text":"ان حسابهم الا على ربي لو تشعرون"},{"SurahNumber":26,"VerseNumber":114,"Text":"وما انا بطارد المومنين"},{"SurahNumber":26,"VerseNumber":115,"Text":"ان انا الا نذير مبين"},{"SurahNumber":26,"VerseNumber":116,"Text":"قالوا لين لم تنته ينوح لتكونن من المرجومين"},{"SurahNumber":26,"VerseNumber":117,"Text":"قال رب ان قومي كذبون"},{"SurahNumber":26,"VerseNumber":118,"Text":"فافتح بيني وبينهم فتح ا ونجني ومن معي من المومنين"},{"SurahNumber":26,"VerseNumber":119,"Text":"فانجينه ومن معه في الفلك المشحون"},{"SurahNumber":26,"VerseNumber":120,"Text":"ثم اغرقنا بعد الباقين"},{"SurahNumber":26,"VerseNumber":121,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":122,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":123,"Text":"كذبت عاد المرسلين"},{"SurahNumber":26,"VerseNumber":124,"Text":"اذ قال لهم اخوهم هود الا تتقون"},{"SurahNumber":26,"VerseNumber":125,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":126,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":127,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":128,"Text":"اتبنون بكل ريع ءاية تعبثون"},{"SurahNumber":26,"VerseNumber":129,"Text":"وتتخذون مصانع لعلكم تخلدون"},{"SurahNumber":26,"VerseNumber":130,"Text":"واذا بطشتم بطشتم جبارين"},{"SurahNumber":26,"VerseNumber":131,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":132,"Text":"واتقوا الذي امدكم بما تعلمون"},{"SurahNumber":26,"VerseNumber":133,"Text":"امدكم بانعم وبنين"},{"SurahNumber":26,"VerseNumber":134,"Text":"وجنت وعيون"},{"SurahNumber":26,"VerseNumber":135,"Text":"اني اخاف عليكم عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":136,"Text":"قالوا سواء علينا اوعظت ام لم تكن من الوعظين"},{"SurahNumber":26,"VerseNumber":137,"Text":"ان هذا الا خلق الاولين"},{"SurahNumber":26,"VerseNumber":138,"Text":"وما نحن بمعذبين"},{"SurahNumber":26,"VerseNumber":139,"Text":"فكذبوه فاهلكنهم ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":140,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":141,"Text":"كذبت ثمود المرسلين"},{"SurahNumber":26,"VerseNumber":142,"Text":"اذ قال لهم اخوهم صلح الا تتقون"},{"SurahNumber":26,"VerseNumber":143,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":144,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":145,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":146,"Text":"اتتركون في ما ههنا ءامنين"},{"SurahNumber":26,"VerseNumber":147,"Text":"في جنت وعيون"},{"SurahNumber":26,"VerseNumber":148,"Text":"وزروع ونخل طلعها هضيم"},{"SurahNumber":26,"VerseNumber":149,"Text":"وتنحتون من الجبال بيوت ا فرهين"},{"SurahNumber":26,"VerseNumber":150,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":151,"Text":"ولا تطيعوا امر المسرفين"},{"SurahNumber":26,"VerseNumber":152,"Text":"الذين يفسدون في الارض ولا يصلحون"},{"SurahNumber":26,"VerseNumber":153,"Text":"قالوا انما انت من المسحرين"},{"SurahNumber":26,"VerseNumber":154,"Text":"ما انت الا بشر مثلنا فات باية ان كنت من الصدقين"},{"SurahNumber":26,"VerseNumber":155,"Text":"قال هذه ناقة لها شرب ولكم شرب يوم معلوم"},{"SurahNumber":26,"VerseNumber":156,"Text":"ولا تمسوها بسوء فياخذكم عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":157,"Text":"فعقروها فاصبحوا ندمين"},{"SurahNumber":26,"VerseNumber":158,"Text":"فاخذهم العذاب ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":159,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":160,"Text":"كذبت قوم لوط المرسلين"},{"SurahNumber":26,"VerseNumber":161,"Text":"اذ قال لهم اخوهم لوط الا تتقون"},{"SurahNumber":26,"VerseNumber":162,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":163,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":164,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":165,"Text":"اتاتون الذكران من العلمين"},{"SurahNumber":26,"VerseNumber":166,"Text":"وتذرون ما خلق لكم ربكم من ازوجكم بل انتم قوم عادون"},{"SurahNumber":26,"VerseNumber":167,"Text":"قالوا لين لم تنته يلوط لتكونن من المخرجين"},{"SurahNumber":26,"VerseNumber":168,"Text":"قال اني لعملكم من القالين"},{"SurahNumber":26,"VerseNumber":169,"Text":"رب نجني واهلي مما يعملون"},{"SurahNumber":26,"VerseNumber":170,"Text":"فنجينه واهله اجمعين"},{"SurahNumber":26,"VerseNumber":171,"Text":"الا عجوز ا في الغبرين"},{"SurahNumber":26,"VerseNumber":172,"Text":"ثم دمرنا الاخرين"},{"SurahNumber":26,"VerseNumber":173,"Text":"وامطرنا عليهم مطر ا فساء مطر المنذرين"},{"SurahNumber":26,"VerseNumber":174,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":175,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":176,"Text":"كذب اصحب ليكة المرسلين"},{"SurahNumber":26,"VerseNumber":177,"Text":"اذ قال لهم شعيب الا تتقون"},{"SurahNumber":26,"VerseNumber":178,"Text":"اني لكم رسول امين"},{"SurahNumber":26,"VerseNumber":179,"Text":"فاتقوا الله واطيعون"},{"SurahNumber":26,"VerseNumber":180,"Text":"وما اسلكم عليه من اجر ان اجري الا على رب العلمين"},{"SurahNumber":26,"VerseNumber":181,"Text":"۞اوفوا الكيل ولا تكونوا من المخسرين"},{"SurahNumber":26,"VerseNumber":182,"Text":"وزنوا بالقسطاس المستقيم"},{"SurahNumber":26,"VerseNumber":183,"Text":"ولا تبخسوا الناس اشياءهم ولا تعثوا في الارض مفسدين"},{"SurahNumber":26,"VerseNumber":184,"Text":"واتقوا الذي خلقكم والجبلة الاولين"},{"SurahNumber":26,"VerseNumber":185,"Text":"قالوا انما انت من المسحرين"},{"SurahNumber":26,"VerseNumber":186,"Text":"وما انت الا بشر مثلنا وان نظنك لمن الكذبين"},{"SurahNumber":26,"VerseNumber":187,"Text":"فاسقط علينا كسف ا من السماء ان كنت من الصدقين"},{"SurahNumber":26,"VerseNumber":188,"Text":"قال ربي اعلم بما تعملون"},{"SurahNumber":26,"VerseNumber":189,"Text":"فكذبوه فاخذهم عذاب يوم الظلة انه كان عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":190,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":191,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":192,"Text":"وانه لتنزيل رب العلمين"},{"SurahNumber":26,"VerseNumber":193,"Text":"نزل به الروح الامين"},{"SurahNumber":26,"VerseNumber":194,"Text":"على قلبك لتكون من المنذرين"},{"SurahNumber":26,"VerseNumber":195,"Text":"بلسان عربي مبين"},{"SurahNumber":26,"VerseNumber":196,"Text":"وانه لفي زبر الاولين"},{"SurahNumber":26,"VerseNumber":197,"Text":"اولم يكن لهم ءاية ان يعلمه علموا بني اسرءيل"},{"SurahNumber":26,"VerseNumber":198,"Text":"ولو نزلنه على بعض الاعجمين"},{"SurahNumber":26,"VerseNumber":199,"Text":"فقراه عليهم ما كانوا به مومنين"},{"SurahNumber":26,"VerseNumber":200,"Text":"كذلك سلكنه في قلوب المجرمين"},{"SurahNumber":26,"VerseNumber":201,"Text":"لا يومنون به حتى يروا العذاب الاليم"},{"SurahNumber":26,"VerseNumber":202,"Text":"فياتيهم بغتة وهم لا يشعرون"},{"SurahNumber":26,"VerseNumber":203,"Text":"فيقولوا هل نحن منظرون"},{"SurahNumber":26,"VerseNumber":204,"Text":"افبعذابنا يستعجلون"},{"SurahNumber":26,"VerseNumber":205,"Text":"افرءيت ان متعنهم سنين"},{"SurahNumber":26,"VerseNumber":206,"Text":"ثم جاءهم ما كانوا يوعدون"},{"SurahNumber":26,"VerseNumber":207,"Text":"ما اغنى عنهم ما كانوا يمتعون"},{"SurahNumber":26,"VerseNumber":208,"Text":"وما اهلكنا من قرية الا لها منذرون"},{"SurahNumber":26,"VerseNumber":209,"Text":"ذكرى وما كنا ظلمين"},{"SurahNumber":26,"VerseNumber":210,"Text":"وما تنزلت به الشيطين"},{"SurahNumber":26,"VerseNumber":211,"Text":"وما ينبغي لهم وما يستطيعون"},{"SurahNumber":26,"VerseNumber":212,"Text":"انهم عن السمع لمعزولون"},{"SurahNumber":26,"VerseNumber":213,"Text":"فلا تدع مع الله الها ءاخر فتكون من المعذبين"},{"SurahNumber":26,"VerseNumber":214,"Text":"وانذر عشيرتك الاقربين"},{"SurahNumber":26,"VerseNumber":215,"Text":"واخفض جناحك لمن اتبعك من المومنين"},{"SurahNumber":26,"VerseNumber":216,"Text":"فان عصوك فقل اني بريء مما تعملون"},{"SurahNumber":26,"VerseNumber":217,"Text":"وتوكل على العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":218,"Text":"الذي يرىك حين تقوم"},{"SurahNumber":26,"VerseNumber":219,"Text":"وتقلبك في السجدين"},{"SurahNumber":26,"VerseNumber":220,"Text":"انه هو السميع العليم"},{"SurahNumber":26,"VerseNumber":221,"Text":"هل انبيكم على من تنزل الشيطين"},{"SurahNumber":26,"VerseNumber":222,"Text":"تنزل على كل افاك اثيم"},{"SurahNumber":26,"VerseNumber":223,"Text":"يلقون السمع واكثرهم كذبون"},{"SurahNumber":26,"VerseNumber":224,"Text":"والشعراء يتبعهم الغاون"},{"SurahNumber":26,"VerseNumber":225,"Text":"الم تر انهم في كل واد يهيمون"},{"SurahNumber":26,"VerseNumber":226,"Text":"وانهم يقولون ما لا يفعلون"},{"SurahNumber":26,"VerseNumber":227,"Text":"الا الذين ءامنوا وعملوا الصلحت وذكروا الله كثير ا وانتصروا من بعد ما ظلموا وسيعلم الذين ظلموا اي منقلب ينقلبون"}]}