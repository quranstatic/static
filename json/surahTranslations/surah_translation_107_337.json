{"Id":337,"LanguageCodes":["sq","sqi"],"Name":"sqi-fetimehdiu-la","Author":"Feti Mehdiu","Language":"Albanian","Description":"Albanian - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":107,"VerseNumber":1,"Text":"A e pe ate i cili pergenjeshtron ate bote"},{"SurahNumber":107,"VerseNumber":2,"Text":"Po ai eshte i cili e refuzon ashper bonjakun"},{"SurahNumber":107,"VerseNumber":3,"Text":"Dhe nuk e ndihmon te varferin ne ushqim"},{"SurahNumber":107,"VerseNumber":4,"Text":"Pra, te mjeret ata qe kryejne namazin"},{"SurahNumber":107,"VerseNumber":5,"Text":"Por te cilet jane indiferente ne namazin e tyre"},{"SurahNumber":107,"VerseNumber":6,"Text":"Dhe te cilet vetem eksponohen"},{"SurahNumber":107,"VerseNumber":7,"Text":"Dhe pengojne dhenien e lemoshes"}]}