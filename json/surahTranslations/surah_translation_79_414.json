{"Id":414,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet1-la","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":79,"VerseNumber":1,"Text":"Ven naziati garka(garkan)"},{"SurahNumber":79,"VerseNumber":2,"Text":"Ven nasitati nesta(nestan)"},{"SurahNumber":79,"VerseNumber":3,"Text":"Ves sabihati sebha(sebhan)"},{"SurahNumber":79,"VerseNumber":4,"Text":"Fes sabikati sebka(sebkan)"},{"SurahNumber":79,"VerseNumber":5,"Text":"Fel mudebbirati emra(emren)"},{"SurahNumber":79,"VerseNumber":6,"Text":"Yevme tercufur racifeh(racifetu)"},{"SurahNumber":79,"VerseNumber":7,"Text":"Tetbeuher radifeh(radifetu)"},{"SurahNumber":79,"VerseNumber":8,"Text":"Kulubun yevmeizin vacifeh(vacifetun)"},{"SurahNumber":79,"VerseNumber":9,"Text":"Ebsaruha hasiah(hasiatun)"},{"SurahNumber":79,"VerseNumber":10,"Text":"Yekulune e inna le merdudune fil hafireh(hafireti)"},{"SurahNumber":79,"VerseNumber":11,"Text":"E iza kunna izamen nahıreh(nahıreten)"},{"SurahNumber":79,"VerseNumber":12,"Text":"Kalu tilke izen kerretun hasireh(hasiretun)"},{"SurahNumber":79,"VerseNumber":13,"Text":"Fe innema hiye zecretun vahıdeh(vahıdetun)"},{"SurahNumber":79,"VerseNumber":14,"Text":"Fe iza hum bis sahireh(sahireti)"},{"SurahNumber":79,"VerseNumber":15,"Text":"Hel etake hadisu musa"},{"SurahNumber":79,"VerseNumber":16,"Text":"Iz nadahu rabbuhu bil vadil mukaddesi tuva(tuven)"},{"SurahNumber":79,"VerseNumber":17,"Text":"Izheb ila fir’avne innehu taga"},{"SurahNumber":79,"VerseNumber":18,"Text":"Fe kul hel leke ila en tezekka"},{"SurahNumber":79,"VerseNumber":19,"Text":"Ve ehdiyeke ila rabbike fe tahsa"},{"SurahNumber":79,"VerseNumber":20,"Text":"Fe erahul ayetel kubra"},{"SurahNumber":79,"VerseNumber":21,"Text":"Fe kezzebe ve asa"},{"SurahNumber":79,"VerseNumber":22,"Text":"Summe edbere yes’a"},{"SurahNumber":79,"VerseNumber":23,"Text":"Fehasere fe nada"},{"SurahNumber":79,"VerseNumber":24,"Text":"Fe kale ene rabbukumul a’la"},{"SurahNumber":79,"VerseNumber":25,"Text":"Fe ehazehullahu nekalel ahıreti vel ula"},{"SurahNumber":79,"VerseNumber":26,"Text":"Inne fi zalike le ıbreten li men yahsa"},{"SurahNumber":79,"VerseNumber":27,"Text":"E entum eseddu halkan emis sema’(semau), benaha"},{"SurahNumber":79,"VerseNumber":28,"Text":"Refea semkeha fe sevvaha"},{"SurahNumber":79,"VerseNumber":29,"Text":"Ve agtase leyleha ve ahrece duhaha"},{"SurahNumber":79,"VerseNumber":30,"Text":"Vel arda ba’de zalike dehaha"},{"SurahNumber":79,"VerseNumber":31,"Text":"Ahrece minha maeha ve mer’aha"},{"SurahNumber":79,"VerseNumber":32,"Text":"Vel cibale ersaha"},{"SurahNumber":79,"VerseNumber":33,"Text":"Metaan lekum ve li en amikum"},{"SurahNumber":79,"VerseNumber":34,"Text":"Fe iza caetit tammetul kubra"},{"SurahNumber":79,"VerseNumber":35,"Text":"Yevme yetezekkerul insanu ma sea"},{"SurahNumber":79,"VerseNumber":36,"Text":"Ve burrizetil cahimu li men yera"},{"SurahNumber":79,"VerseNumber":37,"Text":"Fe emma men taga"},{"SurahNumber":79,"VerseNumber":38,"Text":"Ve aserel hayated dunya"},{"SurahNumber":79,"VerseNumber":39,"Text":"Fe innel cahime hiyel me’va"},{"SurahNumber":79,"VerseNumber":40,"Text":"Ve emma men hafe makame rabbihi ve nehennefse anil heva"},{"SurahNumber":79,"VerseNumber":41,"Text":"Fe innel cennete hiyel me’va"},{"SurahNumber":79,"VerseNumber":42,"Text":"Yes’eluneke anis saati eyyane mursaha"},{"SurahNumber":79,"VerseNumber":43,"Text":"Fime ente min zikraha"},{"SurahNumber":79,"VerseNumber":44,"Text":"Ila rabbike muntehaha"},{"SurahNumber":79,"VerseNumber":45,"Text":"Innema ente munziru men yahsaha"},{"SurahNumber":79,"VerseNumber":46,"Text":"Ke ennehum yevme yerevneha lem yelbesu illa asiyyeten ev duhaha"}]}