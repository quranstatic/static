{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":92,"VerseNumber":1,"Text":"Ved natten det bedække"},{"SurahNumber":92,"VerseNumber":2,"Text":"Dagen det afslører"},{"SurahNumber":92,"VerseNumber":3,"Text":"Ham oprettede mandlig kvindelig"},{"SurahNumber":92,"VerseNumber":4,"Text":"Deres arbejder er af adskillige arter"},{"SurahNumber":92,"VerseNumber":5,"Text":"Angående ham giver velgørenhed vedligeholde retmæssighed"},{"SurahNumber":92,"VerseNumber":6,"Text":"Og upholds scripture"},{"SurahNumber":92,"VerseNumber":7,"Text":"Vi forestå ham lykke"},{"SurahNumber":92,"VerseNumber":8,"Text":"Han som er nærige han er rige"},{"SurahNumber":92,"VerseNumber":9,"Text":"Og disbelieves scripture"},{"SurahNumber":92,"VerseNumber":10,"Text":"Vi forestå ham elendighed"},{"SurahNumber":92,"VerseNumber":11,"Text":"Hans penge ikke hjælpes ham hvornår han falder"},{"SurahNumber":92,"VerseNumber":12,"Text":"Vi forsyner vejledningen"},{"SurahNumber":92,"VerseNumber":13,"Text":"Vi kontrollerer Herefter også ligesom den liv"},{"SurahNumber":92,"VerseNumber":14,"Text":"Jeg advarer jer blussende Hellfire"},{"SurahNumber":92,"VerseNumber":15,"Text":"Ingen forbrænder therein frelser de wicked"},{"SurahNumber":92,"VerseNumber":16,"Text":"Som disbelieves sving"},{"SurahNumber":92,"VerseNumber":17,"Text":"Undladende det være de righteous"},{"SurahNumber":92,"VerseNumber":18,"Text":"Som giver hans penge velgørenhed"},{"SurahNumber":92,"VerseNumber":19,"Text":"Søgende intet tilbage"},{"SurahNumber":92,"VerseNumber":20,"Text":"Søgende kun hans Lord mest Højeste"},{"SurahNumber":92,"VerseNumber":21,"Text":"Han certainly attain frelsning"}]}