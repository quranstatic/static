{"Id":418,"LanguageCodes":["tr","tur"],"Name":"tur-muhammetabay-la","Author":"Muhammet Abay","Language":"Turkish","Description":"Turkish - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":44,"VerseNumber":1,"Text":"ha-mim"},{"SurahNumber":44,"VerseNumber":2,"Text":"velkitabi-lmubin"},{"SurahNumber":44,"VerseNumber":3,"Text":"inna enzelnahu fi leyletim mubaraketin inna kunna munzirin"},{"SurahNumber":44,"VerseNumber":4,"Text":"fiha yufraku kullu emrin hakim"},{"SurahNumber":44,"VerseNumber":5,"Text":"emram min `indina. inna kunna mursilin"},{"SurahNumber":44,"VerseNumber":6,"Text":"rahmetem mir rabbik. innehu huve-ssemi`u-l`alim"},{"SurahNumber":44,"VerseNumber":7,"Text":"rabbi-ssemavati vel'ardi vema beynehuma. in kuntum mukinin"},{"SurahNumber":44,"VerseNumber":8,"Text":"la ilahe illa huve yuhyi veyumit. rabbukum verabbu abaikumu-l'evvelin"},{"SurahNumber":44,"VerseNumber":9,"Text":"bel hum fi sekkiy yel`abun"},{"SurahNumber":44,"VerseNumber":10,"Text":"fertekib yevme te'ti-ssemau biduhanim mubin"},{"SurahNumber":44,"VerseNumber":11,"Text":"yagse-nnas. haza `azabun elim"},{"SurahNumber":44,"VerseNumber":12,"Text":"rabbene-ksif `anne-l`azabe inna mu'minun"},{"SurahNumber":44,"VerseNumber":13,"Text":"enna lehumu-zzikra vekad caehum rasulum mubin"},{"SurahNumber":44,"VerseNumber":14,"Text":"summe tevellev `anhu vekalu mu`allemum mecnun"},{"SurahNumber":44,"VerseNumber":15,"Text":"inna kasifu-l`azabi kalilen innekum `aidun"},{"SurahNumber":44,"VerseNumber":16,"Text":"yevme nebtisu-lbatsete-lkubra. inna muntekimun"},{"SurahNumber":44,"VerseNumber":17,"Text":"velekad fetenna kablehum kavme fir`avne vecaehum rasulun kerim"},{"SurahNumber":44,"VerseNumber":18,"Text":"en eddu ileyye `ibade-llah. inni lekum rasulun emin"},{"SurahNumber":44,"VerseNumber":19,"Text":"veel la ta`lu `ale-llah. inni atikum bisultanim mubin"},{"SurahNumber":44,"VerseNumber":20,"Text":"veinni `uztu birabbi verabbikum en tercumun"},{"SurahNumber":44,"VerseNumber":21,"Text":"veil lem tu'minu li fa`tezilun"},{"SurahNumber":44,"VerseNumber":22,"Text":"fede`a rabbehu enne haulai kavmum mucrimun"},{"SurahNumber":44,"VerseNumber":23,"Text":"feesri bi`ibadi leylen innekum muttebe`un"},{"SurahNumber":44,"VerseNumber":24,"Text":"vetruki-lbahra rahva. innehum cundum mugrakun"},{"SurahNumber":44,"VerseNumber":25,"Text":"kem teraku min cennativ ve`uyun"},{"SurahNumber":44,"VerseNumber":26,"Text":"vezuru`iv vemekamin kerim"},{"SurahNumber":44,"VerseNumber":27,"Text":"vena`metin kanu fiha fakihin"},{"SurahNumber":44,"VerseNumber":28,"Text":"kezalik. veevrasnaha kavmen aharin"},{"SurahNumber":44,"VerseNumber":29,"Text":"fema beket `aleyhimu-ssemau vel'ardu vema kanu munzarin"},{"SurahNumber":44,"VerseNumber":30,"Text":"velekad necceyna beni israile mine-l`azabi-lmuhin"},{"SurahNumber":44,"VerseNumber":31,"Text":"min fir`avn. innehu kane `aliyem mine-lmusrifin"},{"SurahNumber":44,"VerseNumber":32,"Text":"velekadi-hternahum `ala `ilmin `ale-l`alemin"},{"SurahNumber":44,"VerseNumber":33,"Text":"veateynahum mine-l'ayati ma fihi belaum mubin"},{"SurahNumber":44,"VerseNumber":34,"Text":"inne haulai leyekulun"},{"SurahNumber":44,"VerseNumber":35,"Text":"in hiye illa mevtetune-l'ula vema nahnu bimunserin"},{"SurahNumber":44,"VerseNumber":36,"Text":"fe'tu biabaina in kuntum sadikin"},{"SurahNumber":44,"VerseNumber":37,"Text":"ehum hayrun em kavmu tubbe`iv vellezine min kablihim. ehleknahum. innehum kanu mucrimin"},{"SurahNumber":44,"VerseNumber":38,"Text":"vema halakne-ssemavati vel'arda vema beynehuma la`ibin"},{"SurahNumber":44,"VerseNumber":39,"Text":"ma halaknahuma illa bilhakki velakinne ekserahum la ya`lemun"},{"SurahNumber":44,"VerseNumber":40,"Text":"inne yevme-lfasli mikatuhum ecme`in"},{"SurahNumber":44,"VerseNumber":41,"Text":"yevme la yugni mevlen `am mevlen sey'ev vela hum yunsarun"},{"SurahNumber":44,"VerseNumber":42,"Text":"illa mer rahime-llah. innehu huve-l`azizu-rrahim"},{"SurahNumber":44,"VerseNumber":43,"Text":"inne secerate-zzekkum"},{"SurahNumber":44,"VerseNumber":44,"Text":"ta`amu-l'esim"},{"SurahNumber":44,"VerseNumber":45,"Text":"kelmuhl. yagli fi-lbutun"},{"SurahNumber":44,"VerseNumber":46,"Text":"kegalyi-lhamim"},{"SurahNumber":44,"VerseNumber":47,"Text":"huzuhu fa`tiluhu ila sevai-lcehim"},{"SurahNumber":44,"VerseNumber":48,"Text":"summe subbu fevka ra'sihi min `azabi-lhamim"},{"SurahNumber":44,"VerseNumber":49,"Text":"zuk. inneke ente-l`azizu-lkerim"},{"SurahNumber":44,"VerseNumber":50,"Text":"inne haza ma kuntum bihi temterun"},{"SurahNumber":44,"VerseNumber":51,"Text":"inne-lmuttekine fi mekamin emin"},{"SurahNumber":44,"VerseNumber":52,"Text":"fi cennativ ve`uyun"},{"SurahNumber":44,"VerseNumber":53,"Text":"yelbesune min sundusiv veistebrakim mutekabilin"},{"SurahNumber":44,"VerseNumber":54,"Text":"kezalik. vezevvecnahum bihurin `in"},{"SurahNumber":44,"VerseNumber":55,"Text":"yed`une fiha bikulli fakihetin aminin"},{"SurahNumber":44,"VerseNumber":56,"Text":"la yezukune fihe-lmevte ille-lmevtete-l'ula. vevekahum `azabe-lcehim"},{"SurahNumber":44,"VerseNumber":57,"Text":"fadlem mir rabbik. zalike huve-lfevzu-l`azim"},{"SurahNumber":44,"VerseNumber":58,"Text":"feinnema yessernahu bilisanike le`allehum yetezekkerun"},{"SurahNumber":44,"VerseNumber":59,"Text":"fertekib innehum murtekibun"}]}