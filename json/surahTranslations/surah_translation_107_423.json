{"Id":423,"LanguageCodes":["tr","tur"],"Name":"tur-sabanpiris","Author":"Saban Piris","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":107,"VerseNumber":1,"Text":"Hüküm gününü yalanlayanı gördün mü"},{"SurahNumber":107,"VerseNumber":2,"Text":"Yetimi itip kakan işte odur"},{"SurahNumber":107,"VerseNumber":3,"Text":"Düşkünü doyurmaktan hoşlanmaz"},{"SurahNumber":107,"VerseNumber":4,"Text":"Öyleyse yazıklar olsun o namaz kılanlara"},{"SurahNumber":107,"VerseNumber":5,"Text":"Kıldıkları namazdan gafil olanlara"},{"SurahNumber":107,"VerseNumber":6,"Text":"Gösteriş yapanlara"},{"SurahNumber":107,"VerseNumber":7,"Text":"Ufak tefek yardıma bile engel olanlara"}]}