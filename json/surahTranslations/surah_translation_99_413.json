{"Id":413,"LanguageCodes":["tr","tur"],"Name":"tur-latinalphabet","Author":"Latin Alphabet","Language":"Turkish","Description":"Turkish","Direction":"ltr","Source":"http://www.qurandatabase.org","Translations":[{"SurahNumber":99,"VerseNumber":1,"Text":"İza zülziletil erdu zilzaleha"},{"SurahNumber":99,"VerseNumber":2,"Text":"Ve ahracetilerdu eskaleha"},{"SurahNumber":99,"VerseNumber":3,"Text":"Ve kalel insanü ma leha"},{"SurahNumber":99,"VerseNumber":4,"Text":"Yevmeizin tühaddisü ahbaraha"},{"SurahNumber":99,"VerseNumber":5,"Text":"Bienne rabbeke evha leha"},{"SurahNumber":99,"VerseNumber":6,"Text":"Yevmeiziy yasdürun nasü eştatel li yürav a´malehüm"},{"SurahNumber":99,"VerseNumber":7,"Text":"Fe mey ya´mel miskale zerratin hayray yerah"},{"SurahNumber":99,"VerseNumber":8,"Text":"Ve mey ya´mel miskale zerratin şerray yerah"}]}