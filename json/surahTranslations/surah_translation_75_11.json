{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":75,"VerseNumber":1,"Text":"Laaa uqsimu bi yawmil qiyaamah"},{"SurahNumber":75,"VerseNumber":2,"Text":"Wa laaa uqsimu bin nafsil lawwaamah"},{"SurahNumber":75,"VerseNumber":3,"Text":"Ayahsabul insaanu al lan najm'a 'izaamah"},{"SurahNumber":75,"VerseNumber":4,"Text":"Balaa qaadireena 'alaaa an nusawwiya banaanah"},{"SurahNumber":75,"VerseNumber":5,"Text":"Bal yureedul insaanu liyafjura amaamah"},{"SurahNumber":75,"VerseNumber":6,"Text":"Yas'alu ayyyaana yawmul qiyaamah"},{"SurahNumber":75,"VerseNumber":7,"Text":"Fa izaa bariqal basar"},{"SurahNumber":75,"VerseNumber":8,"Text":"We khasafal qamar"},{"SurahNumber":75,"VerseNumber":9,"Text":"Wa jumi'ash shamusu wal qamar"},{"SurahNumber":75,"VerseNumber":10,"Text":"Yaqoolul insaanu yaw ma 'izin aynal mafarr"},{"SurahNumber":75,"VerseNumber":11,"Text":"Kallaa laa wazar"},{"SurahNumber":75,"VerseNumber":12,"Text":"Ilaa rabbika yawma 'izinil mustaqarr"},{"SurahNumber":75,"VerseNumber":13,"Text":"Yunabba 'ul insaanu yawma 'izim bimaa qaddama wa akhkhar"},{"SurahNumber":75,"VerseNumber":14,"Text":"Balil insaanu 'alaa nafsihee baseerah"},{"SurahNumber":75,"VerseNumber":15,"Text":"Wa law alqaa ma'aazeerah"},{"SurahNumber":75,"VerseNumber":16,"Text":"Laa tuharrik bihee lisaa naka lita'jala bih"},{"SurahNumber":75,"VerseNumber":17,"Text":"Inna 'alainaa jam'ahoo wa qur aanah"},{"SurahNumber":75,"VerseNumber":18,"Text":"Fa izaa qaraanaahu fattabi' qur aanah"},{"SurahNumber":75,"VerseNumber":19,"Text":"Summa inna 'alainaa bayaanah"},{"SurahNumber":75,"VerseNumber":20,"Text":"Kallaa bal tuhibboonal 'aajilah"},{"SurahNumber":75,"VerseNumber":21,"Text":"Wa tazaroonal Aakhirah"},{"SurahNumber":75,"VerseNumber":22,"Text":"Wujoohuny yawma 'izin naadirah"},{"SurahNumber":75,"VerseNumber":23,"Text":"Ilaa rabbihaa naazirah"},{"SurahNumber":75,"VerseNumber":24,"Text":"Wa wujoohuny yawma 'izim baasirah"},{"SurahNumber":75,"VerseNumber":25,"Text":"Tazunnu any yuf'ala bihaa faaqirah"},{"SurahNumber":75,"VerseNumber":26,"Text":"Kallaaa izaa balaghatit taraaqee"},{"SurahNumber":75,"VerseNumber":27,"Text":"Wa qeela man raaq"},{"SurahNumber":75,"VerseNumber":28,"Text":"Wa zanna annahul firaaq"},{"SurahNumber":75,"VerseNumber":29,"Text":"Waltaffatis saaqu bissaaq"},{"SurahNumber":75,"VerseNumber":30,"Text":"Ilaa rabbika yawma'izinil masaaq"},{"SurahNumber":75,"VerseNumber":31,"Text":"Falaa saddaqa wa laa sallaa"},{"SurahNumber":75,"VerseNumber":32,"Text":"Wa laakin kazzaba wa tawalla"},{"SurahNumber":75,"VerseNumber":33,"Text":"Summa zahaba ilaaa ahlihee yatamatta"},{"SurahNumber":75,"VerseNumber":34,"Text":"Awlaa laka fa awlaa"},{"SurahNumber":75,"VerseNumber":35,"Text":"Summa awlaa laka fa awla"},{"SurahNumber":75,"VerseNumber":36,"Text":"Ayahsabul insaanu anyytraka sudaa"},{"SurahNumber":75,"VerseNumber":37,"Text":"Alam yaku nutfatam mim maniyyiny yumnaa"},{"SurahNumber":75,"VerseNumber":38,"Text":"Summa kaana 'alaqata fakhalaq fasawwaa"},{"SurahNumber":75,"VerseNumber":39,"Text":"Faja'ala minhuz zawjayniz zakara wal unsaa"},{"SurahNumber":75,"VerseNumber":40,"Text":"Alaisa zaalika biqaadirin 'alaaa any yuhyiyal mawtaa"}]}