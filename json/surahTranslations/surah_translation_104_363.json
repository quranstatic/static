{"Id":363,"LanguageCodes":["tg","tgk"],"Name":"tgk-abdolmohammaday-la","Author":"Abdolmohammad Ayati","Language":"Tajik","Description":"Tajik - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":104,"VerseNumber":1,"Text":"Voj ʙar har ƣajʙatkunandai ajʙcue"},{"SurahNumber":104,"VerseNumber":2,"Text":"on ki mole cam' kard va hisoʙi on nigah dost"},{"SurahNumber":104,"VerseNumber":3,"Text":"Mepindorad, ki doroijas covidonas gardonad"},{"SurahNumber":104,"VerseNumber":4,"Text":"Na cunin ast! ki Uro dar hutamah andozand"},{"SurahNumber":104,"VerseNumber":5,"Text":"Va tu ci doni, ki hutamah cist"},{"SurahNumber":104,"VerseNumber":6,"Text":"Otasi afruxtai Xudost"},{"SurahNumber":104,"VerseNumber":7,"Text":"ki ʙar dilho ƣalaʙa mejoʙad"},{"SurahNumber":104,"VerseNumber":8,"Text":"On otas az har su dar mijonason giriftaast"},{"SurahNumber":104,"VerseNumber":9,"Text":"dar sutunhoi darozi otasi darovarda sudaast"}]}