{"Id":61,"LanguageCodes":["bs","bos"],"Name":"bos-besimkorkut-la","Author":"Besim Korkut","Language":"Bosnian","Description":"Bosnian - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":95,"VerseNumber":1,"Text":"Tako mi smokve i masline"},{"SurahNumber":95,"VerseNumber":2,"Text":"i Sinajske gore"},{"SurahNumber":95,"VerseNumber":3,"Text":"i grada ovog, sigurnog –"},{"SurahNumber":95,"VerseNumber":4,"Text":"Mi covjeka stvaramo u skladu najljepsem"},{"SurahNumber":95,"VerseNumber":5,"Text":"zatim cemo ga u najnakazniji lik vratiti"},{"SurahNumber":95,"VerseNumber":6,"Text":"samo ne one koji budu vjerovali i dobra djela cinili, njih ceka nagrada neprekidna"},{"SurahNumber":95,"VerseNumber":7,"Text":"Pa sta te onda navodi da porices onaj svijet –"},{"SurahNumber":95,"VerseNumber":8,"Text":"zar Allah nije sudija najpravedniji"}]}