{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":67,"VerseNumber":1,"Text":"تبرك الذي بيده الملك وهو على كل شيء قدير"},{"SurahNumber":67,"VerseNumber":2,"Text":"الذي خلق الموت والحيوة ليبلوكم ايكم احسن عمل ا وهو العزيز الغفور"},{"SurahNumber":67,"VerseNumber":3,"Text":"الذي خلق سبع سموت طباق ا ما ترى في خلق الرحمن من تفوت فارجع البصر هل ترى من فطور"},{"SurahNumber":67,"VerseNumber":4,"Text":"ثم ارجع البصر كرتين ينقلب اليك البصر خاسي ا وهو حسير"},{"SurahNumber":67,"VerseNumber":5,"Text":"ولقد زينا السماء الدنيا بمصبيح وجعلنها رجوم ا للشيطين واعتدنا لهم عذاب السعير"},{"SurahNumber":67,"VerseNumber":6,"Text":"وللذين كفروا بربهم عذاب جهنم وبيس المصير"},{"SurahNumber":67,"VerseNumber":7,"Text":"اذا القوا فيها سمعوا لها شهيق ا وهي تفور"},{"SurahNumber":67,"VerseNumber":8,"Text":"تكاد تميز من الغيظ كلما القي فيها فوج سالهم خزنتها الم ياتكم نذير"},{"SurahNumber":67,"VerseNumber":9,"Text":"قالوا بلى قد جاءنا نذير فكذبنا وقلنا ما نزل الله من شيء ان انتم الا في ضلل كبير"},{"SurahNumber":67,"VerseNumber":10,"Text":"وقالوا لو كنا نسمع او نعقل ما كنا في اصحب السعير"},{"SurahNumber":67,"VerseNumber":11,"Text":"فاعترفوا بذنبهم فسحق ا لاصحب السعير"},{"SurahNumber":67,"VerseNumber":12,"Text":"ان الذين يخشون ربهم بالغيب لهم مغفرة واجر كبير"},{"SurahNumber":67,"VerseNumber":13,"Text":"واسروا قولكم او اجهروا به انه عليم بذات الصدور"},{"SurahNumber":67,"VerseNumber":14,"Text":"الا يعلم من خلق وهو اللطيف الخبير"},{"SurahNumber":67,"VerseNumber":15,"Text":"هو الذي جعل لكم الارض ذلول ا فامشوا في مناكبها وكلوا من رزقه واليه النشور"},{"SurahNumber":67,"VerseNumber":16,"Text":"ءامنتم من في السماء ان يخسف بكم الارض فاذا هي تمور"},{"SurahNumber":67,"VerseNumber":17,"Text":"ام امنتم من في السماء ان يرسل عليكم حاصب ا فستعلمون كيف نذير"},{"SurahNumber":67,"VerseNumber":18,"Text":"ولقد كذب الذين من قبلهم فكيف كان نكير"},{"SurahNumber":67,"VerseNumber":19,"Text":"اولم يروا الى الطير فوقهم صفت ويقبضن ما يمسكهن الا الرحمن انه بكل شيء بصير"},{"SurahNumber":67,"VerseNumber":20,"Text":"امن هذا الذي هو جند لكم ينصركم من دون الرحمن ان الكفرون الا في غرور"},{"SurahNumber":67,"VerseNumber":21,"Text":"امن هذا الذي يرزقكم ان امسك رزقه بل لجوا في عتو ونفور"},{"SurahNumber":67,"VerseNumber":22,"Text":"افمن يمشي مكبا على وجهه اهدى امن يمشي سويا على صرط مستقيم"},{"SurahNumber":67,"VerseNumber":23,"Text":"قل هو الذي انشاكم وجعل لكم السمع والابصر والافدة قليل ا ما تشكرون"},{"SurahNumber":67,"VerseNumber":24,"Text":"قل هو الذي ذراكم في الارض واليه تحشرون"},{"SurahNumber":67,"VerseNumber":25,"Text":"ويقولون متى هذا الوعد ان كنتم صدقين"},{"SurahNumber":67,"VerseNumber":26,"Text":"قل انما العلم عند الله وانما انا نذير مبين"},{"SurahNumber":67,"VerseNumber":27,"Text":"فلما راوه زلفة سيت وجوه الذين كفروا وقيل هذا الذي كنتم به تدعون"},{"SurahNumber":67,"VerseNumber":28,"Text":"قل ارءيتم ان اهلكني الله ومن معي او رحمنا فمن يجير الكفرين من عذاب اليم"},{"SurahNumber":67,"VerseNumber":29,"Text":"قل هو الرحمن ءامنا به وعليه توكلنا فستعلمون من هو في ضلل مبين"},{"SurahNumber":67,"VerseNumber":30,"Text":"قل ارءيتم ان اصبح ماوكم غور ا فمن ياتيكم بماء معين"},{"SurahNumber":68,"VerseNumber":1,"Text":"ن والقلم وما يسطرون"},{"SurahNumber":68,"VerseNumber":2,"Text":"ما انت بنعمة ربك بمجنون"},{"SurahNumber":68,"VerseNumber":3,"Text":"وان لك لاجرا غير ممنون"},{"SurahNumber":68,"VerseNumber":4,"Text":"وانك لعلى خلق عظيم"},{"SurahNumber":68,"VerseNumber":5,"Text":"فستبصر ويبصرون"},{"SurahNumber":68,"VerseNumber":6,"Text":"باييكم المفتون"},{"SurahNumber":68,"VerseNumber":7,"Text":"ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بالمهتدين"},{"SurahNumber":68,"VerseNumber":8,"Text":"فلا تطع المكذبين"},{"SurahNumber":68,"VerseNumber":9,"Text":"ودوا لو تدهن فيدهنون"},{"SurahNumber":68,"VerseNumber":10,"Text":"ولا تطع كل حلاف مهين"},{"SurahNumber":68,"VerseNumber":11,"Text":"هماز مشاء بنميم"},{"SurahNumber":68,"VerseNumber":12,"Text":"مناع للخير معتد اثيم"},{"SurahNumber":68,"VerseNumber":13,"Text":"عتل بعد ذلك زنيم"},{"SurahNumber":68,"VerseNumber":14,"Text":"ان كان ذا مال وبنين"},{"SurahNumber":68,"VerseNumber":15,"Text":"اذا تتلى عليه ءايتنا قال اسطير الاولين"},{"SurahNumber":68,"VerseNumber":16,"Text":"سنسمه على الخرطوم"},{"SurahNumber":68,"VerseNumber":17,"Text":"انا بلونهم كما بلونا اصحب الجنة اذ اقسموا ليصرمنها مصبحين"},{"SurahNumber":68,"VerseNumber":18,"Text":"ولا يستثنون"},{"SurahNumber":68,"VerseNumber":19,"Text":"فطاف عليها طايف من ربك وهم نايمون"},{"SurahNumber":68,"VerseNumber":20,"Text":"فاصبحت كالصريم"},{"SurahNumber":68,"VerseNumber":21,"Text":"فتنادوا مصبحين"},{"SurahNumber":68,"VerseNumber":22,"Text":"ان اغدوا على حرثكم ان كنتم صرمين"},{"SurahNumber":68,"VerseNumber":23,"Text":"فانطلقوا وهم يتخفتون"},{"SurahNumber":68,"VerseNumber":24,"Text":"ان لا يدخلنها اليوم عليكم مسكين"},{"SurahNumber":68,"VerseNumber":25,"Text":"وغدوا على حرد قدرين"},{"SurahNumber":68,"VerseNumber":26,"Text":"فلما راوها قالوا انا لضالون"},{"SurahNumber":68,"VerseNumber":27,"Text":"بل نحن محرومون"},{"SurahNumber":68,"VerseNumber":28,"Text":"قال اوسطهم الم اقل لكم لولا تسبحون"},{"SurahNumber":68,"VerseNumber":29,"Text":"قالوا سبحن ربنا انا كنا ظلمين"},{"SurahNumber":68,"VerseNumber":30,"Text":"فاقبل بعضهم على بعض يتلومون"},{"SurahNumber":68,"VerseNumber":31,"Text":"قالوا يويلنا انا كنا طغين"},{"SurahNumber":68,"VerseNumber":32,"Text":"عسى ربنا ان يبدلنا خير ا منها انا الى ربنا رغبون"},{"SurahNumber":68,"VerseNumber":33,"Text":"كذلك العذاب ولعذاب الاخرة اكبر لو كانوا يعلمون"},{"SurahNumber":68,"VerseNumber":34,"Text":"ان للمتقين عند ربهم جنت النعيم"},{"SurahNumber":68,"VerseNumber":35,"Text":"افنجعل المسلمين كالمجرمين"},{"SurahNumber":68,"VerseNumber":36,"Text":"ما لكم كيف تحكمون"},{"SurahNumber":68,"VerseNumber":37,"Text":"ام لكم كتب فيه تدرسون"},{"SurahNumber":68,"VerseNumber":38,"Text":"ان لكم فيه لما تخيرون"},{"SurahNumber":68,"VerseNumber":39,"Text":"ام لكم ايمن علينا بلغة الى يوم القيمة ان لكم لما تحكمون"},{"SurahNumber":68,"VerseNumber":40,"Text":"سلهم ايهم بذلك زعيم"},{"SurahNumber":68,"VerseNumber":41,"Text":"ام لهم شركاء فلياتوا بشركايهم ان كانوا صدقين"},{"SurahNumber":68,"VerseNumber":42,"Text":"يوم يكشف عن ساق ويدعون الى السجود فلا يستطيعون"},{"SurahNumber":68,"VerseNumber":43,"Text":"خشعة ابصرهم ترهقهم ذلة وقد كانوا يدعون الى السجود وهم سلمون"},{"SurahNumber":68,"VerseNumber":44,"Text":"فذرني ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون"},{"SurahNumber":68,"VerseNumber":45,"Text":"واملي لهم ان كيدي متين"},{"SurahNumber":68,"VerseNumber":46,"Text":"ام تسلهم اجر ا فهم من مغرم مثقلون"},{"SurahNumber":68,"VerseNumber":47,"Text":"ام عندهم الغيب فهم يكتبون"},{"SurahNumber":68,"VerseNumber":48,"Text":"فاصبر لحكم ربك ولا تكن كصاحب الحوت اذ نادى وهو مكظوم"},{"SurahNumber":68,"VerseNumber":49,"Text":"لولا ان تدركه نعمة من ربه لنبذ بالعراء وهو مذموم"},{"SurahNumber":68,"VerseNumber":50,"Text":"فاجتبه ربه فجعله من الصلحين"},{"SurahNumber":68,"VerseNumber":51,"Text":"وان يكاد الذين كفروا ليزلقونك بابصرهم لما سمعوا الذكر ويقولون انه لمجنون"},{"SurahNumber":68,"VerseNumber":52,"Text":"وما هو الا ذكر للعلمين"},{"SurahNumber":69,"VerseNumber":1,"Text":"الحاقة"},{"SurahNumber":69,"VerseNumber":2,"Text":"ما الحاقة"},{"SurahNumber":69,"VerseNumber":3,"Text":"وما ادرىك ما الحاقة"},{"SurahNumber":69,"VerseNumber":4,"Text":"كذبت ثمود وعاد بالقارعة"},{"SurahNumber":69,"VerseNumber":5,"Text":"فاما ثمود فاهلكوا بالطاغية"},{"SurahNumber":69,"VerseNumber":6,"Text":"واما عاد فاهلكوا بريح صرصر عاتية"},{"SurahNumber":69,"VerseNumber":7,"Text":"سخرها عليهم سبع ليال وثمنية ايام حسوم ا فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"},{"SurahNumber":69,"VerseNumber":8,"Text":"فهل ترى لهم من باقية"},{"SurahNumber":69,"VerseNumber":9,"Text":"وجاء فرعون ومن قبله والموتفكت بالخاطية"},{"SurahNumber":69,"VerseNumber":10,"Text":"فعصوا رسول ربهم فاخذهم اخذة رابية"},{"SurahNumber":69,"VerseNumber":11,"Text":"انا لما طغا الماء حملنكم في الجارية"},{"SurahNumber":69,"VerseNumber":12,"Text":"لنجعلها لكم تذكرة وتعيها اذن وعية"},{"SurahNumber":69,"VerseNumber":13,"Text":"فاذا نفخ في الصور نفخة وحدة"},{"SurahNumber":69,"VerseNumber":14,"Text":"وحملت الارض والجبال فدكتا دكة وحدة"},{"SurahNumber":69,"VerseNumber":15,"Text":"فيوميذ وقعت الواقعة"},{"SurahNumber":69,"VerseNumber":16,"Text":"وانشقت السماء فهي يوميذ واهية"},{"SurahNumber":69,"VerseNumber":17,"Text":"والملك على ارجايها ويحمل عرش ربك فوقهم يوميذ ثمنية"},{"SurahNumber":69,"VerseNumber":18,"Text":"يوميذ تعرضون لا تخفى منكم خافية"},{"SurahNumber":69,"VerseNumber":19,"Text":"فاما من اوتي كتبه بيمينه فيقول هاوم اقرءوا كتبيه"},{"SurahNumber":69,"VerseNumber":20,"Text":"اني ظننت اني ملق حسابيه"},{"SurahNumber":69,"VerseNumber":21,"Text":"فهو في عيشة راضية"},{"SurahNumber":69,"VerseNumber":22,"Text":"في جنة عالية"},{"SurahNumber":69,"VerseNumber":23,"Text":"قطوفها دانية"},{"SurahNumber":69,"VerseNumber":24,"Text":"كلوا واشربوا هنيا بما اسلفتم في الايام الخالية"},{"SurahNumber":69,"VerseNumber":25,"Text":"واما من اوتي كتبه بشماله فيقول يليتني لم اوت كتبيه"},{"SurahNumber":69,"VerseNumber":26,"Text":"ولم ادر ما حسابيه"},{"SurahNumber":69,"VerseNumber":27,"Text":"يليتها كانت القاضية"},{"SurahNumber":69,"VerseNumber":28,"Text":"ما اغنى عني ماليه"},{"SurahNumber":69,"VerseNumber":29,"Text":"هلك عني سلطنيه"},{"SurahNumber":69,"VerseNumber":30,"Text":"خذوه فغلوه"},{"SurahNumber":69,"VerseNumber":31,"Text":"ثم الجحيم صلوه"},{"SurahNumber":69,"VerseNumber":32,"Text":"ثم في سلسلة ذرعها سبعون ذراع ا فاسلكوه"},{"SurahNumber":69,"VerseNumber":33,"Text":"انه كان لا يومن بالله العظيم"},{"SurahNumber":69,"VerseNumber":34,"Text":"ولا يحض على طعام المسكين"},{"SurahNumber":69,"VerseNumber":35,"Text":"فليس له اليوم ههنا حميم"},{"SurahNumber":69,"VerseNumber":36,"Text":"ولا طعام الا من غسلين"},{"SurahNumber":69,"VerseNumber":37,"Text":"لا ياكله الا الخطون"},{"SurahNumber":69,"VerseNumber":38,"Text":"فلا اقسم بما تبصرون"},{"SurahNumber":69,"VerseNumber":39,"Text":"وما لا تبصرون"},{"SurahNumber":69,"VerseNumber":40,"Text":"انه لقول رسول كريم"},{"SurahNumber":69,"VerseNumber":41,"Text":"وما هو بقول شاعر قليل ا ما تومنون"},{"SurahNumber":69,"VerseNumber":42,"Text":"ولا بقول كاهن قليل ا ما تذكرون"},{"SurahNumber":69,"VerseNumber":43,"Text":"تنزيل من رب العلمين"},{"SurahNumber":69,"VerseNumber":44,"Text":"ولو تقول علينا بعض الاقاويل"},{"SurahNumber":69,"VerseNumber":45,"Text":"لاخذنا منه باليمين"},{"SurahNumber":69,"VerseNumber":46,"Text":"ثم لقطعنا منه الوتين"},{"SurahNumber":69,"VerseNumber":47,"Text":"فما منكم من احد عنه حجزين"},{"SurahNumber":69,"VerseNumber":48,"Text":"وانه لتذكرة للمتقين"},{"SurahNumber":69,"VerseNumber":49,"Text":"وانا لنعلم ان منكم مكذبين"},{"SurahNumber":69,"VerseNumber":50,"Text":"وانه لحسرة على الكفرين"},{"SurahNumber":69,"VerseNumber":51,"Text":"وانه لحق اليقين"},{"SurahNumber":69,"VerseNumber":52,"Text":"فسبح باسم ربك العظيم"},{"SurahNumber":70,"VerseNumber":1,"Text":"سال سايل بعذاب واقع"},{"SurahNumber":70,"VerseNumber":2,"Text":"للكفرين ليس له دافع"},{"SurahNumber":70,"VerseNumber":3,"Text":"من الله ذي المعارج"},{"SurahNumber":70,"VerseNumber":4,"Text":"تعرج المليكة والروح اليه في يوم كان مقداره خمسين الف سنة"},{"SurahNumber":70,"VerseNumber":5,"Text":"فاصبر صبر ا جميلا"},{"SurahNumber":70,"VerseNumber":6,"Text":"انهم يرونه بعيد ا"},{"SurahNumber":70,"VerseNumber":7,"Text":"ونرىه قريب ا"},{"SurahNumber":70,"VerseNumber":8,"Text":"يوم تكون السماء كالمهل"},{"SurahNumber":70,"VerseNumber":9,"Text":"وتكون الجبال كالعهن"},{"SurahNumber":70,"VerseNumber":10,"Text":"ولا يسل حميم حميم ا"},{"SurahNumber":70,"VerseNumber":11,"Text":"يبصرونهم يود المجرم لو يفتدي من عذاب يوميذ ببنيه"},{"SurahNumber":70,"VerseNumber":12,"Text":"وصحبته واخيه"},{"SurahNumber":70,"VerseNumber":13,"Text":"وفصيلته التي تويه"},{"SurahNumber":70,"VerseNumber":14,"Text":"ومن في الارض جميع ا ثم ينجيه"},{"SurahNumber":70,"VerseNumber":15,"Text":"كلا انها لظى"},{"SurahNumber":70,"VerseNumber":16,"Text":"نزاعة للشوى"},{"SurahNumber":70,"VerseNumber":17,"Text":"تدعوا من ادبر وتولى"},{"SurahNumber":70,"VerseNumber":18,"Text":"وجمع فاوعى"},{"SurahNumber":70,"VerseNumber":19,"Text":"۞ان الانسن خلق هلوعا"},{"SurahNumber":70,"VerseNumber":20,"Text":"اذا مسه الشر جزوع ا"},{"SurahNumber":70,"VerseNumber":21,"Text":"واذا مسه الخير منوعا"},{"SurahNumber":70,"VerseNumber":22,"Text":"الا المصلين"},{"SurahNumber":70,"VerseNumber":23,"Text":"الذين هم على صلاتهم دايمون"},{"SurahNumber":70,"VerseNumber":24,"Text":"والذين في امولهم حق معلوم"},{"SurahNumber":70,"VerseNumber":25,"Text":"للسايل والمحروم"},{"SurahNumber":70,"VerseNumber":26,"Text":"والذين يصدقون بيوم الدين"},{"SurahNumber":70,"VerseNumber":27,"Text":"والذين هم من عذاب ربهم مشفقون"},{"SurahNumber":70,"VerseNumber":28,"Text":"ان عذاب ربهم غير مامون"},{"SurahNumber":70,"VerseNumber":29,"Text":"والذين هم لفروجهم حفظون"},{"SurahNumber":70,"VerseNumber":30,"Text":"الا على ازوجهم او ما ملكت ايمنهم فانهم غير ملومين"},{"SurahNumber":70,"VerseNumber":31,"Text":"فمن ابتغى وراء ذلك فاوليك هم العادون"},{"SurahNumber":70,"VerseNumber":32,"Text":"والذين هم لامنتهم وعهدهم رعون"},{"SurahNumber":70,"VerseNumber":33,"Text":"والذين هم بشهدتهم قايمون"},{"SurahNumber":70,"VerseNumber":34,"Text":"والذين هم على صلاتهم يحافظون"},{"SurahNumber":70,"VerseNumber":35,"Text":"اوليك في جنت مكرمون"},{"SurahNumber":70,"VerseNumber":36,"Text":"فمال الذين كفروا قبلك مهطعين"},{"SurahNumber":70,"VerseNumber":37,"Text":"عن اليمين وعن الشمال عزين"},{"SurahNumber":70,"VerseNumber":38,"Text":"ايطمع كل امري منهم ان يدخل جنة نعيم"},{"SurahNumber":70,"VerseNumber":39,"Text":"كلا انا خلقنهم مما يعلمون"},{"SurahNumber":70,"VerseNumber":40,"Text":"فلا اقسم برب المشرق والمغرب انا لقدرون"},{"SurahNumber":70,"VerseNumber":41,"Text":"على ان نبدل خير ا منهم وما نحن بمسبوقين"},{"SurahNumber":70,"VerseNumber":42,"Text":"فذرهم يخوضوا ويلعبوا حتى يلقوا يومهم الذي يوعدون"},{"SurahNumber":70,"VerseNumber":43,"Text":"يوم يخرجون من الاجداث سراع ا كانهم الى نصب يوفضون"},{"SurahNumber":70,"VerseNumber":44,"Text":"خشعة ابصرهم ترهقهم ذلة ذلك اليوم الذي كانوا يوعدون"},{"SurahNumber":71,"VerseNumber":1,"Text":"انا ارسلنا نوحا الى قومه ان انذر قومك من قبل ان ياتيهم عذاب اليم"},{"SurahNumber":71,"VerseNumber":2,"Text":"قال يقوم اني لكم نذير مبين"},{"SurahNumber":71,"VerseNumber":3,"Text":"ان اعبدوا الله واتقوه واطيعون"},{"SurahNumber":71,"VerseNumber":4,"Text":"يغفر لكم من ذنوبكم ويوخركم الى اجل مسمى ان اجل الله اذا جاء لا يوخر لو كنتم تعلمون"},{"SurahNumber":71,"VerseNumber":5,"Text":"قال رب اني دعوت قومي ليل ا ونهار ا"},{"SurahNumber":71,"VerseNumber":6,"Text":"فلم يزدهم دعاءي الا فرار ا"},{"SurahNumber":71,"VerseNumber":7,"Text":"واني كلما دعوتهم لتغفر لهم جعلوا اصبعهم في ءاذانهم واستغشوا ثيابهم واصروا واستكبروا استكبار ا"},{"SurahNumber":71,"VerseNumber":8,"Text":"ثم اني دعوتهم جهار ا"},{"SurahNumber":71,"VerseNumber":9,"Text":"ثم اني اعلنت لهم واسررت لهم اسرار ا"},{"SurahNumber":71,"VerseNumber":10,"Text":"فقلت استغفروا ربكم انه كان غفار ا"},{"SurahNumber":71,"VerseNumber":11,"Text":"يرسل السماء عليكم مدرار ا"},{"SurahNumber":71,"VerseNumber":12,"Text":"ويمددكم بامول وبنين ويجعل لكم جنت ويجعل لكم انهر ا"},{"SurahNumber":71,"VerseNumber":13,"Text":"ما لكم لا ترجون لله وقار ا"},{"SurahNumber":71,"VerseNumber":14,"Text":"وقد خلقكم اطوارا"},{"SurahNumber":71,"VerseNumber":15,"Text":"الم تروا كيف خلق الله سبع سموت طباق ا"},{"SurahNumber":71,"VerseNumber":16,"Text":"وجعل القمر فيهن نور ا وجعل الشمس سراج ا"},{"SurahNumber":71,"VerseNumber":17,"Text":"والله انبتكم من الارض نبات ا"},{"SurahNumber":71,"VerseNumber":18,"Text":"ثم يعيدكم فيها ويخرجكم اخراج ا"},{"SurahNumber":71,"VerseNumber":19,"Text":"والله جعل لكم الارض بساط ا"},{"SurahNumber":71,"VerseNumber":20,"Text":"لتسلكوا منها سبل ا فجاج ا"},{"SurahNumber":71,"VerseNumber":21,"Text":"قال نوح رب انهم عصوني واتبعوا من لم يزده ماله وولده الا خسار ا"},{"SurahNumber":71,"VerseNumber":22,"Text":"ومكروا مكر ا كبار ا"},{"SurahNumber":71,"VerseNumber":23,"Text":"وقالوا لا تذرن ءالهتكم ولا تذرن ود ا ولا سواع ا ولا يغوث ويعوق ونسر ا"},{"SurahNumber":71,"VerseNumber":24,"Text":"وقد اضلوا كثير ا ولا تزد الظلمين الا ضلل ا"},{"SurahNumber":71,"VerseNumber":25,"Text":"مما خطيتهم اغرقوا فادخلوا نار ا فلم يجدوا لهم من دون الله انصار ا"},{"SurahNumber":71,"VerseNumber":26,"Text":"وقال نوح رب لا تذر على الارض من الكفرين ديارا"},{"SurahNumber":71,"VerseNumber":27,"Text":"انك ان تذرهم يضلوا عبادك ولا يلدوا الا فاجر ا كفار ا"},{"SurahNumber":71,"VerseNumber":28,"Text":"رب اغفر لي ولولدي ولمن دخل بيتي مومن ا وللمومنين والمومنت ولا تزد الظلمين الا تبارا"},{"SurahNumber":72,"VerseNumber":1,"Text":"قل اوحي الي انه استمع نفر من الجن فقالوا انا سمعنا قرءانا عجب ا"},{"SurahNumber":72,"VerseNumber":2,"Text":"يهدي الى الرشد فامنا به ولن نشرك بربنا احد ا"},{"SurahNumber":72,"VerseNumber":3,"Text":"وانه تعلى جد ربنا ما اتخذ صحبة ولا ولد ا"},{"SurahNumber":72,"VerseNumber":4,"Text":"وانه كان يقول سفيهنا على الله شطط ا"},{"SurahNumber":72,"VerseNumber":5,"Text":"وانا ظننا ان لن تقول الانس والجن على الله كذب ا"},{"SurahNumber":72,"VerseNumber":6,"Text":"وانه كان رجال من الانس يعوذون برجال من الجن فزادوهم رهق ا"},{"SurahNumber":72,"VerseNumber":7,"Text":"وانهم ظنوا كما ظننتم ان لن يبعث الله احد ا"},{"SurahNumber":72,"VerseNumber":8,"Text":"وانا لمسنا السماء فوجدنها مليت حرس ا شديد ا وشهب ا"},{"SurahNumber":72,"VerseNumber":9,"Text":"وانا كنا نقعد منها مقعد للسمع فمن يستمع الان يجد له شهاب ا رصد ا"},{"SurahNumber":72,"VerseNumber":10,"Text":"وانا لا ندري اشر اريد بمن في الارض ام اراد بهم ربهم رشد ا"},{"SurahNumber":72,"VerseNumber":11,"Text":"وانا منا الصلحون ومنا دون ذلك كنا طرايق قدد ا"},{"SurahNumber":72,"VerseNumber":12,"Text":"وانا ظننا ان لن نعجز الله في الارض ولن نعجزه هرب ا"},{"SurahNumber":72,"VerseNumber":13,"Text":"وانا لما سمعنا الهدى ءامنا به فمن يومن بربه فلا يخاف بخس ا ولا رهق ا"},{"SurahNumber":72,"VerseNumber":14,"Text":"وانا منا المسلمون ومنا القسطون فمن اسلم فاوليك تحروا رشد ا"},{"SurahNumber":72,"VerseNumber":15,"Text":"واما القسطون فكانوا لجهنم حطب ا"},{"SurahNumber":72,"VerseNumber":16,"Text":"والو استقموا على الطريقة لاسقينهم ماء غدق ا"},{"SurahNumber":72,"VerseNumber":17,"Text":"لنفتنهم فيه ومن يعرض عن ذكر ربه يسلكه عذاب ا صعد ا"},{"SurahNumber":72,"VerseNumber":18,"Text":"وان المسجد لله فلا تدعوا مع الله احد ا"},{"SurahNumber":72,"VerseNumber":19,"Text":"وانه لما قام عبد الله يدعوه كادوا يكونون عليه لبد ا"},{"SurahNumber":72,"VerseNumber":20,"Text":"قل انما ادعوا ربي ولا اشرك به احد ا"},{"SurahNumber":72,"VerseNumber":21,"Text":"قل اني لا املك لكم ضر ا ولا رشد ا"},{"SurahNumber":72,"VerseNumber":22,"Text":"قل اني لن يجيرني من الله احد ولن اجد من دونه ملتحدا"},{"SurahNumber":72,"VerseNumber":23,"Text":"الا بلغ ا من الله ورسلته ومن يعص الله ورسوله فان له نار جهنم خلدين فيها ابدا"},{"SurahNumber":72,"VerseNumber":24,"Text":"حتى اذا راوا ما يوعدون فسيعلمون من اضعف ناصر ا واقل عدد ا"},{"SurahNumber":72,"VerseNumber":25,"Text":"قل ان ادري اقريب ما توعدون ام يجعل له ربي امدا"},{"SurahNumber":72,"VerseNumber":26,"Text":"علم الغيب فلا يظهر على غيبه احدا"},{"SurahNumber":72,"VerseNumber":27,"Text":"الا من ارتضى من رسول فانه يسلك من بين يديه ومن خلفه رصد ا"},{"SurahNumber":72,"VerseNumber":28,"Text":"ليعلم ان قد ابلغوا رسلت ربهم واحاط بما لديهم واحصى كل شيء عددا"},{"SurahNumber":73,"VerseNumber":1,"Text":"يايها المزمل"},{"SurahNumber":73,"VerseNumber":2,"Text":"قم اليل الا قليل ا"},{"SurahNumber":73,"VerseNumber":3,"Text":"نصفه او انقص منه قليلا"},{"SurahNumber":73,"VerseNumber":4,"Text":"او زد عليه ورتل القرءان ترتيلا"},{"SurahNumber":73,"VerseNumber":5,"Text":"انا سنلقي عليك قول ا ثقيلا"},{"SurahNumber":73,"VerseNumber":6,"Text":"ان ناشية اليل هي اشد وط ا واقوم قيلا"},{"SurahNumber":73,"VerseNumber":7,"Text":"ان لك في النهار سبح ا طويل ا"},{"SurahNumber":73,"VerseNumber":8,"Text":"واذكر اسم ربك وتبتل اليه تبتيل ا"},{"SurahNumber":73,"VerseNumber":9,"Text":"رب المشرق والمغرب لا اله الا هو فاتخذه وكيل ا"},{"SurahNumber":73,"VerseNumber":10,"Text":"واصبر على ما يقولون واهجرهم هجر ا جميل ا"},{"SurahNumber":73,"VerseNumber":11,"Text":"وذرني والمكذبين اولي النعمة ومهلهم قليلا"},{"SurahNumber":73,"VerseNumber":12,"Text":"ان لدينا انكال ا وجحيم ا"},{"SurahNumber":73,"VerseNumber":13,"Text":"وطعام ا ذا غصة وعذابا اليم ا"},{"SurahNumber":73,"VerseNumber":14,"Text":"يوم ترجف الارض والجبال وكانت الجبال كثيب ا مهيلا"},{"SurahNumber":73,"VerseNumber":15,"Text":"انا ارسلنا اليكم رسول ا شهدا عليكم كما ارسلنا الى فرعون رسول ا"},{"SurahNumber":73,"VerseNumber":16,"Text":"فعصى فرعون الرسول فاخذنه اخذ ا وبيل ا"},{"SurahNumber":73,"VerseNumber":17,"Text":"فكيف تتقون ان كفرتم يوم ا يجعل الولدن شيبا"},{"SurahNumber":73,"VerseNumber":18,"Text":"السماء منفطر به كان وعده مفعولا"},{"SurahNumber":73,"VerseNumber":19,"Text":"ان هذه تذكرة فمن شاء اتخذ الى ربه سبيلا"},{"SurahNumber":73,"VerseNumber":20,"Text":"۞ان ربك يعلم انك تقوم ادنى من ثلثي اليل ونصفه وثلثه وطايفة من الذين معك والله يقدر اليل والنهار علم ان لن تحصوه فتاب عليكم فاقرءوا ما تيسر من القرءان علم ان سيكون منكم مرضى وءاخرون يضربون في الارض يبتغون من فضل الله وءاخرون يقتلون في سبيل الله فاقرءوا ما تيسر منه واقيموا الصلوة وءاتوا الزكوة واقرضوا الله قرضا حسن ا وما تقدموا لانفسكم من خير تجدوه عند الله هو خير ا واعظم اجر ا واستغفروا الله ان الله غفور رحيم"},{"SurahNumber":74,"VerseNumber":1,"Text":"يايها المدثر"},{"SurahNumber":74,"VerseNumber":2,"Text":"قم فانذر"},{"SurahNumber":74,"VerseNumber":3,"Text":"وربك فكبر"},{"SurahNumber":74,"VerseNumber":4,"Text":"وثيابك فطهر"},{"SurahNumber":74,"VerseNumber":5,"Text":"والرجز فاهجر"},{"SurahNumber":74,"VerseNumber":6,"Text":"ولا تمنن تستكثر"},{"SurahNumber":74,"VerseNumber":7,"Text":"ولربك فاصبر"},{"SurahNumber":74,"VerseNumber":8,"Text":"فاذا نقر في الناقور"},{"SurahNumber":74,"VerseNumber":9,"Text":"فذلك يوميذ يوم عسير"},{"SurahNumber":74,"VerseNumber":10,"Text":"على الكفرين غير يسير"},{"SurahNumber":74,"VerseNumber":11,"Text":"ذرني ومن خلقت وحيد ا"},{"SurahNumber":74,"VerseNumber":12,"Text":"وجعلت له مال ا ممدود ا"},{"SurahNumber":74,"VerseNumber":13,"Text":"وبنين شهود ا"},{"SurahNumber":74,"VerseNumber":14,"Text":"ومهدت له تمهيد ا"},{"SurahNumber":74,"VerseNumber":15,"Text":"ثم يطمع ان ازيد"},{"SurahNumber":74,"VerseNumber":16,"Text":"كلا انه كان لايتنا عنيد ا"},{"SurahNumber":74,"VerseNumber":17,"Text":"سارهقه صعودا"},{"SurahNumber":74,"VerseNumber":18,"Text":"انه فكر وقدر"},{"SurahNumber":74,"VerseNumber":19,"Text":"فقتل كيف قدر"},{"SurahNumber":74,"VerseNumber":20,"Text":"ثم قتل كيف قدر"},{"SurahNumber":74,"VerseNumber":21,"Text":"ثم نظر"},{"SurahNumber":74,"VerseNumber":22,"Text":"ثم عبس وبسر"},{"SurahNumber":74,"VerseNumber":23,"Text":"ثم ادبر واستكبر"},{"SurahNumber":74,"VerseNumber":24,"Text":"فقال ان هذا الا سحر يوثر"},{"SurahNumber":74,"VerseNumber":25,"Text":"ان هذا الا قول البشر"},{"SurahNumber":74,"VerseNumber":26,"Text":"ساصليه سقر"},{"SurahNumber":74,"VerseNumber":27,"Text":"وما ادرىك ما سقر"},{"SurahNumber":74,"VerseNumber":28,"Text":"لا تبقي ولا تذر"},{"SurahNumber":74,"VerseNumber":29,"Text":"لواحة للبشر"},{"SurahNumber":74,"VerseNumber":30,"Text":"عليها تسعة عشر"},{"SurahNumber":74,"VerseNumber":31,"Text":"وما جعلنا اصحب النار الا مليكة وما جعلنا عدتهم الا فتنة للذين كفروا ليستيقن الذين اوتوا الكتب ويزداد الذين ءامنوا ايمن ا ولا يرتاب الذين اوتوا الكتب والمومنون وليقول الذين في قلوبهم مرض والكفرون ماذا اراد الله بهذا مثل ا كذلك يضل الله من يشاء ويهدي من يشاء وما يعلم جنود ربك الا هو وما هي الا ذكرى للبشر"},{"SurahNumber":74,"VerseNumber":32,"Text":"كلا والقمر"},{"SurahNumber":74,"VerseNumber":33,"Text":"واليل اذ ادبر"},{"SurahNumber":74,"VerseNumber":34,"Text":"والصبح اذا اسفر"},{"SurahNumber":74,"VerseNumber":35,"Text":"انها لاحدى الكبر"},{"SurahNumber":74,"VerseNumber":36,"Text":"نذير ا للبشر"},{"SurahNumber":74,"VerseNumber":37,"Text":"لمن شاء منكم ان يتقدم او يتاخر"},{"SurahNumber":74,"VerseNumber":38,"Text":"كل نفس بما كسبت رهينة"},{"SurahNumber":74,"VerseNumber":39,"Text":"الا اصحب اليمين"},{"SurahNumber":74,"VerseNumber":40,"Text":"في جنت يتساءلون"},{"SurahNumber":74,"VerseNumber":41,"Text":"عن المجرمين"},{"SurahNumber":74,"VerseNumber":42,"Text":"ما سلككم في سقر"},{"SurahNumber":74,"VerseNumber":43,"Text":"قالوا لم نك من المصلين"},{"SurahNumber":74,"VerseNumber":44,"Text":"ولم نك نطعم المسكين"},{"SurahNumber":74,"VerseNumber":45,"Text":"وكنا نخوض مع الخايضين"},{"SurahNumber":74,"VerseNumber":46,"Text":"وكنا نكذب بيوم الدين"},{"SurahNumber":74,"VerseNumber":47,"Text":"حتى اتىنا اليقين"},{"SurahNumber":74,"VerseNumber":48,"Text":"فما تنفعهم شفعة الشفعين"},{"SurahNumber":74,"VerseNumber":49,"Text":"فما لهم عن التذكرة معرضين"},{"SurahNumber":74,"VerseNumber":50,"Text":"كانهم حمر مستنفرة"},{"SurahNumber":74,"VerseNumber":51,"Text":"فرت من قسورة"},{"SurahNumber":74,"VerseNumber":52,"Text":"بل يريد كل امري منهم ان يوتى صحف ا منشرة"},{"SurahNumber":74,"VerseNumber":53,"Text":"كلا بل لا يخافون الاخرة"},{"SurahNumber":74,"VerseNumber":54,"Text":"كلا انه تذكرة"},{"SurahNumber":74,"VerseNumber":55,"Text":"فمن شاء ذكره"},{"SurahNumber":74,"VerseNumber":56,"Text":"وما يذكرون الا ان يشاء الله هو اهل التقوى واهل المغفرة"},{"SurahNumber":75,"VerseNumber":1,"Text":"لا اقسم بيوم القيمة"},{"SurahNumber":75,"VerseNumber":2,"Text":"ولا اقسم بالنفس اللوامة"},{"SurahNumber":75,"VerseNumber":3,"Text":"ايحسب الانسن الن نجمع عظامه"},{"SurahNumber":75,"VerseNumber":4,"Text":"بلى قدرين على ان نسوي بنانه"},{"SurahNumber":75,"VerseNumber":5,"Text":"بل يريد الانسن ليفجر امامه"},{"SurahNumber":75,"VerseNumber":6,"Text":"يسل ايان يوم القيمة"},{"SurahNumber":75,"VerseNumber":7,"Text":"فاذا برق البصر"},{"SurahNumber":75,"VerseNumber":8,"Text":"وخسف القمر"},{"SurahNumber":75,"VerseNumber":9,"Text":"وجمع الشمس والقمر"},{"SurahNumber":75,"VerseNumber":10,"Text":"يقول الانسن يوميذ اين المفر"},{"SurahNumber":75,"VerseNumber":11,"Text":"كلا لا وزر"},{"SurahNumber":75,"VerseNumber":12,"Text":"الى ربك يوميذ المستقر"},{"SurahNumber":75,"VerseNumber":13,"Text":"ينبوا الانسن يوميذ بما قدم واخر"},{"SurahNumber":75,"VerseNumber":14,"Text":"بل الانسن على نفسه بصيرة"},{"SurahNumber":75,"VerseNumber":15,"Text":"ولو القى معاذيره"},{"SurahNumber":75,"VerseNumber":16,"Text":"لا تحرك به لسانك لتعجل به"},{"SurahNumber":75,"VerseNumber":17,"Text":"ان علينا جمعه وقرءانه"},{"SurahNumber":75,"VerseNumber":18,"Text":"فاذا قرانه فاتبع قرءانه"},{"SurahNumber":75,"VerseNumber":19,"Text":"ثم ان علينا بيانه"},{"SurahNumber":75,"VerseNumber":20,"Text":"كلا بل تحبون العاجلة"},{"SurahNumber":75,"VerseNumber":21,"Text":"وتذرون الاخرة"},{"SurahNumber":75,"VerseNumber":22,"Text":"وجوه يوميذ ناضرة"},{"SurahNumber":75,"VerseNumber":23,"Text":"الى ربها ناظرة"},{"SurahNumber":75,"VerseNumber":24,"Text":"ووجوه يوميذ باسرة"},{"SurahNumber":75,"VerseNumber":25,"Text":"تظن ان يفعل بها فاقرة"},{"SurahNumber":75,"VerseNumber":26,"Text":"كلا اذا بلغت التراقي"},{"SurahNumber":75,"VerseNumber":27,"Text":"وقيل من راق"},{"SurahNumber":75,"VerseNumber":28,"Text":"وظن انه الفراق"},{"SurahNumber":75,"VerseNumber":29,"Text":"والتفت الساق بالساق"},{"SurahNumber":75,"VerseNumber":30,"Text":"الى ربك يوميذ المساق"},{"SurahNumber":75,"VerseNumber":31,"Text":"فلا صدق ولا صلى"},{"SurahNumber":75,"VerseNumber":32,"Text":"ولكن كذب وتولى"},{"SurahNumber":75,"VerseNumber":33,"Text":"ثم ذهب الى اهله يتمطى"},{"SurahNumber":75,"VerseNumber":34,"Text":"اولى لك فاولى"},{"SurahNumber":75,"VerseNumber":35,"Text":"ثم اولى لك فاولى"},{"SurahNumber":75,"VerseNumber":36,"Text":"ايحسب الانسن ان يترك سدى"},{"SurahNumber":75,"VerseNumber":37,"Text":"الم يك نطفة من مني يمنى"},{"SurahNumber":75,"VerseNumber":38,"Text":"ثم كان علقة فخلق فسوى"},{"SurahNumber":75,"VerseNumber":39,"Text":"فجعل منه الزوجين الذكر والانثى"},{"SurahNumber":75,"VerseNumber":40,"Text":"اليس ذلك بقدر على ان يحي الموتى"},{"SurahNumber":76,"VerseNumber":1,"Text":"هل اتى على الانسن حين من الدهر لم يكن شي ا مذكورا"},{"SurahNumber":76,"VerseNumber":2,"Text":"انا خلقنا الانسن من نطفة امشاج نبتليه فجعلنه سميعا بصيرا"},{"SurahNumber":76,"VerseNumber":3,"Text":"انا هدينه السبيل اما شاكر ا واما كفورا"},{"SurahNumber":76,"VerseNumber":4,"Text":"انا اعتدنا للكفرين سلسلا واغلل ا وسعيرا"},{"SurahNumber":76,"VerseNumber":5,"Text":"ان الابرار يشربون من كاس كان مزاجها كافورا"},{"SurahNumber":76,"VerseNumber":6,"Text":"عين ا يشرب بها عباد الله يفجرونها تفجير ا"},{"SurahNumber":76,"VerseNumber":7,"Text":"يوفون بالنذر ويخافون يوم ا كان شره مستطير ا"},{"SurahNumber":76,"VerseNumber":8,"Text":"ويطعمون الطعام على حبه مسكين ا ويتيم ا واسيرا"},{"SurahNumber":76,"VerseNumber":9,"Text":"انما نطعمكم لوجه الله لا نريد منكم جزاء ولا شكورا"},{"SurahNumber":76,"VerseNumber":10,"Text":"انا نخاف من ربنا يوما عبوس ا قمطرير ا"},{"SurahNumber":76,"VerseNumber":11,"Text":"فوقىهم الله شر ذلك اليوم ولقىهم نضرة وسرور ا"},{"SurahNumber":76,"VerseNumber":12,"Text":"وجزىهم بما صبروا جنة وحرير ا"},{"SurahNumber":76,"VerseNumber":13,"Text":"متكين فيها على الارايك لا يرون فيها شمس ا ولا زمهرير ا"},{"SurahNumber":76,"VerseNumber":14,"Text":"ودانية عليهم ظللها وذللت قطوفها تذليل ا"},{"SurahNumber":76,"VerseNumber":15,"Text":"ويطاف عليهم بانية من فضة واكواب كانت قواريرا"},{"SurahNumber":76,"VerseNumber":16,"Text":"قواريرا من فضة قدروها تقدير ا"},{"SurahNumber":76,"VerseNumber":17,"Text":"ويسقون فيها كاس ا كان مزاجها زنجبيلا"},{"SurahNumber":76,"VerseNumber":18,"Text":"عين ا فيها تسمى سلسبيل ا"},{"SurahNumber":76,"VerseNumber":19,"Text":"۞ويطوف عليهم ولدن مخلدون اذا رايتهم حسبتهم لولو ا منثور ا"},{"SurahNumber":76,"VerseNumber":20,"Text":"واذا رايت ثم رايت نعيم ا وملك ا كبيرا"},{"SurahNumber":76,"VerseNumber":21,"Text":"عليهم ثياب سندس خضر واستبرق وحلوا اساور من فضة وسقىهم ربهم شراب ا طهورا"},{"SurahNumber":76,"VerseNumber":22,"Text":"ان هذا كان لكم جزاء وكان سعيكم مشكورا"},{"SurahNumber":76,"VerseNumber":23,"Text":"انا نحن نزلنا عليك القرءان تنزيل ا"},{"SurahNumber":76,"VerseNumber":24,"Text":"فاصبر لحكم ربك ولا تطع منهم ءاثما او كفور ا"},{"SurahNumber":76,"VerseNumber":25,"Text":"واذكر اسم ربك بكرة واصيل ا"},{"SurahNumber":76,"VerseNumber":26,"Text":"ومن اليل فاسجد له وسبحه ليل ا طويلا"},{"SurahNumber":76,"VerseNumber":27,"Text":"ان هولاء يحبون العاجلة ويذرون وراءهم يوم ا ثقيل ا"},{"SurahNumber":76,"VerseNumber":28,"Text":"نحن خلقنهم وشددنا اسرهم واذا شينا بدلنا امثلهم تبديلا"},{"SurahNumber":76,"VerseNumber":29,"Text":"ان هذه تذكرة فمن شاء اتخذ الى ربه سبيل ا"},{"SurahNumber":76,"VerseNumber":30,"Text":"وما تشاءون الا ان يشاء الله ان الله كان عليما حكيم ا"},{"SurahNumber":76,"VerseNumber":31,"Text":"يدخل من يشاء في رحمته والظلمين اعد لهم عذابا اليما"},{"SurahNumber":77,"VerseNumber":1,"Text":"والمرسلت عرف ا"},{"SurahNumber":77,"VerseNumber":2,"Text":"فالعصفت عصف ا"},{"SurahNumber":77,"VerseNumber":3,"Text":"والنشرت نشر ا"},{"SurahNumber":77,"VerseNumber":4,"Text":"فالفرقت فرق ا"},{"SurahNumber":77,"VerseNumber":5,"Text":"فالملقيت ذكرا"},{"SurahNumber":77,"VerseNumber":6,"Text":"عذرا او نذرا"},{"SurahNumber":77,"VerseNumber":7,"Text":"انما توعدون لوقع"},{"SurahNumber":77,"VerseNumber":8,"Text":"فاذا النجوم طمست"},{"SurahNumber":77,"VerseNumber":9,"Text":"واذا السماء فرجت"},{"SurahNumber":77,"VerseNumber":10,"Text":"واذا الجبال نسفت"},{"SurahNumber":77,"VerseNumber":11,"Text":"واذا الرسل اقتت"},{"SurahNumber":77,"VerseNumber":12,"Text":"لاي يوم اجلت"},{"SurahNumber":77,"VerseNumber":13,"Text":"ليوم الفصل"},{"SurahNumber":77,"VerseNumber":14,"Text":"وما ادرىك ما يوم الفصل"},{"SurahNumber":77,"VerseNumber":15,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":16,"Text":"الم نهلك الاولين"},{"SurahNumber":77,"VerseNumber":17,"Text":"ثم نتبعهم الاخرين"},{"SurahNumber":77,"VerseNumber":18,"Text":"كذلك نفعل بالمجرمين"},{"SurahNumber":77,"VerseNumber":19,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":20,"Text":"الم نخلقكم من ماء مهين"},{"SurahNumber":77,"VerseNumber":21,"Text":"فجعلنه في قرار مكين"},{"SurahNumber":77,"VerseNumber":22,"Text":"الى قدر معلوم"},{"SurahNumber":77,"VerseNumber":23,"Text":"فقدرنا فنعم القدرون"},{"SurahNumber":77,"VerseNumber":24,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":25,"Text":"الم نجعل الارض كفاتا"},{"SurahNumber":77,"VerseNumber":26,"Text":"احياء واموت ا"},{"SurahNumber":77,"VerseNumber":27,"Text":"وجعلنا فيها روسي شمخت واسقينكم ماء فرات ا"},{"SurahNumber":77,"VerseNumber":28,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":29,"Text":"انطلقوا الى ما كنتم به تكذبون"},{"SurahNumber":77,"VerseNumber":30,"Text":"انطلقوا الى ظل ذي ثلث شعب"},{"SurahNumber":77,"VerseNumber":31,"Text":"لا ظليل ولا يغني من اللهب"},{"SurahNumber":77,"VerseNumber":32,"Text":"انها ترمي بشرر كالقصر"},{"SurahNumber":77,"VerseNumber":33,"Text":"كانه جملت صفر"},{"SurahNumber":77,"VerseNumber":34,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":35,"Text":"هذا يوم لا ينطقون"},{"SurahNumber":77,"VerseNumber":36,"Text":"ولا يوذن لهم فيعتذرون"},{"SurahNumber":77,"VerseNumber":37,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":38,"Text":"هذا يوم الفصل جمعنكم والاولين"},{"SurahNumber":77,"VerseNumber":39,"Text":"فان كان لكم كيد فكيدون"},{"SurahNumber":77,"VerseNumber":40,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":41,"Text":"ان المتقين في ظلل وعيون"},{"SurahNumber":77,"VerseNumber":42,"Text":"وفوكه مما يشتهون"},{"SurahNumber":77,"VerseNumber":43,"Text":"كلوا واشربوا هنيا بما كنتم تعملون"},{"SurahNumber":77,"VerseNumber":44,"Text":"انا كذلك نجزي المحسنين"},{"SurahNumber":77,"VerseNumber":45,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":46,"Text":"كلوا وتمتعوا قليلا انكم مجرمون"},{"SurahNumber":77,"VerseNumber":47,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":48,"Text":"واذا قيل لهم اركعوا لا يركعون"},{"SurahNumber":77,"VerseNumber":49,"Text":"ويل يوميذ للمكذبين"},{"SurahNumber":77,"VerseNumber":50,"Text":"فباي حديث بعده يومنون"}]}