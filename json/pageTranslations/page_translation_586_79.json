{"Id":79,"LanguageCodes":["cs","ces"],"Name":"ces-hadiabdollahian-la","Author":"Hadi Abdollahian","Language":"Czech","Description":"Czech - Transliteration","Direction":"ltr","Source":"Contact:+989354664487","Translations":[{"SurahNumber":81,"VerseNumber":1,"Text":"When slunit se balit"},{"SurahNumber":81,"VerseNumber":2,"Text":"Vystupovat zriceni do kady jinam"},{"SurahNumber":81,"VerseNumber":3,"Text":"Hora otirat out"},{"SurahNumber":81,"VerseNumber":4,"Text":"Znovuzplozeni zastavit"},{"SurahNumber":81,"VerseNumber":5,"Text":"Zvire predvolat"},{"SurahNumber":81,"VerseNumber":6,"Text":"Ocean urcit v plamenech"},{"SurahNumber":81,"VerseNumber":7,"Text":"Duse uzdravit jejich lod"},{"SurahNumber":81,"VerseNumber":8,"Text":"Holka pohrbit ijici poadovat"},{"SurahNumber":81,"VerseNumber":9,"Text":"Pro ktery zlocin ena znemonit"},{"SurahNumber":81,"VerseNumber":10,"Text":"Zapis utocit modni"},{"SurahNumber":81,"VerseNumber":11,"Text":"Nebe odstranujici"},{"SurahNumber":81,"VerseNumber":12,"Text":"Peklo zapalit"},{"SurahNumber":81,"VerseNumber":13,"Text":"Raj podat"},{"SurahNumber":81,"VerseNumber":14,"Text":"Naprosty duse modni vsechno to privest"},{"SurahNumber":81,"VerseNumber":15,"Text":"Ja solemnly prisahat mlecna draha"},{"SurahNumber":81,"VerseNumber":16,"Text":"Presne tekouci jejich draha"},{"SurahNumber":81,"VerseNumber":17,"Text":"Na vecer to dostat"},{"SurahNumber":81,"VerseNumber":18,"Text":"A morn to dychat"},{"SurahNumber":81,"VerseNumber":19,"Text":"Tento jsem projev honorable hlasatel"},{"SurahNumber":81,"VerseNumber":20,"Text":"Zmocnit Vlastnik Dosadit celkem snest"},{"SurahNumber":81,"VerseNumber":21,"Text":"On poslouchat sverit"},{"SurahNumber":81,"VerseNumber":22,"Text":"Svuj stoupenec (Rashad) ne jsem senzacni"},{"SurahNumber":81,"VerseNumber":23,"Text":"On prijmout jemu vyska obzor"},{"SurahNumber":81,"VerseNumber":24,"Text":"On byl vezeni zpet kady zprava"},{"SurahNumber":81,"VerseNumber":25,"Text":"To ne jsem povidat vyradit souit"},{"SurahNumber":81,"VerseNumber":26,"Text":"okamite pak tam ty odchod"},{"SurahNumber":81,"VerseNumber":27,"Text":"Tento jsem poslani uplne lide"},{"SurahNumber":81,"VerseNumber":28,"Text":"Ty adat odchod rovny"},{"SurahNumber":81,"VerseNumber":29,"Text":"Kady ty ucinil to jsem proto prosit BUH Magnat vesmir"}]}