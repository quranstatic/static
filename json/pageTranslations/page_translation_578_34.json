{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":75,"VerseNumber":20,"Text":"كلا بل تحبون العاجلة"},{"SurahNumber":75,"VerseNumber":21,"Text":"وتذرون الاخرة"},{"SurahNumber":75,"VerseNumber":22,"Text":"وجوه يوميذ ناضرة"},{"SurahNumber":75,"VerseNumber":23,"Text":"الى ربها ناظرة"},{"SurahNumber":75,"VerseNumber":24,"Text":"ووجوه يوميذ باسرة"},{"SurahNumber":75,"VerseNumber":25,"Text":"تظن ان يفعل بها فاقرة"},{"SurahNumber":75,"VerseNumber":26,"Text":"كلا اذا بلغت التراقي"},{"SurahNumber":75,"VerseNumber":27,"Text":"وقيل من راق"},{"SurahNumber":75,"VerseNumber":28,"Text":"وظن انه الفراق"},{"SurahNumber":75,"VerseNumber":29,"Text":"والتفت الساق بالساق"},{"SurahNumber":75,"VerseNumber":30,"Text":"الى ربك يوميذ المساق"},{"SurahNumber":75,"VerseNumber":31,"Text":"فلا صدق ولا صلى"},{"SurahNumber":75,"VerseNumber":32,"Text":"ولكن كذب وتولى"},{"SurahNumber":75,"VerseNumber":33,"Text":"ثم ذهب الى اهله يتمطى"},{"SurahNumber":75,"VerseNumber":34,"Text":"اولى لك فاولى"},{"SurahNumber":75,"VerseNumber":35,"Text":"ثم اولى لك فاولى"},{"SurahNumber":75,"VerseNumber":36,"Text":"ايحسب الانسن ان يترك سدى"},{"SurahNumber":75,"VerseNumber":37,"Text":"الم يك نطفة من مني يمنى"},{"SurahNumber":75,"VerseNumber":38,"Text":"ثم كان علقة فخلق فسوى"},{"SurahNumber":75,"VerseNumber":39,"Text":"فجعل منه الزوجين الذكر والانثى"},{"SurahNumber":75,"VerseNumber":40,"Text":"اليس ذلك بقدر على ان يحي الموتى"},{"SurahNumber":76,"VerseNumber":1,"Text":"هل اتى على الانسن حين من الدهر لم يكن شي ا مذكورا"},{"SurahNumber":76,"VerseNumber":2,"Text":"انا خلقنا الانسن من نطفة امشاج نبتليه فجعلنه سميعا بصيرا"},{"SurahNumber":76,"VerseNumber":3,"Text":"انا هدينه السبيل اما شاكر ا واما كفورا"},{"SurahNumber":76,"VerseNumber":4,"Text":"انا اعتدنا للكفرين سلسلا واغلل ا وسعيرا"},{"SurahNumber":76,"VerseNumber":5,"Text":"ان الابرار يشربون من كاس كان مزاجها كافورا"}]}