{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":81,"VerseNumber":1,"Text":"Si kammu hofaama"},{"SurahNumber":81,"VerseNumber":2,"Text":"Si koode caami"},{"SurahNumber":81,"VerseNumber":3,"Text":"Si pelle ɗe nawaama"},{"SurahNumber":81,"VerseNumber":4,"Text":"Si geloobi cowiiɗi dudnaama"},{"SurahNumber":81,"VerseNumber":5,"Text":"Si kulle ladde ndenndinaama"},{"SurahNumber":81,"VerseNumber":6,"Text":"Si maaje ɗe kuɓɓaama"},{"SurahNumber":81,"VerseNumber":7,"Text":"Si pittaali cinndindiraama"},{"SurahNumber":81,"VerseNumber":8,"Text":"Si ubbangel na wuuri naamnaama"},{"SurahNumber":81,"VerseNumber":9,"Text":"Hol bakkaat ngel wardaa"},{"SurahNumber":81,"VerseNumber":10,"Text":"Si ɗeri caaktaama"},{"SurahNumber":81,"VerseNumber":11,"Text":"Si kammu o huuftaama"},{"SurahNumber":81,"VerseNumber":12,"Text":"Si yiite huɓɓaama"},{"SurahNumber":81,"VerseNumber":13,"Text":"Si aljanna ɓallinaama"},{"SurahNumber":81,"VerseNumber":14,"Text":"Fittaandu annda ko ndu addi"},{"SurahNumber":81,"VerseNumber":15,"Text":"Mbeɗa woondira koode birniiɗe"},{"SurahNumber":81,"VerseNumber":16,"Text":"Dogooje birnatooɗe"},{"SurahNumber":81,"VerseNumber":17,"Text":"Mi woondarii jemma si mbo arii e niɓɓidde"},{"SurahNumber":81,"VerseNumber":18,"Text":"E subaka si annoore mum feeñi"},{"SurahNumber":81,"VerseNumber":19,"Text":"wonnde pellet, [Alqur'aana] ko konngol Nulaaɗo tedduɗo"},{"SurahNumber":81,"VerseNumber":20,"Text":"jom semmbe ka jom arsi dokkaaɗo daraja. @Corrected"},{"SurahNumber":81,"VerseNumber":21,"Text":"ɗoftaaɗo toon koolaaɗo"},{"SurahNumber":81,"VerseNumber":22,"Text":"Gondiijo mon o wonah haangaaka"},{"SurahNumber":81,"VerseNumber":23,"Text":"Ko goongo O yi*ii mbo to jookli laaɓtuɗi"},{"SurahNumber":81,"VerseNumber":24,"Text":"O wonah ɗawoowo yottinde mon mbirniindi"},{"SurahNumber":81,"VerseNumber":25,"Text":"Quraana wonah konngol seytaan diddaaɗo yurmeende on"},{"SurahNumber":81,"VerseNumber":26,"Text":"Hoɗo njahaton"},{"SurahNumber":81,"VerseNumber":27,"Text":"Ndee [Alqur’aanaare] wonaali, si wanaa waaju tageefo ngon"},{"SurahNumber":81,"VerseNumber":28,"Text":"Kala faalaaɗo e mon fooccaade"},{"SurahNumber":81,"VerseNumber":29,"Text":"On faaletaake fooccaade si wonah Alla haaje ɗuum jom binnde On"}]}