{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":103,"VerseNumber":1,"Text":"S wakud"},{"SurahNumber":103,"VerseNumber":2,"Text":"Ih, amdan, war ccekk, ar di lexsaoa"},{"SurahNumber":103,"VerseNumber":3,"Text":"Kkes ed wid iumnen, ixeddmen ayen ilhan, ippemyenhun ar tidep, ippemyenhun ar uubeo"},{"SurahNumber":104,"VerseNumber":1,"Text":"A ssxev n yal aqeooav alemmam"},{"SurahNumber":104,"VerseNumber":2,"Text":"I ijemmlen ayla, ieeppeb it"},{"SurahNumber":104,"VerseNumber":3,"Text":"Inwa, s wayla, ad iwekkeo"},{"SurahNumber":104,"VerseNumber":4,"Text":"Xaii! Ad ippuveggeo ar Tuevimt"},{"SurahNumber":104,"VerseNumber":5,"Text":"Anwa ara k yinin d acu p Tuevimt"},{"SurahNumber":104,"VerseNumber":6,"Text":"D times n Oebbi i smendagen"},{"SurahNumber":104,"VerseNumber":7,"Text":"i ippalin $ef ulawen"},{"SurahNumber":104,"VerseNumber":8,"Text":"Neppat, ih, t$umm iten"},{"SurahNumber":104,"VerseNumber":9,"Text":"s tgwejda $wezzifen"},{"SurahNumber":105,"VerseNumber":1,"Text":"Day ur twalav ara amek Ixdem, Mass ik, i At Lfil"},{"SurahNumber":105,"VerseNumber":2,"Text":"Day ur Issexoeb ara tixidas nnsen"},{"SurahNumber":105,"VerseNumber":3,"Text":"Iuzen asen d ifoax ikkaten. i"},{"SurahNumber":105,"VerseNumber":4,"Text":"Iieggioen d fellasen id$a$en iweccmen"},{"SurahNumber":105,"VerseNumber":5,"Text":"Irra ten am alim ipwafeen"}]}