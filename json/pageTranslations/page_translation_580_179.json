{"Id":179,"LanguageCodes":["ff","ful"],"Name":"ful-rowadtranslatio","Author":"Rowad Translation Center And Islamhouse","Language":"Fulah","Description":"Fulah","Direction":"ltr","Source":"https://quranenc.com/en/browse/fulani_rwwad","Translations":[{"SurahNumber":76,"VerseNumber":26,"Text":"E jemma he sujjan Mbo cubahinanɗaa Mbo jemma juutɗo"},{"SurahNumber":76,"VerseNumber":27,"Text":"Pellet ɓe na njiɗi nguurndam aduna yaawɗam gasde ɓe acca caggal mum en ñalawma nisɗo"},{"SurahNumber":76,"VerseNumber":28,"Text":"Ko Minen tagi on Min cemmbini terɗo si Min haajaama Min gostoroo on nandadiiɓe mon gostogol"},{"SurahNumber":76,"VerseNumber":29,"Text":"Ko nde woni waaju goongo kala faalaaɗo daɗde nanngira faade e joomiiko ruttorde"},{"SurahNumber":76,"VerseNumber":30,"Text":"On paaletaake si wonah Alla haaje pellet Alla ko Ganndo ko ñeeño"},{"SurahNumber":76,"VerseNumber":31,"Text":"Hombo naatna mbo O welaa e yurmeene Makko O heblanii tooñɓe ɓe lepte muusɗe"},{"SurahNumber":77,"VerseNumber":1,"Text":"O woondirii keneeli dewandirɗi hono siiru puccu"},{"SurahNumber":77,"VerseNumber":2,"Text":"E bippooji bippugol"},{"SurahNumber":77,"VerseNumber":3,"Text":"E carooji cargolo"},{"SurahNumber":77,"VerseNumber":4,"Text":"E ceerndooji ceerndugol"},{"SurahNumber":77,"VerseNumber":5,"Text":"E tellinooji wahyu"},{"SurahNumber":77,"VerseNumber":6,"Text":"Gantingol wollo jeertingol"},{"SurahNumber":77,"VerseNumber":7,"Text":"ko podante ɗon ko, ko goongo"},{"SurahNumber":77,"VerseNumber":8,"Text":"Si koode ɗe momtaama"},{"SurahNumber":77,"VerseNumber":9,"Text":"Si kammu o huuftaama"},{"SurahNumber":77,"VerseNumber":10,"Text":"Si pelle ɗe nawaama"},{"SurahNumber":77,"VerseNumber":11,"Text":"Saanga Nelaaɓe kappana waktu"},{"SurahNumber":77,"VerseNumber":12,"Text":"E ñalawma dirtinaaɗo"},{"SurahNumber":77,"VerseNumber":13,"Text":"Wonande ñalngu ceerndugol"},{"SurahNumber":77,"VerseNumber":14,"Text":"Holko anndin ma ñalngu ceerndugol"},{"SurahNumber":77,"VerseNumber":15,"Text":"haray nden ñalnde, bone woodanii fennuɓe ɓen"},{"SurahNumber":77,"VerseNumber":16,"Text":"Mbela Min kalkaani adinooɓe ɓe"},{"SurahNumber":77,"VerseNumber":17,"Text":"Refti Min ndewni heen sakkatiiɓe ɓe"},{"SurahNumber":77,"VerseNumber":18,"Text":"Ko hono nih Min mbaɗata bonɓe ɓe"},{"SurahNumber":77,"VerseNumber":19,"Text":"haray nden ñalnde, bone woodanii fennuɓe ɓen"}]}