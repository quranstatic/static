{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":54,"VerseNumber":50,"Text":"وما امرنا الا وحدة كلمح بالبصر"},{"SurahNumber":54,"VerseNumber":51,"Text":"ولقد اهلكنا اشياعكم فهل من مدكر"},{"SurahNumber":54,"VerseNumber":52,"Text":"وكل شيء فعلوه في الزبر"},{"SurahNumber":54,"VerseNumber":53,"Text":"وكل صغير وكبير مستطر"},{"SurahNumber":54,"VerseNumber":54,"Text":"ان المتقين في جنت ونهر"},{"SurahNumber":54,"VerseNumber":55,"Text":"في مقعد صدق عند مليك مقتدر"},{"SurahNumber":55,"VerseNumber":1,"Text":"الرحمن"},{"SurahNumber":55,"VerseNumber":2,"Text":"علم القرءان"},{"SurahNumber":55,"VerseNumber":3,"Text":"خلق الانسن"},{"SurahNumber":55,"VerseNumber":4,"Text":"علمه البيان"},{"SurahNumber":55,"VerseNumber":5,"Text":"الشمس والقمر بحسبان"},{"SurahNumber":55,"VerseNumber":6,"Text":"والنجم والشجر يسجدان"},{"SurahNumber":55,"VerseNumber":7,"Text":"والسماء رفعها ووضع الميزان"},{"SurahNumber":55,"VerseNumber":8,"Text":"الا تطغوا في الميزان"},{"SurahNumber":55,"VerseNumber":9,"Text":"واقيموا الوزن بالقسط ولا تخسروا الميزان"},{"SurahNumber":55,"VerseNumber":10,"Text":"والارض وضعها للانام"},{"SurahNumber":55,"VerseNumber":11,"Text":"فيها فكهة والنخل ذات الاكمام"},{"SurahNumber":55,"VerseNumber":12,"Text":"والحب ذو العصف والريحان"},{"SurahNumber":55,"VerseNumber":13,"Text":"فباي ءالاء ربكما تكذبان"},{"SurahNumber":55,"VerseNumber":14,"Text":"خلق الانسن من صلصل كالفخار"},{"SurahNumber":55,"VerseNumber":15,"Text":"وخلق الجان من مارج من نار"},{"SurahNumber":55,"VerseNumber":16,"Text":"فباي ءالاء ربكما تكذبان"}]}