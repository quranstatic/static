{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":68,"VerseNumber":16,"Text":"سنسمه على الخرطوم"},{"SurahNumber":68,"VerseNumber":17,"Text":"انا بلونهم كما بلونا اصحب الجنة اذ اقسموا ليصرمنها مصبحين"},{"SurahNumber":68,"VerseNumber":18,"Text":"ولا يستثنون"},{"SurahNumber":68,"VerseNumber":19,"Text":"فطاف عليها طايف من ربك وهم نايمون"},{"SurahNumber":68,"VerseNumber":20,"Text":"فاصبحت كالصريم"},{"SurahNumber":68,"VerseNumber":21,"Text":"فتنادوا مصبحين"},{"SurahNumber":68,"VerseNumber":22,"Text":"ان اغدوا على حرثكم ان كنتم صرمين"},{"SurahNumber":68,"VerseNumber":23,"Text":"فانطلقوا وهم يتخفتون"},{"SurahNumber":68,"VerseNumber":24,"Text":"ان لا يدخلنها اليوم عليكم مسكين"},{"SurahNumber":68,"VerseNumber":25,"Text":"وغدوا على حرد قدرين"},{"SurahNumber":68,"VerseNumber":26,"Text":"فلما راوها قالوا انا لضالون"},{"SurahNumber":68,"VerseNumber":27,"Text":"بل نحن محرومون"},{"SurahNumber":68,"VerseNumber":28,"Text":"قال اوسطهم الم اقل لكم لولا تسبحون"},{"SurahNumber":68,"VerseNumber":29,"Text":"قالوا سبحن ربنا انا كنا ظلمين"},{"SurahNumber":68,"VerseNumber":30,"Text":"فاقبل بعضهم على بعض يتلومون"},{"SurahNumber":68,"VerseNumber":31,"Text":"قالوا يويلنا انا كنا طغين"},{"SurahNumber":68,"VerseNumber":32,"Text":"عسى ربنا ان يبدلنا خير ا منها انا الى ربنا رغبون"},{"SurahNumber":68,"VerseNumber":33,"Text":"كذلك العذاب ولعذاب الاخرة اكبر لو كانوا يعلمون"},{"SurahNumber":68,"VerseNumber":34,"Text":"ان للمتقين عند ربهم جنت النعيم"},{"SurahNumber":68,"VerseNumber":35,"Text":"افنجعل المسلمين كالمجرمين"},{"SurahNumber":68,"VerseNumber":36,"Text":"ما لكم كيف تحكمون"},{"SurahNumber":68,"VerseNumber":37,"Text":"ام لكم كتب فيه تدرسون"},{"SurahNumber":68,"VerseNumber":38,"Text":"ان لكم فيه لما تخيرون"},{"SurahNumber":68,"VerseNumber":39,"Text":"ام لكم ايمن علينا بلغة الى يوم القيمة ان لكم لما تحكمون"},{"SurahNumber":68,"VerseNumber":40,"Text":"سلهم ايهم بذلك زعيم"},{"SurahNumber":68,"VerseNumber":41,"Text":"ام لهم شركاء فلياتوا بشركايهم ان كانوا صدقين"},{"SurahNumber":68,"VerseNumber":42,"Text":"يوم يكشف عن ساق ويدعون الى السجود فلا يستطيعون"}]}