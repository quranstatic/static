{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":103,"VerseNumber":1,"Text":"والعصر"},{"SurahNumber":103,"VerseNumber":2,"Text":"ان الانسن لفي خسر"},{"SurahNumber":103,"VerseNumber":3,"Text":"الا الذين ءامنوا وعملوا الصلحت وتواصوا بالحق وتواصوا بالصبر"},{"SurahNumber":104,"VerseNumber":1,"Text":"ويل لكل همزة لمزة"},{"SurahNumber":104,"VerseNumber":2,"Text":"الذي جمع مال ا وعدده"},{"SurahNumber":104,"VerseNumber":3,"Text":"يحسب ان ماله اخلده"},{"SurahNumber":104,"VerseNumber":4,"Text":"كلا لينبذن في الحطمة"},{"SurahNumber":104,"VerseNumber":5,"Text":"وما ادرىك ما الحطمة"},{"SurahNumber":104,"VerseNumber":6,"Text":"نار الله الموقدة"},{"SurahNumber":104,"VerseNumber":7,"Text":"التي تطلع على الافدة"},{"SurahNumber":104,"VerseNumber":8,"Text":"انها عليهم موصدة"},{"SurahNumber":104,"VerseNumber":9,"Text":"في عمد ممددة"},{"SurahNumber":105,"VerseNumber":1,"Text":"الم تر كيف فعل ربك باصحب الفيل"},{"SurahNumber":105,"VerseNumber":2,"Text":"الم يجعل كيدهم في تضليل"},{"SurahNumber":105,"VerseNumber":3,"Text":"وارسل عليهم طيرا ابابيل"},{"SurahNumber":105,"VerseNumber":4,"Text":"ترميهم بحجارة من سجيل"},{"SurahNumber":105,"VerseNumber":5,"Text":"فجعلهم كعصف ماكول"}]}