{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":37,"VerseNumber":103,"Text":"فلما اسلما وتله للجبين"},{"SurahNumber":37,"VerseNumber":104,"Text":"وندينه ان يابرهيم"},{"SurahNumber":37,"VerseNumber":105,"Text":"قد صدقت الرءيا انا كذلك نجزي المحسنين"},{"SurahNumber":37,"VerseNumber":106,"Text":"ان هذا لهو البلوا المبين"},{"SurahNumber":37,"VerseNumber":107,"Text":"وفدينه بذبح عظيم"},{"SurahNumber":37,"VerseNumber":108,"Text":"وتركنا عليه في الاخرين"},{"SurahNumber":37,"VerseNumber":109,"Text":"سلم على ابرهيم"},{"SurahNumber":37,"VerseNumber":110,"Text":"كذلك نجزي المحسنين"},{"SurahNumber":37,"VerseNumber":111,"Text":"انه من عبادنا المومنين"},{"SurahNumber":37,"VerseNumber":112,"Text":"وبشرنه باسحق نبي ا من الصلحين"},{"SurahNumber":37,"VerseNumber":113,"Text":"وبركنا عليه وعلى اسحق ومن ذريتهما محسن وظالم لنفسه مبين"},{"SurahNumber":37,"VerseNumber":114,"Text":"ولقد مننا على موسى وهرون"},{"SurahNumber":37,"VerseNumber":115,"Text":"ونجينهما وقومهما من الكرب العظيم"},{"SurahNumber":37,"VerseNumber":116,"Text":"ونصرنهم فكانوا هم الغلبين"},{"SurahNumber":37,"VerseNumber":117,"Text":"وءاتينهما الكتب المستبين"},{"SurahNumber":37,"VerseNumber":118,"Text":"وهدينهما الصرط المستقيم"},{"SurahNumber":37,"VerseNumber":119,"Text":"وتركنا عليهما في الاخرين"},{"SurahNumber":37,"VerseNumber":120,"Text":"سلم على موسى وهرون"},{"SurahNumber":37,"VerseNumber":121,"Text":"انا كذلك نجزي المحسنين"},{"SurahNumber":37,"VerseNumber":122,"Text":"انهما من عبادنا المومنين"},{"SurahNumber":37,"VerseNumber":123,"Text":"وان الياس لمن المرسلين"},{"SurahNumber":37,"VerseNumber":124,"Text":"اذ قال لقومه الا تتقون"},{"SurahNumber":37,"VerseNumber":125,"Text":"اتدعون بعل ا وتذرون احسن الخلقين"},{"SurahNumber":37,"VerseNumber":126,"Text":"الله ربكم ورب ءابايكم الاولين"}]}