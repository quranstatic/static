{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":26,"VerseNumber":184,"Text":"واتقوا الذي خلقكم والجبلة الاولين"},{"SurahNumber":26,"VerseNumber":185,"Text":"قالوا انما انت من المسحرين"},{"SurahNumber":26,"VerseNumber":186,"Text":"وما انت الا بشر مثلنا وان نظنك لمن الكذبين"},{"SurahNumber":26,"VerseNumber":187,"Text":"فاسقط علينا كسف ا من السماء ان كنت من الصدقين"},{"SurahNumber":26,"VerseNumber":188,"Text":"قال ربي اعلم بما تعملون"},{"SurahNumber":26,"VerseNumber":189,"Text":"فكذبوه فاخذهم عذاب يوم الظلة انه كان عذاب يوم عظيم"},{"SurahNumber":26,"VerseNumber":190,"Text":"ان في ذلك لاية وما كان اكثرهم مومنين"},{"SurahNumber":26,"VerseNumber":191,"Text":"وان ربك لهو العزيز الرحيم"},{"SurahNumber":26,"VerseNumber":192,"Text":"وانه لتنزيل رب العلمين"},{"SurahNumber":26,"VerseNumber":193,"Text":"نزل به الروح الامين"},{"SurahNumber":26,"VerseNumber":194,"Text":"على قلبك لتكون من المنذرين"},{"SurahNumber":26,"VerseNumber":195,"Text":"بلسان عربي مبين"},{"SurahNumber":26,"VerseNumber":196,"Text":"وانه لفي زبر الاولين"},{"SurahNumber":26,"VerseNumber":197,"Text":"اولم يكن لهم ءاية ان يعلمه علموا بني اسرءيل"},{"SurahNumber":26,"VerseNumber":198,"Text":"ولو نزلنه على بعض الاعجمين"},{"SurahNumber":26,"VerseNumber":199,"Text":"فقراه عليهم ما كانوا به مومنين"},{"SurahNumber":26,"VerseNumber":200,"Text":"كذلك سلكنه في قلوب المجرمين"},{"SurahNumber":26,"VerseNumber":201,"Text":"لا يومنون به حتى يروا العذاب الاليم"},{"SurahNumber":26,"VerseNumber":202,"Text":"فياتيهم بغتة وهم لا يشعرون"},{"SurahNumber":26,"VerseNumber":203,"Text":"فيقولوا هل نحن منظرون"},{"SurahNumber":26,"VerseNumber":204,"Text":"افبعذابنا يستعجلون"},{"SurahNumber":26,"VerseNumber":205,"Text":"افرءيت ان متعنهم سنين"},{"SurahNumber":26,"VerseNumber":206,"Text":"ثم جاءهم ما كانوا يوعدون"}]}