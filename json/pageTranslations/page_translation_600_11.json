{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":100,"VerseNumber":10,"Text":"Wa hussila maa fis sudoor"},{"SurahNumber":100,"VerseNumber":11,"Text":"Inna rabbahum bihim yauma 'izil la khabeer"},{"SurahNumber":101,"VerseNumber":1,"Text":"Al qaari'ah"},{"SurahNumber":101,"VerseNumber":2,"Text":"Mal qaariah"},{"SurahNumber":101,"VerseNumber":3,"Text":"Wa maa adraaka mal qaari'ah"},{"SurahNumber":101,"VerseNumber":4,"Text":"Yauma ya koonun naasu kal farashil mabthooth"},{"SurahNumber":101,"VerseNumber":5,"Text":"Wa ta koonul jibalu kal 'ihnil manfoosh"},{"SurahNumber":101,"VerseNumber":6,"Text":"Fa-amma man thaqulat mawa zeenuh"},{"SurahNumber":101,"VerseNumber":7,"Text":"Fahuwa fee 'ishatir raadiyah"},{"SurahNumber":101,"VerseNumber":8,"Text":"Wa amma man khaffat mawa zeenuh"},{"SurahNumber":101,"VerseNumber":9,"Text":"Fa-ummuhu haawiyah"},{"SurahNumber":101,"VerseNumber":10,"Text":"Wa maa adraaka maa hiyah"},{"SurahNumber":101,"VerseNumber":11,"Text":"Naarun hamiyah"},{"SurahNumber":102,"VerseNumber":1,"Text":"Al haaku mut takathur"},{"SurahNumber":102,"VerseNumber":2,"Text":"Hatta zurtumul-maqaabir"},{"SurahNumber":102,"VerseNumber":3,"Text":"Kalla sawfa ta'lamoon"},{"SurahNumber":102,"VerseNumber":4,"Text":"Thumma kalla sawfa ta'lamoon"},{"SurahNumber":102,"VerseNumber":5,"Text":"Kalla law ta'lamoona 'ilmal yaqeen"},{"SurahNumber":102,"VerseNumber":6,"Text":"Latara-wun nal jaheem"},{"SurahNumber":102,"VerseNumber":7,"Text":"Thumma latara wunnaha 'ainal yaqeen"},{"SurahNumber":102,"VerseNumber":8,"Text":"Thumma latus alunna yauma-izin 'anin na'eem"}]}