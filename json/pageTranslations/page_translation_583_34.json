{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":78,"VerseNumber":31,"Text":"ان للمتقين مفازا"},{"SurahNumber":78,"VerseNumber":32,"Text":"حدايق واعنب ا"},{"SurahNumber":78,"VerseNumber":33,"Text":"وكواعب اتراب ا"},{"SurahNumber":78,"VerseNumber":34,"Text":"وكاس ا دهاق ا"},{"SurahNumber":78,"VerseNumber":35,"Text":"لا يسمعون فيها لغو ا ولا كذب ا"},{"SurahNumber":78,"VerseNumber":36,"Text":"جزاء من ربك عطاء حساب ا"},{"SurahNumber":78,"VerseNumber":37,"Text":"رب السموت والارض وما بينهما الرحمن لا يملكون منه خطاب ا"},{"SurahNumber":78,"VerseNumber":38,"Text":"يوم يقوم الروح والمليكة صف ا لا يتكلمون الا من اذن له الرحمن وقال صواب ا"},{"SurahNumber":78,"VerseNumber":39,"Text":"ذلك اليوم الحق فمن شاء اتخذ الى ربه مابا"},{"SurahNumber":78,"VerseNumber":40,"Text":"انا انذرنكم عذاب ا قريب ا يوم ينظر المرء ما قدمت يداه ويقول الكافر يليتني كنت تربا"},{"SurahNumber":79,"VerseNumber":1,"Text":"والنزعت غرق ا"},{"SurahNumber":79,"VerseNumber":2,"Text":"والنشطت نشط ا"},{"SurahNumber":79,"VerseNumber":3,"Text":"والسبحت سبح ا"},{"SurahNumber":79,"VerseNumber":4,"Text":"فالسبقت سبق ا"},{"SurahNumber":79,"VerseNumber":5,"Text":"فالمدبرت امر ا"},{"SurahNumber":79,"VerseNumber":6,"Text":"يوم ترجف الراجفة"},{"SurahNumber":79,"VerseNumber":7,"Text":"تتبعها الرادفة"},{"SurahNumber":79,"VerseNumber":8,"Text":"قلوب يوميذ واجفة"},{"SurahNumber":79,"VerseNumber":9,"Text":"ابصرها خشعة"},{"SurahNumber":79,"VerseNumber":10,"Text":"يقولون اءنا لمردودون في الحافرة"},{"SurahNumber":79,"VerseNumber":11,"Text":"اءذا كنا عظم ا نخرة"},{"SurahNumber":79,"VerseNumber":12,"Text":"قالوا تلك اذ ا كرة خاسرة"},{"SurahNumber":79,"VerseNumber":13,"Text":"فانما هي زجرة وحدة"},{"SurahNumber":79,"VerseNumber":14,"Text":"فاذا هم بالساهرة"},{"SurahNumber":79,"VerseNumber":15,"Text":"هل اتىك حديث موسى"}]}