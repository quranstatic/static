{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":69,"VerseNumber":35,"Text":"فليس له اليوم ههنا حميم"},{"SurahNumber":69,"VerseNumber":36,"Text":"ولا طعام الا من غسلين"},{"SurahNumber":69,"VerseNumber":37,"Text":"لا ياكله الا الخطون"},{"SurahNumber":69,"VerseNumber":38,"Text":"فلا اقسم بما تبصرون"},{"SurahNumber":69,"VerseNumber":39,"Text":"وما لا تبصرون"},{"SurahNumber":69,"VerseNumber":40,"Text":"انه لقول رسول كريم"},{"SurahNumber":69,"VerseNumber":41,"Text":"وما هو بقول شاعر قليل ا ما تومنون"},{"SurahNumber":69,"VerseNumber":42,"Text":"ولا بقول كاهن قليل ا ما تذكرون"},{"SurahNumber":69,"VerseNumber":43,"Text":"تنزيل من رب العلمين"},{"SurahNumber":69,"VerseNumber":44,"Text":"ولو تقول علينا بعض الاقاويل"},{"SurahNumber":69,"VerseNumber":45,"Text":"لاخذنا منه باليمين"},{"SurahNumber":69,"VerseNumber":46,"Text":"ثم لقطعنا منه الوتين"},{"SurahNumber":69,"VerseNumber":47,"Text":"فما منكم من احد عنه حجزين"},{"SurahNumber":69,"VerseNumber":48,"Text":"وانه لتذكرة للمتقين"},{"SurahNumber":69,"VerseNumber":49,"Text":"وانا لنعلم ان منكم مكذبين"},{"SurahNumber":69,"VerseNumber":50,"Text":"وانه لحسرة على الكفرين"},{"SurahNumber":69,"VerseNumber":51,"Text":"وانه لحق اليقين"},{"SurahNumber":69,"VerseNumber":52,"Text":"فسبح باسم ربك العظيم"},{"SurahNumber":70,"VerseNumber":1,"Text":"سال سايل بعذاب واقع"},{"SurahNumber":70,"VerseNumber":2,"Text":"للكفرين ليس له دافع"},{"SurahNumber":70,"VerseNumber":3,"Text":"من الله ذي المعارج"},{"SurahNumber":70,"VerseNumber":4,"Text":"تعرج المليكة والروح اليه في يوم كان مقداره خمسين الف سنة"},{"SurahNumber":70,"VerseNumber":5,"Text":"فاصبر صبر ا جميلا"},{"SurahNumber":70,"VerseNumber":6,"Text":"انهم يرونه بعيد ا"},{"SurahNumber":70,"VerseNumber":7,"Text":"ونرىه قريب ا"},{"SurahNumber":70,"VerseNumber":8,"Text":"يوم تكون السماء كالمهل"},{"SurahNumber":70,"VerseNumber":9,"Text":"وتكون الجبال كالعهن"},{"SurahNumber":70,"VerseNumber":10,"Text":"ولا يسل حميم حميم ا"}]}