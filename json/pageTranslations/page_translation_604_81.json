{"Id":81,"LanguageCodes":["cs","ces"],"Name":"ces-prekladihrbek-la","Author":"Preklad I. Hrbek","Language":"Czech","Description":"Czech - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":112,"VerseNumber":1,"Text":"Rci: \"On Buh je jedinecny"},{"SurahNumber":112,"VerseNumber":2,"Text":"Buh sam o sobe vecny"},{"SurahNumber":112,"VerseNumber":3,"Text":"Neplodil a nebyl zplozen"},{"SurahNumber":112,"VerseNumber":4,"Text":"a neni nikoho, kdo je mu roven"},{"SurahNumber":113,"VerseNumber":1,"Text":"Rci: \"Utikam se v ochranu Pana zare jitrni"},{"SurahNumber":113,"VerseNumber":2,"Text":"pred zlem Jeho stvoreni"},{"SurahNumber":113,"VerseNumber":3,"Text":"pred zlem temnoty, kdyz se siri"},{"SurahNumber":113,"VerseNumber":4,"Text":"pred zlem zen, jez do uzlu prskaji"},{"SurahNumber":113,"VerseNumber":5,"Text":"pred zlem zavistnika, kdyz zavidi"},{"SurahNumber":114,"VerseNumber":1,"Text":"Rci: \"Utikam se v ochranu Pana lidi"},{"SurahNumber":114,"VerseNumber":2,"Text":"vladce lidi"},{"SurahNumber":114,"VerseNumber":3,"Text":"Boha lidi"},{"SurahNumber":114,"VerseNumber":4,"Text":"pred zlem naseptavace pokradmeho"},{"SurahNumber":114,"VerseNumber":5,"Text":"jenz naseptava do hrudi lidi"},{"SurahNumber":114,"VerseNumber":6,"Text":"at jiz je z dzinu ci z lidi"}]}