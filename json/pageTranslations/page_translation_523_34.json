{"Id":34,"LanguageCodes":["ar","ara"],"Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":51,"VerseNumber":52,"Text":"كذلك ما اتى الذين من قبلهم من رسول الا قالوا ساحر او مجنون"},{"SurahNumber":51,"VerseNumber":53,"Text":"اتواصوا به بل هم قوم طاغون"},{"SurahNumber":51,"VerseNumber":54,"Text":"فتول عنهم فما انت بملوم"},{"SurahNumber":51,"VerseNumber":55,"Text":"وذكر فان الذكرى تنفع المومنين"},{"SurahNumber":51,"VerseNumber":56,"Text":"وما خلقت الجن والانس الا ليعبدون"},{"SurahNumber":51,"VerseNumber":57,"Text":"ما اريد منهم من رزق وما اريد ان يطعمون"},{"SurahNumber":51,"VerseNumber":58,"Text":"ان الله هو الرزاق ذو القوة المتين"},{"SurahNumber":51,"VerseNumber":59,"Text":"فان للذين ظلموا ذنوب ا مثل ذنوب اصحبهم فلا يستعجلون"},{"SurahNumber":51,"VerseNumber":60,"Text":"فويل للذين كفروا من يومهم الذي يوعدون"},{"SurahNumber":52,"VerseNumber":1,"Text":"والطور"},{"SurahNumber":52,"VerseNumber":2,"Text":"وكتب مسطور"},{"SurahNumber":52,"VerseNumber":3,"Text":"في رق منشور"},{"SurahNumber":52,"VerseNumber":4,"Text":"والبيت المعمور"},{"SurahNumber":52,"VerseNumber":5,"Text":"والسقف المرفوع"},{"SurahNumber":52,"VerseNumber":6,"Text":"والبحر المسجور"},{"SurahNumber":52,"VerseNumber":7,"Text":"ان عذاب ربك لوقع"},{"SurahNumber":52,"VerseNumber":8,"Text":"ما له من دافع"},{"SurahNumber":52,"VerseNumber":9,"Text":"يوم تمور السماء مور ا"},{"SurahNumber":52,"VerseNumber":10,"Text":"وتسير الجبال سير ا"},{"SurahNumber":52,"VerseNumber":11,"Text":"فويل يوميذ للمكذبين"},{"SurahNumber":52,"VerseNumber":12,"Text":"الذين هم في خوض يلعبون"},{"SurahNumber":52,"VerseNumber":13,"Text":"يوم يدعون الى نار جهنم دعا"},{"SurahNumber":52,"VerseNumber":14,"Text":"هذه النار التي كنتم بها تكذبون"}]}