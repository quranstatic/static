{"Id":11,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la1","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"https://quran411.com","Translations":[{"SurahNumber":79,"VerseNumber":16,"Text":"Iz nadaahu rabbuhu bil waadil-muqad dasi tuwa"},{"SurahNumber":79,"VerseNumber":17,"Text":"Izhab ilaa fir'auna innahu taghaa"},{"SurahNumber":79,"VerseNumber":18,"Text":"Faqul hal laka ilaa-an tazakka"},{"SurahNumber":79,"VerseNumber":19,"Text":"Wa ahdi yaka ila rabbika fatakh sha"},{"SurahNumber":79,"VerseNumber":20,"Text":"Fa araahul-aayatal kubra"},{"SurahNumber":79,"VerseNumber":21,"Text":"Fa kazzaba wa asaa"},{"SurahNumber":79,"VerseNumber":22,"Text":"Thumma adbara yas'aa"},{"SurahNumber":79,"VerseNumber":23,"Text":"Fa hashara fanada"},{"SurahNumber":79,"VerseNumber":24,"Text":"Faqala ana rabbu kumul-a'laa"},{"SurahNumber":79,"VerseNumber":25,"Text":"Fa-akha zahul laahu nakalal aakhirati wal-oola"},{"SurahNumber":79,"VerseNumber":26,"Text":"Inna fee zaalika la'ibratal limaiy-yaksha"},{"SurahNumber":79,"VerseNumber":27,"Text":"A-antum a shaddu khalqan amis samaa-u banaaha"},{"SurahNumber":79,"VerseNumber":28,"Text":"Raf'a sam kaha fasaw waaha"},{"SurahNumber":79,"VerseNumber":29,"Text":"Wa aghtasha lailaha wa akhraja duhaaha"},{"SurahNumber":79,"VerseNumber":30,"Text":"Wal arda b'ada zaalika dahaaha"},{"SurahNumber":79,"VerseNumber":31,"Text":"Akhraja minha maa-aha wa mar 'aaha"},{"SurahNumber":79,"VerseNumber":32,"Text":"Wal jibala arsaaha"},{"SurahNumber":79,"VerseNumber":33,"Text":"Mataa'al lakum wali an 'aamikum"},{"SurahNumber":79,"VerseNumber":34,"Text":"Fa-izaa jaaa'atit taaam matul kubraa"},{"SurahNumber":79,"VerseNumber":35,"Text":"Yauma Yata zakkarul insaanu ma sa'aa"},{"SurahNumber":79,"VerseNumber":36,"Text":"Wa burrizatil-jaheemu limany-yaraa"},{"SurahNumber":79,"VerseNumber":37,"Text":"Fa ammaa man taghaa"},{"SurahNumber":79,"VerseNumber":38,"Text":"Wa aasaral hayaatad dunyaa"},{"SurahNumber":79,"VerseNumber":39,"Text":"Fa innal jaheema hiyal maawaa"},{"SurahNumber":79,"VerseNumber":40,"Text":"Wa ammaa man khaafa maqaama Rabbihee wa nahan nafsa 'anil hawaa"},{"SurahNumber":79,"VerseNumber":41,"Text":"Fa innal jannata hiyal maawaa"},{"SurahNumber":79,"VerseNumber":42,"Text":"Yas'aloonaka 'anis saa'ati ayyaana mursaahaa"},{"SurahNumber":79,"VerseNumber":43,"Text":"Feema anta min zikraahaa"},{"SurahNumber":79,"VerseNumber":44,"Text":"Ilaa Rabbika muntahaa haa"},{"SurahNumber":79,"VerseNumber":45,"Text":"Innamaaa anta munziru maiy yakshaahaa"},{"SurahNumber":79,"VerseNumber":46,"Text":"Ka annahum Yawma yarawnahaa lam yalbasooo illaa 'ashiyyatan aw duhaahaa"}]}