{"Id":248,"LanguageCodes":["ml","mal"],"Name":"mal-muhammadkarakun-la","Author":"Muhammad Karakunnu And Vanidas Elayavoor","Language":"Malayalam","Description":"Malayalam - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":109,"VerseNumber":1,"Text":"parayuka: allayea satyanisedhikale"},{"SurahNumber":109,"VerseNumber":2,"Text":"ninnal aradhikkunnavaye nan aradhikkunnilla"},{"SurahNumber":109,"VerseNumber":3,"Text":"nan aradhikkunnatine aradhikkunnavaralla ninnal"},{"SurahNumber":109,"VerseNumber":4,"Text":"ninnal aradhikkunnavaye aradhikkunnavanalla nan"},{"SurahNumber":109,"VerseNumber":5,"Text":"nan aradhikkunnatine aradhikkunnavaralla ninnalum"},{"SurahNumber":109,"VerseNumber":6,"Text":"ninnalkk ninnalute matam. enikk enre matam"},{"SurahNumber":110,"VerseNumber":1,"Text":"allahuvinre sahayavum vijayavum vannettiyal"},{"SurahNumber":110,"VerseNumber":2,"Text":"janam kuttankuttamayi daivika matattil katannuvarunnat ni kanukayum ceytal"},{"SurahNumber":110,"VerseNumber":3,"Text":"ninre nathane ni stuticc valttuka. avaneat papameacanam tetuka. tirccayayum avan pascattapam svikarikkunnavanan"},{"SurahNumber":111,"VerseNumber":1,"Text":"abulahabinre iru karannalum nasikkatte. avan nasiccirikkunnu"},{"SurahNumber":111,"VerseNumber":2,"Text":"avanre svattea avan sampadiccatea avanneattum upakariccilla"},{"SurahNumber":111,"VerseNumber":3,"Text":"alikkattunna narakattilavan cennettum"},{"SurahNumber":111,"VerseNumber":4,"Text":"virak cumakkunna avanre bharyayum"},{"SurahNumber":111,"VerseNumber":5,"Text":"avalute kaluttil intappana narukeantulla kayarunt"}]}