{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":56,"VerseNumber":17,"Text":"Tezzin asen ilmeeiyen iwekkoen"},{"SurahNumber":56,"VerseNumber":18,"Text":"s tbuqalin, iqbucen, akked lkas n tissit iufan"},{"SurahNumber":56,"VerseNumber":19,"Text":"Yis, ur ppavnen, ur sekkoen"},{"SurahNumber":56,"VerseNumber":20,"Text":"Izwayen, seg wid smenyafen"},{"SurahNumber":56,"VerseNumber":21,"Text":"Aksum n yifoax, seg win cedhan"},{"SurahNumber":56,"VerseNumber":22,"Text":"Tieuoiyin lain"},{"SurahNumber":56,"VerseNumber":23,"Text":"am tyaqutin iffren"},{"SurahNumber":56,"VerseNumber":24,"Text":"D arraz n wayen xeddmen"},{"SurahNumber":56,"VerseNumber":25,"Text":"Ur din sellen awal n leqbaea, ne$ n tuccva"},{"SurahNumber":56,"VerseNumber":26,"Text":"siwa awal n tifrat, tifrat"},{"SurahNumber":56,"VerseNumber":27,"Text":"At uyeffus, at uyeffus, d acu ten"},{"SurahNumber":56,"VerseNumber":28,"Text":"Ar tzegwart, war isennanen"},{"SurahNumber":56,"VerseNumber":29,"Text":"Tulmatin iurwen"},{"SurahNumber":56,"VerseNumber":30,"Text":"Tili iwsaen"},{"SurahNumber":56,"VerseNumber":31,"Text":"Aman ippazalen"},{"SurahNumber":56,"VerseNumber":32,"Text":"Aias n yizwayen"},{"SurahNumber":56,"VerseNumber":33,"Text":"ur zegzawit, ur ppugedlen"},{"SurahNumber":56,"VerseNumber":34,"Text":"Imieoeen aalayen"},{"SurahNumber":56,"VerseNumber":35,"Text":"D Nekwni itent Ixelqen"},{"SurahNumber":56,"VerseNumber":36,"Text":"Nerra tent d tieuoiyin"},{"SurahNumber":56,"VerseNumber":37,"Text":"peemmilent, d tizyiwin"},{"SurahNumber":56,"VerseNumber":38,"Text":"i at uyeffus"},{"SurahNumber":56,"VerseNumber":39,"Text":"Seg imenza, deg aias idsen"},{"SurahNumber":56,"VerseNumber":40,"Text":"Seg ineggura, deg aias idsen"},{"SurahNumber":56,"VerseNumber":41,"Text":"At uzelmav, at uzelmav, d acu ten"},{"SurahNumber":56,"VerseNumber":42,"Text":"Deg wavu, akked waman io$an"},{"SurahNumber":56,"VerseNumber":43,"Text":"Tili n wabbu io$an"},{"SurahNumber":56,"VerseNumber":44,"Text":"Ur degs tasmuvi, lekyasa"},{"SurahNumber":56,"VerseNumber":45,"Text":"Ih, nitni zik, llan owan"},{"SurahNumber":56,"VerseNumber":46,"Text":"Zgan deg ubekkav abarar"},{"SurahNumber":56,"VerseNumber":47,"Text":"Llan qqaoen: \"ma nemmut, nu$al d akal, akked i$san, day a d a$ d rren"},{"SurahNumber":56,"VerseNumber":48,"Text":"Akked imezwura nne$ imenza"},{"SurahNumber":56,"VerseNumber":49,"Text":"Ini: \"ih, imezwura akked ineggura"},{"SurahNumber":56,"VerseNumber":50,"Text":"ad ppujemmlen ar temlilit n wass ipwassnen"}]}