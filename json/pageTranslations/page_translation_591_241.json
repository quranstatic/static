{"Id":241,"LanguageCodes":["luy"],"Name":"luy-mohammadramadha","Author":"Mohammad Ramadhan Sebwa, Ramadhan Hajj Ndeka, Ismail Were, Musa Murasi Salim Kukuya, Yusuf Shiundu Malala, Yahya Wafula Ojwang, Haroon Albert Msah","Language":"Luyia","Description":"Luyia","Direction":"ltr","Source":"https://www.islamic-invitation.com/","Translations":[{"SurahNumber":86,"VerseNumber":1,"Text":"Nditsuba khulikulu nende shokhwitsa eshilo"},{"SurahNumber":86,"VerseNumber":2,"Text":"Ne nishina eshikhumanyia mbu nishina esho shitsanga eshilo"},{"SurahNumber":86,"VerseNumber":3,"Text":"Ing’ining’ini yili nende obulafu obunji"},{"SurahNumber":86,"VerseNumber":4,"Text":"Buli shiamundu ali nende umuhenganga"},{"SurahNumber":86,"VerseNumber":5,"Text":"Ne Omundu ahenge yalongwa okhurulana nende shina"},{"SurahNumber":86,"VerseNumber":6,"Text":"Yalongwa\tokhurulana\tkhu\tmatsi karulanga nikamitsikha"},{"SurahNumber":86,"VerseNumber":7,"Text":"Karulanga hakarikari mulukongofuma nende omukongo"},{"SurahNumber":86,"VerseNumber":8,"Text":"Toto bwene Nyasaye ali nende obunyali bwokhumukalusia"},{"SurahNumber":86,"VerseNumber":9,"Text":"Inyanga ya kalia kefisa kalirebwa habulafu"},{"SurahNumber":86,"VerseNumber":10,"Text":"Abwene aho shaliba nende amani nohomba Omuhabini tawe"},{"SurahNumber":86,"VerseNumber":11,"Text":"Nditsuba\tkhulikilu\tlili\tnende amakalushilo"},{"SurahNumber":86,"VerseNumber":12,"Text":"Nende liloba liarukhanga"},{"SurahNumber":86,"VerseNumber":13,"Text":"Toto\tbwene\tlino\tnilikhuwa lwokhwawukhasia"},{"SurahNumber":86,"VerseNumber":14,"Text":"Nalio shilili emibayo tawe"},{"SurahNumber":86,"VerseNumber":15,"Text":"Toto bwene abo batutanga bututa"},{"SurahNumber":86,"VerseNumber":16,"Text":"Na siesi ndamanya obututi bwabu po"},{"SurahNumber":86,"VerseNumber":17,"Text":"Mana bahelesie ebise abakhayi- kaala kaala"},{"SurahNumber":87,"VerseNumber":1,"Text":"Witsoomie elira lia Nyasaye wuwo Owoluyali"},{"SurahNumber":87,"VerseNumber":2,"Text":"Owaloonga mana naleng’aasia"},{"SurahNumber":87,"VerseNumber":3,"Text":"ulia wabalilila mana nalunjisia"},{"SurahNumber":87,"VerseNumber":4,"Text":"ulia Owamesia olwayilo"},{"SurahNumber":87,"VerseNumber":5,"Text":"Mana nalukhola oluumila olumali"},{"SurahNumber":87,"VerseNumber":6,"Text":"Khulakhusomesia mana sholibilila tawe"},{"SurahNumber":87,"VerseNumber":7,"Text":"halali Nyasaye nachama, toto ye amanyile kali habulafu nende kefisile"},{"SurahNumber":87,"VerseNumber":8,"Text":"Ne khutsia okhuwanguyilisia kabe amangu"},{"SurahNumber":87,"VerseNumber":9,"Text":"Mana itsulisia, nikali mbu okhwitsulisia khukhoonya"},{"SurahNumber":87,"VerseNumber":10,"Text":"Yetsa okhwitsulila ulia uria"},{"SurahNumber":87,"VerseNumber":11,"Text":"Ne yetsa okhwira ehale nayo omukori"},{"SurahNumber":87,"VerseNumber":12,"Text":"Ulia ulinjila mumulilo omukhongo"},{"SurahNumber":87,"VerseNumber":13,"Text":"Mana\tshalifwamwo\tnohomba shalihonokokhamwo tawe"},{"SurahNumber":87,"VerseNumber":14,"Text":"Toto yamala okhubura ulia wetswenula"},{"SurahNumber":87,"VerseNumber":15,"Text":"Mana niyetsulila elira lia Nyasaye wuwe Omulesi mana nalaama"}]}