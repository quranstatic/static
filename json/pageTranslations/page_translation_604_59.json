{"Id":59,"LanguageCodes":["ber"],"Name":"ber-ramdaneatmansou-la","Author":"Ramdane At Mansour","Language":"Berber","Description":"Berber - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":112,"VerseNumber":1,"Text":"Ini: \"Neppa, Oebbi Yiwen"},{"SurahNumber":112,"VerseNumber":2,"Text":"Oebbi, ur T ippissin yiwen"},{"SurahNumber":112,"VerseNumber":3,"Text":"Ur Iuriw, ur T uriwen"},{"SurahNumber":112,"VerseNumber":4,"Text":"Ur illi, d nned iS, yiwen"},{"SurahNumber":113,"VerseNumber":1,"Text":"Ini: \"ad iyi Iuueo, Mass n wanqao"},{"SurahNumber":113,"VerseNumber":2,"Text":"Si lhemm n wayen Ixelq"},{"SurahNumber":113,"VerseNumber":3,"Text":"si lhemm n tillas ma ssant"},{"SurahNumber":113,"VerseNumber":4,"Text":"si lhemm n tid ipuuvun di tyersiwin"},{"SurahNumber":113,"VerseNumber":5,"Text":"si lhemm um$ecci, ma iusem"},{"SurahNumber":114,"VerseNumber":1,"Text":"Ini: \"ad iyi Iuueo, Mass n yimdanen"},{"SurahNumber":114,"VerseNumber":2,"Text":"Agellid n yimdanen"},{"SurahNumber":114,"VerseNumber":3,"Text":"Oebbi n yimdanen"},{"SurahNumber":114,"VerseNumber":4,"Text":"si lhemm u$way n win ipfezwin"},{"SurahNumber":114,"VerseNumber":5,"Text":"i i$ewwun idmaren n yimdanen"},{"SurahNumber":114,"VerseNumber":6,"Text":"si leonun u seg yimdanen"}]}