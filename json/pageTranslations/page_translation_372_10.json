{"Id":10,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":26,"VerseNumber":112,"Text":"Qala wama AAilmee bima kanoo yaAAmaloona"},{"SurahNumber":26,"VerseNumber":113,"Text":"In hisabuhum illa AAala rabbee law tashAAuroona"},{"SurahNumber":26,"VerseNumber":114,"Text":"Wama ana bitaridi almumineena"},{"SurahNumber":26,"VerseNumber":115,"Text":"In ana illa natheerun mubeenun"},{"SurahNumber":26,"VerseNumber":116,"Text":"Qaloo lain lam tantahi ya noohu latakoonanna mina almarjoomeena"},{"SurahNumber":26,"VerseNumber":117,"Text":"Qala rabbi inna qawmee kaththabooni"},{"SurahNumber":26,"VerseNumber":118,"Text":"Faiftah baynee wabaynahum fathan wanajjinee waman maAAiya mina almumineena"},{"SurahNumber":26,"VerseNumber":119,"Text":"Faanjaynahu waman maAAahu fee alfulki almashhooni"},{"SurahNumber":26,"VerseNumber":120,"Text":"Thumma aghraqna baAAdu albaqeena"},{"SurahNumber":26,"VerseNumber":121,"Text":"Inna fee thalika laayatan wama kana aktharuhum mumineena"},{"SurahNumber":26,"VerseNumber":122,"Text":"Wainna rabbaka lahuwa alAAazeezu alrraheemu"},{"SurahNumber":26,"VerseNumber":123,"Text":"Kaththabat AAadun almursaleena"},{"SurahNumber":26,"VerseNumber":124,"Text":"Ith qala lahum akhoohum hoodun ala tattaqoona"},{"SurahNumber":26,"VerseNumber":125,"Text":"Innee lakum rasoolun ameenun"},{"SurahNumber":26,"VerseNumber":126,"Text":"Faittaqoo Allaha waateeAAooni"},{"SurahNumber":26,"VerseNumber":127,"Text":"Wama asalukum AAalayhi min ajrin in ajriya illa AAala rabbi alAAalameena"},{"SurahNumber":26,"VerseNumber":128,"Text":"Atabnoona bikulli reeAAin ayatan taAAbathoona"},{"SurahNumber":26,"VerseNumber":129,"Text":"Watattakhithoona masaniAAa laAAallakum takhludoona"},{"SurahNumber":26,"VerseNumber":130,"Text":"Waitha batashtum batashtum jabbareena"},{"SurahNumber":26,"VerseNumber":131,"Text":"Faittaqoo Allaha waateeAAooni"},{"SurahNumber":26,"VerseNumber":132,"Text":"Waittaqoo allathee amaddakum bima taAAlamoona"},{"SurahNumber":26,"VerseNumber":133,"Text":"Amaddakum bianAAamin wabaneena"},{"SurahNumber":26,"VerseNumber":134,"Text":"Wajannatin waAAuyoonin"},{"SurahNumber":26,"VerseNumber":135,"Text":"Innee akhafu AAalaykum AAathaba yawmin AAatheemin"},{"SurahNumber":26,"VerseNumber":136,"Text":"Qaloo sawaon AAalayna awaAAathta am lam takun mina alwaAAitheena"}]}