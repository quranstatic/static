{"Id":10,"LanguageCodes":["ar","ara"],"Name":"ara-quran-la","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Transliteration","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":26,"VerseNumber":184,"Text":"Waittaqoo allathee khalaqakum waaljibillata alawwaleena"},{"SurahNumber":26,"VerseNumber":185,"Text":"Qaloo innama anta mina almusahhareena"},{"SurahNumber":26,"VerseNumber":186,"Text":"Wama anta illa basharun mithluna wain nathunnuka lamina alkathibeena"},{"SurahNumber":26,"VerseNumber":187,"Text":"Faasqit AAalayna kisafan mina alssamai in kunta mina alssadiqeena"},{"SurahNumber":26,"VerseNumber":188,"Text":"Qala rabbee aAAlamu bima taAAmaloona"},{"SurahNumber":26,"VerseNumber":189,"Text":"Fakaththaboohu faakhathahum AAathabu yawmi alththullati innahu kana AAathaba yawmin AAatheemin"},{"SurahNumber":26,"VerseNumber":190,"Text":"Inna fee thalika laayatan wama kana aktharuhum mumineena"},{"SurahNumber":26,"VerseNumber":191,"Text":"Wainna rabbaka lahuwa alAAazeezu alrraheemu"},{"SurahNumber":26,"VerseNumber":192,"Text":"Wainnahu latanzeelu rabbi alAAalameena"},{"SurahNumber":26,"VerseNumber":193,"Text":"Nazala bihi alrroohu alameenu"},{"SurahNumber":26,"VerseNumber":194,"Text":"AAala qalbika litakoona mina almunthireena"},{"SurahNumber":26,"VerseNumber":195,"Text":"Bilisanin AAarabiyyin mubeenin"},{"SurahNumber":26,"VerseNumber":196,"Text":"Wainnahu lafee zuburi alawwaleena"},{"SurahNumber":26,"VerseNumber":197,"Text":"Awalam yakun lahum ayatan an yaAAlamahu AAulamao banee israeela"},{"SurahNumber":26,"VerseNumber":198,"Text":"Walaw nazzalnahu AAala baAAdi alaAAjameena"},{"SurahNumber":26,"VerseNumber":199,"Text":"Faqaraahu AAalayhim ma kanoo bihi mumineena"},{"SurahNumber":26,"VerseNumber":200,"Text":"Kathalika salaknahu fee quloobi almujrimeena"},{"SurahNumber":26,"VerseNumber":201,"Text":"La yuminoona bihi hatta yarawoo alAAathaba alaleema"},{"SurahNumber":26,"VerseNumber":202,"Text":"Fayatiyahum baghtatan wahum la yashAAuroona"},{"SurahNumber":26,"VerseNumber":203,"Text":"Fayaqooloo hal nahnu muntharoona"},{"SurahNumber":26,"VerseNumber":204,"Text":"AfabiAAathabina yastaAAjiloona"},{"SurahNumber":26,"VerseNumber":205,"Text":"Afaraayta in mattaAAnahum sineena"},{"SurahNumber":26,"VerseNumber":206,"Text":"Thumma jaahum ma kanoo yooAAadoona"}]}