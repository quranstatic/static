{"Id":359,"LanguageCodes":["te","tel"],"Name":"tel-abdulraheemmoha-la","Author":"Abdul Raheem Mohammad Moulana","Language":"Telugu","Description":"Telugu - Transliteration","Direction":"ltr","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":78,"VerseNumber":1,"Text":"e visayanni gurinci varu (okarinokaru) prasnincukuntunnaru"},{"SurahNumber":78,"VerseNumber":2,"Text":"a maha vartanu gurincena"},{"SurahNumber":78,"VerseNumber":3,"Text":"Denini gurincaite varu bhedabhiprayalanu kaligi unnaro"},{"SurahNumber":78,"VerseNumber":4,"Text":"adi kadu! Varu tvaralone danini telusukogalaru"},{"SurahNumber":78,"VerseNumber":5,"Text":"enta matramu kadu! Varu tvaralone danini telusukogalaru"},{"SurahNumber":78,"VerseNumber":6,"Text":"emi? Memu bhumini parupuga ceyaleda"},{"SurahNumber":78,"VerseNumber":7,"Text":"mariyu parvatalanu mekuluga"},{"SurahNumber":78,"VerseNumber":8,"Text":"mariyu memu mim'malni (stri-purusula) jantaluga srstincamu"},{"SurahNumber":78,"VerseNumber":9,"Text":"mariyu memu nidranu, miku visranti niccediga cesamu"},{"SurahNumber":78,"VerseNumber":10,"Text":"mariyu ratrini acchadanaga cesamu"},{"SurahNumber":78,"VerseNumber":11,"Text":"mariyu pagatini jivanopadhi samayanga cesamu"},{"SurahNumber":78,"VerseNumber":12,"Text":"mariyu memu mipaina patistamaina edu (akasalanu) nirmincamu"},{"SurahNumber":78,"VerseNumber":13,"Text":"mariyu (andulo) prakasince dipanni (suryunni) uncamu"},{"SurahNumber":78,"VerseNumber":14,"Text":"mariyu meghala nundi dharapatanga varsanni kuripincamu"},{"SurahNumber":78,"VerseNumber":15,"Text":"danito memu dhan'yam mariyu paccikanu (cetlu cemalanu) perigincataniki"},{"SurahNumber":78,"VerseNumber":16,"Text":"mariyu dattamaina totalanu"},{"SurahNumber":78,"VerseNumber":17,"Text":"niscayanga, tirpudinam oka nirnita samayam"},{"SurahNumber":78,"VerseNumber":18,"Text":"a roju baka udabadinappudu! Appudu miranta gumpulu gumpuluga leci vastaru"},{"SurahNumber":78,"VerseNumber":19,"Text":"mariyu akasam teruvabadutundi, andulo dvaralu erpadutayi"},{"SurahNumber":78,"VerseNumber":20,"Text":"mariyu parvatalu endamavuluga adrsyamai potayi"},{"SurahNumber":78,"VerseNumber":21,"Text":"niscayanga, narakam oka matu"},{"SurahNumber":78,"VerseNumber":22,"Text":"dhikkarula gamyasthanam"},{"SurahNumber":78,"VerseNumber":23,"Text":"andulo varu yugala tarabadi untaru"},{"SurahNumber":78,"VerseNumber":24,"Text":"andulo varu elanti calladananni gani mariyu (callani) paniyanni gani cavi cudaru"},{"SurahNumber":78,"VerseNumber":25,"Text":"salasala kage niru mariyu cimu lanti muriki (paniyam) tappa"},{"SurahNumber":78,"VerseNumber":26,"Text":"(vari karmalaku) tagina purti pratiphalanga"},{"SurahNumber":78,"VerseNumber":27,"Text":"vastavaniki varu lekka tisukobadutundani asincaledu"},{"SurahNumber":78,"VerseNumber":28,"Text":"paiga varu ma sucanalanu (ayat lanu) asatyalani tiraskarincaru"},{"SurahNumber":78,"VerseNumber":29,"Text":"mariyu memu (varu cesina) pratidanini oka pustakanlo vrasi pettamu"},{"SurahNumber":78,"VerseNumber":30,"Text":"kavuna miru (mi karmala phalitanni) cavi cudandi. Endukante, memu miku siksa tappa maremi adhikam ceyamu"}]}