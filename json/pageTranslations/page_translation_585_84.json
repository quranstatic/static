{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":80,"VerseNumber":1,"Text":"Han (Muhammad) frowned og dreje"},{"SurahNumber":80,"VerseNumber":2,"Text":"Hvornår blænde mand kom ham"},{"SurahNumber":80,"VerseNumber":3,"Text":"Hvordan du kender? Han purify"},{"SurahNumber":80,"VerseNumber":4,"Text":"Han skiftes heed gavn fra meddelelsen"},{"SurahNumber":80,"VerseNumber":5,"Text":"Angående rige manden"},{"SurahNumber":80,"VerseNumber":6,"Text":"Du gav ham Deres opmærksomhed"},{"SurahNumber":80,"VerseNumber":7,"Text":"Jævne skønt du ikke garantere hans frelsning"},{"SurahNumber":80,"VerseNumber":8,"Text":"En kom jer eagerly"},{"SurahNumber":80,"VerseNumber":9,"Text":"Og er virkelig ærværdige"},{"SurahNumber":80,"VerseNumber":10,"Text":"Du tilsidesatte ham"},{"SurahNumber":80,"VerseNumber":11,"Text":"Indeed den er påmindelse"},{"SurahNumber":80,"VerseNumber":12,"Text":"Whoever viljer tage heed"},{"SurahNumber":80,"VerseNumber":13,"Text":"Ind ærværdige scriptures"},{"SurahNumber":80,"VerseNumber":14,"Text":"Exalted pure"},{"SurahNumber":80,"VerseNumber":15,"Text":"(Skrevet) hænderne bude"},{"SurahNumber":80,"VerseNumber":16,"Text":"Som er ærværdige righteous"},{"SurahNumber":80,"VerseNumber":17,"Text":"Ve til menneskelig være han er så unappreciative"},{"SurahNumber":80,"VerseNumber":18,"Text":"hvad Han oprettede ham fra"},{"SurahNumber":80,"VerseNumber":19,"Text":"Fra tiny nedgang Han opretter ham formgive ham"},{"SurahNumber":80,"VerseNumber":20,"Text":"Derefter Han punkt stien ham"},{"SurahNumber":80,"VerseNumber":21,"Text":"Derefter Han lægger ham død into graven"},{"SurahNumber":80,"VerseNumber":22,"Text":"Hvornår Han viljer Han resurrects ham"},{"SurahNumber":80,"VerseNumber":23,"Text":"Han uphold Hans commandments"},{"SurahNumber":80,"VerseNumber":24,"Text":"Lade mennesket overvejer hans mad"},{"SurahNumber":80,"VerseNumber":25,"Text":"Vi hælder vandet storsindet"},{"SurahNumber":80,"VerseNumber":26,"Text":"Derefter vi splittede jorden åbne"},{"SurahNumber":80,"VerseNumber":27,"Text":"Vi vokser det korn"},{"SurahNumber":80,"VerseNumber":28,"Text":"drue græsgang"},{"SurahNumber":80,"VerseNumber":29,"Text":"Olivener håndflader"},{"SurahNumber":80,"VerseNumber":30,"Text":"Mangfoldighed frugthaver"},{"SurahNumber":80,"VerseNumber":31,"Text":"Frugter grønsager"},{"SurahNumber":80,"VerseNumber":32,"Text":"Til forsyne liv understøttelse I Deres dyr"},{"SurahNumber":80,"VerseNumber":33,"Text":"Derefter hvornår slaget kommer vedtage"},{"SurahNumber":80,"VerseNumber":34,"Text":"Den er dagen hvornår en flees hans boder"},{"SurahNumber":80,"VerseNumber":35,"Text":"Fra hans mor og far"},{"SurahNumber":80,"VerseNumber":36,"Text":"Fra hans ægtefælle børn"},{"SurahNumber":80,"VerseNumber":37,"Text":"Hvert en af dem på den dag bekymre omkring hans egen destiny"},{"SurahNumber":80,"VerseNumber":38,"Text":"visse ansigter den dag være glade"},{"SurahNumber":80,"VerseNumber":39,"Text":"le lykkelige"},{"SurahNumber":80,"VerseNumber":40,"Text":"Øvrige ansigt at dag bedækkes elendighed"},{"SurahNumber":80,"VerseNumber":41,"Text":"Overvældede samvittighedsnag"},{"SurahNumber":80,"VerseNumber":42,"Text":"De er wicked tvivlerne"}]}