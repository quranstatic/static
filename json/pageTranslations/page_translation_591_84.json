{"Id":84,"LanguageCodes":["da","dan"],"Name":"dan-hadiabdollahian","Author":"Hadi Abdollahian","Language":"Danish","Description":"Danish","Direction":"ltr","Source":"https://www.alqurantranslation.com/ , Contact:+989354664487","Translations":[{"SurahNumber":86,"VerseNumber":1,"Text":"Ved himlen Al-Taareq"},{"SurahNumber":86,"VerseNumber":2,"Text":"Du kender hvad Al-Taareq gøre være"},{"SurahNumber":86,"VerseNumber":3,"Text":"Lyse stjernen"},{"SurahNumber":86,"VerseNumber":4,"Text":"Absolut enhver godt bevogtes"},{"SurahNumber":86,"VerseNumber":5,"Text":"lade menneskelige reflektere hans anlæggelse"},{"SurahNumber":86,"VerseNumber":6,"Text":"Han oprettedes fra udstødte flydende"},{"SurahNumber":86,"VerseNumber":7,"Text":"Fra mellem rygraden viscera"},{"SurahNumber":86,"VerseNumber":8,"Text":"Han er certainly able til resurrect ham"},{"SurahNumber":86,"VerseNumber":9,"Text":"Dagen al hemmeligheder bliver kendt"},{"SurahNumber":86,"VerseNumber":10,"Text":"Han ikke have magt nor hjælper"},{"SurahNumber":86,"VerseNumber":11,"Text":"Ved himlen returnerer (vandet)"},{"SurahNumber":86,"VerseNumber":12,"Text":"Ved jorden revner vokser planter"},{"SurahNumber":86,"VerseNumber":13,"Text":"Den er seriøsee narration"},{"SurahNumber":86,"VerseNumber":14,"Text":"Ikke til tages lightly"},{"SurahNumber":86,"VerseNumber":15,"Text":"De plot scheme"},{"SurahNumber":86,"VerseNumber":16,"Text":"Men så gøre jeg"},{"SurahNumber":86,"VerseNumber":17,"Text":"Netop respit tvivlerne korte respit"},{"SurahNumber":87,"VerseNumber":1,"Text":"Forherlige navnet Deres Lord mest Højeste"},{"SurahNumber":87,"VerseNumber":2,"Text":"Han opretter forme"},{"SurahNumber":87,"VerseNumber":3,"Text":"Han formgiver guides"},{"SurahNumber":87,"VerseNumber":4,"Text":"Han producerer græsgangen"},{"SurahNumber":87,"VerseNumber":5,"Text":"Derefter drejer det lyse hø"},{"SurahNumber":87,"VerseNumber":6,"Text":"Vi recitere jer; ikke glemme"},{"SurahNumber":87,"VerseNumber":7,"Text":"Alt er ind overensstemmelse med gud ville Han kender hvad erklæres hvad skjules"},{"SurahNumber":87,"VerseNumber":8,"Text":"Vi forestå jer lettest stien"},{"SurahNumber":87,"VerseNumber":9,"Text":"Derfor du påminde; måske påmindelsen gavne"},{"SurahNumber":87,"VerseNumber":10,"Text":"Ærværdige viljen tager heed"},{"SurahNumber":87,"VerseNumber":11,"Text":"wicked viljen undlader det"},{"SurahNumber":87,"VerseNumber":12,"Text":"Følgelig han lide great Hellfire"},{"SurahNumber":87,"VerseNumber":13,"Text":"Hvori han aldrig omkommer nor ophold alive"},{"SurahNumber":87,"VerseNumber":14,"Text":"Succesrige indeed er en indfrir hans sjælen"},{"SurahNumber":87,"VerseNumber":15,"Text":"Ved mindes navnet hans Lord observing kontakten bønner (Salat)"}]}